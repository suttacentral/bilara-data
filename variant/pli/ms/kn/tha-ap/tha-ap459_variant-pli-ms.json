  ],
  "tha-ap459:6.4": [
    "sarīrassāsukhepanā → sarīrakāyukhepanā (s2, s3) | sarīrassānukhepanā (pts1)"
  ],
  "tha-ap459:7.1": [
    "vihanantaṃ → vihanati (s2, s3, mr) | viharanti (pts1)"
  ],
  "tha-ap459:7.4": [
    "phassayissāmi → passayissāmi (mr)"
  ]
