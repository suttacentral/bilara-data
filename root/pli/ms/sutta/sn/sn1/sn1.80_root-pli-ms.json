{
  "sn1.80:0.1": "Saṁyutta Nikāya 1.80 ",
  "sn1.80:0.2": "8. Chetvāvagga ",
  "sn1.80:0.3": "Pajjotasutta ",
  "sn1.80:1.1": "“Kiṁsu lokasmi pajjoto, ",
  "sn1.80:1.2": "kiṁsu lokasmi jāgaro; ",
  "sn1.80:1.3": "Kiṁsu kamme sajīvānaṁ, ",
  "sn1.80:1.4": "kimassa iriyāpatho. ",
  "sn1.80:2.1": "Kiṁsu alasaṁ analasañca, ",
  "sn1.80:2.2": "Mātā puttaṁva posati; ",
  "sn1.80:2.3": "Kiṁ bhūtā upajīvanti, ",
  "sn1.80:2.4": "Ye pāṇā pathavissitā”ti. ",
  "sn1.80:3.1": "“Paññā lokasmi pajjoto, ",
  "sn1.80:3.2": "sati lokasmi jāgaro; ",
  "sn1.80:3.3": "Gāvo kamme sajīvānaṁ, ",
  "sn1.80:3.4": "sītassa iriyāpatho. ",
  "sn1.80:4.1": "Vuṭṭhi alasaṁ analasañca, ",
  "sn1.80:4.2": "Mātā puttaṁva posati; ",
  "sn1.80:4.3": "Vuṭṭhiṁ bhūtā upajīvanti, ",
  "sn1.80:4.4": "Ye pāṇā pathavissitā”ti. "
}