{
  "sn1.77:0.1": "Saṁyutta Nikāya 1.77 ",
  "sn1.77:0.2": "8. Chetvāvagga ",
  "sn1.77:0.3": "Issariyasutta ",
  "sn1.77:1.1": "“Kiṁsu issariyaṁ loke, ",
  "sn1.77:1.2": "kiṁsu bhaṇḍānamuttamaṁ; ",
  "sn1.77:1.3": "Kiṁsu satthamalaṁ loke, ",
  "sn1.77:1.4": "kiṁsu lokasmimabbudaṁ. ",
  "sn1.77:2.1": "Kiṁsu harantaṁ vārenti, ",
  "sn1.77:2.2": "haranto pana ko piyo; ",
  "sn1.77:2.3": "Kiṁsu punappunāyantaṁ, ",
  "sn1.77:2.4": "abhinandanti paṇḍitā”ti. ",
  "sn1.77:3.1": "“Vaso issariyaṁ loke, ",
  "sn1.77:3.2": "itthī bhaṇḍānamuttamaṁ; ",
  "sn1.77:3.3": "Kodho satthamalaṁ loke, ",
  "sn1.77:3.4": "corā lokasmimabbudā. ",
  "sn1.77:4.1": "Coraṁ harantaṁ vārenti, ",
  "sn1.77:4.2": "haranto samaṇo piyo; ",
  "sn1.77:4.3": "Samaṇaṁ punappunāyantaṁ, ",
  "sn1.77:4.4": "abhinandanti paṇḍitā”ti. "
}