{
  "sn1.29:0.1": "Saṁyutta Nikāya 1.29 ",
  "sn1.29:0.2": "3. Sattivagga ",
  "sn1.29:0.3": "Catucakkasutta ",
  "sn1.29:1.1": "“Catucakkaṁ navadvāraṁ, ",
  "sn1.29:1.2": "puṇṇaṁ lobhena saṁyutaṁ; ",
  "sn1.29:1.3": "Paṅkajātaṁ mahāvīra, ",
  "sn1.29:1.4": "kathaṁ yātrā bhavissatī”ti. ",
  "sn1.29:2.1": "“Chetvā naddhiṁ varattañca, ",
  "sn1.29:2.2": "icchā lobhañca pāpakaṁ; ",
  "sn1.29:2.3": "Samūlaṁ taṇhamabbuyha, ",
  "sn1.29:2.4": "evaṁ yātrā bhavissatī”ti. "
}