{
  "sn1.74:0.1": "Saṁyutta Nikāya 1.74 ",
  "sn1.74:0.2": "8. Chetvāvagga ",
  "sn1.74:0.3": "Vuṭṭhisutta ",
  "sn1.74:1.1": "“Kiṁsu uppatataṁ seṭṭhaṁ, ",
  "sn1.74:1.2": "kiṁsu nipatataṁ varaṁ; ",
  "sn1.74:1.3": "Kiṁsu pavajamānānaṁ, ",
  "sn1.74:1.4": "kiṁsu pavadataṁ varan”ti. ",
  "sn1.74:2.1": "“Bījaṁ uppatataṁ seṭṭhaṁ, ",
  "sn1.74:2.2": "vuṭṭhi nipatataṁ varā; ",
  "sn1.74:2.3": "Gāvo pavajamānānaṁ, ",
  "sn1.74:2.4": "putto pavadataṁ varo”ti. ",
  "sn1.74:3.1": "“Vijjā uppatataṁ seṭṭhā, ",
  "sn1.74:3.2": "avijjā nipatataṁ varā; ",
  "sn1.74:3.3": "Saṅgho pavajamānānaṁ, ",
  "sn1.74:3.4": "buddho pavadataṁ varo”ti. "
}