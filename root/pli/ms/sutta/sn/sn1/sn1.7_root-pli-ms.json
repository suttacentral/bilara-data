{
  "sn1.7:0.1": "Saṁyutta Nikāya 1.7 ",
  "sn1.7:0.2": "1. Naḷavagga ",
  "sn1.7:0.3": "Appaṭividitasutta ",
  "sn1.7:1.1": "Sāvatthinidānaṁ. ",
  "sn1.7:1.2": "Ekamantaṁ ṭhitā kho sā devatā bhagavato santike imaṁ gāthaṁ abhāsi: ",
  "sn1.7:2.1": "“Yesaṁ dhammā appaṭividitā, ",
  "sn1.7:2.2": "Paravādesu nīyare; ",
  "sn1.7:2.3": "Suttā te nappabujjhanti, ",
  "sn1.7:2.4": "Kālo tesaṁ pabujjhitun”ti. ",
  "sn1.7:3.1": "“Yesaṁ dhammā suppaṭividitā, ",
  "sn1.7:3.2": "Paravādesu na nīyare; ",
  "sn1.7:3.3": "Te sambuddhā sammadaññā, ",
  "sn1.7:3.4": "Caranti visame saman”ti. "
}