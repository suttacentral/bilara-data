{
  "sn1.22:0.1": "Saṁyutta Nikāya 1.22 ",
  "sn1.22:0.2": "3. Sattivagga ",
  "sn1.22:0.3": "Phusatisutta ",
  "sn1.22:1.1": "“Nāphusantaṁ phusati ca, ",
  "sn1.22:1.2": "phusantañca tato phuse; ",
  "sn1.22:1.3": "Tasmā phusantaṁ phusati, ",
  "sn1.22:1.4": "appaduṭṭhapadosinan”ti. ",
  "sn1.22:2.1": "“Yo appaduṭṭhassa narassa dussati, ",
  "sn1.22:2.2": "Suddhassa posassa anaṅgaṇassa; ",
  "sn1.22:2.3": "Tameva bālaṁ pacceti pāpaṁ, ",
  "sn1.22:2.4": "Sukhumo rajo paṭivātaṁva khitto”ti. "
}