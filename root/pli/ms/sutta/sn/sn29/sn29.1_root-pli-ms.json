{
  "sn29.1:0.1": "Saṁyutta Nikāya 29.1 ",
  "sn29.1:0.2": "1. Nāgavagga ",
  "sn29.1:0.3": "Suddhikasutta ",
  "sn29.1:1.1": "Sāvatthinidānaṁ. ",
  "sn29.1:1.2": "“Catasso imā, bhikkhave, nāgayoniyo. ",
  "sn29.1:1.3": "Katamā catasso? ",
  "sn29.1:1.4": "Aṇḍajā nāgā, jalābujā nāgā, saṁsedajā nāgā, opapātikā nāgā—",
  "sn29.1:1.5": "imā kho, bhikkhave, catasso nāgayoniyo”ti. ",
  "sn29.1:1.6": "Paṭhamaṁ. "
}