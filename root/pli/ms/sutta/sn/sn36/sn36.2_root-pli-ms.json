{
  "sn36.2:0.1": "Saṁyutta Nikāya 36.2 ",
  "sn36.2:0.2": "1. Sagāthāvagga ",
  "sn36.2:0.3": "Sukhasutta ",
  "sn36.2:1.1": "“Tisso imā, bhikkhave, vedanā. ",
  "sn36.2:1.2": "Katamā tisso? ",
  "sn36.2:1.3": "Sukhā vedanā, dukkhā vedanā, adukkhamasukhā vedanā—",
  "sn36.2:1.4": "imā kho, bhikkhave, tisso vedanāti. ",
  "sn36.2:2.1": "Sukhaṁ vā yadi vā dukkhaṁ, ",
  "sn36.2:2.2": "adukkhamasukhaṁ saha; ",
  "sn36.2:2.3": "Ajjhattañca bahiddhā ca, ",
  "sn36.2:2.4": "yaṁ kiñci atthi veditaṁ. ",
  "sn36.2:3.1": "Etaṁ dukkhanti ñatvāna, ",
  "sn36.2:3.2": "mosadhammaṁ palokinaṁ; ",
  "sn36.2:3.3": "Phussa phussa vayaṁ passaṁ, ",
  "sn36.2:3.4": "evaṁ tattha virajjatī”ti. ",
  "sn36.2:3.5": "Dutiyaṁ. "
}