{
  "bv10:0.1": "Buddhavaṁsa",
  "bv10:0.2": "10. Padumabuddhavaṁsa",
  "bv10:1.1": "“Anomadassissa aparena,",
  "bv10:1.2": "Sambuddho dvipaduttamo;",
  "bv10:1.3": "Padumo nāma nāmena,",
  "bv10:1.4": "Asamo appaṭipuggalo.",
  "bv10:2.1": "Tassāpi asamaṁ sīlaṁ,",
  "bv10:2.2": "samādhipi anantako;",
  "bv10:2.3": "Asaṅkheyyaṁ ñāṇavaraṁ,",
  "bv10:2.4": "vimuttipi anūpamā.",
  "bv10:3.1": "Tassāpi atulatejassa,",
  "bv10:3.2": "dhammacakkappavattane;",
  "bv10:3.3": "Abhisamayā tayo āsuṁ,",
  "bv10:3.4": "mahātamapavāhanā.",
  "bv10:4.1": "Paṭhamābhisamaye buddho,",
  "bv10:4.2": "koṭisatamabodhayi;",
  "bv10:4.3": "Dutiyābhisamaye dhīro,",
  "bv10:4.4": "navutikoṭimabodhayi.",
  "bv10:5.1": "Yadā ca padumo buddho,",
  "bv10:5.2": "ovadī sakamatrajaṁ;",
  "bv10:5.3": "Tadā asītikoṭīnaṁ,",
  "bv10:5.4": "tatiyābhisamayo ahu.",
  "bv10:6.1": "Sannipātā tayo āsuṁ,",
  "bv10:6.2": "padumassa mahesino;",
  "bv10:6.3": "Koṭisatasahassānaṁ,",
  "bv10:6.4": "paṭhamo āsi samāgamo.",
  "bv10:7.1": "Kathinatthārasamaye,",
  "bv10:7.2": "uppanne kathinacīvare;",
  "bv10:7.3": "Dhammasenāpatitthāya,",
  "bv10:7.4": "bhikkhū sibbiṁsu cīvaraṁ.",
  "bv10:8.1": "Tadā te vimalā bhikkhū,",
  "bv10:8.2": "chaḷabhiññā mahiddhikā;",
  "bv10:8.3": "Tīṇi satasahassāni,",
  "bv10:8.4": "samiṁsu aparājitā.",
  "bv10:9.1": "Punāparaṁ so narāsabho,",
  "bv10:9.2": "pavane vāsaṁ upāgami;",
  "bv10:9.3": "Tadā samāgamo āsi,",
  "bv10:9.4": "dvinnaṁ satasahassinaṁ.",
  "bv10:10.1": "Ahaṁ tena samayena,",
  "bv10:10.2": "sīho āsiṁ migādhibhū;",
  "bv10:10.3": "Vivekamanubrūhantaṁ,",
  "bv10:10.4": "pavane addasaṁ jinaṁ.",
  "bv10:11.1": "Vanditvā sirasā pāde,",
  "bv10:11.2": "katvāna taṁ padakkhiṇaṁ;",
  "bv10:11.3": "Tikkhattuṁ abhināditvā,",
  "bv10:11.4": "sattāhaṁ jinamupaṭṭhahaṁ.",
  "bv10:12.1": "Sattāhaṁ varasamāpattiyā,",
  "bv10:12.2": "Vuṭṭhahitvā tathāgato;",
  "bv10:12.3": "Manasā cintayitvāna,",
  "bv10:12.4": "Koṭibhikkhū samānayi.",
  "bv10:13.1": "Tadāpi so mahāvīro,",
  "bv10:13.2": "tesaṁ majjhe viyākari;",
  "bv10:13.3": "‘Aparimeyyito kappe,",
  "bv10:13.4": "ayaṁ buddho bhavissati.",
  "bv10:14.1": "Padhānaṁ padahitvāna,",
  "bv10:14.2": "…pe…",
  "bv10:14.3": "hessāma sammukhā imaṁ’.",
  "bv10:15.1": "Tassāpi vacanaṁ sutvā,",
  "bv10:15.2": "bhiyyo cittaṁ pasādayiṁ;",
  "bv10:15.3": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv10:15.4": "dasapāramipūriyā.",
  "bv10:16.1": "Campakaṁ nāma nagaraṁ,",
  "bv10:16.2": "asamo nāma khattiyo;",
  "bv10:16.3": "Asamā nāma janikā,",
  "bv10:16.4": "padumassa mahesino.",
  "bv10:17.1": "Dasavassasahassāni,",
  "bv10:17.2": "agāraṁ ajjha so vasi;",
  "bv10:17.3": "Nandā-vasu-yasuttarā,",
  "bv10:17.4": "tayo pāsādamuttamā.",
  "bv10:18.1": "Tettiṁsa ca sahassāni,",
  "bv10:18.2": "nāriyo samalaṅkatā;",
  "bv10:18.3": "Uttarā nāma sā nārī,",
  "bv10:18.4": "rammo nāmāsi atrajo.",
  "bv10:19.1": "Nimitte caturo disvā,",
  "bv10:19.2": "rathayānena nikkhami;",
  "bv10:19.3": "Anūnaaṭṭhamāsāni,",
  "bv10:19.4": "padhānaṁ padahī jino.",
  "bv10:20.1": "Brahmunā yācito santo,",
  "bv10:20.2": "padumo lokanāyako;",
  "bv10:20.3": "Vatti cakkaṁ mahāvīro,",
  "bv10:20.4": "dhanañcuyyānamuttame.",
  "bv10:21.1": "Sālo ca upasālo ca,",
  "bv10:21.2": "ahesuṁ aggasāvakā;",
  "bv10:21.3": "Varuṇo nāmupaṭṭhāko,",
  "bv10:21.4": "padumassa mahesino.",
  "bv10:22.1": "Rādhā ceva surādhā ca,",
  "bv10:22.2": "ahesuṁ aggasāvikā;",
  "bv10:22.3": "Bodhi tassa bhagavato,",
  "bv10:22.4": "mahāsoṇoti vuccati.",
  "bv10:23.1": "Bhiyyo ceva asamo ca,",
  "bv10:23.2": "ahesuṁ aggupaṭṭhakā;",
  "bv10:23.3": "Rucī ca nandarāmā ca,",
  "bv10:23.4": "ahesuṁ aggupaṭṭhikā.",
  "bv10:24.1": "Aṭṭhapaṇṇāsaratanaṁ,",
  "bv10:24.2": "accuggato mahāmuni;",
  "bv10:24.3": "Pabhā niddhāvatī tassa,",
  "bv10:24.4": "asamā sabbaso disā.",
  "bv10:25.1": "Candappabhā sūriyappabhā,",
  "bv10:25.2": "ratanaggimaṇippabhā;",
  "bv10:25.3": "Sabbāpi tā hatā honti,",
  "bv10:25.4": "patvā jinapabhuttamaṁ.",
  "bv10:26.1": "Vassasatasahassāni,",
  "bv10:26.2": "āyu vijjati tāvade;",
  "bv10:26.3": "Tāvatā tiṭṭhamāno so,",
  "bv10:26.4": "tāresi janataṁ bahuṁ.",
  "bv10:27.1": "Paripakkamānase satte,",
  "bv10:27.2": "bodhayitvā asesato;",
  "bv10:27.3": "Sesake anusāsitvā,",
  "bv10:27.4": "nibbuto so sasāvako.",
  "bv10:28.1": "Uragova tacaṁ jiṇṇaṁ,",
  "bv10:28.2": "vaddhapattaṁva pādapo;",
  "bv10:28.3": "Jahitvā sabbasaṅkhāre,",
  "bv10:28.4": "nibbuto so yathā sikhī.",
  "bv10:29.1": "Padumo jinavaro satthā,",
  "bv10:29.2": "Dhammārāmamhi nibbuto;",
  "bv10:29.3": "Dhātuvitthārikaṁ āsi,",
  "bv10:29.4": "Tesu tesu padesato”ti. ",
  "bv10:30.1": "Padumassa bhagavato vaṁso aṭṭhamo."
}