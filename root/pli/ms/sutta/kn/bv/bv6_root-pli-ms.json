{
  "bv6:0.1": "Buddhavaṁsa",
  "bv6:0.2": "6. Sumanabuddhavaṁsa",
  "bv6:1.1": "“Maṅgalassa aparena,",
  "bv6:1.2": "sumano nāma nāyako;",
  "bv6:1.3": "Sabbadhammehi asamo,",
  "bv6:1.4": "sabbasattānamuttamo.",
  "bv6:2.1": "Tadā amatabheriṁ so,",
  "bv6:2.2": "āhanī mekhale pure;",
  "bv6:2.3": "Dhammasaṅkhasamāyuttaṁ,",
  "bv6:2.4": "navaṅgaṁ jinasāsanaṁ.",
  "bv6:3.1": "Nijjinitvā kilese so,",
  "bv6:3.2": "patvā sambodhimuttamaṁ;",
  "bv6:3.3": "Māpesi nagaraṁ satthā,",
  "bv6:3.4": "saddhammapuravaruttamaṁ.",
  "bv6:4.1": "Nirantaraṁ akuṭilaṁ,",
  "bv6:4.2": "ujuṁ vipulavitthataṁ;",
  "bv6:4.3": "Māpesi so mahāvīthiṁ,",
  "bv6:4.4": "satipaṭṭhānavaruttamaṁ.",
  "bv6:5.1": "Phale cattāri sāmaññe,",
  "bv6:5.2": "catasso paṭisambhidā;",
  "bv6:5.3": "Chaḷabhiññāṭṭhasamāpattī,",
  "bv6:5.4": "pasāresi tattha vīthiyaṁ.",
  "bv6:6.1": "Ye appamattā akhilā,",
  "bv6:6.2": "hirivīriyehupāgatā;",
  "bv6:6.3": "Te te ime guṇavare,",
  "bv6:6.4": "ādiyanti yathā sukhaṁ.",
  "bv6:7.1": "Evametena yogena,",
  "bv6:7.2": "uddharanto mahājanaṁ;",
  "bv6:7.3": "Bodhesi paṭhamaṁ satthā,",
  "bv6:7.4": "koṭisatasahassiyo.",
  "bv6:8.1": "Yamhi kāle mahāvīro,",
  "bv6:8.2": "ovadī titthiye gaṇe;",
  "bv6:8.3": "Koṭisahassābhisamiṁsu,",
  "bv6:8.4": "dutiye dhammadesane.",
  "bv6:9.1": "Yadā devā manussā ca,",
  "bv6:9.2": "samaggā ekamānasā;",
  "bv6:9.3": "Nirodhapañhaṁ pucchiṁsu,",
  "bv6:9.4": "saṁsayañcāpi mānasaṁ.",
  "bv6:10.1": "Tadāpi dhammadesane,",
  "bv6:10.2": "nirodhaparidīpane;",
  "bv6:10.3": "Navutikoṭisahassānaṁ,",
  "bv6:10.4": "tatiyābhisamayo ahu.",
  "bv6:11.1": "Sannipātā tayo āsuṁ,",
  "bv6:11.2": "sumanassa mahesino;",
  "bv6:11.3": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv6:11.4": "santacittāna tādinaṁ.",
  "bv6:12.1": "Vassaṁvutthassa bhagavato,",
  "bv6:12.2": "abhighuṭṭhe pavāraṇe;",
  "bv6:12.3": "Koṭisatasahassehi,",
  "bv6:12.4": "pavāresi tathāgato.",
  "bv6:13.1": "Tatoparaṁ sannipāte,",
  "bv6:13.2": "vimale kañcanapabbate;",
  "bv6:13.3": "Navutikoṭisahassānaṁ,",
  "bv6:13.4": "dutiyo āsi samāgamo.",
  "bv6:14.1": "Yadā sakko devarājā,",
  "bv6:14.2": "buddhadassanupāgami;",
  "bv6:14.3": "Asītikoṭisahassānaṁ,",
  "bv6:14.4": "tatiyo āsi samāgamo.",
  "bv6:15.1": "Ahaṁ tena samayena,",
  "bv6:15.2": "nāgarājā mahiddhiko;",
  "bv6:15.3": "Atulo nāma nāmena,",
  "bv6:15.4": "ussannakusalasañcayo.",
  "bv6:16.1": "Tadāhaṁ nāgabhavanā,",
  "bv6:16.2": "nikkhamitvā sañātibhi;",
  "bv6:16.3": "Nāgānaṁ dibbaturiyehi,",
  "bv6:16.4": "sasaṅghaṁ jinamupaṭṭhahiṁ.",
  "bv6:17.1": "Koṭisatasahassānaṁ,",
  "bv6:17.2": "annapānena tappayiṁ;",
  "bv6:17.3": "Paccekadussayugaṁ datvā,",
  "bv6:17.4": "saraṇaṁ tamupāgamiṁ.",
  "bv6:18.1": "Sopi maṁ buddho byākāsi,",
  "bv6:18.2": "sumano lokanāyako;",
  "bv6:18.3": "‘Aparimeyyito kappe,",
  "bv6:18.4": "ayaṁ buddho bhavissati.",
  "bv6:19.1": "Padhānaṁ padahitvāna,",
  "bv6:19.2": "…pe…",
  "bv6:19.3": "hessāma sammukhā imaṁ’.",
  "bv6:20.1": "Tassāpi vacanaṁ sutvā,",
  "bv6:20.2": "bhiyyo cittaṁ pasādayiṁ;",
  "bv6:20.3": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv6:20.4": "dasapāramipūriyā.",
  "bv6:21.1": "Nagaraṁ mekhalaṁ nāma,",
  "bv6:21.2": "sudatto nāma khattiyo;",
  "bv6:21.3": "Sirimā nāma janikā,",
  "bv6:21.4": "sumanassa mahesino.",
  "bv6:22.1": "Navavassasahassāni,",
  "bv6:22.2": "agāraṁ ajjha so vasi;",
  "bv6:22.3": "Cando sucando vaṭaṁso ca,",
  "bv6:22.4": "tayo pāsādamuttamā.",
  "bv6:23.1": "Tesaṭṭhisatasahassāni,",
  "bv6:23.2": "nāriyo samalaṅkatā;",
  "bv6:23.3": "Vaṭaṁsikā nāma nārī,",
  "bv6:23.4": "anūpamo nāma atrajo.",
  "bv6:24.1": "Nimitte caturo disvā,",
  "bv6:24.2": "hatthiyānena nikkhami;",
  "bv6:24.3": "Anūnadasamāsāni,",
  "bv6:24.4": "padhānaṁ padahī jino.",
  "bv6:25.1": "Brahmunā yācito santo,",
  "bv6:25.2": "sumano lokanāyako;",
  "bv6:25.3": "Vatti cakkaṁ mahāvīro,",
  "bv6:25.4": "mekhale puramuttame.",
  "bv6:26.1": "Saraṇo bhāvitatto ca,",
  "bv6:26.2": "ahesuṁ aggasāvakā;",
  "bv6:26.3": "Udeno nāmupaṭṭhāko,",
  "bv6:26.4": "sumanassa mahesino.",
  "bv6:27.1": "Soṇā ca upasoṇā ca,",
  "bv6:27.2": "ahesuṁ aggasāvikā;",
  "bv6:27.3": "Sopi buddho amitayaso,",
  "bv6:27.4": "nāgamūle abujjhatha.",
  "bv6:28.1": "Varuṇo ceva saraṇo ca,",
  "bv6:28.2": "ahesuṁ aggupaṭṭhakā;",
  "bv6:28.3": "Cālā ca upacālā ca,",
  "bv6:28.4": "ahesuṁ aggupaṭṭhikā.",
  "bv6:29.1": "Uccattanena so buddho,",
  "bv6:29.2": "navutihatthamuggato;",
  "bv6:29.3": "Kañcanagghiyasaṅkāso,",
  "bv6:29.4": "dasasahassī virocati.",
  "bv6:30.1": "Navutivassasahassāni,",
  "bv6:30.2": "āyu vijjati tāvade;",
  "bv6:30.3": "Tāvatā tiṭṭhamāno so,",
  "bv6:30.4": "tāresi janataṁ bahuṁ.",
  "bv6:31.1": "Tāraṇīye tārayitvā,",
  "bv6:31.2": "bodhanīye ca bodhayi;",
  "bv6:31.3": "Parinibbāyi sambuddho,",
  "bv6:31.4": "uḷurājāva atthami.",
  "bv6:32.1": "Te ca khīṇāsavā bhikkhū,",
  "bv6:32.2": "so ca buddho asādiso;",
  "bv6:32.3": "Atulappabhaṁ dassayitvā,",
  "bv6:32.4": "nibbutā ye mahāyasā.",
  "bv6:33.1": "Tañca ñāṇaṁ atuliyaṁ,",
  "bv6:33.2": "Tāni ca atulāni ratanāni;",
  "bv6:33.3": "Sabbaṁ tamantarahitaṁ,",
  "bv6:33.4": "Nanu rittā sabbasaṅkhārā.",
  "bv6:34.1": "Sumano yasadharo buddho,",
  "bv6:34.2": "Aṅgārāmamhi nibbuto;",
  "bv6:34.3": "Tattheva tassa jinathūpo,",
  "bv6:34.4": "Catuyojanamuggato”ti. ",
  "bv6:35.1": "Sumanassa bhagavato vaṁso catuttho."
}