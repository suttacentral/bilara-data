{
  "bv28:0.1": "Buddhavaṁsa",
  "bv28:0.2": "28. Buddhapakiṇṇakakaṇḍa",
  "bv28:1.1": "Aparimeyyito kappe,",
  "bv28:1.2": "caturo āsuṁ vināyakā;",
  "bv28:1.3": "Taṇhaṅkaro medhaṅkaro,",
  "bv28:1.4": "athopi saraṇaṅkaro;",
  "bv28:1.5": "Dīpaṅkaro ca sambuddho,",
  "bv28:1.6": "ekakappamhi te jinā.",
  "bv28:2.1": "Dīpaṅkarassa aparena,",
  "bv28:2.2": "koṇḍañño nāma nāyako;",
  "bv28:2.3": "Ekova ekakappamhi,",
  "bv28:2.4": "tāresi janataṁ bahuṁ.",
  "bv28:3.1": "Dīpaṅkarassa bhagavato,",
  "bv28:3.2": "koṇḍaññassa ca satthuno;",
  "bv28:3.3": "Etesaṁ antarā kappā,",
  "bv28:3.4": "gaṇanāto asaṅkhiyā.",
  "bv28:4.1": "Koṇḍaññassa aparena,",
  "bv28:4.2": "maṅgalo nāma nāyako;",
  "bv28:4.3": "Tesampi antarā kappā,",
  "bv28:4.4": "gaṇanāto asaṅkhiyā.",
  "bv28:5.1": "Maṅgalo ca sumano ca,",
  "bv28:5.2": "revato sobhito muni;",
  "bv28:5.3": "Tepi buddhā ekakappe,",
  "bv28:5.4": "cakkhumanto pabhaṅkarā.",
  "bv28:6.1": "Sobhitassa aparena,",
  "bv28:6.2": "anomadassī mahāyaso;",
  "bv28:6.3": "Tesampi antarā kappā,",
  "bv28:6.4": "gaṇanāto asaṅkhiyā.",
  "bv28:7.1": "Anomadassī padumo,",
  "bv28:7.2": "nārado cāpi nāyako;",
  "bv28:7.3": "Tepi buddhā ekakappe,",
  "bv28:7.4": "tamantakārakā munī.",
  "bv28:8.1": "Nāradassa aparena,",
  "bv28:8.2": "Padumuttaro nāma nāyako;",
  "bv28:8.3": "Ekakappamhi uppanno,",
  "bv28:8.4": "Tāresi janataṁ bahuṁ.",
  "bv28:9.1": "Nāradassa bhagavato,",
  "bv28:9.2": "padumuttarassa satthuno;",
  "bv28:9.3": "Tesampi antarā kappā,",
  "bv28:9.4": "gaṇanāto asaṅkhiyā.",
  "bv28:10.1": "Kappasatasahassamhi,",
  "bv28:10.2": "eko āsi mahāmuni;",
  "bv28:10.3": "Padumuttaro lokavidū,",
  "bv28:10.4": "āhutīnaṁ paṭiggaho.",
  "bv28:11.1": "Tiṁsakappasahassamhi,",
  "bv28:11.2": "duve āsuṁ vināyakā;",
  "bv28:11.3": "Sumedho ca sujāto ca,",
  "bv28:11.4": "orato padumuttarā.",
  "bv28:12.1": "Aṭṭhārase kappasate,",
  "bv28:12.2": "tayo āsuṁ vināyakā;",
  "bv28:12.3": "Piyadassī atthadassī,",
  "bv28:12.4": "dhammadassī ca nāyakā.",
  "bv28:13.1": "Orato ca sujātassa,",
  "bv28:13.2": "sambuddhā dvipaduttamā;",
  "bv28:13.3": "Ekakappamhi te buddhā,",
  "bv28:13.4": "loke appaṭipuggalā.",
  "bv28:14.1": "Catunnavutito kappe,",
  "bv28:14.2": "eko āsi mahāmuni;",
  "bv28:14.3": "Siddhattho so lokavidū,",
  "bv28:14.4": "sallakatto anuttaro.",
  "bv28:15.1": "Dvenavute ito kappe,",
  "bv28:15.2": "duve āsuṁ vināyakā;",
  "bv28:15.3": "Tisso phusso ca sambuddhā,",
  "bv28:15.4": "asamā appaṭipuggalā.",
  "bv28:16.1": "Ekanavutito kappe,",
  "bv28:16.2": "vipassī nāma nāyako;",
  "bv28:16.3": "Sopi buddho kāruṇiko,",
  "bv28:16.4": "satte mocesi bandhanā.",
  "bv28:17.1": "Ekatiṁse ito kappe,",
  "bv28:17.2": "duve āsuṁ vināyakā;",
  "bv28:17.3": "Sikhī ca vessabhū ceva,",
  "bv28:17.4": "asamā appaṭipuggalā.",
  "bv28:18.1": "Imamhi bhaddake kappe,",
  "bv28:18.2": "tayo āsuṁ vināyakā;",
  "bv28:18.3": "Kakusandho koṇāgamano,",
  "bv28:18.4": "kassapo cāpi nāyako.",
  "bv28:19.1": "Ahametarahi sambuddho,",
  "bv28:19.2": "metteyyo cāpi hessati;",
  "bv28:19.3": "Etepime pañca buddhā,",
  "bv28:19.4": "dhīrā lokānukampakā.",
  "bv28:20.1": "Etesaṁ dhammarājūnaṁ,",
  "bv28:20.2": "Aññesaṁnekakoṭinaṁ;",
  "bv28:20.3": "Ācikkhitvāna taṁ maggaṁ,",
  "bv28:20.4": "Nibbutā te sasāvakāti. ",
  "bv28:21.1": "Buddhapakiṇṇakakaṇḍaṁ niṭṭhitaṁ."
}