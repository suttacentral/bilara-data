{
  "cp35:0.1": "Cariyāpiṭaka",
  "cp35:0.2": "Yudhañjayavagga",
  "cp35:0.3": "Upekkhāpāramī 1",
  "cp35:0.4": "Mahālomahaṁsacariya",
  "cp35:1.1": "“Susāne seyyaṁ kappemi,",
  "cp35:1.2": "Chavaṭṭhikaṁ upanidhāyahaṁ;",
  "cp35:1.3": "Gāmaṇḍalā upāgantvā,",
  "cp35:1.4": "Rūpaṁ dassentinappakaṁ.",
  "cp35:2.1": "Apare gandhamālañca,",
  "cp35:2.2": "bhojanaṁ vividhaṁ bahuṁ;",
  "cp35:2.3": "Upāyanānūpanenti,",
  "cp35:2.4": "haṭṭhā saṁviggamānasā.",
  "cp35:3.1": "Ye me dukkhaṁ upaharanti,",
  "cp35:3.2": "ye ca denti sukhaṁ mama;",
  "cp35:3.3": "Sabbesaṁ samako homi,",
  "cp35:3.4": "dayā kopo na vijjati.",
  "cp35:4.1": "Sukhadukkhe tulābhūto,",
  "cp35:4.2": "Yasesu ayasesu ca;",
  "cp35:4.3": "Sabbattha samako homi,",
  "cp35:4.4": "Esā me upekkhāpāramī”ti. ",
  "cp35:5.1": "Mahālomahaṁsacariyaṁ pannarasamaṁ.",
  "cp35:6.1": "Yudhañjayavaggo tatiyo.",
  "cp35:7.1": "Tassuddānaṁ",
  "cp35:8.1": "Yudhañjayo somanasso,",
  "cp35:8.2": "ayogharabhisena ca;",
  "cp35:8.3": "Soṇanando mūgapakkho,",
  "cp35:8.4": "kapirājā saccasavhayo.",
  "cp35:9.1": "Vaṭṭako maccharājā ca,",
  "cp35:9.2": "kaṇhadīpāyano isi;",
  "cp35:9.3": "Sutasomo puna āsiṁ,",
  "cp35:9.4": "sāmo ca ekarājahu;",
  "cp35:9.5": "Upekkhāpāramī āsi,",
  "cp35:9.6": "iti vutthaṁ mahesinā.",
  "cp35:10.1": "Evaṁ bahubbidhaṁ dukkhaṁ,",
  "cp35:10.2": "sampattī ca bahubbidhā;",
  "cp35:10.3": "Bhavābhave anubhavitvā,",
  "cp35:10.4": "patto sambodhimuttamaṁ.",
  "cp35:11.1": "Datvā dātabbakaṁ dānaṁ,",
  "cp35:11.2": "sīlaṁ pūretvā asesato;",
  "cp35:11.3": "Nekkhamme pāramiṁ gantvā,",
  "cp35:11.4": "patto sambodhimuttamaṁ.",
  "cp35:12.1": "Paṇḍite paripucchitvā,",
  "cp35:12.2": "vīriyaṁ katvāna muttamaṁ;",
  "cp35:12.3": "Khantiyā pāramiṁ gantvā,",
  "cp35:12.4": "patto sambodhimuttamaṁ.",
  "cp35:13.1": "Katvā daḷhamadhiṭṭhānaṁ,",
  "cp35:13.2": "saccavācānurakkhiya;",
  "cp35:13.3": "Mettāya pāramiṁ gantvā,",
  "cp35:13.4": "patto sambodhimuttamaṁ.",
  "cp35:14.1": "Lābhālābhe yasāyase,",
  "cp35:14.2": "sammānanāvamānane;",
  "cp35:14.3": "Sabbattha samako hutvā,",
  "cp35:14.4": "patto sambodhimuttamaṁ.",
  "cp35:15.1": "Kosajjaṁ bhayato disvā,",
  "cp35:15.2": "vīriyārambhañca khemato;",
  "cp35:15.3": "Āraddhavīriyā hotha,",
  "cp35:15.4": "esā buddhānusāsanī.",
  "cp35:16.1": "Vivādaṁ bhayato disvā,",
  "cp35:16.2": "avivādañca khemato;",
  "cp35:16.3": "Samaggā sakhilā hotha,",
  "cp35:16.4": "esā buddhānusāsanī.",
  "cp35:17.1": "Pamādaṁ bhayato disvā,",
  "cp35:17.2": "appamādañca khemato;",
  "cp35:17.3": "Bhāvethaṭṭhaṅgikaṁ maggaṁ,",
  "cp35:17.4": "esā buddhānusāsanī.",
  "cp35:18.1": "Itthaṁ sudaṁ bhagavā attano pubbacariyaṁ sambhāvayamāno buddhāpadāniyaṁ nāma dhammapariyāyaṁ abhāsitthāti.",
  "cp35:19.1": "Cariyāpiṭakaṁ niṭṭhitaṁ."
}