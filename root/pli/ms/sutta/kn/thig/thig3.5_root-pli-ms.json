{
  "thig3.5:0.1": "Therīgāthā 3.5",
  "thig3.5:0.2": "Tikanipāta",
  "thig3.5:0.3": "Ubbiritherīgāthā",
  "thig3.5:1.1": "“Amma jīvāti vanamhi kandasi,",
  "thig3.5:1.2": "Attānaṁ adhigaccha ubbiri;",
  "thig3.5:1.3": "Cullāsītisahassāni,",
  "thig3.5:1.4": "Sabbā jīvasanāmikā;",
  "thig3.5:1.5": "Etamhāḷāhane daḍḍhā,",
  "thig3.5:1.6": "Tāsaṁ kamanusocasi”.",
  "thig3.5:2.1": "“Abbahī vata me sallaṁ,",
  "thig3.5:2.2": "duddasaṁ hadayassitaṁ;",
  "thig3.5:2.3": "Yaṁ me sokaparetāya,",
  "thig3.5:2.4": "dhītusokaṁ byapānudi.",
  "thig3.5:3.1": "Sājja abbūḷhasallāhaṁ,",
  "thig3.5:3.2": "Nicchātā parinibbutā;",
  "thig3.5:3.3": "Buddhaṁ dhammañca saṅghañca,",
  "thig3.5:3.4": "Upemi saraṇaṁ munin”ti.",
  "thig3.5:4.1": "… Ubbirī therī …."
}