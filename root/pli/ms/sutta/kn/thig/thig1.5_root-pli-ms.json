{
  "thig1.5:0.1": "Therīgāthā 1.5",
  "thig1.5:0.2": "Ekakanipāta",
  "thig1.5:0.3": "Aññatarātissātherīgāthā",
  "thig1.5:1.1": "“Tisse yuñjassu dhammehi,",
  "thig1.5:1.2": "khaṇo taṁ mā upaccagā;",
  "thig1.5:1.3": "Khaṇātītā hi socanti,",
  "thig1.5:1.4": "nirayamhi samappitā”ti.",
  "thig1.5:2.1": "… Aññatarā tissā therī …."
}