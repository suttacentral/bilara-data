{
  "thig6.6:0.1": "Therīgāthā 6.6",
  "thig6.6:0.2": "Chakkanipāta",
  "thig6.6:0.3": "Mahāpajāpatigotamītherīgāthā",
  "thig6.6:1.1": "“Buddha vīra namo tyatthu,",
  "thig6.6:1.2": "sabbasattānamuttama;",
  "thig6.6:1.3": "Yo maṁ dukkhā pamocesi,",
  "thig6.6:1.4": "aññañca bahukaṁ janaṁ.",
  "thig6.6:2.1": "Sabbadukkhaṁ pariññātaṁ,",
  "thig6.6:2.2": "Hetutaṇhā visositā;",
  "thig6.6:2.3": "Bhāvito aṭṭhaṅgiko maggo,",
  "thig6.6:2.4": "Nirodho phusito mayā.",
  "thig6.6:3.1": "Mātā putto pitā bhātā,",
  "thig6.6:3.2": "ayyakā ca pure ahuṁ;",
  "thig6.6:3.3": "Yathābhuccamajānantī,",
  "thig6.6:3.4": "saṁsariṁhaṁ anibbisaṁ.",
  "thig6.6:4.1": "Diṭṭho hi me so bhagavā,",
  "thig6.6:4.2": "antimoyaṁ samussayo;",
  "thig6.6:4.3": "Vikkhīṇo jātisaṁsāro,",
  "thig6.6:4.4": "natthi dāni punabbhavo.",
  "thig6.6:5.1": "Āraddhavīriye pahitatte,",
  "thig6.6:5.2": "Niccaṁ daḷhaparakkame;",
  "thig6.6:5.3": "Samagge sāvake passe,",
  "thig6.6:5.4": "Esā buddhāna vandanā.",
  "thig6.6:6.1": "Bahūnaṁ vata atthāya,",
  "thig6.6:6.2": "Māyā janayi gotamaṁ;",
  "thig6.6:6.3": "Byādhimaraṇatunnānaṁ,",
  "thig6.6:6.4": "Dukkhakkhandhaṁ byapānudī”ti.",
  "thig6.6:7.1": "… Mahāpajāpatigotamī therī …."
}