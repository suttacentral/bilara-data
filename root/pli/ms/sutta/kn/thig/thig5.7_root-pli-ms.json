{
  "thig5.7:0.1": "Therīgāthā 5.7",
  "thig5.7:0.2": "Pañcakanipāta",
  "thig5.7:0.3": "Sakulātherīgāthā",
  "thig5.7:1.1": "“Agārasmiṁ vasantīhaṁ,",
  "thig5.7:1.2": "dhammaṁ sutvāna bhikkhuno;",
  "thig5.7:1.3": "Addasaṁ virajaṁ dhammaṁ,",
  "thig5.7:1.4": "nibbānaṁ padamaccutaṁ.",
  "thig5.7:2.1": "Sāhaṁ puttaṁ dhītarañca,",
  "thig5.7:2.2": "dhanadhaññañca chaḍḍiya;",
  "thig5.7:2.3": "Kese chedāpayitvāna,",
  "thig5.7:2.4": "pabbajiṁ anagāriyaṁ.",
  "thig5.7:3.1": "Sikkhamānā ahaṁ santī,",
  "thig5.7:3.2": "bhāventī maggamañjasaṁ;",
  "thig5.7:3.3": "Pahāsiṁ rāgadosañca,",
  "thig5.7:3.4": "tadekaṭṭhe ca āsave.",
  "thig5.7:4.1": "Bhikkhunī upasampajja,",
  "thig5.7:4.2": "pubbajātimanussariṁ;",
  "thig5.7:4.3": "Dibbacakkhu visodhitaṁ,",
  "thig5.7:4.4": "vimalaṁ sādhubhāvitaṁ.",
  "thig5.7:5.1": "Saṅkhāre parato disvā,",
  "thig5.7:5.2": "hetujāte palokite;",
  "thig5.7:5.3": "Pahāsiṁ āsave sabbe,",
  "thig5.7:5.4": "sītibhūtāmhi nibbutā”ti.",
  "thig5.7:6.1": "… Sakulā therī …."
}