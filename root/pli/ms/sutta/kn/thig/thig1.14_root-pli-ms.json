{
  "thig1.14:0.1": "Therīgāthā 1.14",
  "thig1.14:0.2": "Ekakanipāta",
  "thig1.14:0.3": "Sumanātherīgāthā",
  "thig1.14:1.1": "“Dhātuyo dukkhato disvā,",
  "thig1.14:1.2": "mā jātiṁ punarāgami;",
  "thig1.14:1.3": "Bhave chandaṁ virājetvā,",
  "thig1.14:1.4": "upasantā carissasī”ti.",
  "thig1.14:2.1": "… Sumanā therī …."
}