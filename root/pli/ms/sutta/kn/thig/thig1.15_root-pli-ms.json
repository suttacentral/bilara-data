{
  "thig1.15:0.1": "Therīgāthā 1.15",
  "thig1.15:0.2": "Ekakanipāta",
  "thig1.15:0.3": "Uttarātherīgāthā",
  "thig1.15:1.1": "“Kāyena saṁvutā āsiṁ,",
  "thig1.15:1.2": "vācāya uda cetasā;",
  "thig1.15:1.3": "Samūlaṁ taṇhamabbuyha,",
  "thig1.15:1.4": "sītibhūtāmhi nibbutā”ti.",
  "thig1.15:2.1": "… Uttarā therī …."
}