{
  "thig1.11:0.1": "Therīgāthā 1.11",
  "thig1.11:0.2": "Ekakanipāta",
  "thig1.11:0.3": "Muttātherīgāthā",
  "thig1.11:1.1": "“Sumuttā sādhumuttāmhi,",
  "thig1.11:1.2": "tīhi khujjehi muttiyā;",
  "thig1.11:1.3": "Udukkhalena musalena,",
  "thig1.11:1.4": "patinā khujjakena ca;",
  "thig1.11:1.5": "Muttāmhi jātimaraṇā,",
  "thig1.11:1.6": "bhavanetti samūhatā”ti.",
  "thig1.11:2.1": "… Muttā therī …."
}