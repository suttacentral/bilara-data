{
  "thig1.16:0.1": "Therīgāthā 1.16",
  "thig1.16:0.2": "Ekakanipāta",
  "thig1.16:0.3": "Vuḍḍhapabbajitasumanātherīgāthā",
  "thig1.16:1.1": "“Sukhaṁ tvaṁ vuḍḍhike sehi,",
  "thig1.16:1.2": "katvā coḷena pārutā;",
  "thig1.16:1.3": "Upasanto hi te rāgo,",
  "thig1.16:1.4": "sītibhūtāsi nibbutā”ti.",
  "thig1.16:2.1": "… Sumanā vuḍḍhapabbajitā therī …."
}