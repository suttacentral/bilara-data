{
  "thig2.6:0.1": "Therīgāthā 2.6",
  "thig2.6:0.2": "Dukanipāta",
  "thig2.6:0.3": "Mettikātherīgāthā",
  "thig2.6:1.1": "“Kiñcāpi khomhi dukkhitā,",
  "thig2.6:1.2": "dubbalā gatayobbanā;",
  "thig2.6:1.3": "Daṇḍamolubbha gacchāmi,",
  "thig2.6:1.4": "pabbataṁ abhirūhiya.",
  "thig2.6:2.1": "Nikkhipitvāna saṅghāṭiṁ,",
  "thig2.6:2.2": "pattakañca nikujjiya;",
  "thig2.6:2.3": "Nisinnā camhi selamhi,",
  "thig2.6:2.4": "atha cittaṁ vimucci me;",
  "thig2.6:2.5": "Tisso vijjā anuppattā,",
  "thig2.6:2.6": "kataṁ buddhassa sāsanan”ti.",
  "thig2.6:3.1": "… Mettikā therī …."
}