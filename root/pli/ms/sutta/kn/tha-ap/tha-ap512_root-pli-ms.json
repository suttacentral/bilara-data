{
  "tha-ap512:0.1": "Therāpadāna",
  "tha-ap512:0.2": "Kaṇikāravagga",
  "tha-ap512:0.3": "10 Nāḷikeraphaladāyakattheraapadāna",
  "tha-ap512:1.1": "“Nagare bandhumatiyā,",
  "tha-ap512:1.2": "ārāmiko ahaṁ tadā;",
  "tha-ap512:1.3": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap512:1.4": "gacchantaṁ anilañjase.",
  "tha-ap512:2.1": "Nāḷikeraphalaṁ gayha,",
  "tha-ap512:2.2": "buddhaseṭṭhassadāsahaṁ;",
  "tha-ap512:2.3": "Ākāse ṭhitako santo,",
  "tha-ap512:2.4": "paṭiggaṇhi mahāyaso.",
  "tha-ap512:3.1": "Vittisañjanano mayhaṁ,",
  "tha-ap512:3.2": "diṭṭhadhammasukhāvaho;",
  "tha-ap512:3.3": "Phalaṁ buddhassa datvāna,",
  "tha-ap512:3.4": "vippasannena cetasā.",
  "tha-ap512:4.1": "Adhigacchiṁ tadā pītiṁ,",
  "tha-ap512:4.2": "vipulañca sukhuttamaṁ;",
  "tha-ap512:4.3": "Uppajjateva ratanaṁ,",
  "tha-ap512:4.4": "nibbattassa tahiṁ tahiṁ.",
  "tha-ap512:5.1": "Ekanavutito kappe,",
  "tha-ap512:5.2": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap512:5.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap512:5.4": "phaladānassidaṁ phalaṁ.",
  "tha-ap512:6.1": "Dibbacakkhu visuddhaṁ me,",
  "tha-ap512:6.2": "samādhikusalo ahaṁ;",
  "tha-ap512:6.3": "Abhiññāpāramippatto,",
  "tha-ap512:6.4": "phaladānassidaṁ phalaṁ.",
  "tha-ap512:7.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap512:7.2": "…pe…",
  "tha-ap512:7.3": "viharāmi anāsavo.",
  "tha-ap512:8.1": "Svāgataṁ vata me āsi,",
  "tha-ap512:8.2": "…pe…",
  "tha-ap512:8.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap512:9.1": "Paṭisambhidā catasso,",
  "tha-ap512:9.2": "…pe…",
  "tha-ap512:9.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap512:10.1": "Itthaṁ sudaṁ āyasmā nāḷikeraphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap512:11.1": "Nāḷikeraphaladāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap512:12.1": "Kaṇikāravaggo ekapaññāsamo.",
  "tha-ap512:13.1": "Tassuddānaṁ",
  "tha-ap512:14.1": "Kaṇikārekapattā ca,",
  "tha-ap512:14.2": "kāsumārī tathāvaṭā;",
  "tha-ap512:14.3": "Pādañca mātuluṅgañca,",
  "tha-ap512:14.4": "ajelīmodameva ca.",
  "tha-ap512:15.1": "Tālaṁ tathā nāḷikeraṁ,",
  "tha-ap512:15.2": "gāthāyo gaṇitā viha;",
  "tha-ap512:15.3": "Ekaṁ gāthāsataṁ hoti,",
  "tha-ap512:15.4": "ūnādhikavivajjitaṁ."
}