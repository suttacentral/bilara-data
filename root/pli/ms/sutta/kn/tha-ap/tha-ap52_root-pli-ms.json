{
  "tha-ap52:0.1": "Therāpadāna",
  "tha-ap52:0.2": "Upālivagga",
  "tha-ap52:0.3": "10. Cundattheraapadāna",
  "tha-ap52:1.1": "“Siddhatthassa bhagavato,",
  "tha-ap52:1.2": "lokajeṭṭhassa tādino;",
  "tha-ap52:1.3": "Agghiyaṁ kārayitvāna,",
  "tha-ap52:1.4": "jātipupphehi chādayiṁ.",
  "tha-ap52:2.1": "Niṭṭhāpetvāna taṁ pupphaṁ,",
  "tha-ap52:2.2": "buddhassa upanāmayiṁ;",
  "tha-ap52:2.3": "Pupphāvasesaṁ paggayha,",
  "tha-ap52:2.4": "buddhassa abhiropayiṁ.",
  "tha-ap52:3.1": "Kañcanagghiyasaṅkāsaṁ,",
  "tha-ap52:3.2": "buddhaṁ lokagganāyakaṁ;",
  "tha-ap52:3.3": "Pasannacitto sumano,",
  "tha-ap52:3.4": "pupphagghiyamupānayiṁ.",
  "tha-ap52:4.1": "Vitiṇṇakaṅkho sambuddho,",
  "tha-ap52:4.2": "tiṇṇoghehi purakkhato;",
  "tha-ap52:4.3": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap52:4.4": "imā gāthā abhāsatha.",
  "tha-ap52:5.1": "‘Dibbagandhaṁ pavāyantaṁ,",
  "tha-ap52:5.2": "yo me pupphagghiyaṁ adā;",
  "tha-ap52:5.3": "Tamahaṁ kittayissāmi,",
  "tha-ap52:5.4": "suṇātha mama bhāsato.",
  "tha-ap52:6.1": "Ito cuto ayaṁ poso,",
  "tha-ap52:6.2": "devasaṅghapurakkhato;",
  "tha-ap52:6.3": "Jātipupphehi parikiṇṇo,",
  "tha-ap52:6.4": "devalokaṁ gamissati.",
  "tha-ap52:7.1": "Ubbiddhaṁ bhavanaṁ tassa,",
  "tha-ap52:7.2": "Sovaṇṇañca maṇīmayaṁ;",
  "tha-ap52:7.3": "Byamhaṁ pātubhavissati,",
  "tha-ap52:7.4": "Puññakammappabhāvitaṁ.",
  "tha-ap52:8.1": "Catusattatikkhattuṁ so,",
  "tha-ap52:8.2": "devarajjaṁ karissati;",
  "tha-ap52:8.3": "Anubhossati sampattiṁ,",
  "tha-ap52:8.4": "accharāhi purakkhato.",
  "tha-ap52:9.1": "Pathabyā rajjaṁ tisataṁ,",
  "tha-ap52:9.2": "vasudhaṁ āvasissati;",
  "tha-ap52:9.3": "Pañcasattatikkhattuñca,",
  "tha-ap52:9.4": "cakkavattī bhavissati.",
  "tha-ap52:10.1": "Dujjayo nāma nāmena,",
  "tha-ap52:10.2": "hessati manujādhipo;",
  "tha-ap52:10.3": "Anubhotvāna taṁ puññaṁ,",
  "tha-ap52:10.4": "sakakammaṁ apassito.",
  "tha-ap52:11.1": "Vinipātaṁ agantvāna,",
  "tha-ap52:11.2": "manussattaṁ gamissati;",
  "tha-ap52:11.3": "Hiraññaṁ tassa nicitaṁ,",
  "tha-ap52:11.4": "koṭisatamanappakaṁ.",
  "tha-ap52:12.1": "Nibbattissati yonimhi,",
  "tha-ap52:12.2": "brāhmaṇe so bhavissati;",
  "tha-ap52:12.3": "Vaṅgantassa suto dhīmā,",
  "tha-ap52:12.4": "sāriyā oraso piyo.",
  "tha-ap52:13.1": "So ca pacchā pabbajitvā,",
  "tha-ap52:13.2": "aṅgīrasassa sāsane;",
  "tha-ap52:13.3": "Cūḷacundoti nāmena,",
  "tha-ap52:13.4": "hessati satthu sāvako.",
  "tha-ap52:14.1": "Sāmaṇerova so santo,",
  "tha-ap52:14.2": "khīṇāsavo bhavissati;",
  "tha-ap52:14.3": "Sabbāsave pariññāya,",
  "tha-ap52:14.4": "nibbāyissatināsavo’.",
  "tha-ap52:15.1": "Upaṭṭhahiṁ mahāvīraṁ,",
  "tha-ap52:15.2": "aññe ca pesale bahū;",
  "tha-ap52:15.3": "Bhātaraṁ me cupaṭṭhāsiṁ,",
  "tha-ap52:15.4": "uttamatthassa pattiyā.",
  "tha-ap52:16.1": "Bhātaraṁ me upaṭṭhitvā,",
  "tha-ap52:16.2": "dhātuṁ pattamhi ohiya;",
  "tha-ap52:16.3": "Sambuddhaṁ upanāmesiṁ,",
  "tha-ap52:16.4": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap52:17.1": "Ubho hatthehi paggayha,",
  "tha-ap52:17.2": "buddho loke sadevake;",
  "tha-ap52:17.3": "Sandassayanto taṁ dhātuṁ,",
  "tha-ap52:17.4": "kittayi aggasāvakaṁ.",
  "tha-ap52:18.1": "Cittañca suvimuttaṁ me,",
  "tha-ap52:18.2": "saddhā mayhaṁ patiṭṭhitā;",
  "tha-ap52:18.3": "Sabbāsave pariññāya,",
  "tha-ap52:18.4": "viharāmi anāsavo.",
  "tha-ap52:19.1": "Paṭisambhidānuppattā,",
  "tha-ap52:19.2": "Vimokkhāpi ca phassitā;",
  "tha-ap52:19.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap52:19.4": "Kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap52:20.1": "Itthaṁ sudaṁ āyasmā cundo thero imā gāthāyo abhāsitthāti.",
  "tha-ap52:21.1": "Cundattherassāpadānaṁ dasamaṁ.",
  "tha-ap52:22.1": "Upālivaggo pañcamo.",
  "tha-ap52:23.1": "Tassuddānaṁ",
  "tha-ap52:24.1": "Upāli soṇo bhaddiyo,",
  "tha-ap52:24.2": "sanniṭṭhāpakahatthiyo;",
  "tha-ap52:24.3": "Chadanaṁ seyyacaṅkamaṁ,",
  "tha-ap52:24.4": "subhaddo cundasavhayo;",
  "tha-ap52:24.5": "Gāthāsataṁ satālīsaṁ,",
  "tha-ap52:24.6": "catasso ca taduttari."
}