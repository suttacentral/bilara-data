{
  "tha-ap172:0.1": "Therāpadāna",
  "tha-ap172:0.2": "Supāricariyavagga",
  "tha-ap172:0.3": "10. Pañcaṅguliyattheraapadāna",
  "tha-ap172:1.1": "“Tisso nāmāsi bhagavā,",
  "tha-ap172:1.2": "lokajeṭṭho narāsabho;",
  "tha-ap172:1.3": "Pavisati gandhakuṭiṁ,",
  "tha-ap172:1.4": "vihārakusalo muni.",
  "tha-ap172:2.1": "Sugandhamālamādāya,",
  "tha-ap172:2.2": "agamāsiṁ jinantikaṁ;",
  "tha-ap172:2.3": "Apasaddo ca sambuddhe,",
  "tha-ap172:2.4": "pañcaṅgulimadāsahaṁ.",
  "tha-ap172:3.1": "Dvenavute ito kappe,",
  "tha-ap172:3.2": "yaṁ gandhamabhiropayiṁ;",
  "tha-ap172:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap172:3.4": "pañcaṅgulissidaṁ phalaṁ.",
  "tha-ap172:4.1": "Dvesattatimhito kappe,",
  "tha-ap172:4.2": "rājā āsiṁ sayampabho;",
  "tha-ap172:4.3": "Sattaratanasampanno,",
  "tha-ap172:4.4": "cakkavattī mahabbalo.",
  "tha-ap172:5.1": "Paṭisambhidā catasso,",
  "tha-ap172:5.2": "…pe…",
  "tha-ap172:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap172:6.1": "Itthaṁ sudaṁ āyasmā pañcaṅguliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap172:7.1": "Pañcaṅguliyattherassāpadānaṁ dasamaṁ.",
  "tha-ap172:8.1": "Supāricariyavaggo sattarasamo.",
  "tha-ap172:9.1": "Tassuddānaṁ",
  "tha-ap172:10.1": "Supāricari kaṇaverī,",
  "tha-ap172:10.2": "khajjako desapūjako;",
  "tha-ap172:10.3": "Kaṇikāro sappidado,",
  "tha-ap172:10.4": "yūthiko dussadāyako;",
  "tha-ap172:10.5": "Māḷo ca pañcaṅguliko,",
  "tha-ap172:10.6": "catupaññāsa gāthakāti."
}