{
  "tha-ap112:0.1": "Therāpadāna",
  "tha-ap112:0.2": "Bhikkhadāyivagga",
  "tha-ap112:0.3": "10. Reṇupūjakattheraapadāna",
  "tha-ap112:1.1": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap112:1.2": "Sataraṁsiṁva bhāṇumaṁ;",
  "tha-ap112:1.3": "Obhāsentaṁ disā sabbā,",
  "tha-ap112:1.4": "Uḷurājaṁva pūritaṁ.",
  "tha-ap112:2.1": "Purakkhataṁ sāvakehi,",
  "tha-ap112:2.2": "sāgareheva medaniṁ;",
  "tha-ap112:2.3": "Nāgaṁ paggayha reṇūhi,",
  "tha-ap112:2.4": "vipassissābhiropayiṁ.",
  "tha-ap112:3.1": "Ekanavutito kappe,",
  "tha-ap112:3.2": "yaṁ reṇumabhiropayiṁ;",
  "tha-ap112:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap112:3.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap112:4.1": "Paṇṇatālīsito kappe,",
  "tha-ap112:4.2": "reṇu nāmāsi khattiyo;",
  "tha-ap112:4.3": "Sattaratanasampanno,",
  "tha-ap112:4.4": "cakkavattī mahabbalo.",
  "tha-ap112:5.1": "Paṭisambhidā catasso,",
  "tha-ap112:5.2": "…pe…",
  "tha-ap112:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap112:6.1": "Itthaṁ sudaṁ āyasmā reṇupūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap112:7.1": "Reṇupūjakattherassāpadānaṁ dasamaṁ.",
  "tha-ap112:8.1": "Bhikkhadāyivaggo ekādasamo.",
  "tha-ap112:9.1": "Tassuddānaṁ",
  "tha-ap112:10.1": "Bhikkhadāyī ñāṇasaññī,",
  "tha-ap112:10.2": "hatthiyo padapūjako;",
  "tha-ap112:10.3": "Muṭṭhipupphī udakado,",
  "tha-ap112:10.4": "naḷamāli upaṭṭhako;",
  "tha-ap112:10.5": "Biḷālidāyī reṇu ca,",
  "tha-ap112:10.6": "gāthāyo cha ca saṭṭhi ca."
}