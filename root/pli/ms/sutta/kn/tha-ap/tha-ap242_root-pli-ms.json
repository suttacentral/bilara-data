{
  "tha-ap242:0.1": "Therāpadāna",
  "tha-ap242:0.2": "Udakāsanavagga",
  "tha-ap242:0.3": "10. Vāsidāyakattheraapadāna",
  "tha-ap242:1.1": "“Kammārohaṁ pure āsiṁ,",
  "tha-ap242:1.2": "tivarāyaṁ puruttame;",
  "tha-ap242:1.3": "Ekā vāsi mayā dinnā,",
  "tha-ap242:1.4": "sayambhuṁ aparājitaṁ.",
  "tha-ap242:2.1": "Catunnavutito kappe,",
  "tha-ap242:2.2": "yaṁ vāsimadadiṁ tadā;",
  "tha-ap242:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap242:2.4": "vāsidānassidaṁ phalaṁ.",
  "tha-ap242:3.1": "Paṭisambhidā catasso,",
  "tha-ap242:3.2": "…pe…",
  "tha-ap242:3.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap242:4.1": "Itthaṁ sudaṁ āyasmā vāsidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap242:5.1": "Vāsidāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap242:6.1": "Udakāsanavaggo catuvīsatimo.",
  "tha-ap242:7.1": "Tassuddānaṁ",
  "tha-ap242:8.1": "Udakāsanabhājanaṁ,",
  "tha-ap242:8.2": "sālapupphī kilañjako;",
  "tha-ap242:8.3": "Vedikā vaṇṇakāro ca,",
  "tha-ap242:8.4": "piyālaambayāgado;",
  "tha-ap242:8.5": "Jagatī vāsidātā ca,",
  "tha-ap242:8.6": "gāthā tiṁsa ca aṭṭha ca."
}