{
  "tha-ap252:0.1": "Therāpadāna",
  "tha-ap252:0.2": "Tuvaradāyakavagga",
  "tha-ap252:0.3": "10 Uddālakadāyakattheraapadāna",
  "tha-ap252:1.1": "“Kakudho nāma nāmena,",
  "tha-ap252:1.2": "sayambhū aparājito;",
  "tha-ap252:1.3": "Pavanā nikkhamitvāna,",
  "tha-ap252:1.4": "anuppatto mahānadiṁ.",
  "tha-ap252:2.1": "Uddālakaṁ gahetvāna,",
  "tha-ap252:2.2": "sayambhussa adāsahaṁ;",
  "tha-ap252:2.3": "Saṁyatassujubhūtassa,",
  "tha-ap252:2.4": "pasannamānaso ahaṁ.",
  "tha-ap252:3.1": "Ekattiṁse ito kappe,",
  "tha-ap252:3.2": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap252:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap252:3.4": "pupphadānassidaṁ phalaṁ.",
  "tha-ap252:4.1": "Paṭisambhidā catasso,",
  "tha-ap252:4.2": "…pe…",
  "tha-ap252:4.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap252:5.1": "Itthaṁ sudaṁ āyasmā uddālakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap252:6.1": "Uddālakadāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap252:7.1": "Tuvaradāyakavaggo pañcavīsatimo.",
  "tha-ap252:8.1": "Tassuddānaṁ",
  "tha-ap252:9.1": "Tuvaranāganaḷinā,",
  "tha-ap252:9.2": "viravī kuṭidhūpako;",
  "tha-ap252:9.3": "Patto dhātusattaliyo,",
  "tha-ap252:9.4": "bimbi uddālakena ca;",
  "tha-ap252:9.5": "Sattatiṁsati gāthāyo,",
  "tha-ap252:9.6": "gaṇitāyo vibhāvibhi."
}