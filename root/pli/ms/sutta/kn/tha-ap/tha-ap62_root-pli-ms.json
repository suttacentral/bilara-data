{
  "tha-ap62:0.1": "Therāpadāna",
  "tha-ap62:0.2": "Bījanivagga",
  "tha-ap62:0.3": "10. Asanabodhiyattheraapadāna",
  "tha-ap62:1.1": "“Jātiyā sattavassohaṁ,",
  "tha-ap62:1.2": "addasaṁ lokanāyakaṁ;",
  "tha-ap62:1.3": "Pasannacitto sumano,",
  "tha-ap62:1.4": "upagacchiṁ naruttamaṁ.",
  "tha-ap62:2.1": "Tissassāhaṁ bhagavato,",
  "tha-ap62:2.2": "lokajeṭṭhassa tādino;",
  "tha-ap62:2.3": "Haṭṭho haṭṭhena cittena,",
  "tha-ap62:2.4": "ropayiṁ bodhimuttamaṁ.",
  "tha-ap62:3.1": "Asano nāmadheyyena,",
  "tha-ap62:3.2": "dharaṇīruhapādapo;",
  "tha-ap62:3.3": "Pañcavasse paricariṁ,",
  "tha-ap62:3.4": "asanaṁ bodhimuttamaṁ.",
  "tha-ap62:4.1": "Pupphitaṁ pādapaṁ disvā,",
  "tha-ap62:4.2": "abbhutaṁ lomahaṁsanaṁ;",
  "tha-ap62:4.3": "Sakaṁ kammaṁ pakittento,",
  "tha-ap62:4.4": "buddhaseṭṭhaṁ upāgamiṁ.",
  "tha-ap62:5.1": "Tisso tadā so sambuddho,",
  "tha-ap62:5.2": "sayambhū aggapuggalo;",
  "tha-ap62:5.3": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap62:5.4": "imā gāthā abhāsatha.",
  "tha-ap62:6.1": "‘Yenāyaṁ ropitā bodhi,",
  "tha-ap62:6.2": "buddhapūjā ca sakkatā;",
  "tha-ap62:6.3": "Tamahaṁ kittayissāmi,",
  "tha-ap62:6.4": "suṇātha mama bhāsato.",
  "tha-ap62:7.1": "Tiṁsakappāni devesu,",
  "tha-ap62:7.2": "devarajjaṁ karissati;",
  "tha-ap62:7.3": "Catusaṭṭhi cakkhattuṁ so,",
  "tha-ap62:7.4": "cakkavattī bhavissati.",
  "tha-ap62:8.1": "Tusitā hi cavitvāna,",
  "tha-ap62:8.2": "sukkamūlena codito;",
  "tha-ap62:8.3": "Dve sampattī anubhotvā,",
  "tha-ap62:8.4": "manussatte ramissati.",
  "tha-ap62:9.1": "Padhānapahitatto so,",
  "tha-ap62:9.2": "upasanto nirūpadhi;",
  "tha-ap62:9.3": "Sabbāsave pariññāya,",
  "tha-ap62:9.4": "nibbāyissatināsavo’.",
  "tha-ap62:10.1": "Vivekamanuyuttohaṁ,",
  "tha-ap62:10.2": "upasanto nirūpadhi;",
  "tha-ap62:10.3": "Nāgova bandhanaṁ chetvā,",
  "tha-ap62:10.4": "viharāmi anāsavo.",
  "tha-ap62:11.1": "Dvenavute ito kappe,",
  "tha-ap62:11.2": "bodhiṁ ropesahaṁ tadā;",
  "tha-ap62:11.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap62:11.4": "bodhiropassidaṁ phalaṁ.",
  "tha-ap62:12.1": "Catusattatito kappe,",
  "tha-ap62:12.2": "daṇḍasenoti vissuto;",
  "tha-ap62:12.3": "Sattaratanasampanno,",
  "tha-ap62:12.4": "cakkavattī tadā ahuṁ.",
  "tha-ap62:13.1": "Tesattatimhito kappe,",
  "tha-ap62:13.2": "sattāhesuṁ mahīpatī;",
  "tha-ap62:13.3": "Samantanemināmena,",
  "tha-ap62:13.4": "rājāno cakkavattino.",
  "tha-ap62:14.1": "Paṇṇavīsatito kappe,",
  "tha-ap62:14.2": "puṇṇako nāma khattiyo;",
  "tha-ap62:14.3": "Sattaratanasampanno,",
  "tha-ap62:14.4": "cakkavattī mahabbalo.",
  "tha-ap62:15.1": "Paṭisambhidā catasso,",
  "tha-ap62:15.2": "…pe…",
  "tha-ap62:15.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap62:16.1": "Itthaṁ sudaṁ āyasmā asanabodhiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap62:17.1": "Asanabodhiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap62:18.1": "Bījanivaggo chaṭṭho.",
  "tha-ap62:19.1": "Tassuddānaṁ",
  "tha-ap62:20.1": "Bījanī sataraṁsī ca,",
  "tha-ap62:20.2": "sayanodakivāhiyo;",
  "tha-ap62:20.3": "Parivāro padīpañca,",
  "tha-ap62:20.4": "dhajo padumapūjako;",
  "tha-ap62:20.5": "Bodhi ca dasamo vutto,",
  "tha-ap62:20.6": "gāthā dvenavuti tathā."
}