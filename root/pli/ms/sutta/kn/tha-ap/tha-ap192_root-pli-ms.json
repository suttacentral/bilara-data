{
  "tha-ap192:0.1": "Therāpadāna",
  "tha-ap192:0.2": "Kuṭajapupphiyavagga",
  "tha-ap192:0.3": "10. Tivaṇṭipupphiyattheraapadāna",
  "tha-ap192:1.1": "“Abhibhūtaṁ panijjhanti,",
  "tha-ap192:1.2": "Sabbe saṅgamma te mamaṁ;",
  "tha-ap192:1.3": "Tesaṁ nijjhāyamānānaṁ,",
  "tha-ap192:1.4": "Pariḷāho ajāyatha.",
  "tha-ap192:2.1": "Sunando nāma nāmena,",
  "tha-ap192:2.2": "buddhassa sāvako tadā;",
  "tha-ap192:2.3": "Dhammadassissa munino,",
  "tha-ap192:2.4": "āgacchi mama santikaṁ.",
  "tha-ap192:3.1": "Ye me baddhacarā āsuṁ,",
  "tha-ap192:3.2": "te me pupphaṁ aduṁ tadā;",
  "tha-ap192:3.3": "Tāhaṁ pupphaṁ gahetvāna,",
  "tha-ap192:3.4": "sāvake abhiropayiṁ.",
  "tha-ap192:4.1": "Sohaṁ kālaṅkato tattha,",
  "tha-ap192:4.2": "punāpi upapajjahaṁ;",
  "tha-ap192:4.3": "Aṭṭhārase kappasate,",
  "tha-ap192:4.4": "vinipātaṁ na gacchahaṁ.",
  "tha-ap192:5.1": "Teraseto kappasate,",
  "tha-ap192:5.2": "aṭṭhāsuṁ dhūmaketuno;",
  "tha-ap192:5.3": "Sattaratanasampannā,",
  "tha-ap192:5.4": "cakkavattī mahabbalā.",
  "tha-ap192:6.1": "Paṭisambhidā catasso,",
  "tha-ap192:6.2": "…pe…",
  "tha-ap192:6.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap192:7.1": "Itthaṁ sudaṁ āyasmā tivaṇṭipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap192:8.1": "Tivaṇṭipupphiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap192:9.1": "Kuṭajapupphiyavaggo ekūnavīsatimo.",
  "tha-ap192:10.1": "Tassuddānaṁ",
  "tha-ap192:11.1": "Kuṭajo bandhujīvī ca,",
  "tha-ap192:11.2": "koṭumbarikahatthiyo;",
  "tha-ap192:11.3": "Isimuggo ca bodhi ca,",
  "tha-ap192:11.4": "ekacintī tikaṇṇiko;",
  "tha-ap192:11.5": "Ekacārī tivaṇṭi ca,",
  "tha-ap192:11.6": "gāthā dvāsaṭṭhi kittitāti."
}