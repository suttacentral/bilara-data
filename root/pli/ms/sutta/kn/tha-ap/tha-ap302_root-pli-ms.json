{
  "tha-ap302:0.1": "Therāpadāna",
  "tha-ap302:0.2": "Citakapūjakavagga",
  "tha-ap302:0.3": "10. Anusaṁsāvakattheraapadāna",
  "tha-ap302:1.1": "“Piṇḍāya caramānāhaṁ,",
  "tha-ap302:1.2": "vipassimaddasaṁ jinaṁ;",
  "tha-ap302:1.3": "Uḷuṅgabhikkhaṁ pādāsiṁ,",
  "tha-ap302:1.4": "dvipadindassa tādino.",
  "tha-ap302:2.1": "Pasannacitto sumano,",
  "tha-ap302:2.2": "abhivādesahaṁ tadā;",
  "tha-ap302:2.3": "Anusaṁsāvayiṁ buddhaṁ,",
  "tha-ap302:2.4": "uttamatthassa pattiyā.",
  "tha-ap302:3.1": "Ekanavutito kappe,",
  "tha-ap302:3.2": "anusaṁsāvayiṁ ahaṁ;",
  "tha-ap302:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap302:3.4": "anusaṁsāvanā phalaṁ.",
  "tha-ap302:4.1": "Paṭisambhidā catasso,",
  "tha-ap302:4.2": "…pe…",
  "tha-ap302:4.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap302:5.1": "Itthaṁ sudaṁ āyasmā anusaṁsāvako thero imā gāthāyo abhāsitthāti.",
  "tha-ap302:6.1": "Anusaṁsāvakattherassāpadānaṁ dasamaṁ.",
  "tha-ap302:6.2": "Citakapūjakavaggo tiṁsatimo.",
  "tha-ap302:7.1": "Tassuddānaṁ",
  "tha-ap302:8.1": "Citakaṁ pārichatto ca,",
  "tha-ap302:8.2": "saddagosīsasantharaṁ;",
  "tha-ap302:8.3": "Pādo padesaṁ saraṇaṁ,",
  "tha-ap302:8.4": "ambo saṁsāvakopi ca;",
  "tha-ap302:8.5": "Aṭṭhatālīsa gāthāyo,",
  "tha-ap302:8.6": "gaṇitāyo vibhāvibhi.",
  "tha-ap302:9.1": "Atha vagguddānaṁ",
  "tha-ap302:10.1": "Kaṇikāro hatthidado,",
  "tha-ap302:10.2": "ālambaṇudakāsanaṁ;",
  "tha-ap302:10.3": "Tuvaraṁ thomako ceva,",
  "tha-ap302:10.4": "ukkhepaṁ sīsupadhānaṁ.",
  "tha-ap302:11.1": "Paṇṇado citapūjī ca,",
  "tha-ap302:11.2": "gāthāyo ceva sabbaso;",
  "tha-ap302:11.3": "Cattāri ca satānīha,",
  "tha-ap302:11.4": "ekapaññāsameva ca.",
  "tha-ap302:12.1": "Pañcavīsasatā sabbā,",
  "tha-ap302:12.2": "dvāsattati taduttari;",
  "tha-ap302:12.3": "Tisataṁ apadānānaṁ,",
  "tha-ap302:12.4": "gaṇitā atthadassibhi.",
  "tha-ap302:13.1": "Kaṇikāravaggadasakaṁ.",
  "tha-ap302:14.1": "Tatiyasatakaṁ samattaṁ."
}