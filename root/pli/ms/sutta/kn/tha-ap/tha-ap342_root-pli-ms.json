{
  "tha-ap342:0.1": "Therāpadāna",
  "tha-ap342:0.2": "Gandhodakavagga",
  "tha-ap342:0.3": "10. Ukkhittapadumiyattheraapadāna",
  "tha-ap342:1.1": "“Nagare haṁsavatiyā,",
  "tha-ap342:1.2": "ahosiṁ māliko tadā;",
  "tha-ap342:1.3": "Ogāhetvā padumasaraṁ,",
  "tha-ap342:1.4": "satapattaṁ ocināmahaṁ.",
  "tha-ap342:2.1": "Padumuttaro nāma jino,",
  "tha-ap342:2.2": "sabbadhammāna pāragū;",
  "tha-ap342:2.3": "Saha satasahassehi,",
  "tha-ap342:2.4": "santacittehi tādibhi.",
  "tha-ap342:3.1": "Khīṇāsavehi suddhehi,",
  "tha-ap342:3.2": "chaḷabhiññehi jhāyibhi;",
  "tha-ap342:3.3": "Mama vuddhiṁ samanvesaṁ,",
  "tha-ap342:3.4": "āgacchi mama santikaṁ.",
  "tha-ap342:4.1": "Disvānahaṁ devadevaṁ,",
  "tha-ap342:4.2": "sayambhuṁ lokanāyakaṁ;",
  "tha-ap342:4.3": "Vaṇṭe chetvā satapattaṁ,",
  "tha-ap342:4.4": "ukkhipimambare tadā.",
  "tha-ap342:5.1": "Yadi buddho tuvaṁ vīra,",
  "tha-ap342:5.2": "lokajeṭṭho narāsabho;",
  "tha-ap342:5.3": "Sayaṁ gantvā satapattā,",
  "tha-ap342:5.4": "matthake dhārayantu te.",
  "tha-ap342:6.1": "Adhiṭṭhahi mahāvīro,",
  "tha-ap342:6.2": "lokajeṭṭho narāsabho;",
  "tha-ap342:6.3": "Buddhassa ānubhāvena,",
  "tha-ap342:6.4": "matthake dhārayiṁsu te.",
  "tha-ap342:7.1": "Tena kammena sukatena,",
  "tha-ap342:7.2": "cetanāpaṇidhīhi ca;",
  "tha-ap342:7.3": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap342:7.4": "tāvatiṁsamagacchahaṁ.",
  "tha-ap342:8.1": "Tattha me sukataṁ byamhaṁ,",
  "tha-ap342:8.2": "satapattanti vuccati;",
  "tha-ap342:8.3": "Saṭṭhiyojanamubbiddhaṁ,",
  "tha-ap342:8.4": "tiṁsayojanavitthataṁ.",
  "tha-ap342:9.1": "Sahassakkhattuṁ devindo,",
  "tha-ap342:9.2": "devarajjamakārayiṁ;",
  "tha-ap342:9.3": "Pañcasattatikkhattuñca,",
  "tha-ap342:9.4": "cakkavattī ahosahaṁ.",
  "tha-ap342:10.1": "Padesarajjaṁ vipulaṁ,",
  "tha-ap342:10.2": "gaṇanāto asaṅkhiyaṁ;",
  "tha-ap342:10.3": "Anubhomi sakaṁ kammaṁ,",
  "tha-ap342:10.4": "pubbe sukatamattano.",
  "tha-ap342:11.1": "Tenevekapadumena,",
  "tha-ap342:11.2": "anubhotvāna sampadā;",
  "tha-ap342:11.3": "Gotamassa bhagavato,",
  "tha-ap342:11.4": "dhammaṁ sacchikariṁ ahaṁ.",
  "tha-ap342:12.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap342:12.2": "bhavā sabbe samūhatā;",
  "tha-ap342:12.3": "Nāgova bandhanaṁ chetvā,",
  "tha-ap342:12.4": "viharāmi anāsavo.",
  "tha-ap342:13.1": "Satasahassito kappe,",
  "tha-ap342:13.2": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap342:13.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap342:13.4": "ekapadumassidaṁ phalaṁ.",
  "tha-ap342:14.1": "Paṭisambhidā catasso,",
  "tha-ap342:14.2": "…pe…",
  "tha-ap342:14.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap342:15.1": "Itthaṁ sudaṁ āyasmā ukkhittapadumiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap342:16.1": "Ukkhittapadumiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap342:17.1": "Gandhodakavaggo catutiṁsatimo.",
  "tha-ap342:18.1": "Tassuddānaṁ",
  "tha-ap342:19.1": "Gandhadhūpo udakañca,",
  "tha-ap342:19.2": "punnāga ekadussakā;",
  "tha-ap342:19.3": "Phusito ca pabhaṅkaro,",
  "tha-ap342:19.4": "kuṭido uttarīyako.",
  "tha-ap342:20.1": "Savanī ekapadumī,",
  "tha-ap342:20.2": "gāthāyo sabbapiṇḍitā;",
  "tha-ap342:20.3": "Ekaṁ gāthāsatañceva,",
  "tha-ap342:20.4": "catutālīsameva ca."
}