{
  "tha-ap472:0.1": "Therāpadāna",
  "tha-ap472:0.2": "Sālakusumiyavagga",
  "tha-ap472:0.3": "10. Nimittabyākaraṇiyattheraapadāna",
  "tha-ap472:1.1": "“Ajjhogāhetvā himavaṁ,",
  "tha-ap472:1.2": "mante vāce mahaṁ tadā;",
  "tha-ap472:1.3": "Catupaññāsasahassāni,",
  "tha-ap472:1.4": "sissā mayhaṁ upaṭṭhahuṁ.",
  "tha-ap472:2.1": "Adhitā vedagū sabbe,",
  "tha-ap472:2.2": "chaḷaṅge pāramiṁ gatā;",
  "tha-ap472:2.3": "Sakavijjāhupatthaddhā,",
  "tha-ap472:2.4": "himavante vasanti te.",
  "tha-ap472:3.1": "Cavitvā tusitā kāyā,",
  "tha-ap472:3.2": "devaputto mahāyaso;",
  "tha-ap472:3.3": "Uppajji mātukucchismiṁ,",
  "tha-ap472:3.4": "sampajāno patissato.",
  "tha-ap472:4.1": "Sambuddhe upapajjante,",
  "tha-ap472:4.2": "dasasahassi kampatha;",
  "tha-ap472:4.3": "Andhā cakkhuṁ alabhiṁsu,",
  "tha-ap472:4.4": "uppajjantamhi nāyake.",
  "tha-ap472:5.1": "Sabbākāraṁ pakampittha,",
  "tha-ap472:5.2": "kevalā vasudhā ayaṁ;",
  "tha-ap472:5.3": "Nigghosasaddaṁ sutvāna,",
  "tha-ap472:5.4": "ubbijjiṁsu mahājanā.",
  "tha-ap472:6.1": "Sabbe janā samāgamma,",
  "tha-ap472:6.2": "āgacchuṁ mama santikaṁ;",
  "tha-ap472:6.3": "‘Vasudhāyaṁ pakampittha,",
  "tha-ap472:6.4": "kiṁ vipāko bhavissati’.",
  "tha-ap472:7.1": "Avacāsiṁ tadā tesaṁ,",
  "tha-ap472:7.2": "‘mā bhetha natthi vo bhayaṁ;",
  "tha-ap472:7.3": "Vissatthā hotha sabbepi,",
  "tha-ap472:7.4": "uppādoyaṁ suvatthiko.",
  "tha-ap472:8.1": "Aṭṭhahetūhi samphussa,",
  "tha-ap472:8.2": "vasudhāyaṁ pakampati;",
  "tha-ap472:8.3": "Tathā nimittā dissanti,",
  "tha-ap472:8.4": "obhāso vipulo mahā.",
  "tha-ap472:9.1": "Asaṁsayaṁ buddhaseṭṭho,",
  "tha-ap472:9.2": "uppajjissati cakkhumā’;",
  "tha-ap472:9.3": "Saññāpetvāna janataṁ,",
  "tha-ap472:9.4": "pañca sīle kathesahaṁ.",
  "tha-ap472:10.1": "Sutvāna pañca sīlāni,",
  "tha-ap472:10.2": "buddhuppādañca dullabhaṁ;",
  "tha-ap472:10.3": "Ubbegajātā sumanā,",
  "tha-ap472:10.4": "tuṭṭhahaṭṭhā ahaṁsu te.",
  "tha-ap472:11.1": "Dvenavute ito kappe,",
  "tha-ap472:11.2": "yaṁ nimittaṁ viyākariṁ;",
  "tha-ap472:11.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap472:11.4": "byākaraṇassidaṁ phalaṁ.",
  "tha-ap472:12.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap472:12.2": "…pe…",
  "tha-ap472:12.3": "viharāmi anāsavo.",
  "tha-ap472:13.1": "Svāgataṁ vata me āsi,",
  "tha-ap472:13.2": "…pe…",
  "tha-ap472:13.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap472:14.1": "Paṭisambhidā catasso,",
  "tha-ap472:14.2": "…pe…",
  "tha-ap472:14.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap472:15.1": "Itthaṁ sudaṁ āyasmā nimittabyākaraṇiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap472:16.1": "Nimittabyākaraṇiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap472:17.1": "Sālakusumiyavaggo sattacattālīsamo.",
  "tha-ap472:18.1": "Tassuddānaṁ",
  "tha-ap472:19.1": "Sālakusumiyo thero,",
  "tha-ap472:19.2": "pūjā nibbāpakopi ca;",
  "tha-ap472:19.3": "Setudo tālavaṇṭī ca,",
  "tha-ap472:19.4": "avaṭalabujappado.",
  "tha-ap472:20.1": "Pilakkhapaṭibhānī ca,",
  "tha-ap472:20.2": "veyyākaraṇiyo dijo;",
  "tha-ap472:20.3": "Dvesattati ca gāthāyo,",
  "tha-ap472:20.4": "gaṇitāyo vibhāvibhi."
}