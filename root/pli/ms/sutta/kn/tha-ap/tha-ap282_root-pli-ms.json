{
  "tha-ap282:0.1": "Therāpadāna",
  "tha-ap282:0.2": "Suvaṇṇabibbohanavagga",
  "tha-ap282:0.3": "10 Taraṇīyattheraapadāna",
  "tha-ap282:1.1": "“Mahāpathamhi visame,",
  "tha-ap282:1.2": "setu kārāpito mayā;",
  "tha-ap282:1.3": "Taraṇatthāya lokassa,",
  "tha-ap282:1.4": "pasanno sehi pāṇibhi.",
  "tha-ap282:2.1": "Ekanavutito kappe,",
  "tha-ap282:2.2": "yo setu kārito mayā;",
  "tha-ap282:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap282:2.4": "setudānassidaṁ phalaṁ.",
  "tha-ap282:3.1": "Pañcapaññāsito kappe,",
  "tha-ap282:3.2": "eko āsiṁ samogadho;",
  "tha-ap282:3.3": "Sattaratanasampanno,",
  "tha-ap282:3.4": "cakkavattī mahabbalo.",
  "tha-ap282:4.1": "Paṭisambhidā catasso,",
  "tha-ap282:4.2": "…pe…",
  "tha-ap282:4.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap282:5.1": "Itthaṁ sudaṁ āyasmā taraṇīyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap282:6.1": "Taraṇīyattherassāpadānaṁ dasamaṁ.",
  "tha-ap282:6.2": "Suvaṇṇabibbohanavaggo aṭṭhavīsatimo.",
  "tha-ap282:7.1": "Tassuddānaṁ",
  "tha-ap282:8.1": "Suvaṇṇaṁ tilamuṭṭhi ca,",
  "tha-ap282:8.2": "caṅkoṭabbhañjanañjalī;",
  "tha-ap282:8.3": "Potthako citamāluvā,",
  "tha-ap282:8.4": "ekapuṇḍarī setunā;",
  "tha-ap282:8.5": "Dvecattālīsa gāthāyo,",
  "tha-ap282:8.6": "gaṇitāyo vibhāvibhīti.",
  "tha-ap282:9.1": "Ekādasamaṁ bhāṇavāraṁ."
}