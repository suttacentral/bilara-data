{
  "tha-ap522:0.1": "Therāpadāna",
  "tha-ap522:0.2": "Phaladāyakavagga",
  "tha-ap522:0.3": "10 Padumadhārikattheraapadāna",
  "tha-ap522:1.1": "“Himavantassāvidūre,",
  "tha-ap522:1.2": "romaso nāma pabbato;",
  "tha-ap522:1.3": "Buddhopi sambhavo nāma,",
  "tha-ap522:1.4": "abbhokāse vasī tadā.",
  "tha-ap522:2.1": "Bhavanā nikkhamitvāna,",
  "tha-ap522:2.2": "padumaṁ dhārayiṁ ahaṁ;",
  "tha-ap522:2.3": "Ekāhaṁ dhārayitvāna,",
  "tha-ap522:2.4": "bhavanaṁ punarāgamiṁ.",
  "tha-ap522:3.1": "Ekatiṁse ito kappe,",
  "tha-ap522:3.2": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap522:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap522:3.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap522:4.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap522:4.2": "…pe…",
  "tha-ap522:4.3": "viharāmi anāsavo.",
  "tha-ap522:5.1": "Svāgataṁ vata me āsi,",
  "tha-ap522:5.2": "…pe…",
  "tha-ap522:5.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap522:6.1": "Paṭisambhidā catasso,",
  "tha-ap522:6.2": "…pe…",
  "tha-ap522:6.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap522:7.1": "Itthaṁ sudaṁ āyasmā padumadhāriko thero imā gāthāyo abhāsitthāti.",
  "tha-ap522:8.1": "Padumadhārikattherassāpadānaṁ dasamaṁ.",
  "tha-ap522:9.1": "Phaladāyakavaggo dvepaññāsamo.",
  "tha-ap522:10.1": "Tassuddānaṁ",
  "tha-ap522:11.1": "Kurañciyaṁ kapitthañca,",
  "tha-ap522:11.2": "kosambamatha ketakaṁ;",
  "tha-ap522:11.3": "Nāgapupphajjunañceva,",
  "tha-ap522:11.4": "kuṭajī ghosasaññako.",
  "tha-ap522:12.1": "Thero ca sabbaphalado,",
  "tha-ap522:12.2": "tathā padumadhāriko;",
  "tha-ap522:12.3": "Asīti cettha gāthāyo,",
  "tha-ap522:12.4": "tisso gāthā taduttari."
}