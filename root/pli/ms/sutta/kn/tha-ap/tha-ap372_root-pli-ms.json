{
  "tha-ap372:0.1": "Therāpadāna",
  "tha-ap372:0.2": "Mandāravapupphiyavagga",
  "tha-ap372:0.3": "10 Saḷalapupphiyattheraapadāna",
  "tha-ap372:1.1": "“Candabhāgānadītīre,",
  "tha-ap372:1.2": "ahosiṁ kinnaro tadā;",
  "tha-ap372:1.3": "Tatthaddasaṁ devadevaṁ,",
  "tha-ap372:1.4": "caṅkamantaṁ narāsabhaṁ.",
  "tha-ap372:2.1": "Ocinitvāna saḷalaṁ,",
  "tha-ap372:2.2": "pupphaṁ buddhassadāsahaṁ;",
  "tha-ap372:2.3": "Upasiṅghi mahāvīro,",
  "tha-ap372:2.4": "saḷalaṁ devagandhikaṁ.",
  "tha-ap372:3.1": "Paṭiggahetvā sambuddho,",
  "tha-ap372:3.2": "vipassī lokanāyako;",
  "tha-ap372:3.3": "Upasiṅghi mahāvīro,",
  "tha-ap372:3.4": "pekkhamānassa me sato.",
  "tha-ap372:4.1": "Pasannacitto sumano,",
  "tha-ap372:4.2": "vanditvā dvipaduttamaṁ;",
  "tha-ap372:4.3": "Añjaliṁ paggahetvāna,",
  "tha-ap372:4.4": "puna pabbatamāruhiṁ.",
  "tha-ap372:5.1": "Ekanavutito kappe,",
  "tha-ap372:5.2": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap372:5.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap372:5.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap372:6.1": "Paṭisambhidā catasso,",
  "tha-ap372:6.2": "…pe…",
  "tha-ap372:6.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap372:7.1": "Itthaṁ sudaṁ āyasmā saḷalapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap372:8.1": "Saḷalapupphiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap372:9.1": "Mandāravapupphiyavaggo sattatiṁsatimo.",
  "tha-ap372:10.1": "Tassuddānaṁ",
  "tha-ap372:11.1": "Mandāravañca kakkāru,",
  "tha-ap372:11.2": "bhisakesarapupphiyo;",
  "tha-ap372:11.3": "Aṅkolako kadambī ca,",
  "tha-ap372:11.4": "uddālī ekacampako;",
  "tha-ap372:11.5": "Timiraṁ saḷalañceva,",
  "tha-ap372:11.6": "gāthā tālīsameva ca."
}