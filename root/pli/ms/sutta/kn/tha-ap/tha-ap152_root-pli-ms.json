{
  "tha-ap152:0.1": "Therāpadāna",
  "tha-ap152:0.2": "Chattavagga",
  "tha-ap152:0.3": "10. Pallaṅkadāyakattheraapadāna",
  "tha-ap152:1.1": "“Sumedhassa bhagavato,",
  "tha-ap152:1.2": "lokajeṭṭhassa tādino;",
  "tha-ap152:1.3": "Pallaṅko hi mayā dinno,",
  "tha-ap152:1.4": "sauttarasapacchado.",
  "tha-ap152:2.1": "Sattaratanasampanno,",
  "tha-ap152:2.2": "pallaṅko āsi so tadā;",
  "tha-ap152:2.3": "Mama saṅkappamaññāya,",
  "tha-ap152:2.4": "nibbattati sadā mama.",
  "tha-ap152:3.1": "Tiṁsakappasahassamhi,",
  "tha-ap152:3.2": "pallaṅkamadadiṁ tadā;",
  "tha-ap152:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap152:3.4": "pallaṅkassa idaṁ phalaṁ.",
  "tha-ap152:4.1": "Vīsakappasahassamhi,",
  "tha-ap152:4.2": "suvaṇṇābhā tayo janā;",
  "tha-ap152:4.3": "Sattaratanasampannā,",
  "tha-ap152:4.4": "cakkavattī mahabbalā.",
  "tha-ap152:5.1": "Paṭisambhidā catasso,",
  "tha-ap152:5.2": "…pe…",
  "tha-ap152:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap152:6.1": "Itthaṁ sudaṁ āyasmā pallaṅkadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap152:7.1": "Pallaṅkadāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap152:8.1": "Chattavaggo pannarasamo.",
  "tha-ap152:9.1": "Tassuddānaṁ",
  "tha-ap152:10.1": "Chattaṁ thambho ca vedi ca,",
  "tha-ap152:10.2": "parivārumapupphiyo;",
  "tha-ap152:10.3": "Anulepo ca maggo ca,",
  "tha-ap152:10.4": "phalako ca vaṭaṁsako;",
  "tha-ap152:10.5": "Pallaṅkadāyī ca gāthāyo,",
  "tha-ap152:10.6": "chappaññāsa pakittitāti."
}