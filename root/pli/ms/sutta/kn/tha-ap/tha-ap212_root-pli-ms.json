{
  "tha-ap212:0.1": "Therāpadāna",
  "tha-ap212:0.2": "Kaṇikārapupphiyavagga",
  "tha-ap212:0.3": "10 Pāpanivāriyattheraapadāna",
  "tha-ap212:1.1": "“Tissassa tu bhagavato,",
  "tha-ap212:1.2": "devadevassa tādino;",
  "tha-ap212:1.3": "Ekacchattaṁ mayā dinnaṁ,",
  "tha-ap212:1.4": "vippasannena cetasā.",
  "tha-ap212:2.1": "Nivutaṁ hoti me pāpaṁ,",
  "tha-ap212:2.2": "kusalassupasampadā;",
  "tha-ap212:2.3": "Ākāse chattaṁ dhārenti,",
  "tha-ap212:2.4": "pubbakammassidaṁ phalaṁ.",
  "tha-ap212:3.1": "Carimaṁ vattate mayhaṁ,",
  "tha-ap212:3.2": "bhavā sabbe samūhatā;",
  "tha-ap212:3.3": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap212:3.4": "sammāsambuddhasāsane.",
  "tha-ap212:4.1": "Dvenavute ito kappe,",
  "tha-ap212:4.2": "yaṁ chattamadadiṁ tadā;",
  "tha-ap212:4.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap212:4.4": "chattadānassidaṁ phalaṁ.",
  "tha-ap212:5.1": "Dvesattatimhito kappe,",
  "tha-ap212:5.2": "aṭṭhāsiṁsu janādhipā;",
  "tha-ap212:5.3": "Mahānidānanāmena,",
  "tha-ap212:5.4": "rājāno cakkavattino.",
  "tha-ap212:6.1": "Paṭisambhidā catasso,",
  "tha-ap212:6.2": "…pe…",
  "tha-ap212:6.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap212:7.1": "Itthaṁ sudaṁ āyasmā pāpanivāriyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap212:8.1": "Pāpanivāriyattherassāpadānaṁ dasamaṁ.",
  "tha-ap212:9.1": "Kaṇikārapupphiyavaggo ekavīsatimo.",
  "tha-ap212:10.1": "Tassuddānaṁ",
  "tha-ap212:11.1": "Kaṇikāro minelañca,",
  "tha-ap212:11.2": "kiṅkaṇi taraṇena ca;",
  "tha-ap212:11.3": "Nigguṇḍipupphī dakado,",
  "tha-ap212:11.4": "salalo ca kuraṇḍako;",
  "tha-ap212:11.5": "Ādhārako pāpavārī,",
  "tha-ap212:11.6": "aṭṭhatālīsa gāthakāti."
}