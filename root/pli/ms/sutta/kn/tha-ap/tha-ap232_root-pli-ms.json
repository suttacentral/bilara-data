{
  "tha-ap232:0.1": "Therāpadāna",
  "tha-ap232:0.2": "Ālambaṇadāyakavagga",
  "tha-ap232:0.3": "10 Ekavandaniyattheraapadāna",
  "tha-ap232:1.1": "“Usabhaṁ pavaraṁ vīraṁ,",
  "tha-ap232:1.2": "vessabhuṁ vijitāvinaṁ;",
  "tha-ap232:1.3": "Pasannacitto sumano,",
  "tha-ap232:1.4": "buddhaseṭṭhamavandahaṁ.",
  "tha-ap232:2.1": "Ekattiṁse ito kappe,",
  "tha-ap232:2.2": "yaṁ kammamakariṁ tadā;",
  "tha-ap232:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap232:2.4": "vandanāya idaṁ phalaṁ.",
  "tha-ap232:3.1": "Catuvīsatikappamhi,",
  "tha-ap232:3.2": "vikatānandanāmako;",
  "tha-ap232:3.3": "Sattaratanasampanno,",
  "tha-ap232:3.4": "cakkavattī mahabbalo.",
  "tha-ap232:4.1": "Paṭisambhidā catasso,",
  "tha-ap232:4.2": "…pe…",
  "tha-ap232:4.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap232:5.1": "Itthaṁ sudaṁ āyasmā ekavandaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap232:6.1": "Ekavandaniyattherassāpadānaṁ dasamaṁ.",
  "tha-ap232:7.1": "Ālambaṇadāyakavaggo tevīsatimo.",
  "tha-ap232:8.1": "Tassuddānaṁ",
  "tha-ap232:9.1": "Ālambaṇañca ajinaṁ,",
  "tha-ap232:9.2": "maṁsadārakkhadāyako;",
  "tha-ap232:9.3": "Abyādhi aṅkolaṁ soṇṇaṁ,",
  "tha-ap232:9.4": "miñjaāveḷavandanaṁ;",
  "tha-ap232:9.5": "Pañcapaññāsa gāthāyo,",
  "tha-ap232:9.6": "gaṇitā atthadassibhi."
}