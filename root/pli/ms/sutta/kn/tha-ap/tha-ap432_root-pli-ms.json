{
  "tha-ap432:0.1": "Therāpadāna",
  "tha-ap432:0.2": "Sakiṁsammajjakavagga",
  "tha-ap432:0.3": "10 Soṇṇakontarikattheraapadāna",
  "tha-ap432:1.1": "“Manobhāvaniyaṁ buddhaṁ,",
  "tha-ap432:1.2": "attadantaṁ samāhitaṁ;",
  "tha-ap432:1.3": "Iriyamānaṁ brahmapathe,",
  "tha-ap432:1.4": "cittavūpasame rataṁ.",
  "tha-ap432:2.1": "Nittiṇṇaoghaṁ sambuddhaṁ,",
  "tha-ap432:2.2": "jhāyiṁ jhānarataṁ muniṁ;",
  "tha-ap432:2.3": "Upatitthaṁ samāpannaṁ,",
  "tha-ap432:2.4": "indivaradalappabhaṁ.",
  "tha-ap432:3.1": "Alābunodakaṁ gayha,",
  "tha-ap432:3.2": "buddhaseṭṭhaṁ upāgamiṁ;",
  "tha-ap432:3.3": "Buddhassa pāde dhovitvā,",
  "tha-ap432:3.4": "alābukamadāsahaṁ.",
  "tha-ap432:4.1": "Āṇāpesi ca sambuddho,",
  "tha-ap432:4.2": "padumuttaranāmako;",
  "tha-ap432:4.3": "‘Iminā dakamāhatvā,",
  "tha-ap432:4.4": "pādamūle ṭhapehi me’.",
  "tha-ap432:5.1": "Sādhūtihaṁ paṭissutvā,",
  "tha-ap432:5.2": "satthugāravatāya ca;",
  "tha-ap432:5.3": "Dakaṁ alābunāhatvā,",
  "tha-ap432:5.4": "buddhaseṭṭhaṁ upāgamiṁ.",
  "tha-ap432:6.1": "Anumodi mahāvīro,",
  "tha-ap432:6.2": "cittaṁ nibbāpayaṁ mama;",
  "tha-ap432:6.3": "‘Iminālābudānena,",
  "tha-ap432:6.4": "saṅkappo te samijjhatu’.",
  "tha-ap432:7.1": "Pannarasesu kappesu,",
  "tha-ap432:7.2": "devaloke ramiṁ ahaṁ;",
  "tha-ap432:7.3": "Tiṁsatikkhattuṁ rājā ca,",
  "tha-ap432:7.4": "cakkavattī ahosahaṁ.",
  "tha-ap432:8.1": "Divā vā yadi vā rattiṁ,",
  "tha-ap432:8.2": "caṅkamantassa tiṭṭhato;",
  "tha-ap432:8.3": "Sovaṇṇaṁ kontaraṁ gayha,",
  "tha-ap432:8.4": "tiṭṭhate purato mama.",
  "tha-ap432:9.1": "Buddhassa datvānalābuṁ,",
  "tha-ap432:9.2": "labhāmi soṇṇakontaraṁ;",
  "tha-ap432:9.3": "Appakampi kataṁ kāraṁ,",
  "tha-ap432:9.4": "vipulaṁ hoti tādisu.",
  "tha-ap432:10.1": "Satasahassito kappe,",
  "tha-ap432:10.2": "yaṁlābumadadiṁ tadā;",
  "tha-ap432:10.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap432:10.4": "alābussa idaṁ phalaṁ.",
  "tha-ap432:11.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap432:11.2": "…pe…",
  "tha-ap432:11.3": "viharāmi anāsavo.",
  "tha-ap432:12.1": "Svāgataṁ vata me āsi,",
  "tha-ap432:12.2": "…pe…",
  "tha-ap432:12.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap432:13.1": "Paṭisambhidā catasso,",
  "tha-ap432:13.2": "…pe…",
  "tha-ap432:13.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap432:14.1": "Itthaṁ sudaṁ āyasmā soṇṇakontariko thero imā gāthāyo abhāsitthāti.",
  "tha-ap432:15.1": "Soṇṇakontarikattherassāpadānaṁ dasamaṁ.",
  "tha-ap432:16.1": "Sakiṁsammajjakavaggo tecattālīsamo.",
  "tha-ap432:17.1": "Tassuddānaṁ",
  "tha-ap432:18.1": "Sakiṁsammajjako thero,",
  "tha-ap432:18.2": "ekadussī ekāsanī;",
  "tha-ap432:18.3": "Kadambakoraṇḍakado,",
  "tha-ap432:18.4": "ghatassavanikopi ca.",
  "tha-ap432:19.1": "Sucintiko kiṅkaṇiko,",
  "tha-ap432:19.2": "soṇṇakontarikopi ca;",
  "tha-ap432:19.3": "Ekagāthāsatañcettha,",
  "tha-ap432:19.4": "ekasattatimeva ca."
}