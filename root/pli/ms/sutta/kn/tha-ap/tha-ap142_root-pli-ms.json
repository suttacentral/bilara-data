{
  "tha-ap142:0.1": "Therāpadāna",
  "tha-ap142:0.2": "Sobhitavagga",
  "tha-ap142:0.3": "10 Piyālaphaladāyakattheraapadāna",
  "tha-ap142:1.1": "“Pārāvato tadā āsiṁ,",
  "tha-ap142:1.2": "paraṁ anuparodhako;",
  "tha-ap142:1.3": "Pabbhāre seyyaṁ kappemi,",
  "tha-ap142:1.4": "avidūre sikhisatthuno.",
  "tha-ap142:2.1": "Sāyaṁ pātañca passāmi,",
  "tha-ap142:2.2": "buddhaṁ lokagganāyakaṁ;",
  "tha-ap142:2.3": "Deyyadhammo ca me natthi,",
  "tha-ap142:2.4": "dvipadindassa tādino.",
  "tha-ap142:3.1": "Piyālaphalamādāya,",
  "tha-ap142:3.2": "agamaṁ buddhasantikaṁ;",
  "tha-ap142:3.3": "Paṭiggahesi bhagavā,",
  "tha-ap142:3.4": "lokajeṭṭho narāsabho.",
  "tha-ap142:4.1": "Tato paraṁ upādāya,",
  "tha-ap142:4.2": "paricāriṁ vināyakaṁ;",
  "tha-ap142:4.3": "Tena cittappasādena,",
  "tha-ap142:4.4": "tattha kālaṅkato ahaṁ.",
  "tha-ap142:5.1": "Ekattiṁse ito kappe,",
  "tha-ap142:5.2": "yaṁ phalaṁ adadiṁ ahaṁ;",
  "tha-ap142:5.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap142:5.4": "phaladānassidaṁ phalaṁ.",
  "tha-ap142:6.1": "Ito pannarase kappe,",
  "tha-ap142:6.2": "tayo āsuṁ piyālino;",
  "tha-ap142:6.3": "Sattaratanasampannā,",
  "tha-ap142:6.4": "cakkavattī mahabbalā.",
  "tha-ap142:7.1": "Paṭisambhidā catasso,",
  "tha-ap142:7.2": "…pe…",
  "tha-ap142:7.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap142:8.1": "Itthaṁ sudaṁ āyasmā piyālaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap142:9.1": "Piyālaphaladāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap142:10.1": "Sobhitavaggo cuddasamo.",
  "tha-ap142:11.1": "Tassuddānaṁ",
  "tha-ap142:12.1": "Sobhitasudassano ca,",
  "tha-ap142:12.2": "candano pupphachadano;",
  "tha-ap142:12.3": "Raho campakapupphī ca,",
  "tha-ap142:12.4": "atthasandassakena ca.",
  "tha-ap142:13.1": "Ekapasādī sāladado,",
  "tha-ap142:13.2": "dasamo phaladāyako;",
  "tha-ap142:13.3": "Gāthāyo sattati dve ca,",
  "tha-ap142:13.4": "gaṇitāyo vibhāvibhi."
}