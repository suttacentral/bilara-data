{
  "tha-ap262:0.1": "Therāpadāna",
  "tha-ap262:0.2": "Thomakavagga",
  "tha-ap262:0.3": "10. Aggapupphiyattheraapadāna",
  "tha-ap262:1.1": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap262:1.2": "Nisinnaṁ pabbatantare;",
  "tha-ap262:1.3": "Obhāsayantaṁ raṁsena,",
  "tha-ap262:1.4": "Sikhinaṁ sikhinaṁ yathā.",
  "tha-ap262:2.1": "Aggajaṁ pupphamādāya,",
  "tha-ap262:2.2": "upāgacchiṁ naruttamaṁ;",
  "tha-ap262:2.3": "Pasannacitto sumano,",
  "tha-ap262:2.4": "buddhassa abhiropayiṁ.",
  "tha-ap262:3.1": "Ekattiṁse ito kappe,",
  "tha-ap262:3.2": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap262:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap262:3.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap262:4.1": "Pañcavīsatikappamhi,",
  "tha-ap262:4.2": "ahosi amitogato;",
  "tha-ap262:4.3": "Sattaratanasampanno,",
  "tha-ap262:4.4": "cakkavattī mahabbalo.",
  "tha-ap262:5.1": "Paṭisambhidā catasso,",
  "tha-ap262:5.2": "…pe…",
  "tha-ap262:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap262:6.1": "Itthaṁ sudaṁ āyasmā aggapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap262:7.1": "Aggapupphiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap262:8.1": "Thomakavaggo chabbīsatimo.",
  "tha-ap262:9.1": "Tassuddānaṁ",
  "tha-ap262:10.1": "Thomakekāsanacitakaṁ,",
  "tha-ap262:10.2": "campako sattapāṭali;",
  "tha-ap262:10.3": "Pānadhi mañjarī paṇṇaṁ,",
  "tha-ap262:10.4": "kuṭido aggapupphiyo;",
  "tha-ap262:10.5": "Gāthāyo gaṇitā cettha,",
  "tha-ap262:10.6": "ekatālīsameva cāti."
}