{
  "tha-ap392:0.1": "Therāpadāna",
  "tha-ap392:0.2": "Avaṭaphalavagga",
  "tha-ap392:0.3": "10 Pubbakammapilotikabuddhaapadāna",
  "tha-ap392:1.1": "Anotattasarāsanne,",
  "tha-ap392:1.2": "ramaṇīye silātale;",
  "tha-ap392:1.3": "Nānāratanapajjote,",
  "tha-ap392:1.4": "nānāgandhavanantare.",
  "tha-ap392:2.1": "Mahatā bhikkhusaṅghena,",
  "tha-ap392:2.2": "pareto lokanāyako;",
  "tha-ap392:2.3": "Āsīno byākarī tattha,",
  "tha-ap392:2.4": "pubbakammāni attano.",
  "tha-ap392:3.1": "“Suṇātha bhikkhavo mayhaṁ,",
  "tha-ap392:3.2": "yaṁ kammaṁ pakataṁ mayā;",
  "tha-ap392:3.3": "Pilotikassa kammassa,",
  "tha-ap392:3.4": "buddhattepi vipaccati.",
  "tha-ap392:4.0": "10.1",
  "tha-ap392:4.1": "Munāḷi nāmahaṁ dhutto,",
  "tha-ap392:4.2": "pubbe aññāsu jātisu;",
  "tha-ap392:4.3": "Paccekabuddhaṁ surabhiṁ,",
  "tha-ap392:4.4": "abbhācikkhiṁ adūsakaṁ.",
  "tha-ap392:5.1": "Tena kammavipākena,",
  "tha-ap392:5.2": "niraye saṁsariṁ ciraṁ;",
  "tha-ap392:5.3": "Bahūvassasahassāni,",
  "tha-ap392:5.4": "dukkhaṁ vedesi vedanaṁ.",
  "tha-ap392:6.1": "Tena kammāvasesena,",
  "tha-ap392:6.2": "idha pacchimake bhave;",
  "tha-ap392:6.3": "Abbhakkhānaṁ mayā laddhaṁ,",
  "tha-ap392:6.4": "sundarikāya kāraṇā.",
  "tha-ap392:7.0": "10.2",
  "tha-ap392:7.1": "Sabbābhibhussa buddhassa,",
  "tha-ap392:7.2": "nando nāmāsi sāvako;",
  "tha-ap392:7.3": "Taṁ abbhakkhāya niraye,",
  "tha-ap392:7.4": "ciraṁ saṁsaritaṁ mayā.",
  "tha-ap392:8.1": "Dasavassasahassāni,",
  "tha-ap392:8.2": "niraye saṁsariṁ ciraṁ;",
  "tha-ap392:8.3": "Manussabhāvaṁ laddhāhaṁ,",
  "tha-ap392:8.4": "abbhakkhānaṁ bahuṁ labhiṁ.",
  "tha-ap392:9.1": "Tena kammāvasesena,",
  "tha-ap392:9.2": "ciñcamānavikā mamaṁ;",
  "tha-ap392:9.3": "Abbhācikkhi abhūtena,",
  "tha-ap392:9.4": "janakāyassa aggato.",
  "tha-ap392:10.0": "10.3",
  "tha-ap392:10.1": "Brāhmaṇo sutavā āsiṁ,",
  "tha-ap392:10.2": "ahaṁ sakkatapūjito;",
  "tha-ap392:10.3": "Mahāvane pañcasate,",
  "tha-ap392:10.4": "mante vācemi māṇave.",
  "tha-ap392:11.1": "Tatthāgato isi bhīmo,",
  "tha-ap392:11.2": "pañcābhiñño mahiddhiko;",
  "tha-ap392:11.3": "Tañcāhaṁ āgataṁ disvā,",
  "tha-ap392:11.4": "abbhācikkhiṁ adūsakaṁ.",
  "tha-ap392:12.1": "Tatohaṁ avacaṁ sisse,",
  "tha-ap392:12.2": "kāmabhogī ayaṁ isi;",
  "tha-ap392:12.3": "Mayhampi bhāsamānassa,",
  "tha-ap392:12.4": "anumodiṁsu māṇavā.",
  "tha-ap392:13.1": "Tato māṇavakā sabbe,",
  "tha-ap392:13.2": "bhikkhamānaṁ kule kule;",
  "tha-ap392:13.3": "Mahājanassa āhaṁsu,",
  "tha-ap392:13.4": "kāmabhogī ayaṁ isi.",
  "tha-ap392:14.1": "Tena kammavipākena,",
  "tha-ap392:14.2": "pañca bhikkhusatā ime;",
  "tha-ap392:14.3": "Abbhakkhānaṁ labhuṁ sabbe,",
  "tha-ap392:14.4": "sundarikāya kāraṇā.",
  "tha-ap392:15.0": "10.4",
  "tha-ap392:15.1": "Vemātubhātaraṁ pubbe,",
  "tha-ap392:15.2": "dhanahetu haniṁ ahaṁ;",
  "tha-ap392:15.3": "Pakkhipiṁ giriduggasmiṁ,",
  "tha-ap392:15.4": "silāya ca apiṁsayiṁ.",
  "tha-ap392:16.1": "Tena kammavipākena,",
  "tha-ap392:16.2": "devadatto silaṁ khipi;",
  "tha-ap392:16.3": "Aṅguṭṭhaṁ piṁsayī pāde,",
  "tha-ap392:16.4": "mama pāsāṇasakkharā.",
  "tha-ap392:17.0": "10.5",
  "tha-ap392:17.1": "Purehaṁ dārako hutvā,",
  "tha-ap392:17.2": "kīḷamāno mahāpathe;",
  "tha-ap392:17.3": "Paccekabuddhaṁ disvāna,",
  "tha-ap392:17.4": "magge sakalikaṁ khipiṁ.",
  "tha-ap392:18.1": "Tena kammavipākena,",
  "tha-ap392:18.2": "idha pacchimake bhave;",
  "tha-ap392:18.3": "Vadhatthaṁ maṁ devadatto,",
  "tha-ap392:18.4": "abhimāre payojayi.",
  "tha-ap392:19.0": "10.6",
  "tha-ap392:19.1": "Hatthāroho pure āsiṁ,",
  "tha-ap392:19.2": "paccekamunimuttamaṁ;",
  "tha-ap392:19.3": "Piṇḍāya vicarantaṁ taṁ,",
  "tha-ap392:19.4": "āsādesiṁ gajenahaṁ.",
  "tha-ap392:20.1": "Tena kammavipākena,",
  "tha-ap392:20.2": "bhanto nāḷāgirī gajo;",
  "tha-ap392:20.3": "Giribbaje puravare,",
  "tha-ap392:20.4": "dāruṇo samupāgami.",
  "tha-ap392:21.0": "10.7",
  "tha-ap392:21.1": "Rājāhaṁ patthivo āsiṁ,",
  "tha-ap392:21.2": "sattiyā purisaṁ haniṁ;",
  "tha-ap392:21.3": "Tena kammavipākena,",
  "tha-ap392:21.4": "niraye paccisaṁ bhusaṁ.",
  "tha-ap392:22.1": "Kammuno tassa sesena,",
  "tha-ap392:22.2": "idāni sakalaṁ mama;",
  "tha-ap392:22.3": "Pāde chaviṁ pakappesi,",
  "tha-ap392:22.4": "na hi kammaṁ vinassati.",
  "tha-ap392:23.0": "10.8",
  "tha-ap392:23.1": "Ahaṁ kevaṭṭagāmasmiṁ,",
  "tha-ap392:23.2": "ahuṁ kevaṭṭadārako;",
  "tha-ap392:23.3": "Macchake ghātite disvā,",
  "tha-ap392:23.4": "janayiṁ somanassakaṁ.",
  "tha-ap392:24.1": "Tena kammavipākena,",
  "tha-ap392:24.2": "sīsadukkhaṁ ahū mama;",
  "tha-ap392:24.3": "Sabbe sakkā ca haññiṁsu,",
  "tha-ap392:24.4": "yadā hani viṭaṭūbho.",
  "tha-ap392:25.0": "10.9",
  "tha-ap392:25.1": "Phussassāhaṁ pāvacane,",
  "tha-ap392:25.2": "sāvake paribhāsayiṁ;",
  "tha-ap392:25.3": "Yavaṁ khādatha bhuñjatha,",
  "tha-ap392:25.4": "mā ca bhuñjatha sālayo.",
  "tha-ap392:26.1": "Tena kammavipākena,",
  "tha-ap392:26.2": "temāsaṁ khāditaṁ yavaṁ;",
  "tha-ap392:26.3": "Nimantito brāhmaṇena,",
  "tha-ap392:26.4": "verañjāyaṁ vasiṁ tadā.",
  "tha-ap392:27.0": "10.10",
  "tha-ap392:27.1": "Nibbuddhe vattamānamhi,",
  "tha-ap392:27.2": "mallaputtaṁ niheṭhayiṁ;",
  "tha-ap392:27.3": "Tena kammavipākena,",
  "tha-ap392:27.4": "piṭṭhidukkhaṁ ahū mama.",
  "tha-ap392:28.0": "10.11",
  "tha-ap392:28.1": "Tikicchako ahaṁ āsiṁ,",
  "tha-ap392:28.2": "seṭṭhiputtaṁ virecayiṁ;",
  "tha-ap392:28.3": "Tena kammavipākena,",
  "tha-ap392:28.4": "hoti pakkhandikā mama.",
  "tha-ap392:29.0": "10.12",
  "tha-ap392:29.1": "Avacāhaṁ jotipālo,",
  "tha-ap392:29.2": "sugataṁ kassapaṁ tadā;",
  "tha-ap392:29.3": "Kuto nu bodhi muṇḍassa,",
  "tha-ap392:29.4": "bodhi paramadullabhā.",
  "tha-ap392:30.1": "Tena kammavipākena,",
  "tha-ap392:30.2": "acariṁ dukkaraṁ bahuṁ;",
  "tha-ap392:30.3": "Chabbassānuruvelāyaṁ,",
  "tha-ap392:30.4": "tato bodhimapāpuṇiṁ.",
  "tha-ap392:31.1": "Nāhaṁ etena maggena,",
  "tha-ap392:31.2": "pāpuṇiṁ bodhimuttamaṁ;",
  "tha-ap392:31.3": "Kummaggena gavesissaṁ,",
  "tha-ap392:31.4": "pubbakammena vārito.",
  "tha-ap392:32.1": "Puññapāpaparikkhīṇo,",
  "tha-ap392:32.2": "sabbasantāpavajjito;",
  "tha-ap392:32.3": "Asoko anupāyāso,",
  "tha-ap392:32.4": "nibbāyissamanāsavo”.",
  "tha-ap392:33.1": "Evaṁ jino viyākāsi,",
  "tha-ap392:33.2": "Bhikkhusaṅghassa aggato;",
  "tha-ap392:33.3": "Sabbābhiññābalappatto,",
  "tha-ap392:33.4": "Anotatte mahāsareti. ",
  "tha-ap392:34.1": "Itthaṁ sudaṁ bhagavā attano pubbacaritaṁ kammapilotikaṁ nāma buddhāpadānadhammapariyāyaṁ abhāsitthāti.",
  "tha-ap392:35.1": "Pubbakammapilotikaṁ nāma buddhāpadānaṁ dasamaṁ.",
  "tha-ap392:36.1": "Avaṭaphalavaggo ekūnacattālīsamo.",
  "tha-ap392:37.1": "Tassuddānaṁ",
  "tha-ap392:38.1": "Avaṭaṁ labujañceva,",
  "tha-ap392:38.2": "udumbarapilakkhu ca;",
  "tha-ap392:38.3": "Phāru vallī ca kadalī,",
  "tha-ap392:38.4": "panaso koṭivīsako.",
  "tha-ap392:39.1": "Pubbakammapiloti ca,",
  "tha-ap392:39.2": "apadānaṁ mahesino;",
  "tha-ap392:39.3": "Gāthāyo ekanavuti,",
  "tha-ap392:39.4": "gaṇitāyo vibhāvibhi.",
  "tha-ap392:40.1": "Cuddasamaṁ bhāṇavāraṁ."
}