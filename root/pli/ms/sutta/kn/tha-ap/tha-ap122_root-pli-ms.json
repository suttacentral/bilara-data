{
  "tha-ap122:0.1": "Therāpadāna",
  "tha-ap122:0.2": "Mahāparivāravagga",
  "tha-ap122:0.3": "10. Jātipūjakattheraapadāna",
  "tha-ap122:1.1": "“Jāyantassa vipassissa,",
  "tha-ap122:1.2": "āloko vipulo ahu;",
  "tha-ap122:1.3": "Pathavī ca pakampittha,",
  "tha-ap122:1.4": "sasāgarā sapabbatā.",
  "tha-ap122:2.1": "Nemittā ca viyākaṁsu,",
  "tha-ap122:2.2": "buddho loke bhavissati;",
  "tha-ap122:2.3": "Aggo ca sabbasattānaṁ,",
  "tha-ap122:2.4": "janataṁ uddharissati.",
  "tha-ap122:3.1": "Nemittānaṁ suṇitvāna,",
  "tha-ap122:3.2": "jātipūjamakāsahaṁ;",
  "tha-ap122:3.3": "Edisā pūjanā natthi,",
  "tha-ap122:3.4": "yādisā jātipūjanā.",
  "tha-ap122:4.1": "Saṅkharitvāna kusalaṁ,",
  "tha-ap122:4.2": "sakaṁ cittaṁ pasādayiṁ;",
  "tha-ap122:4.3": "Jātipūjaṁ karitvāna,",
  "tha-ap122:4.4": "tattha kālaṅkato ahaṁ.",
  "tha-ap122:5.1": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap122:5.2": "devattaṁ atha mānusaṁ;",
  "tha-ap122:5.3": "Sabbe satte abhibhomi,",
  "tha-ap122:5.4": "jātipūjāyidaṁ phalaṁ.",
  "tha-ap122:6.1": "Dhātiyo maṁ upaṭṭhanti,",
  "tha-ap122:6.2": "mama cittavasānugā;",
  "tha-ap122:6.3": "Na tā sakkonti kopetuṁ,",
  "tha-ap122:6.4": "jātipūjāyidaṁ phalaṁ.",
  "tha-ap122:7.1": "Ekanavutito kappe,",
  "tha-ap122:7.2": "yaṁ pūjamakariṁ tadā;",
  "tha-ap122:7.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap122:7.4": "jātipūjāyidaṁ phalaṁ.",
  "tha-ap122:8.1": "Supāricariyā nāma,",
  "tha-ap122:8.2": "catuttiṁsa janādhipā;",
  "tha-ap122:8.3": "Ito tatiyakappamhi,",
  "tha-ap122:8.4": "cakkavattī mahabbalā.",
  "tha-ap122:9.1": "Paṭisambhidā catasso,",
  "tha-ap122:9.2": "…pe…",
  "tha-ap122:9.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap122:10.1": "Itthaṁ sudaṁ āyasmā jātipūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap122:11.1": "Jātipūjakattherassāpadānaṁ dasamaṁ.",
  "tha-ap122:12.1": "Mahāparivāravaggo dvādasamo.",
  "tha-ap122:13.1": "Tassuddānaṁ",
  "tha-ap122:14.1": "Parivārasumaṅgalā,",
  "tha-ap122:14.2": "saraṇāsanapupphiyā;",
  "tha-ap122:14.3": "Citapūjī buddhasaññī,",
  "tha-ap122:14.4": "maggupaṭṭhānajātinā;",
  "tha-ap122:14.5": "Gāthāyo navuti vuttā,",
  "tha-ap122:14.6": "gaṇitāyo vibhāvihi."
}