{
  "dhp383:0.1": "Khuddakanikāya",
  "dhp383:0.2": "Dhammapada",
  "dhp383:0.3": "Brāhmaṇavagga",
  "dhp383:0.4": "Pasādabahulabrāhmaṇavatthu",
  "dhp383:1": "Chinda sotaṃ parakkamma,",
  "dhp383:2": "kāme panuda brāhmaṇa;",
  "dhp383:3": "Saṅkhārānaṃ khayaṃ ñatvā,",
  "dhp383:4": "akataññūsi brāhmaṇa.",
  "dhp384:0": "Sambahulabhikkhuvatthu",
  "dhp384:1": "Yadā dvayesu dhammesu,",
  "dhp384:2": "pāragū hoti brāhmaṇo;",
  "dhp384:3": "Athassa sabbe saṃyogā,",
  "dhp384:4": "atthaṃ gacchanti jānato.",
  "dhp385:0": "Māravatthu",
  "dhp385:1": "Yassa pāraṃ apāraṃ vā,",
  "dhp385:2": "pārāpāraṃ na vijjati;",
  "dhp385:3": "Vītaddaraṃ visaṃyuttaṃ,",
  "dhp385:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp386:0": "Aññatarabrāhmaṇavatthu",
  "dhp386:1": "Jhāyiṃ virajamāsīnaṃ,",
  "dhp386:2": "katakiccamanāsavaṃ;",
  "dhp386:3": "Uttamatthamanuppattaṃ,",
  "dhp386:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp387:0": "Ānandattheravatthu",
  "dhp387:1": "Divā tapati ādicco,",
  "dhp387:2": "rattimābhāti candimā;",
  "dhp387:3": "Sannaddho khattiyo tapati,",
  "dhp387:4": "jhāyī tapati brāhmaṇo;",
  "dhp387:5": "Atha sabbamahorattiṃ,",
  "dhp387:6": "buddho tapati tejasā.",
  "dhp388:0": "Aññatarabrāhmaṇapabbajitavatthu",
  "dhp388:1": "Bāhitapāpoti brāhmaṇo,",
  "dhp388:2": "Samacariyā samaṇoti vuccati;",
  "dhp388:3": "Pabbājayamattano malaṃ,",
  "dhp388:4": "Tasmā “pabbajito”ti vuccati.",
  "dhp389:0": "Sāriputtattheravatthu",
  "dhp389:1": "Na brāhmaṇassa pahareyya,",
  "dhp389:2": "nāssa muñcetha brāhmaṇo;",
  "dhp389:3": "Dhī brāhmaṇassa hantāraṃ,",
  "dhp389:4": "tato dhī yassa muñcati.",
  "dhp390:1": "Na brāhmaṇassetadakiñci seyyo,",
  "dhp390:2": "Yadā nisedho manaso piyehi;",
  "dhp390:3": "Yato yato hiṃsamano nivattati,",
  "dhp390:4": "Tato tato sammatimeva dukkhaṃ.",
  "dhp391:0": "Mahāpajāpatigotamīvatthu",
  "dhp391:1": "Yassa kāyena vācāya,",
  "dhp391:2": "manasā natthi dukkaṭaṃ;",
  "dhp391:3": "Saṃvutaṃ tīhi ṭhānehi,",
  "dhp391:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp392:0": "Sāriputtattheravatthu",
  "dhp392:1": "Yamhā dhammaṃ vijāneyya,",
  "dhp392:2": "sammāsambuddhadesitaṃ;",
  "dhp392:3": "Sakkaccaṃ taṃ namasseyya,",
  "dhp392:4": "aggihuttaṃva brāhmaṇo.",
  "dhp393:0": "Jaṭilabrāhmaṇavatthu",
  "dhp393:1": "Na jaṭāhi na gottena,",
  "dhp393:2": "na jaccā hoti brāhmaṇo;",
  "dhp393:3": "Yamhi saccañca dhammo ca,",
  "dhp393:4": "so sucī so ca brāhmaṇo.",
  "dhp394:0": "Kuhakabrāhmaṇavatthu",
  "dhp394:1": "Kiṃ te jaṭāhi dummedha,",
  "dhp394:2": "kiṃ te ajinasāṭiyā;",
  "dhp394:3": "Abbhantaraṃ te gahanaṃ,",
  "dhp394:4": "bāhiraṃ parimajjasi.",
  "dhp395:0": "Kisāgotamīvatthu",
  "dhp395:1": "Paṃsukūladharaṃ jantuṃ,",
  "dhp395:2": "kisaṃ dhamanisanthataṃ;",
  "dhp395:3": "Ekaṃ vanasmiṃ jhāyantaṃ,",
  "dhp395:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp396:0": "Ekabrāhmaṇavatthu",
  "dhp396:1": "Na cāhaṃ brāhmaṇaṃ brūmi,",
  "dhp396:2": "yonijaṃ mattisambhavaṃ;",
  "dhp396:3": "Bhovādi nāma so hoti,",
  "dhp396:4": "sace hoti sakiñcano;",
  "dhp396:5": "Akiñcanaṃ anādānaṃ,",
  "dhp396:6": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp397:0": "Uggasenaseṭṭhiputtavatthu",
  "dhp397:1": "Sabbasaṃyojanaṃ chetvā,",
  "dhp397:2": "yo ve na paritassati;",
  "dhp397:3": "Saṅgātigaṃ visaṃyuttaṃ,",
  "dhp397:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp398:0": "Dvebrāhmaṇavatthu",
  "dhp398:1": "Chetvā naddhiṃ varattañca,",
  "dhp398:2": "sandānaṃ sahanukkamaṃ;",
  "dhp398:3": "Ukkhittapalighaṃ buddhaṃ,",
  "dhp398:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp399:0": "Akkosakabhāradvājavatthu",
  "dhp399:1": "Akkosaṃ vadhabandhañca,",
  "dhp399:2": "aduṭṭho yo titikkhati;",
  "dhp399:3": "Khantībalaṃ balānīkaṃ,",
  "dhp399:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp400:0": "Sāriputtattheravatthu",
  "dhp400:1": "Akkodhanaṃ vatavantaṃ,",
  "dhp400:2": "sīlavantaṃ anussadaṃ;",
  "dhp400:3": "Dantaṃ antimasārīraṃ,",
  "dhp400:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp401:0": "Uppalavaṇṇāttherīvatthu",
  "dhp401:1": "Vāri pokkharapatteva,",
  "dhp401:2": "āraggeriva sāsapo;",
  "dhp401:3": "Yo na limpati kāmesu,",
  "dhp401:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp402:0": "Aññatarabrāhmaṇavatthu",
  "dhp402:1": "Yo dukkhassa pajānāti,",
  "dhp402:2": "idheva khayamattano;",
  "dhp402:3": "Pannabhāraṃ visaṃyuttaṃ,",
  "dhp402:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp403:0": "Khemābhikkhunīvatthu",
  "dhp403:1": "Gambhīrapaññaṃ medhāviṃ,",
  "dhp403:2": "maggāmaggassa kovidaṃ;",
  "dhp403:3": "Uttamatthamanuppattaṃ,",
  "dhp403:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp404:0": "Pabbhāravāsitissattheravatthu",
  "dhp404:1": "Asaṃsaṭṭhaṃ gahaṭṭhehi,",
  "dhp404:2": "anāgārehi cūbhayaṃ;",
  "dhp404:3": "Anokasārimappicchaṃ,",
  "dhp404:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp405:0": "Aññatarabhikkhuvatthu",
  "dhp405:1": "Nidhāya daṇḍaṃ bhūtesu,",
  "dhp405:2": "tasesu thāvaresu ca;",
  "dhp405:3": "Yo na hanti na ghāteti,",
  "dhp405:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp406:0": "Sāmaṇerānaṃvatthu",
  "dhp406:1": "Aviruddhaṃ viruddhesu,",
  "dhp406:2": "attadaṇḍesu nibbutaṃ;",
  "dhp406:3": "Sādānesu anādānaṃ,",
  "dhp406:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp407:0": "Mahāpanthakattheravatthu",
  "dhp407:1": "Yassa rāgo ca doso ca,",
  "dhp407:2": "māno makkho ca pātito;",
  "dhp407:3": "Sāsaporiva āraggā,",
  "dhp407:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp408:0": "Pilindavacchattheravatthu",
  "dhp408:1": "Akakkasaṃ viññāpaniṃ,",
  "dhp408:2": "giraṃ saccamudīraye;",
  "dhp408:3": "Yāya nābhisaje kañci,",
  "dhp408:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp409:0": "Aññatarattheravatthu",
  "dhp409:1": "Yodha dīghaṃ va rassaṃ vā,",
  "dhp409:2": "aṇuṃ thūlaṃ subhāsubhaṃ;",
  "dhp409:3": "Loke adinnaṃ nādiyati,",
  "dhp409:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp410:0": "Sāriputtattheravatthu",
  "dhp410:1": "Āsā yassa na vijjanti,",
  "dhp410:2": "asmiṃ loke paramhi ca;",
  "dhp410:3": "Nirāsāsaṃ visaṃyuttaṃ,",
  "dhp410:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp411:0": "Mahāmoggallānattheravatthu",
  "dhp411:1": "Yassālayā na vijjanti,",
  "dhp411:2": "aññāya akathaṅkathī;",
  "dhp411:3": "Amatogadhamanuppattaṃ,",
  "dhp411:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp412:0": "Revatattheravatthu",
  "dhp412:1": "Yodha puññañca pāpañca,",
  "dhp412:2": "ubho saṅgamupaccagā;",
  "dhp412:3": "Asokaṃ virajaṃ suddhaṃ,",
  "dhp412:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp413:0": "Candābhattheravatthu",
  "dhp413:1": "Candaṃva vimalaṃ suddhaṃ,",
  "dhp413:2": "vippasannamanāvilaṃ;",
  "dhp413:3": "Nandībhavaparikkhīṇaṃ,",
  "dhp413:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp414:0": "Sīvalittheravatthu",
  "dhp414:1": "Yomaṃ palipathaṃ duggaṃ,",
  "dhp414:2": "saṃsāraṃ mohamaccagā;",
  "dhp414:3": "Tiṇṇo pāraṅgato jhāyī,",
  "dhp414:4": "anejo akathaṃkathī;",
  "dhp414:5": "Anupādāya nibbuto,",
  "dhp414:6": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp415:0": "Sundarasamuddattheravatthu",
  "dhp415:1": "Yodha kāme pahantvāna,",
  "dhp415:2": "anāgāro paribbaje;",
  "dhp415:3": "Kāmabhavaparikkhīṇaṃ,",
  "dhp415:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp416:0": "Jaṭilattheravatthu",
  "dhp416:1": "Yodha taṇhaṃ pahantvāna,",
  "dhp416:2": "anāgāro paribbaje;",
  "dhp416:3": "Taṇhābhavaparikkhīṇaṃ,",
  "dhp416:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp416:5.0": "Jotikattheravatthu",
  "dhp416:6": "Yodha taṇhaṃ pahantvāna,",
  "dhp416:7": "anāgāro paribbaje;",
  "dhp416:8": "Taṇhābhavaparikkhīṇaṃ,",
  "dhp416:9": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp417:0": "Naṭaputtakattheravatthu",
  "dhp417:1": "Hitvā mānusakaṃ yogaṃ,",
  "dhp417:2": "dibbaṃ yogaṃ upaccagā;",
  "dhp417:3": "Sabbayogavisaṃyuttaṃ,",
  "dhp417:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp418:0": "Naṭaputtakattheravatthu",
  "dhp418:1": "Hitvā ratiñca aratiñca,",
  "dhp418:2": "sītibhūtaṃ nirūpadhiṃ;",
  "dhp418:3": "Sabbalokābhibhuṃ vīraṃ,",
  "dhp418:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp419:0": "Vaṅgīsattheravatthu",
  "dhp419:1": "Cutiṃ yo vedi sattānaṃ,",
  "dhp419:2": "upapattiñca sabbaso;",
  "dhp419:3": "Asattaṃ sugataṃ buddhaṃ,",
  "dhp419:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp420:1": "Yassa gatiṃ na jānanti,",
  "dhp420:2": "devā gandhabbamānusā;",
  "dhp420:3": "Khīṇāsavaṃ arahantaṃ,",
  "dhp420:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp421:0": "Dhammadinnāttherīvatthu",
  "dhp421:1": "Yassa pure ca pacchā ca,",
  "dhp421:2": "majjhe ca natthi kiñcanaṃ;",
  "dhp421:3": "Akiñcanaṃ anādānaṃ,",
  "dhp421:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp422:0": "Aṅgulimālattheravatthu",
  "dhp422:1": "Usabhaṃ pavaraṃ vīraṃ,",
  "dhp422:2": "mahesiṃ vijitāvinaṃ;",
  "dhp422:3": "Anejaṃ nhātakaṃ buddhaṃ,",
  "dhp422:4": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp423:0": "Devahitabrāhmaṇavatthu",
  "dhp423:1": "Pubbenivāsaṃ yo vedi,",
  "dhp423:2": "saggāpāyañca passati;",
  "dhp423:3": "Atho jātikkhayaṃ patto,",
  "dhp423:4": "abhiññāvosito muni;",
  "dhp423:5": "Sabbavositavosānaṃ,",
  "dhp423:6": "tamahaṃ brūmi brāhmaṇaṃ.",
  "dhp423:7": "Brāhmaṇavaggo chabbīsatimo.",
  "dhp423:8": "Ettāvatā sabbapaṭhame yamakavagge cuddasa vatthūni, appamādavagge nava, cittavagge nava, pupphavagge dvādasa, bālavagge pannarasa, paṇḍitavaggeekādasa, arahantavagge dasa, sahassavagge cuddasa, pāpavagge dvādasa, daṇḍavagge ekādasa, jarāvagge nava, attavagge dasa, lokavagge ekādasa, buddhavagge nava, sukhavagge aṭṭha, piyavagge nava, kodhavagge aṭṭha, malavaggedvādasa, dhammaṭṭhavagge dasa, maggavagge dvādasa, pakiṇṇakavagge nava, nirayavagge nava, nāgavagge aṭṭha, taṇhāvagge dvādasa, bhikkhuvagge dvādasa, brāhmaṇavagge cattālīsāti pañcādhikāni tīṇi vatthusatāni.",
  "dhp423:9": "Satevīsacatussatā,",
  "dhp423:10": "catusaccavibhāvinā;",
  "dhp423:11": "Satattayañca vatthūnaṃ,",
  "dhp423:12": "pañcādhikaṃ samuṭṭhitāti.",
  "dhp423:13": "Yamakappamādo cittaṃ,",
  "dhp423:14": "pupphaṃ bālena paṇḍito;",
  "dhp423:15": "Arahanto sahassañca,",
  "dhp423:16": "pāpaṃ daṇḍena te dasa.",
  "dhp423:17": "Jarā attā ca loko ca,",
  "dhp423:18": "buddho sukhaṃ piyena ca;",
  "dhp423:19": "Kodho malañca dhammaṭṭho,",
  "dhp423:20": "maggavaggena vīsati.",
  "dhp423:21": "Pakiṇṇaṃ nirayo nāgo,",
  "dhp423:22": "taṇhā bhikkhu ca brāhmaṇo;",
  "dhp423:23": "Ete chabbīsati vaggā,",
  "dhp423:24": "desitādiccabandhunā.",
  "dhp423:25": "Yamake vīsati gāthā,",
  "dhp423:26": "appamādamhi dvādasa;",
  "dhp423:27": "Ekādasa cittavagge,",
  "dhp423:28": "pupphavaggamhi soḷasa.",
  "dhp423:29": "Bāle ca soḷasa gāthā,",
  "dhp423:30": "paṇḍitamhi catuddasa;",
  "dhp423:31": "Arahante dasa gāthā,",
  "dhp423:32": "sahasse honti soḷasa.",
  "dhp423:33": "Terasa pāpavaggamhi,",
  "dhp423:34": "daṇḍamhi dasa satta ca;",
  "dhp423:35": "Ekādasa jarā vagge,",
  "dhp423:36": "attavaggamhi tā dasa.",
  "dhp423:37": "Dvādasa lokavaggamhi,",
  "dhp423:38": "buddhavaggamhi ṭhārasa;",
  "dhp423:39": "Sukhe ca piyavagge ca,",
  "dhp423:40": "gāthāyo honti dvādasa.",
  "dhp423:41": "Cuddasa kodhavaggamhi,",
  "dhp423:42": "malavaggekavīsati;",
  "dhp423:43": "Sattarasa ca dhammaṭṭhe,",
  "dhp423:44": "maggavagge sattarasa.",
  "dhp423:45": "Pakiṇṇe soḷasa gāthā,",
  "dhp423:46": "niraye nāge ca cuddasa;",
  "dhp423:47": "Chabbīsa taṇhāvaggamhi,",
  "dhp423:48": "tevīsa bhikkhuvaggikā.",
  "dhp423:49": "Ekatālīsagāthāyo,",
  "dhp423:50": "brāhmaṇe vaggamuttame;",
  "dhp423:51": "Gāthāsatāni cattāri,",
  "dhp423:52": "tevīsa ca punāpare;",
  "dhp423:53": "Dhammapade nipātamhi,",
  "dhp423:54": "desitādiccabandhunāti.",
  "dhp423:55": "Dhammapadapāḷi samattā."
}