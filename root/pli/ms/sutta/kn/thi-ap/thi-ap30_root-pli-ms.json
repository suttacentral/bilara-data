{
  "thi-ap30:0.1": "Therīapadāna",
  "thi-ap30:0.2": "Kuṇḍalakesīvagga",
  "thi-ap30:0.3": "10 Yasodharāpamukhaaṭṭhārasabhikkhunīsahassaapadāna",
  "thi-ap30:1.1": "Aṭṭhārasasahassāni,",
  "thi-ap30:1.2": "bhikkhunī sakyasambhavā;",
  "thi-ap30:1.3": "Yasodharāpamukhāni,",
  "thi-ap30:1.4": "sambuddhaṁ upasaṅkamuṁ.",
  "thi-ap30:2.1": "Aṭṭhārasasahassāni,",
  "thi-ap30:2.2": "sabbā honti mahiddhikā;",
  "thi-ap30:2.3": "Vandantī munino pāde,",
  "thi-ap30:2.4": "ārocenti yathābalaṁ.",
  "thi-ap30:3.1": "“Jāti khīṇā jarā byādhi,",
  "thi-ap30:3.2": "maraṇañca mahāmuni;",
  "thi-ap30:3.3": "Anāsavaṁ padaṁ santaṁ,",
  "thi-ap30:3.4": "amataṁ yāma nāyaka.",
  "thi-ap30:4.1": "Khalitañce pure atthi,",
  "thi-ap30:4.2": "sabbāsampi mahāmuni;",
  "thi-ap30:4.3": "Aparādhamajānantī,",
  "thi-ap30:4.4": "khama amhaṁ vināyaka”.",
  "thi-ap30:5.1": "“Iddhiñcāpi nidassetha,",
  "thi-ap30:5.2": "mama sāsanakārikā;",
  "thi-ap30:5.3": "Parisānañca sabbāsaṁ,",
  "thi-ap30:5.4": "kaṅkhaṁ chindatha yāvatā”.",
  "thi-ap30:6.1": "“Yasodharā mahāvīra,",
  "thi-ap30:6.2": "manāpā piyadassanā;",
  "thi-ap30:6.3": "Sabbā tuyhaṁ mahāvīra,",
  "thi-ap30:6.4": "agārasmiṁ pajāpati.",
  "thi-ap30:7.1": "Thīnaṁ satasahassānaṁ,",
  "thi-ap30:7.2": "navutīnaṁ chaduttari;",
  "thi-ap30:7.3": "Agāre te mayaṁ vīra,",
  "thi-ap30:7.4": "pāmokkhā sabbā issarā.",
  "thi-ap30:8.1": "Rūpācāraguṇūpetā,",
  "thi-ap30:8.2": "yobbanaṭṭhā piyaṁvadā;",
  "thi-ap30:8.3": "Sabbā no apacāyanti,",
  "thi-ap30:8.4": "devatā viya mānusā.",
  "thi-ap30:9.1": "Aṭṭhārasasahassāni,",
  "thi-ap30:9.2": "sabbā sākiyasambhavā;",
  "thi-ap30:9.3": "Yasodharāsahassāni,",
  "thi-ap30:9.4": "pāmokkhā issarā tadā.",
  "thi-ap30:10.1": "Kāmadhātumatikkamma,",
  "thi-ap30:10.2": "saṇṭhitā rūpadhātuyā;",
  "thi-ap30:10.3": "Rūpena sadisā natthi,",
  "thi-ap30:10.4": "sahassānaṁ mahāmuni”.",
  "thi-ap30:11.1": "Sambuddhaṁ abhivādetvā,",
  "thi-ap30:11.2": "iddhiṁ dassaṁsu satthuno;",
  "thi-ap30:11.3": "Nekā nānāvidhākārā,",
  "thi-ap30:11.4": "mahāiddhīpi dassayuṁ.",
  "thi-ap30:12.1": "Cakkavāḷasamaṁ kāyaṁ,",
  "thi-ap30:12.2": "sīsaṁ uttarato kuru;",
  "thi-ap30:12.3": "Ubho pakkhā duve dīpā,",
  "thi-ap30:12.4": "jambudīpaṁ sarīrato.",
  "thi-ap30:13.1": "Dakkhiṇañca saraṁ piñchaṁ,",
  "thi-ap30:13.2": "nānāsākhā tu pattakā;",
  "thi-ap30:13.3": "Candañca sūriyañcakkhi,",
  "thi-ap30:13.4": "merupabbatato sikhaṁ.",
  "thi-ap30:14.1": "Cakkavāḷagiriṁ tuṇḍaṁ,",
  "thi-ap30:14.2": "jamburukkhaṁ samūlakaṁ;",
  "thi-ap30:14.3": "Bījamānā upāgantvā,",
  "thi-ap30:14.4": "vandantī lokanāyakaṁ.",
  "thi-ap30:15.1": "Hatthivaṇṇaṁ tathevassaṁ,",
  "thi-ap30:15.2": "pabbataṁ jaladhiṁ tathā;",
  "thi-ap30:15.3": "Candañca sūriyaṁ meruṁ,",
  "thi-ap30:15.4": "sakkavaṇṇañca dassayuṁ.",
  "thi-ap30:16.1": "“Yasodharā mayaṁ vīra,",
  "thi-ap30:16.2": "pāde vandāma cakkhuma;",
  "thi-ap30:16.3": "Tava cirapabhāvena,",
  "thi-ap30:16.4": "nipphannā naranāyaka.",
  "thi-ap30:17.1": "Iddhīsu ca vasī homa,",
  "thi-ap30:17.2": "dibbāya sotadhātuyā;",
  "thi-ap30:17.3": "Cetopariyañāṇassa,",
  "thi-ap30:17.4": "vasī homa mahāmune.",
  "thi-ap30:18.1": "Pubbenivāsaṁ jānāmi,",
  "thi-ap30:18.2": "dibbacakkhu visodhitaṁ;",
  "thi-ap30:18.3": "Sabbāsavā parikkhīṇā,",
  "thi-ap30:18.4": "natthi dāni punabbhavo.",
  "thi-ap30:19.1": "Atthadhammaniruttīsu,",
  "thi-ap30:19.2": "paṭibhāne tatheva ca;",
  "thi-ap30:19.3": "Ñāṇaṁ amhaṁ mahāvīra,",
  "thi-ap30:19.4": "uppannaṁ tava santike.",
  "thi-ap30:20.1": "Pubbānaṁ lokanāthānaṁ,",
  "thi-ap30:20.2": "saṅgamaṁ no nidassitaṁ;",
  "thi-ap30:20.3": "Adhikārā bahū amhaṁ,",
  "thi-ap30:20.4": "tuyhatthāya mahāmune.",
  "thi-ap30:21.1": "Yaṁ amhaṁ pūritaṁ kammaṁ,",
  "thi-ap30:21.2": "kusalaṁ sarase mune;",
  "thi-ap30:21.3": "Tuyhatthāya mahāvīra,",
  "thi-ap30:21.4": "puññānupacitāni no.",
  "thi-ap30:22.1": "Abhabbaṭṭhāne vajjetvā,",
  "thi-ap30:22.2": "vārayimha anācaraṁ;",
  "thi-ap30:22.3": "Tuyhatthāya mahāvīra,",
  "thi-ap30:22.4": "cattāni jīvitāni no.",
  "thi-ap30:23.1": "Nekakoṭisahassāni,",
  "thi-ap30:23.2": "bhariyatthāyadāsi no;",
  "thi-ap30:23.3": "Na tattha vimanā homa,",
  "thi-ap30:23.4": "tuyhatthāya mahāmune.",
  "thi-ap30:24.1": "Nekakoṭisahassāni,",
  "thi-ap30:24.2": "upakārāyadāsi no;",
  "thi-ap30:24.3": "Na tattha vimanā homa,",
  "thi-ap30:24.4": "tuyhatthāya mahāmune.",
  "thi-ap30:25.1": "Nekakoṭisahassāni,",
  "thi-ap30:25.2": "bhojanatthāyadāsi no;",
  "thi-ap30:25.3": "Na tattha vimanā homa,",
  "thi-ap30:25.4": "tuyhatthāya mahāmune.",
  "thi-ap30:26.1": "Nekakoṭisahassāni,",
  "thi-ap30:26.2": "jīvitāni cajimhase;",
  "thi-ap30:26.3": "Bhayamokkhaṁ karissāma,",
  "thi-ap30:26.4": "jīvitāni cajimhase.",
  "thi-ap30:27.1": "Aṅgagate alaṅkāre,",
  "thi-ap30:27.2": "vatthe nānāvidhe bahū;",
  "thi-ap30:27.3": "Itthibhaṇḍe na gūhāma,",
  "thi-ap30:27.4": "tuyhatthāya mahāmune.",
  "thi-ap30:28.1": "Dhanadhaññapariccāgaṁ,",
  "thi-ap30:28.2": "gāmāni nigamāni ca;",
  "thi-ap30:28.3": "Khettaṁ puttā ca dhītā ca,",
  "thi-ap30:28.4": "pariccattā mahāmune.",
  "thi-ap30:29.1": "Hatthī assā gavā cāpi,",
  "thi-ap30:29.2": "dāsiyo paricārikā;",
  "thi-ap30:29.3": "Tuyhatthāya mahāvīra,",
  "thi-ap30:29.4": "pariccattaṁ asaṅkhiyaṁ.",
  "thi-ap30:30.1": "Yaṁ amhe paṭimantesi,",
  "thi-ap30:30.2": "‘dānaṁ dassāma yācake’;",
  "thi-ap30:30.3": "Vimanaṁ no na passāma,",
  "thi-ap30:30.4": "dadato dānamuttamaṁ.",
  "thi-ap30:31.1": "Nānāvidhaṁ bahuṁ dukkhaṁ,",
  "thi-ap30:31.2": "saṁsāre ca bahubbidhe;",
  "thi-ap30:31.3": "Tuyhatthāya mahāvīra,",
  "thi-ap30:31.4": "anubhuttaṁ asaṅkhiyaṁ.",
  "thi-ap30:32.1": "Sukhappattānumodāma,",
  "thi-ap30:32.2": "na ca dukkhesu dummanā;",
  "thi-ap30:32.3": "Sabbattha tulitā homa,",
  "thi-ap30:32.4": "tuyhatthāya mahāmune.",
  "thi-ap30:33.1": "Anumaggena sambuddho,",
  "thi-ap30:33.2": "yaṁ dhammaṁ abhinīhari;",
  "thi-ap30:33.3": "Anubhotvā sukhaṁ dukkhaṁ,",
  "thi-ap30:33.4": "patto bodhiṁ mahāmune.",
  "thi-ap30:34.1": "Brahmadevañca sambuddhaṁ,",
  "thi-ap30:34.2": "gotamaṁ lokanāyakaṁ;",
  "thi-ap30:34.3": "Aññesaṁ lokanāthānaṁ,",
  "thi-ap30:34.4": "saṅgamaṁ tehi no bahū.",
  "thi-ap30:35.1": "Adhikāraṁ bahuṁ amhe,",
  "thi-ap30:35.2": "tuyhatthāya mahāmune;",
  "thi-ap30:35.3": "Gavesato buddhadhamme,",
  "thi-ap30:35.4": "mayaṁ te paricārikā.",
  "thi-ap30:36.1": "Kappe ca satasahasse,",
  "thi-ap30:36.2": "caturo ca asaṅkhiye;",
  "thi-ap30:36.3": "Dīpaṅkaro mahāvīro,",
  "thi-ap30:36.4": "uppajji lokanāyako.",
  "thi-ap30:37.1": "Paccantadesavisaye,",
  "thi-ap30:37.2": "nimantetvā tathāgataṁ;",
  "thi-ap30:37.3": "Tassa āgamanaṁ maggaṁ,",
  "thi-ap30:37.4": "sodhenti tuṭṭhamānasā.",
  "thi-ap30:38.1": "Tena kālena so āsi,",
  "thi-ap30:38.2": "sumedho nāma brāhmaṇo;",
  "thi-ap30:38.3": "Maggañca paṭiyādesi,",
  "thi-ap30:38.4": "āyato sabbadassino.",
  "thi-ap30:39.1": "Tena kālena ahumha,",
  "thi-ap30:39.2": "sabbā brāhmaṇasambhavā;",
  "thi-ap30:39.3": "Thalūdajāni pupphāni,",
  "thi-ap30:39.4": "āharimha samāgamaṁ.",
  "thi-ap30:40.1": "Tasmiṁ so samaye buddho,",
  "thi-ap30:40.2": "dīpaṅkaro mahāyaso;",
  "thi-ap30:40.3": "Viyākāsi mahāvīro,",
  "thi-ap30:40.4": "isimuggatamānasaṁ.",
  "thi-ap30:41.1": "Calatī ravatī puthavī,",
  "thi-ap30:41.2": "saṅkampati sadevake;",
  "thi-ap30:41.3": "Tassa kammaṁ pakittente,",
  "thi-ap30:41.4": "isimuggatamānasaṁ.",
  "thi-ap30:42.1": "Devakaññā manussā ca,",
  "thi-ap30:42.2": "mayañcāpi sadevakā;",
  "thi-ap30:42.3": "Nānāpūjanīyaṁ bhaṇḍaṁ,",
  "thi-ap30:42.4": "pūjayitvāna patthayuṁ.",
  "thi-ap30:43.1": "Tesaṁ buddho viyākāsi,",
  "thi-ap30:43.2": "jotidīpa sanāmako;",
  "thi-ap30:43.3": "‘Ajja ye patthitā atthi,",
  "thi-ap30:43.4": "te bhavissanti sammukhā’.",
  "thi-ap30:44.1": "Aparimeyye ito kappe,",
  "thi-ap30:44.2": "yaṁ no buddho viyākari;",
  "thi-ap30:44.3": "Taṁ vācamanumodentā,",
  "thi-ap30:44.4": "evaṅkārī ahumha no.",
  "thi-ap30:45.1": "Tassa kammassa sukatassa,",
  "thi-ap30:45.2": "tassa cittaṁ pasādayuṁ;",
  "thi-ap30:45.3": "Devamānusikaṁ yoniṁ,",
  "thi-ap30:45.4": "anubhotvā asaṅkhiyaṁ.",
  "thi-ap30:46.1": "Sukhadukkhenubhotvāna,",
  "thi-ap30:46.2": "devesu mānusesu ca;",
  "thi-ap30:46.3": "Pacchime bhave sampatte,",
  "thi-ap30:46.4": "jātāmha sākiye kule.",
  "thi-ap30:47.1": "Rūpavatī bhogavatī,",
  "thi-ap30:47.2": "yasasīlavatī tato;",
  "thi-ap30:47.3": "Sabbaṅgasampadā homa,",
  "thi-ap30:47.4": "kulesu abhisakkatā.",
  "thi-ap30:48.1": "Lābhaṁ silokaṁ sakkāraṁ,",
  "thi-ap30:48.2": "lokadhammasamāgamaṁ;",
  "thi-ap30:48.3": "Cittañca dukkhitaṁ natthi,",
  "thi-ap30:48.4": "vasāma akutobhayā.",
  "thi-ap30:49.1": "Vuttañhetaṁ bhagavatā,",
  "thi-ap30:49.2": "‘rañño antepure tadā;",
  "thi-ap30:49.3": "Khattiyānaṁ pure vīra,",
  "thi-ap30:49.4": "upakārañca niddisi.",
  "thi-ap30:50.1": "Upakārā ca yā nārī,",
  "thi-ap30:50.2": "yā ca nārī sukhe dukhe;",
  "thi-ap30:50.3": "Atthakkhāyī ca yā nārī,",
  "thi-ap30:50.4": "yā ca nārīnukampikā.",
  "thi-ap30:51.1": "Dhammaṁ care sucaritaṁ,",
  "thi-ap30:51.2": "na naṁ duccaritaṁ care;",
  "thi-ap30:51.3": "Dhammacārī sukhaṁ seti,",
  "thi-ap30:51.4": "asmiṁ loke paramhi ca’.",
  "thi-ap30:52.1": "Agāraṁ vijahitvāna,",
  "thi-ap30:52.2": "pabbajimhanagāriyaṁ;",
  "thi-ap30:52.3": "Aḍḍhamāse asampatte,",
  "thi-ap30:52.4": "catusaccaṁ phusimha no.",
  "thi-ap30:53.1": "Cīvaraṁ piṇḍapātañca,",
  "thi-ap30:53.2": "paccayaṁ sayanāsanaṁ;",
  "thi-ap30:53.3": "Upanenti bahū amhe,",
  "thi-ap30:53.4": "sāgarasseva ūmiyo.",
  "thi-ap30:54.1": "Kilesā jhāpitā amhaṁ,",
  "thi-ap30:54.2": "bhavā sabbe samūhatā;",
  "thi-ap30:54.3": "Nāgīva bandhanaṁ chetvā,",
  "thi-ap30:54.4": "viharāma anāsavā.",
  "thi-ap30:55.1": "Svāgataṁ vata no āsi,",
  "thi-ap30:55.2": "mama buddhassa santike;",
  "thi-ap30:55.3": "Tisso vijjā anuppattā,",
  "thi-ap30:55.4": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap30:56.1": "Paṭisambhidā catasso,",
  "thi-ap30:56.2": "vimokkhāpi ca aṭṭhime;",
  "thi-ap30:56.3": "Chaḷabhiññā sacchikatā,",
  "thi-ap30:56.4": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap30:57.1": "Evaṁ bahuvidhaṁ dukkhaṁ,",
  "thi-ap30:57.2": "sampattī ca bahubbidhā;",
  "thi-ap30:57.3": "Visuddhabhāvaṁ sampattā,",
  "thi-ap30:57.4": "labhāma sabbasampadā.",
  "thi-ap30:58.1": "Yā dadanti sakattānaṁ,",
  "thi-ap30:58.2": "puññatthāya mahesino;",
  "thi-ap30:58.3": "Sahāyasampadā honti,",
  "thi-ap30:58.4": "nibbānapadamasaṅkhataṁ.",
  "thi-ap30:59.1": "Parikkhīṇaṁ atītañca,",
  "thi-ap30:59.2": "paccuppannaṁ anāgataṁ;",
  "thi-ap30:59.3": "Sabbakammampi no khīṇaṁ,",
  "thi-ap30:59.4": "pāde vandāma cakkhuma”.",
  "thi-ap30:60.1": "“Nibbānāya vadantīnaṁ,",
  "thi-ap30:60.2": "Kiṁ vo vakkhāma uttari;",
  "thi-ap30:60.3": "Santasaṅkhatadosañhi,",
  "thi-ap30:60.4": "Pappotha amataṁ padaṁ”. ",
  "thi-ap30:61.1": "Itthaṁ sudaṁ yasodharāpamukhāni aṭṭhārasabhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap30:62.1": "Yasodharāpamukhaaṭṭhārasabhikkhunīsahassāpadānaṁ dasamaṁ.",
  "thi-ap30:63.1": "Kuṇḍalakesīvaggo tatiyo.",
  "thi-ap30:64.1": "Tassuddānaṁ",
  "thi-ap30:65.1": "Kuṇḍalā gotamī ceva,",
  "thi-ap30:65.2": "dhammadinnā ca sakulā;",
  "thi-ap30:65.3": "Varanandā ca soṇā ca,",
  "thi-ap30:65.4": "kāpilānī yasodharā.",
  "thi-ap30:66.1": "Dasasahassabhikkhunī,",
  "thi-ap30:66.2": "aṭṭhārasasahassakā;",
  "thi-ap30:66.3": "Gāthāsatāni cattāri,",
  "thi-ap30:66.4": "cha ca sattatimeva ca."
}