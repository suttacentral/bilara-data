{
  "ja530:0.1": "Jātaka",
  "ja530:0.2": "Saṭṭhinipāta",
  "ja530:0.3": "Soṇakavagga",
  "ja530:0.4": "2. Saṅkiccajātaka",
  "ja530:1.1": "Disvā nisinnaṁ rājānaṁ,",
  "ja530:1.2": "brahmadattaṁ rathesabhaṁ;",
  "ja530:1.3": "Athassa paṭivedesi,",
  "ja530:1.4": "“yassāsi anukampako.",
  "ja530:2.1": "Saṅkiccāyaṁ anuppatto,",
  "ja530:2.2": "isīnaṁ sādhusammato;",
  "ja530:2.3": "Taramānarūpo niyyāhi,",
  "ja530:2.4": "khippaṁ passa mahesinaṁ”.",
  "ja530:3.1": "Tato ca rājā taramāno,",
  "ja530:3.2": "yuttamāruyha sandanaṁ;",
  "ja530:3.3": "Mittāmaccaparibyūḷho,",
  "ja530:3.4": "agamāsi rathesabho.",
  "ja530:4.1": "Nikkhippa pañca kakudhāni,",
  "ja530:4.2": "kāsīnaṁ raṭṭhavaḍḍhano;",
  "ja530:4.3": "Vālabījanimuṇhīsaṁ,",
  "ja530:4.4": "khaggaṁ chattañcupāhanaṁ.",
  "ja530:5.1": "Oruyha rājā yānamhā,",
  "ja530:5.2": "ṭhapayitvā paṭicchadaṁ;",
  "ja530:5.3": "Āsīnaṁ dāyapassasmiṁ,",
  "ja530:5.4": "saṅkiccamupasaṅkami.",
  "ja530:6.1": "Upasaṅkamitvā so rājā,",
  "ja530:6.2": "sammodi isinā saha;",
  "ja530:6.3": "Taṁ kathaṁ vītisāretvā,",
  "ja530:6.4": "ekamantaṁ upāvisi.",
  "ja530:7.1": "Ekamantaṁ nisinnova,",
  "ja530:7.2": "atha kālaṁ amaññatha;",
  "ja530:7.3": "Tato pāpāni kammāni,",
  "ja530:7.4": "pucchituṁ paṭipajjatha.",
  "ja530:8.1": "“Isiṁ pucchāma saṅkiccaṁ,",
  "ja530:8.2": "isīnaṁ sādhusammataṁ;",
  "ja530:8.3": "Āsīnaṁ dāyapassasmiṁ,",
  "ja530:8.4": "isisaṅghapurakkhataṁ.",
  "ja530:9.1": "Kaṁ gatiṁ pecca gacchanti,",
  "ja530:9.2": "narā dhammāticārino;",
  "ja530:9.3": "Aticiṇṇo mayā dhammo,",
  "ja530:9.4": "taṁ me akkhāhi pucchito”.",
  "ja530:10.1": "Isī avaca saṅkicco,",
  "ja530:10.2": "kāsīnaṁ raṭṭhavaḍḍhanaṁ;",
  "ja530:10.3": "Āsīnaṁ dāyapassasmiṁ,",
  "ja530:10.4": "“mahārāja suṇohi me.",
  "ja530:11.1": "Uppathena vajantassa,",
  "ja530:11.2": "yo maggamanusāsati;",
  "ja530:11.3": "Tassa ce vacanaṁ kayirā,",
  "ja530:11.4": "nāssa maggeyya kaṇṭako.",
  "ja530:12.1": "Adhammaṁ paṭipannassa,",
  "ja530:12.2": "yo dhammamanusāsati;",
  "ja530:12.3": "Tassa ce vacanaṁ kayirā,",
  "ja530:12.4": "na so gaccheyya duggatiṁ”.",
  "ja530:13.1": "“Dhammo patho mahārāja,",
  "ja530:13.2": "adhammo pana uppatho;",
  "ja530:13.3": "Adhammo nirayaṁ neti,",
  "ja530:13.4": "dhammo pāpeti suggatiṁ.",
  "ja530:14.1": "Adhammacārino rāja,",
  "ja530:14.2": "narā visamajīvino;",
  "ja530:14.3": "Yaṁ gatiṁ pecca gacchanti,",
  "ja530:14.4": "niraye te suṇohi me.",
  "ja530:15.1": "Sañjīvo kāḷasutto ca,",
  "ja530:15.2": "saṅghāto dve ca roruvā;",
  "ja530:15.3": "Athāparo mahāvīci,",
  "ja530:15.4": "tāpano ca patāpano.",
  "ja530:16.1": "Iccete aṭṭha nirayā,",
  "ja530:16.2": "akkhātā duratikkamā;",
  "ja530:16.3": "Ākiṇṇā luddakammehi,",
  "ja530:16.4": "paccekā soḷasussadā.",
  "ja530:17.1": "Kadariyatāpanā ghorā,",
  "ja530:17.2": "accimanto mahabbhayā;",
  "ja530:17.3": "Lomahaṁsanarūpā ca,",
  "ja530:17.4": "bhesmā paṭibhayā dukhā.",
  "ja530:18.1": "Catukkaṇṇā catudvārā,",
  "ja530:18.2": "vibhattā bhāgaso mitā;",
  "ja530:18.3": "Ayopākārapariyantā,",
  "ja530:18.4": "ayasā paṭikujjitā.",
  "ja530:19.1": "Tesaṁ ayomayā bhūmi,",
  "ja530:19.2": "jalitā tejasā yutā;",
  "ja530:19.3": "Samantā yojanasataṁ,",
  "ja530:19.4": "phuṭā tiṭṭhanti sabbadā.",
  "ja530:20.1": "Ete patanti niraye,",
  "ja530:20.2": "uddhampādā avaṁsirā;",
  "ja530:20.3": "Isīnaṁ ativattāro,",
  "ja530:20.4": "saññatānaṁ tapassinaṁ.",
  "ja530:21.1": "Te bhūnahuno paccanti,",
  "ja530:21.2": "macchā bilakatā yathā;",
  "ja530:21.3": "Saṁvacchare asaṅkheyye,",
  "ja530:21.4": "narā kibbisakārino.",
  "ja530:22.1": "Ḍayhamānena gattena,",
  "ja530:22.2": "niccaṁ santarabāhiraṁ;",
  "ja530:22.3": "Nirayā nādhigacchanti,",
  "ja530:22.4": "dvāraṁ nikkhamanesino.",
  "ja530:23.1": "Puratthimena dhāvanti,",
  "ja530:23.2": "tato dhāvanti pacchato;",
  "ja530:23.3": "Uttarenapi dhāvanti,",
  "ja530:23.4": "tato dhāvanti dakkhiṇaṁ;",
  "ja530:23.5": "Yaṁ yañhi dvāraṁ gacchanti,",
  "ja530:23.6": "taṁ tadeva pidhīyare.",
  "ja530:24.1": "Bahūni vassasahassāni,",
  "ja530:24.2": "janā nirayagāmino;",
  "ja530:24.3": "Bāhā paggayha kandanti,",
  "ja530:24.4": "patvā dukkhaṁ anappakaṁ.",
  "ja530:25.1": "Āsīvisaṁva kupitaṁ,",
  "ja530:25.2": "tejassiṁ duratikkamaṁ;",
  "ja530:25.3": "Na sādhurūpe āsīde,",
  "ja530:25.4": "saññatānaṁ tapassinaṁ.",
  "ja530:26.1": "Atikāyo mahissāso,",
  "ja530:26.2": "ajjuno kekakādhipo;",
  "ja530:26.3": "Sahassabāhu ucchinno,",
  "ja530:26.4": "isimāsajja gotamaṁ.",
  "ja530:27.1": "Arajaṁ rajasā vacchaṁ,",
  "ja530:27.2": "Kisaṁ avakiriya daṇḍakī;",
  "ja530:27.3": "Tālova mūlato chinno,",
  "ja530:27.4": "Sa rājā vibhavaṅgato.",
  "ja530:28.1": "Upahacca manaṁ majjho,",
  "ja530:28.2": "mātaṅgasmiṁ yasassine;",
  "ja530:28.3": "Sapārisajjo ucchinno,",
  "ja530:28.4": "majjhāraññaṁ tadā ahu.",
  "ja530:29.1": "Kaṇhadīpāyanāsajja,",
  "ja530:29.2": "Isiṁ andhakaveṇḍayo;",
  "ja530:29.3": "Aññoññaṁ musalā hantvā,",
  "ja530:29.4": "Sampattā yamasādhanaṁ.",
  "ja530:30.1": "Athāyaṁ isinā satto,",
  "ja530:30.2": "antalikkhacaro pure;",
  "ja530:30.3": "Pāvekkhi pathaviṁ cecco,",
  "ja530:30.4": "hīnatto pattapariyāyaṁ.",
  "ja530:31.1": "Tasmā hi chandāgamanaṁ,",
  "ja530:31.2": "nappasaṁsanti paṇḍitā;",
  "ja530:31.3": "Aduṭṭhacitto bhāseyya,",
  "ja530:31.4": "giraṁ saccūpasaṁhitaṁ.",
  "ja530:32.1": "Manasā ce paduṭṭhena,",
  "ja530:32.2": "yo naro pekkhate muniṁ;",
  "ja530:32.3": "Vijjācaraṇasampannaṁ,",
  "ja530:32.4": "gantā so nirayaṁ adho.",
  "ja530:33.1": "Ye vuḍḍhe paribhāsanti,",
  "ja530:33.2": "pharusūpakkamā janā;",
  "ja530:33.3": "Anapaccā adāyādā,",
  "ja530:33.4": "tālavatthu bhavanti te.",
  "ja530:34.1": "Yo ca pabbajitaṁ hanti,",
  "ja530:34.2": "katakiccaṁ mahesinaṁ;",
  "ja530:34.3": "Sa kāḷasutte niraye,",
  "ja530:34.4": "cirarattāya paccati.",
  "ja530:35.1": "Yo ca rājā adhammaṭṭho,",
  "ja530:35.2": "raṭṭhaviddhaṁsano mago;",
  "ja530:35.3": "Tāpayitvā janapadaṁ,",
  "ja530:35.4": "tāpane pecca paccati.",
  "ja530:36.1": "So ca vassasahassāni,",
  "ja530:36.2": "sataṁ dibbāni paccati;",
  "ja530:36.3": "Accisaṅghapareto so,",
  "ja530:36.4": "dukkhaṁ vedeti vedanaṁ.",
  "ja530:37.1": "Tassa aggisikhā kāyā,",
  "ja530:37.2": "niccharanti pabhassarā;",
  "ja530:37.3": "Tejobhakkhassa gattāni,",
  "ja530:37.4": "lomehi ca nakhehi ca.",
  "ja530:38.1": "Ḍayhamānena gattena,",
  "ja530:38.2": "niccaṁ santarabāhiraṁ;",
  "ja530:38.3": "Dukkhābhitunno nadati,",
  "ja530:38.4": "nāgo tuttaṭṭito yathā.",
  "ja530:39.1": "Yo lobhā pitaraṁ hanti,",
  "ja530:39.2": "dosā vā purisādhamo;",
  "ja530:39.3": "Sa kāḷasutte niraye,",
  "ja530:39.4": "cirarattāya paccati.",
  "ja530:40.1": "Sa tādiso paccati lohakumbhiyaṁ,",
  "ja530:40.2": "Pakkañca sattīhi hananti nittacaṁ;",
  "ja530:40.3": "Andhaṁ karitvā muttakarīsabhakkhaṁ,",
  "ja530:40.4": "Khāre nimujjanti tathāvidhaṁ naraṁ.",
  "ja530:41.1": "Tattaṁ pakkuthitamayoguḷañca,",
  "ja530:41.2": "Dīghe ca phāle cirarattatāpite;",
  "ja530:41.3": "Vikkhambhamādāya vibandharajjubhi,",
  "ja530:41.4": "Vivaṭe mukhe sampavisanti rakkhasā.",
  "ja530:42.1": "Sāmā ca soṇā sabalā ca gijjhā,",
  "ja530:42.2": "Kākoḷasaṅghā ca dijā ayomukhā;",
  "ja530:42.3": "Saṅgamma khādanti vipphandamānaṁ,",
  "ja530:42.4": "Jivhaṁ vibhajja vighāsaṁ salohitaṁ.",
  "ja530:43.1": "Taṁ daḍḍhatālaṁ paribhinnagattaṁ,",
  "ja530:43.2": "Nippothayantā anuvicaranti rakkhasā;",
  "ja530:43.3": "Ratī hi nesaṁ dukhino panītare,",
  "ja530:43.4": "Etādisasmiṁ niraye vasanti;",
  "ja530:43.5": "Ye keci loke idha pettighātino.",
  "ja530:44.1": "Putto ca mātaraṁ hantvā,",
  "ja530:44.2": "ito gantvā yamakkhayaṁ;",
  "ja530:44.3": "Bhusamāpajjate dukkhaṁ,",
  "ja530:44.4": "attakammaphalūpago.",
  "ja530:45.1": "Amanussā atibalā,",
  "ja530:45.2": "hantāraṁ janayantiyā;",
  "ja530:45.3": "Ayomayehi vālehi,",
  "ja530:45.4": "pīḷayanti punappunaṁ.",
  "ja530:46.1": "Tamassavaṁ sakā gattā,",
  "ja530:46.2": "rudhiraṁ attasambhavaṁ;",
  "ja530:46.3": "Tambalohavilīnaṁva,",
  "ja530:46.4": "tattaṁ pāyenti mattighaṁ.",
  "ja530:47.1": "Jigucchaṁ kuṇapaṁ pūtiṁ,",
  "ja530:47.2": "Duggandhaṁ gūthakaddamaṁ;",
  "ja530:47.3": "Pubbalohitasaṅkāsaṁ,",
  "ja530:47.4": "Rahadamogayha tiṭṭhati.",
  "ja530:48.1": "Tamenaṁ kimayo tattha,",
  "ja530:48.2": "atikāyā ayomukhā;",
  "ja530:48.3": "Chaviṁ bhetvāna khādanti,",
  "ja530:48.4": "saṅgiddhā maṁsalohite.",
  "ja530:49.1": "So ca taṁ nirayaṁ patto,",
  "ja530:49.2": "nimuggo sataporisaṁ;",
  "ja530:49.3": "Pūtikaṁ kuṇapaṁ vāti,",
  "ja530:49.4": "samantā satayojanaṁ.",
  "ja530:50.1": "Cakkhumāpi hi cakkhūhi,",
  "ja530:50.2": "tena gandhena jīyati;",
  "ja530:50.3": "Etādisaṁ brahmadatta,",
  "ja530:50.4": "mātugho labhate dukhaṁ.",
  "ja530:51.1": "Khuradhāramanukkamma,",
  "ja530:51.2": "tikkhaṁ durabhisambhavaṁ;",
  "ja530:51.3": "Patanti gabbhapātiyo,",
  "ja530:51.4": "duggaṁ vetaraṇiṁ nadiṁ.",
  "ja530:52.1": "Ayomayā simbaliyo,",
  "ja530:52.2": "soḷasaṅgulakaṇṭakā;",
  "ja530:52.3": "Ubhato abhilambanti,",
  "ja530:52.4": "duggaṁ vetaraṇiṁ nadiṁ.",
  "ja530:53.1": "Te accimanto tiṭṭhanti,",
  "ja530:53.2": "aggikkhandhāva ārakā;",
  "ja530:53.3": "Ādittā jātavedena,",
  "ja530:53.4": "uddhaṁ yojanamuggatā.",
  "ja530:54.1": "Ete vajanti niraye,",
  "ja530:54.2": "tatte tikhiṇakaṇṭake;",
  "ja530:54.3": "Nāriyo ca aticārā,",
  "ja530:54.4": "narā ca paradāragū.",
  "ja530:55.1": "Te patanti adhokkhandhā,",
  "ja530:55.2": "vivattā vihatā puthū;",
  "ja530:55.3": "Sayanti vinividdhaṅgā,",
  "ja530:55.4": "dīghaṁ jagganti sabbadā.",
  "ja530:56.1": "Tato ratyā vivasāne,",
  "ja530:56.2": "mahatiṁ pabbatūpamaṁ;",
  "ja530:56.3": "Lohakumbhiṁ pavajjanti,",
  "ja530:56.4": "tattaṁ aggisamūdakaṁ.",
  "ja530:57.1": "Evaṁ divā ca ratto ca,",
  "ja530:57.2": "dussīlā mohapārutā;",
  "ja530:57.3": "Anubhonti sakaṁ kammaṁ,",
  "ja530:57.4": "pubbe dukkaṭamattano.",
  "ja530:58.1": "Yā ca bhariyā dhanakkītā,",
  "ja530:58.2": "sāmikaṁ atimaññati;",
  "ja530:58.3": "Sassuṁ vā sasuraṁ vāpi,",
  "ja530:58.4": "jeṭṭhaṁ vāpi nanandaraṁ.",
  "ja530:59.1": "Tassā vaṅkena jivhaggaṁ,",
  "ja530:59.2": "nibbahanti sabandhanaṁ;",
  "ja530:59.3": "Sa byāmamattaṁ kiminaṁ,",
  "ja530:59.4": "jivhaṁ passati attani;",
  "ja530:59.5": "Viññāpetuṁ na sakkoti,",
  "ja530:59.6": "tāpane pecca paccati.",
  "ja530:60.1": "Orabbhikā sūkarikā,",
  "ja530:60.2": "macchikā migabandhakā;",
  "ja530:60.3": "Corā goghātakā luddā,",
  "ja530:60.4": "avaṇṇe vaṇṇakārakā.",
  "ja530:61.1": "Sattīhi lohakūṭehi,",
  "ja530:61.2": "nettiṁsehi usūhi ca;",
  "ja530:61.3": "Haññamānā khāranadiṁ,",
  "ja530:61.4": "papatanti avaṁsirā.",
  "ja530:62.1": "Sāyaṁ pāto kūṭakārī,",
  "ja530:62.2": "ayokūṭehi haññati;",
  "ja530:62.3": "Tato vantaṁ durattānaṁ,",
  "ja530:62.4": "paresaṁ bhuñjare sadā.",
  "ja530:63.1": "Dhaṅkā bheraṇḍakā gijjhā,",
  "ja530:63.2": "kākoḷā ca ayomukhā;",
  "ja530:63.3": "Vipphandamānaṁ khādanti,",
  "ja530:63.4": "naraṁ kibbisakārakaṁ.",
  "ja530:64.1": "Ye migena migaṁ hanti,",
  "ja530:64.2": "pakkhiṁ vā pana pakkhinā;",
  "ja530:64.3": "Asanto rajasā channā,",
  "ja530:64.4": "gantā te nirayussadaṁ.",
  "ja530:65.1": "Santo ca uddhaṁ gacchanti,",
  "ja530:65.2": "suciṇṇenidha kammunā;",
  "ja530:65.3": "Suciṇṇassa phalaṁ passa,",
  "ja530:65.4": "saindā devā sabrahmakā.",
  "ja530:66.1": "Taṁ taṁ brūmi mahārāja,",
  "ja530:66.2": "Dhammaṁ raṭṭhapatī cara;",
  "ja530:66.3": "Tathā rāja carāhi dhammaṁ,",
  "ja530:66.4": "Yathā taṁ suciṇṇaṁ nānutappeyya pacchā”ti. ",
  "ja530:67.1": "Saṅkiccajātakaṁ dutiyaṁ.",
  "ja530:68.1": "Saṭṭhinipātaṁ niṭṭhitaṁ.",
  "ja530:69.1": "Tassuddānaṁ",
  "ja530:70.1": "Atha saṭṭhinipātamhi,",
  "ja530:70.2": "Suṇātha mama bhāsitaṁ;",
  "ja530:70.3": "Jātakasavhayano pavaro,",
  "ja530:70.4": "Soṇakaarindamasavhayano;",
  "ja530:70.5": "Tathā vuttarathesabhakiccavaroti."
}