{
  "ja300:0.1": "Jātaka",
  "ja300:0.2": "Tikanipāta",
  "ja300:0.3": "Kumbhavagga",
  "ja300:0.4": "10. Vakajātaka",
  "ja300:1.1": "“Parapāṇarodhā jīvanto,",
  "ja300:1.2": "maṁsalohitabhojano;",
  "ja300:1.3": "Vako vataṁ samādāya,",
  "ja300:1.4": "upapajji uposathaṁ.",
  "ja300:2.1": "Tassa sakko vataññāya,",
  "ja300:2.2": "ajarūpenupāgami;",
  "ja300:2.3": "Vītatapo ajjhappatto,",
  "ja300:2.4": "bhañji lohitapo tapaṁ.",
  "ja300:3.1": "Evameva idhekacce,",
  "ja300:3.2": "Samādānamhi dubbalā;",
  "ja300:3.3": "Lahuṁ karonti attānaṁ,",
  "ja300:3.4": "Vakova ajakāraṇā”ti. ",
  "ja300:4.1": "Vakajātakaṁ dasamaṁ.",
  "ja300:4.2": "Kumbhavaggo pañcamo.",
  "ja300:5.1": "Tassuddānaṁ",
  "ja300:6.1": "Varakumbha supattasirivhayano,",
  "ja300:6.2": "Sucisammata bindusaro cusabho;",
  "ja300:6.3": "Saritaṁpati caṇḍi jarākapinā,",
  "ja300:6.4": "Atha makkaṭiyā vakakena dasāti.",
  "ja300:7.1": "Atha vagguddānaṁ",
  "ja300:8.1": "Saṅkappo padumo ceva,",
  "ja300:8.2": "Udapānena tatiyaṁ;",
  "ja300:8.3": "Abbhantaraṁ ghaṭabhedaṁ,",
  "ja300:8.4": "Tikanipātamhilaṅkatanti.",
  "ja300:9.1": "Tikanipātaṁ niṭṭhitaṁ."
}