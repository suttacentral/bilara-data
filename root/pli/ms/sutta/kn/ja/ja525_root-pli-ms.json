{
  "ja525:0.1": "Jātaka",
  "ja525:0.2": "Cattālīsanipāta",
  "ja525:0.3": "Tesakuṇavagga",
  "ja525:0.4": "5 Cūḷasutasomajātaka",
  "ja525:1.1": "“Āmantayāmi nigamaṁ,",
  "ja525:1.2": "mittāmacce parissaje;",
  "ja525:1.3": "Sirasmiṁ palitaṁ jātaṁ,",
  "ja525:1.4": "pabbajjaṁ dāni rocahaṁ”.",
  "ja525:2.1": "“Abhumme kathaṁ nu bhaṇasi,",
  "ja525:2.2": "Sallaṁ me deva urasi kappesi;",
  "ja525:2.3": "Sattasatā te bhariyā,",
  "ja525:2.4": "Kathaṁ nu te tā bhavissanti”.",
  "ja525:3.1": "“Paññāyihinti etā,",
  "ja525:3.2": "Daharā aññampi tā gamissanti;",
  "ja525:3.3": "Saggañcassa patthayāno,",
  "ja525:3.4": "Tena ahaṁ pabbajissāmi”.",
  "ja525:4.1": "“Dulladdhaṁ me āsi sutasoma,",
  "ja525:4.2": "Yassa te homahaṁ mātā;",
  "ja525:4.3": "Yaṁ me vilapantiyā,",
  "ja525:4.4": "Anapekkho pabbajasi deva.",
  "ja525:5.1": "Dulladdhaṁ me āsi sutasoma,",
  "ja525:5.2": "Yaṁ taṁ ahaṁ vijāyissaṁ;",
  "ja525:5.3": "Yaṁ me vilapantiyā,",
  "ja525:5.4": "Anapekkho pabbajasi deva”.",
  "ja525:6.1": "“Ko nāmeso dhammo,",
  "ja525:6.2": "Sutasoma kā ca nāma pabbajjā;",
  "ja525:6.3": "Yaṁ no amhe jiṇṇe,",
  "ja525:6.4": "Anapekkho pabbajasi deva.",
  "ja525:7.1": "Puttāpi tuyhaṁ bahavo,",
  "ja525:7.2": "daharā appattayobbanā;",
  "ja525:7.3": "Mañjū tepi taṁ apassantā,",
  "ja525:7.4": "maññe dukkhaṁ nigacchanti”.",
  "ja525:8.1": "“Puttehi ca me etehi,",
  "ja525:8.2": "Daharehi appattayobbanehi;",
  "ja525:8.3": "Mañjūhi sabbehipi tumhehi,",
  "ja525:8.4": "Cirampi ṭhatvā vināsabhāvo”.",
  "ja525:9.1": "“Chinnaṁ nu tuyhaṁ hadayaṁ,",
  "ja525:9.2": "Adu te karuṇā ca natthi amhesu;",
  "ja525:9.3": "Yaṁ no vikandantiyo,",
  "ja525:9.4": "Anapekkho pabbajasi deva”.",
  "ja525:10.1": "“Na ca mayhaṁ chinnaṁ hadayaṁ,",
  "ja525:10.2": "Atthi karuṇāpi mayhaṁ tumhesu;",
  "ja525:10.3": "Saggañca patthayāno,",
  "ja525:10.4": "Tena ahaṁ pabbajissāmi”.",
  "ja525:11.1": "“Dulladdhaṁ me āsi,",
  "ja525:11.2": "Sutasoma yassa te ahaṁ bhariyā;",
  "ja525:11.3": "Yaṁ me vilapantiyā,",
  "ja525:11.4": "Anapekkho pabbajasi deva.",
  "ja525:12.1": "Dulladdhaṁ me āsi,",
  "ja525:12.2": "Sutasoma yassa te ahaṁ bhariyā;",
  "ja525:12.3": "Yaṁ me kucchipaṭisandhiṁ,",
  "ja525:12.4": "Anapekkho pabbajasi deva.",
  "ja525:13.1": "Paripakko me gabbho,",
  "ja525:13.2": "Kucchigato yāva naṁ vijāyāmi;",
  "ja525:13.3": "Māhaṁ ekā vidhavā,",
  "ja525:13.4": "Pacchā dukkhāni addakkhiṁ”.",
  "ja525:14.1": "“Paripakko te gabbho,",
  "ja525:14.2": "Kucchigato iṅgha tvaṁ vijāyassu;",
  "ja525:14.3": "Puttaṁ anomavaṇṇaṁ,",
  "ja525:14.4": "Taṁ hitvā pabbajissāmi”.",
  "ja525:15.1": "“Mā tvaṁ cande rudi,",
  "ja525:15.2": "Mā soci vanatimiramattakkhi;",
  "ja525:15.3": "Āroha varapāsādaṁ,",
  "ja525:15.4": "Anapekkho ahaṁ gamissāmi”.",
  "ja525:16.1": "“Ko taṁ amma kopesi,",
  "ja525:16.2": "Kiṁ rodasi pekkhasi ca maṁ bāḷhaṁ;",
  "ja525:16.3": "Kaṁ avajjhaṁ ghātemi,",
  "ja525:16.4": "Ñātīnaṁ udikkhamānānaṁ”.",
  "ja525:17.1": "“Na hi so sakkā hantuṁ,",
  "ja525:17.2": "Vijitāvī yo maṁ tāta kopesi;",
  "ja525:17.3": "Pitā te maṁ tāta avaca,",
  "ja525:17.4": "Anapekkho ahaṁ gamissāmi”.",
  "ja525:18.1": "“Yohaṁ pubbe niyyāmi,",
  "ja525:18.2": "Uyyānaṁ mattakuñjare ca yodhemi;",
  "ja525:18.3": "Sutasome pabbajite,",
  "ja525:18.4": "Kathaṁ nu dāni karissāmi”.",
  "ja525:19.1": "“Mātucca me rudantyā,",
  "ja525:19.2": "Jeṭṭhassa ca bhātuno akāmassa;",
  "ja525:19.3": "Hatthepi te gahessaṁ,",
  "ja525:19.4": "Na hi gacchasi no akāmānaṁ”.",
  "ja525:20.1": "“Uṭṭhehi tvaṁ dhāti,",
  "ja525:20.2": "Imaṁ kumāraṁ ramehi aññattha;",
  "ja525:20.3": "Mā me paripanthamakāsi,",
  "ja525:20.4": "Saggaṁ mama patthayānassa”.",
  "ja525:21.1": "“Yaṁnūnimaṁ dadeyyaṁ pabhaṅkaraṁ,",
  "ja525:21.2": "Ko nu me imināttho;",
  "ja525:21.3": "Sutasome pabbajite,",
  "ja525:21.4": "Kiṁ nu menaṁ karissāmi”.",
  "ja525:22.1": "“Koso ca tuyhaṁ vipulo,",
  "ja525:22.2": "Koṭṭhāgārañca tuyhaṁ paripūraṁ;",
  "ja525:22.3": "Pathavī ca tuyhaṁ vijitā,",
  "ja525:22.4": "Ramassu mā pabbaji deva”.",
  "ja525:23.1": "“Koso ca mayhaṁ vipulo,",
  "ja525:23.2": "Koṭṭhāgārañca mayhaṁ paripūraṁ;",
  "ja525:23.3": "Pathavī ca mayhaṁ vijitā,",
  "ja525:23.4": "Taṁ hitvā pabbajissāmi”.",
  "ja525:24.1": "“Mayhampi dhanaṁ pahūtaṁ,",
  "ja525:24.2": "Saṅkhātuṁ nopi deva sakkomi;",
  "ja525:24.3": "Taṁ te dadāmi sabbampi,",
  "ja525:24.4": "Ramassu mā pabbaji deva”.",
  "ja525:25.1": "“Jānāmi dhanaṁ pahūtaṁ,",
  "ja525:25.2": "Kulavaddhana pūjito tayā casmi;",
  "ja525:25.3": "Saggañca patthayāno,",
  "ja525:25.4": "Tena ahaṁ pabbajissāmi.",
  "ja525:26.1": "Ukkaṇṭhitosmi bāḷhaṁ,",
  "ja525:26.2": "Arati maṁ somadatta āvisati;",
  "ja525:26.3": "Bahukāpi me antarāyā,",
  "ja525:26.4": "Ajjevāhaṁ pabbajissāmi”.",
  "ja525:27.1": "“Idañca tuyhaṁ rucitaṁ,",
  "ja525:27.2": "Sutasoma ajjeva dāni tvaṁ pabbaja;",
  "ja525:27.3": "Ahampi pabbajissāmi,",
  "ja525:27.4": "Na ussahe tayā vinā ahaṁ ṭhātuṁ”.",
  "ja525:28.1": "“Na hi sakkā pabbajituṁ,",
  "ja525:28.2": "Nagare na hi paccati janapade ca;",
  "ja525:28.3": "Sutasome pabbajite,",
  "ja525:28.4": "Kathaṁ nu dāni karissāma.",
  "ja525:29.1": "Upanīyatidaṁ maññe,",
  "ja525:29.2": "Parittaṁ udakaṁva caṅkavāramhi;",
  "ja525:29.3": "Evaṁ suparittake jīvite,",
  "ja525:29.4": "Na ca pamajjituṁ kālo.",
  "ja525:30.1": "Upanīyatidaṁ maññe,",
  "ja525:30.2": "Parittaṁ udakaṁva caṅkavāramhi;",
  "ja525:30.3": "Evaṁ suparittake jīvite,",
  "ja525:30.4": "Andhabālā pamajjanti.",
  "ja525:31.1": "Te vaḍḍhayanti nirayaṁ,",
  "ja525:31.2": "Tiracchānayoniñca pettivisayañca;",
  "ja525:31.3": "Taṇhāya bandhanabaddhā,",
  "ja525:31.4": "Vaḍḍhenti asurakāyaṁ”.",
  "ja525:32.1": "“Ūhaññate rajaggaṁ,",
  "ja525:32.2": "Avidūre pubbakamhi ca pāsāde;",
  "ja525:32.3": "Maññe no kesā chinnā,",
  "ja525:32.4": "Yasassino dhammarājassa”.",
  "ja525:33.1": "“Ayamassa pāsādo,",
  "ja525:33.2": "Sovaṇṇapupphamālyavītikiṇṇo;",
  "ja525:33.3": "Yahimanuvicari rājā,",
  "ja525:33.4": "Parikiṇṇo itthāgārehi.",
  "ja525:34.1": "Ayamassa pāsādo,",
  "ja525:34.2": "Sovaṇṇapupphamālyavītikiṇṇo;",
  "ja525:34.3": "Yahimanuvicari rājā,",
  "ja525:34.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:35.1": "Idamassa kūṭāgāraṁ,",
  "ja525:35.2": "Sovaṇṇapupphamālyavītikiṇṇaṁ;",
  "ja525:35.3": "Yahimanuvicari rājā,",
  "ja525:35.4": "Parikiṇṇo itthāgārehi.",
  "ja525:36.1": "Idamassa kūṭāgāraṁ,",
  "ja525:36.2": "Sovaṇṇapupphamālyavītikiṇṇaṁ;",
  "ja525:36.3": "Yahimanuvicari rājā,",
  "ja525:36.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:37.1": "Ayamassa asokavanikā,",
  "ja525:37.2": "Supupphitā sabbakālikā rammā;",
  "ja525:37.3": "Yahimanuvicari rājā,",
  "ja525:37.4": "Parikiṇṇo itthāgārehi.",
  "ja525:38.1": "Ayamassa asokavanikā,",
  "ja525:38.2": "Supupphitā sabbakālikā rammā;",
  "ja525:38.3": "Yahimanuvicari rājā,",
  "ja525:38.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:39.1": "Idamassa uyyānaṁ,",
  "ja525:39.2": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:39.3": "Yahimanuvicari rājā,",
  "ja525:39.4": "Parikiṇṇo itthāgārehi.",
  "ja525:40.1": "Idamassa uyyānaṁ,",
  "ja525:40.2": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:40.3": "Yahimanuvicari rājā,",
  "ja525:40.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:41.1": "Idamassa kaṇikāravanaṁ,",
  "ja525:41.2": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:41.3": "Yahimanuvicari rājā,",
  "ja525:41.4": "Parikiṇṇo itthāgārehi.",
  "ja525:42.1": "Idamassa kaṇikāravanaṁ,",
  "ja525:42.2": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:42.3": "Yahimanuvicari rājā,",
  "ja525:42.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:43.1": "Idamassa pāṭalivanaṁ,",
  "ja525:43.2": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:43.3": "Yahimanuvicari rājā,",
  "ja525:43.4": "Parikiṇṇo itthāgārehi.",
  "ja525:44.1": "Idamassa pāṭalivanaṁ,",
  "ja525:44.2": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:44.3": "Yahimanuvicari rājā,",
  "ja525:44.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:45.1": "Idamassa ambavanaṁ,",
  "ja525:45.2": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:45.3": "Yahimanuvicari rājā,",
  "ja525:45.4": "Parikiṇṇo itthāgārehi.",
  "ja525:46.1": "Idamassa ambavanaṁ,",
  "ja525:46.2": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:46.3": "Yahimanuvicari rājā,",
  "ja525:46.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:47.1": "Ayamassa pokkharaṇī,",
  "ja525:47.2": "Sañchannā aṇḍajehi vītikiṇṇā;",
  "ja525:47.3": "Yahimanuvicari rājā,",
  "ja525:47.4": "Parikiṇṇo itthāgārehi.",
  "ja525:48.1": "Ayamassa pokkharaṇī,",
  "ja525:48.2": "Sañchannā aṇḍajehi vītikiṇṇā;",
  "ja525:48.3": "Yahimanuvicari rājā,",
  "ja525:48.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:49.1": "Rājā vo kho pabbajito,",
  "ja525:49.2": "Sutasomo rajjaṁ imaṁ pahatvāna;",
  "ja525:49.3": "Kāsāyavatthavasano,",
  "ja525:49.4": "Nāgova ekako carati”.",
  "ja525:50.1": "“Māssu pubbe ratikīḷitāni,",
  "ja525:50.2": "Hasitāni ca anussarittha;",
  "ja525:50.3": "Mā vo kāmā haniṁsu,",
  "ja525:50.4": "Rammaṁ hi sudassanaṁ nagaraṁ.",
  "ja525:51.1": "Mettacittañca bhāvetha,",
  "ja525:51.2": "Appamāṇaṁ divā ca ratto ca;",
  "ja525:51.3": "Agacchittha devapuraṁ,",
  "ja525:51.4": "Āvāsaṁ puññakamminan”ti. ",
  "ja525:52.1": "Cūḷasutasomajātakaṁ pañcamaṁ.",
  "ja525:53.1": "Cattālīsanipātaṁ niṭṭhitaṁ.",
  "ja525:54.1": "Tassuddānaṁ",
  "ja525:55.1": "Suvapaṇḍitajambukakuṇḍalino,",
  "ja525:55.2": "Varakaññamalambusajātakañca;",
  "ja525:55.3": "Pavaruttamasaṅkhasirīvhayako,",
  "ja525:55.4": "Sutasomaarindhamarājavaro."
}