{
  "ja422:0.1": "Jātaka",
  "ja422:0.2": "Aṭṭhakanipāta",
  "ja422:0.3": "Kaccānivagga",
  "ja422:0.4": "6. Cetiyajātaka",
  "ja422:1.1": "“Dhammo have hato hanti,",
  "ja422:1.2": "Nāhato hanti kiñcanaṁ;",
  "ja422:1.3": "Tasmā hi dhammaṁ na hane,",
  "ja422:1.4": "Mā tvaṁ dhammo hato hani. ",
  "ja422:2.1": "Alikaṁ bhāsamānassa,",
  "ja422:2.2": "apakkamanti devatā;",
  "ja422:2.3": "Pūtikañca mukhaṁ vāti,",
  "ja422:2.4": "sakaṭṭhānā ca dhaṁsati;",
  "ja422:2.5": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:2.6": "aññathā naṁ viyākare.",
  "ja422:3.1": "Sace hi saccaṁ bhaṇasi,",
  "ja422:3.2": "hohi rāja yathā pure;",
  "ja422:3.3": "Musā ce bhāsase rāja,",
  "ja422:3.4": "bhūmiyaṁ tiṭṭha cetiya. ",
  "ja422:4.1": "Akāle vassatī tassa,",
  "ja422:4.2": "kāle tassa na vassati;",
  "ja422:4.3": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:4.4": "aññathā naṁ viyākare.",
  "ja422:5.1": "Sace hi saccaṁ bhaṇasi,",
  "ja422:5.2": "hohi rāja yathā pure;",
  "ja422:5.3": "Musā ce bhāsase rāja,",
  "ja422:5.4": "bhūmiṁ pavisa cetiya. ",
  "ja422:6.1": "Jivhā tassa dvidhā hoti,",
  "ja422:6.2": "uragasseva disampati;",
  "ja422:6.3": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:6.4": "aññathā naṁ viyākare.",
  "ja422:7.1": "Sace hi saccaṁ bhaṇasi,",
  "ja422:7.2": "hohi rāja yathā pure;",
  "ja422:7.3": "Musā ce bhāsase rāja,",
  "ja422:7.4": "bhiyyo pavisa cetiya. ",
  "ja422:8.1": "Jivhā tassa na bhavati,",
  "ja422:8.2": "macchasseva disampati;",
  "ja422:8.3": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:8.4": "aññathā naṁ viyākare.",
  "ja422:9.1": "Sace hi saccaṁ bhaṇasi,",
  "ja422:9.2": "hohi rāja yathā pure;",
  "ja422:9.3": "Musā ce bhāsase rāja,",
  "ja422:9.4": "bhiyyo pavisa cetiya. ",
  "ja422:10.1": "Thiyova tassa jāyanti,",
  "ja422:10.2": "na pumā jāyare kule;",
  "ja422:10.3": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:10.4": "aññathā naṁ viyākare.",
  "ja422:11.1": "Sace hi saccaṁ bhaṇasi,",
  "ja422:11.2": "hohi rāja yathā pure;",
  "ja422:11.3": "Musā ce bhāsase rāja,",
  "ja422:11.4": "bhiyyo pavisa cetiya. ",
  "ja422:12.1": "Puttā tassa na bhavanti,",
  "ja422:12.2": "pakkamanti disodisaṁ;",
  "ja422:12.3": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:12.4": "aññathā naṁ viyākare.",
  "ja422:13.1": "Sace hi saccaṁ bhaṇasi,",
  "ja422:13.2": "hohi rāja yathā pure;",
  "ja422:13.3": "Musā ce bhāsase rāja,",
  "ja422:13.4": "bhiyyo pavisa cetiya”. ",
  "ja422:14.1": "“Sa rājā isinā satto,",
  "ja422:14.2": "antalikkhacaro pure;",
  "ja422:14.3": "Pāvekkhi pathaviṁ cecco,",
  "ja422:14.4": "hīnatto patva pariyāyaṁ.",
  "ja422:15.1": "Tasmā hi chandāgamanaṁ,",
  "ja422:15.2": "Nappasaṁsanti paṇḍitā;",
  "ja422:15.3": "Aduṭṭhacitto bhāseyya,",
  "ja422:15.4": "Giraṁ saccūpasaṁhitanti”. ",
  "ja422:16.1": "Cetiyajātakaṁ chaṭṭhaṁ."
}