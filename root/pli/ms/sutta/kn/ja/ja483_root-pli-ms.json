{
  "ja483:0.1": "Jātaka",
  "ja483:0.2": "Terasakanipāta",
  "ja483:0.3": "Ambavagga",
  "ja483:0.4": "10. Sarabhamigajātaka",
  "ja483:1.1": "“Āsīsetheva puriso,",
  "ja483:1.2": "na nibbindeyya paṇḍito;",
  "ja483:1.3": "Passāmi vohaṁ attānaṁ,",
  "ja483:1.4": "yathā icchiṁ tathā ahu.",
  "ja483:2.1": "Āsīsetheva puriso,",
  "ja483:2.2": "na nibbindeyya paṇḍito;",
  "ja483:2.3": "Passāmi vohaṁ attānaṁ,",
  "ja483:2.4": "udakā thalamubbhataṁ.",
  "ja483:3.1": "Vāyametheva puriso,",
  "ja483:3.2": "na nibbindeyya paṇḍito;",
  "ja483:3.3": "Passāmi vohaṁ attānaṁ,",
  "ja483:3.4": "yathā icchiṁ tathā ahu.",
  "ja483:4.1": "Vāyametheva puriso,",
  "ja483:4.2": "na nibbindeyya paṇḍito;",
  "ja483:4.3": "Passāmi vohaṁ attānaṁ,",
  "ja483:4.4": "udakā thalamubbhataṁ.",
  "ja483:5.1": "Dukkhūpanītopi naro sapañño,",
  "ja483:5.2": "Āsaṁ na chindeyya sukhāgamāya;",
  "ja483:5.3": "Bahū hi phassā ahitā hitā ca,",
  "ja483:5.4": "Avitakkitā maccamupabbajanti.",
  "ja483:6.1": "Acintitampi bhavati,",
  "ja483:6.2": "cintitampi vinassati;",
  "ja483:6.3": "Na hi cintāmayā bhogā,",
  "ja483:6.4": "itthiyā purisassa vā”.",
  "ja483:7.1": "“Sarabhaṁ giriduggasmiṁ,",
  "ja483:7.2": "yaṁ tvaṁ anusarī pure;",
  "ja483:7.3": "Alīnacittassa tuvaṁ,",
  "ja483:7.4": "vikkantamanujīvasi.",
  "ja483:8.1": "Yo taṁ viduggā narakā samuddhari,",
  "ja483:8.2": "Silāya yoggaṁ sarabho karitvā;",
  "ja483:8.3": "Dukkhūpanītaṁ maccumukhā pamocayi,",
  "ja483:8.4": "Alīnacittaṁ ta migaṁ vadesi”.",
  "ja483:9.1": "“Kiṁ tvaṁ nu tattheva tadā ahosi,",
  "ja483:9.2": "Udāhu te koci naṁ etadakkhā;",
  "ja483:9.3": "Vivaṭṭacchaddo nusi sabbadassī,",
  "ja483:9.4": "Ñāṇaṁ nu te brāhmaṇa bhiṁsarūpaṁ”.",
  "ja483:10.1": "“Na cevahaṁ tattha tadā ahosiṁ,",
  "ja483:10.2": "Na cāpi me koci naṁ etadakkhā;",
  "ja483:10.3": "Gāthāpadānañca subhāsitānaṁ,",
  "ja483:10.4": "Atthaṁ tadānenti janinda dhīrā”.",
  "ja483:11.1": "“Ādāya pattiṁ paraviriyaghātiṁ,",
  "ja483:11.2": "Cāpe saraṁ kiṁ vicikicchase tuvaṁ;",
  "ja483:11.3": "Nunno saro sarabhaṁ hantu khippaṁ,",
  "ja483:11.4": "Annañhi etaṁ varapañña rañño”.",
  "ja483:12.1": "“Addhā pajānāmi ahampi etaṁ,",
  "ja483:12.2": "Annaṁ migo brāhmaṇa khattiyassa;",
  "ja483:12.3": "Pubbe katañca apacāyamāno,",
  "ja483:12.4": "Tasmā migaṁ sarabhaṁ no hanāmi”.",
  "ja483:13.1": "“Neso migo mahārāja,",
  "ja483:13.2": "asureso disampati;",
  "ja483:13.3": "Etaṁ hantvā manussinda,",
  "ja483:13.4": "bhavassu amarādhipo.",
  "ja483:14.1": "Sace ca rājā vicikicchase tuvaṁ,",
  "ja483:14.2": "Hantuṁ migaṁ sarabhaṁ sahāyakaṁ;",
  "ja483:14.3": "Saputtadāro naravīraseṭṭha,",
  "ja483:14.4": "Gantā tuvaṁ vetaraṇiṁ yamassa”.",
  "ja483:15.1": "“Kāmaṁ ahaṁ jānapadā ca sabbe,",
  "ja483:15.2": "Puttā ca dārā ca sahāyasaṅghā;",
  "ja483:15.3": "Gacchemu taṁ vetaraṇiṁ yamassa,",
  "ja483:15.4": "Na tveva hañño mama pāṇado yo.",
  "ja483:16.1": "Ayaṁ migo kicchagatassa mayhaṁ,",
  "ja483:16.2": "Ekassa kattā vivanasmi ghore;",
  "ja483:16.3": "Taṁ tādisaṁ pubbakiccaṁ saranto,",
  "ja483:16.4": "Jānaṁ mahābrahme kathaṁ haneyyaṁ”.",
  "ja483:17.1": "“Mittābhirādhī cirameva jīva,",
  "ja483:17.2": "Rajjaṁ imaṁ dhammaguṇe pasāsa;",
  "ja483:17.3": "Nārīgaṇehi paricāriyanto,",
  "ja483:17.4": "Modassu raṭṭhe tidiveva vāsavo.",
  "ja483:18.1": "Akkodhano niccapasannacitto,",
  "ja483:18.2": "Sabbātithī yācayogo bhavitvā;",
  "ja483:18.3": "Datvā ca bhutvā ca yathānubhāvaṁ,",
  "ja483:18.4": "Anindito saggamupehi ṭhānan”ti. ",
  "ja483:19.1": "Sarabhamigajātakaṁ dasamaṁ.",
  "ja483:20.1": "Terasakanipātaṁ niṭṭhitaṁ.",
  "ja483:21.1": "Tassuddānaṁ",
  "ja483:22.1": "Varaamba kuṭhāri sahaṁsavaro,",
  "ja483:22.2": "Atharaññasmiṁ dūtakapañcamako;",
  "ja483:22.3": "Atha bodhi akitti sutakkarinā,",
  "ja483:22.4": "Atha rurumigenaparo sarabhoti."
}