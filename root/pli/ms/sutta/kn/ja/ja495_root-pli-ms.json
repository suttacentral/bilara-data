{
  "ja495:0.1": "Jātaka",
  "ja495:0.2": "Pakiṇṇakanipāta",
  "ja495:0.3": "Sālikedāravagga",
  "ja495:0.4": "12. Dasabrāhmaṇajātaka",
  "ja495:1.1": "Rājā avoca vidhuraṁ,",
  "ja495:1.2": "Dhammakāmo yudhiṭṭhilo;",
  "ja495:1.3": "“Brāhmaṇe vidhura pariyesa,",
  "ja495:1.4": "Sīlavante bahussute.",
  "ja495:2.1": "Virate methunā dhammā,",
  "ja495:2.2": "Ye me bhuñjeyyu bhojanaṁ;",
  "ja495:2.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:2.4": "Yattha dinnaṁ mahapphalaṁ”.",
  "ja495:3.1": "“Dullabhā brāhmaṇā deva,",
  "ja495:3.2": "sīlavanto bahussutā;",
  "ja495:3.3": "Viratā methunā dhammā,",
  "ja495:3.4": "ye te bhuñjeyyu bhojanaṁ.",
  "ja495:4.1": "Dasa khalu mahārāja,",
  "ja495:4.2": "yā tā brāhmaṇajātiyo;",
  "ja495:4.3": "Tesaṁ vibhaṅgaṁ vicayaṁ,",
  "ja495:4.4": "vitthārena suṇohi me.",
  "ja495:5.1": "Pasibbake gahetvāna,",
  "ja495:5.2": "puṇṇe mūlassa saṁvute;",
  "ja495:5.3": "Osadhikāyo ganthenti,",
  "ja495:5.4": "nhāpayanti japanti ca.",
  "ja495:6.1": "Tikicchakasamā rāja,",
  "ja495:6.2": "tepi vuccanti brāhmaṇā;",
  "ja495:6.3": "Akkhātā te mahārāja,",
  "ja495:6.4": "tādise nipatāmase”. ",
  "ja495:7.1": "“Apetā te ca brahmaññā,",
  "ja495:7.2": "(iti rājā korabyo)",
  "ja495:7.3": "Na te vuccanti brāhmaṇā;",
  "ja495:7.4": "Aññe vidhura pariyesa,",
  "ja495:7.5": "Sīlavante bahussute.",
  "ja495:8.1": "Virate methunā dhammā,",
  "ja495:8.2": "Ye me bhuñjeyyu bhojanaṁ;",
  "ja495:8.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:8.4": "Yattha dinnaṁ mahapphalaṁ”.",
  "ja495:9.1": "“Kiṅkiṇikāyo gahetvā,",
  "ja495:9.2": "ghosenti puratopi te;",
  "ja495:9.3": "Pesanānipi gacchanti,",
  "ja495:9.4": "rathacariyāsu sikkhare.",
  "ja495:10.1": "Paricārakasamā rāja,",
  "ja495:10.2": "tepi vuccanti brāhmaṇā;",
  "ja495:10.3": "Akkhātā te mahārāja,",
  "ja495:10.4": "tādise nipatāmase”. ",
  "ja495:11.1": "“Apetā te ca brahmaññā,",
  "ja495:11.2": "(iti rājā korabyo)",
  "ja495:11.3": "Na te vuccanti brāhmaṇā;",
  "ja495:11.4": "Aññe vidhura pariyesa,",
  "ja495:11.5": "Sīlavante bahussute.",
  "ja495:12.1": "Virate methunā dhammā,",
  "ja495:12.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:12.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:12.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:13.1": "“Kamaṇḍaluṁ gahetvāna,",
  "ja495:13.2": "vaṅkadaṇḍañca brāhmaṇā;",
  "ja495:13.3": "Paccupessanti rājāno,",
  "ja495:13.4": "gāmesu nigamesu ca;",
  "ja495:13.5": "Nādinne vuṭṭhahissāma,",
  "ja495:13.6": "gāmamhi vā vanamhi vā.",
  "ja495:14.1": "Niggāhakasamā rāja,",
  "ja495:14.2": "tepi vuccanti brāhmaṇā;",
  "ja495:14.3": "Akkhātā te mahārāja,",
  "ja495:14.4": "tādise nipatāmase”. ",
  "ja495:15.1": "“Apetā te ca brahmaññā,",
  "ja495:15.2": "(iti rājā korabyo)",
  "ja495:15.3": "Na te vuccanti brāhmaṇā;",
  "ja495:15.4": "Aññe vidhura pariyesa,",
  "ja495:15.5": "Sīlavante bahussute.",
  "ja495:16.1": "Virate methunā dhammā,",
  "ja495:16.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:16.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:16.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:17.1": "“Parūḷhakacchanakhalomā,",
  "ja495:17.2": "paṅkadantā rajassirā;",
  "ja495:17.3": "Okiṇṇā rajareṇūhi,",
  "ja495:17.4": "yācakā vicaranti te.",
  "ja495:18.1": "Khāṇughātasamā rāja,",
  "ja495:18.2": "tepi vuccanti brāhmaṇā;",
  "ja495:18.3": "Akkhātā te mahārāja,",
  "ja495:18.4": "tādise nipatāmase”. ",
  "ja495:19.1": "“Apetā te ca brahmaññā,",
  "ja495:19.2": "(iti rājā korabyo)",
  "ja495:19.3": "Na te vuccanti brāhmaṇā;",
  "ja495:19.4": "Aññe vidhura pariyesa,",
  "ja495:19.5": "Sīlavante bahussute.",
  "ja495:20.1": "Virate methunā dhammā,",
  "ja495:20.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:20.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:20.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:21.1": "“Harītakaṁ āmalakaṁ,",
  "ja495:21.2": "Ambaṁ jambuṁ vibhītakaṁ;",
  "ja495:21.3": "Labujaṁ dantapoṇāni,",
  "ja495:21.4": "Beluvā badarāni ca.",
  "ja495:22.1": "Rājāyatanaṁ ucchupuṭaṁ,",
  "ja495:22.2": "dhūmanettaṁ madhuañjanaṁ;",
  "ja495:22.3": "Uccāvacāni paṇiyāni,",
  "ja495:22.4": "vipaṇenti janādhipa.",
  "ja495:23.1": "Vāṇijakasamā rāja,",
  "ja495:23.2": "tepi vuccanti brāhmaṇā;",
  "ja495:23.3": "Akkhātā te mahārāja,",
  "ja495:23.4": "tādise nipatāmase”. ",
  "ja495:24.1": "“Apetā te ca brahmaññā,",
  "ja495:24.2": "(iti rājā korabyo)",
  "ja495:24.3": "Na te vuccanti brāhmaṇā;",
  "ja495:24.4": "Aññe vidhura pariyesa,",
  "ja495:24.5": "Sīlavante bahussute.",
  "ja495:25.1": "Virate methunā dhammā,",
  "ja495:25.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:25.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:25.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:26.1": "“Kasivāṇijjaṁ kārenti,",
  "ja495:26.2": "posayanti ajeḷake;",
  "ja495:26.3": "Kumāriyo pavecchanti,",
  "ja495:26.4": "vivāhantāvahanti ca.",
  "ja495:27.1": "Samā ambaṭṭhavessehi,",
  "ja495:27.2": "tepi vuccanti brāhmaṇā;",
  "ja495:27.3": "Akkhātā te mahārāja,",
  "ja495:27.4": "tādise nipatāmase”. ",
  "ja495:28.1": "“Apetā te ca brahmaññā,",
  "ja495:28.2": "(iti rājā korabyo)",
  "ja495:28.3": "Na te vuccanti brāhmaṇā;",
  "ja495:28.4": "Aññe vidhura pariyesa,",
  "ja495:28.5": "Sīlavante bahussute.",
  "ja495:29.1": "Virate methunā dhammā,",
  "ja495:29.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:29.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:29.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:30.1": "“Nikkhittabhikkhaṁ bhuñjanti,",
  "ja495:30.2": "gāmesveke purohitā;",
  "ja495:30.3": "Bahū te paripucchanti,",
  "ja495:30.4": "aṇḍacchedā nilañchakā.",
  "ja495:31.1": "Pasūpi tattha haññanti,",
  "ja495:31.2": "mahiṁsā sūkarā ajā;",
  "ja495:31.3": "Goghātakasamā rāja,",
  "ja495:31.4": "tepi vuccanti brāhmaṇā;",
  "ja495:31.5": "Akkhātā te mahārāja,",
  "ja495:31.6": "tādise nipatāmase”. ",
  "ja495:32.1": "“Apetā te ca brahmaññā,",
  "ja495:32.2": "(iti rājā korabyo)",
  "ja495:32.3": "Na te vuccanti brāhmaṇā;",
  "ja495:32.4": "Aññe vidhura pariyesa,",
  "ja495:32.5": "Sīlavante bahussute.",
  "ja495:33.1": "Virate methunā dhammā,",
  "ja495:33.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:33.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:33.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:34.1": "“Asicammaṁ gahetvāna,",
  "ja495:34.2": "khaggaṁ paggayha brāhmaṇā;",
  "ja495:34.3": "Vessapathesu tiṭṭhanti,",
  "ja495:34.4": "satthaṁ abbāhayantipi.",
  "ja495:35.1": "Samā gopanisādehi,",
  "ja495:35.2": "tepi vuccanti brāhmaṇā;",
  "ja495:35.3": "Akkhātā te mahārāja,",
  "ja495:35.4": "tādise nipatāmase”. ",
  "ja495:36.1": "“Apetā te ca brahmaññā,",
  "ja495:36.2": "(iti rājā korabyo)",
  "ja495:36.3": "Na te vuccanti brāhmaṇā;",
  "ja495:36.4": "Aññe vidhura pariyesa,",
  "ja495:36.5": "Sīlavante bahussute.",
  "ja495:37.1": "Virate methunā dhammā,",
  "ja495:37.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:37.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:37.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:38.1": "“Araññe kuṭikaṁ katvā,",
  "ja495:38.2": "kūṭāni kārayanti te;",
  "ja495:38.3": "Sasabiḷāre bādhenti,",
  "ja495:38.4": "āgodhā macchakacchapaṁ.",
  "ja495:39.1": "Te luddakasamā rāja,",
  "ja495:39.2": "tepi vuccanti brāhmaṇā;",
  "ja495:39.3": "Akkhātā te mahārāja,",
  "ja495:39.4": "tādise nipatāmase”. ",
  "ja495:40.1": "“Apetā te ca brahmaññā,",
  "ja495:40.2": "(iti rājā korabyo)",
  "ja495:40.3": "Na te vuccanti brāhmaṇā;",
  "ja495:40.4": "Aññe vidhura pariyesa,",
  "ja495:40.5": "Sīlavante bahussute.",
  "ja495:41.1": "Virate methunā dhammā,",
  "ja495:41.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:41.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:41.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:42.1": "“Aññe dhanassa kāmā hi,",
  "ja495:42.2": "heṭṭhāmañce pasakkitā;",
  "ja495:42.3": "Rājāno upari nhāyanti,",
  "ja495:42.4": "somayāge upaṭṭhite.",
  "ja495:43.1": "Malamajjakasamā rāja,",
  "ja495:43.2": "Tepi vuccanti brāhmaṇā;",
  "ja495:43.3": "Akkhātā te mahārāja,",
  "ja495:43.4": "Tādise nipatāmase”. ",
  "ja495:44.1": "“Apetā te ca brahmaññā,",
  "ja495:44.2": "(iti rājā korabyo)",
  "ja495:44.3": "Na te vuccanti brāhmaṇā;",
  "ja495:44.4": "Aññe vidhura pariyesa,",
  "ja495:44.5": "Sīlavante bahussute.",
  "ja495:45.1": "Virate methunā dhammā,",
  "ja495:45.2": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:45.3": "Dakkhiṇaṁ samma dassāma,",
  "ja495:45.4": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:46.1": "“Atthi kho brāhmaṇā deva,",
  "ja495:46.2": "sīlavanto bahussutā;",
  "ja495:46.3": "Viratā methunā dhammā,",
  "ja495:46.4": "ye te bhuñjeyyu bhojanaṁ.",
  "ja495:47.1": "Ekañca bhattaṁ bhuñjanti,",
  "ja495:47.2": "na ca majjaṁ pivanti te;",
  "ja495:47.3": "Akkhātā te mahārāja,",
  "ja495:47.4": "tādise nipatāmase”.",
  "ja495:48.1": "“Ete kho brāhmaṇā vidhura,",
  "ja495:48.2": "Sīlavanto bahussutā;",
  "ja495:48.3": "Ete vidhura pariyesa,",
  "ja495:48.4": "Khippañca ne nimantayā”ti.",
  "ja495:49.1": "Dasabrāhmaṇajātakaṁ dvādasamaṁ."
}