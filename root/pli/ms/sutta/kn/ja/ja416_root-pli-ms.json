{
  "ja416:0.1": "Jātaka",
  "ja416:0.2": "Sattakanipāta",
  "ja416:0.3": "Gandhāravagga",
  "ja416:0.4": "11. Parantapajātaka",
  "ja416:1.1": "“Āgamissati me pāpaṁ,",
  "ja416:1.2": "āgamissati me bhayaṁ;",
  "ja416:1.3": "Tadā hi calitā sākhā,",
  "ja416:1.4": "manussena migena vā”.",
  "ja416:2.1": "“Bhīruyā nūna me kāmo,",
  "ja416:2.2": "avidūre vasantiyā;",
  "ja416:2.3": "Karissati kisaṁ paṇḍuṁ,",
  "ja416:2.4": "sāva sākhā parantapaṁ.",
  "ja416:3.1": "Socayissati maṁ kantā,",
  "ja416:3.2": "gāme vasamaninditā;",
  "ja416:3.3": "Karissati kisaṁ paṇḍuṁ,",
  "ja416:3.4": "sāva sākhā parantapaṁ.",
  "ja416:4.1": "Tayā maṁ asitāpaṅgi,",
  "ja416:4.2": "sitāni bhaṇitāni ca;",
  "ja416:4.3": "Kisaṁ paṇḍuṁ karissanti,",
  "ja416:4.4": "sāva sākhā parantapaṁ”.",
  "ja416:5.1": "“Agamā nūna so saddo,",
  "ja416:5.2": "asaṁsi nūna so tava;",
  "ja416:5.3": "Akkhātaṁ nūna taṁ tena,",
  "ja416:5.4": "yo taṁ sākhamakampayi.",
  "ja416:6.1": "Idaṁ kho taṁ samāgamma,",
  "ja416:6.2": "mama bālassa cintitaṁ;",
  "ja416:6.3": "Tadā hi calitā sākhā,",
  "ja416:6.4": "manussena migena vā”.",
  "ja416:7.1": "“Tatheva tvaṁ avedesi,",
  "ja416:7.2": "Avañci pitaraṁ mama;",
  "ja416:7.3": "Hantvā sākhāhi chādento,",
  "ja416:7.4": "Āgamissati me bhayan”ti. ",
  "ja416:8.1": "Parantapajātakaṁ ekādasamaṁ.",
  "ja416:8.2": "Gandhāravaggo dutiyo.",
  "ja416:9.1": "Tassuddānaṁ",
  "ja416:10.1": "Varagāma mahākapi bhaggava ca,",
  "ja416:10.2": "Daḷhadhamma sakuñjara kesavaro;",
  "ja416:10.3": "Urago vidhuro puna jāgarataṁ,",
  "ja416:10.4": "Atha kosalādhipa parantapa cāti.",
  "ja416:11.1": "Atha vagguddānaṁ",
  "ja416:12.1": "Atha sattanipātamhi,",
  "ja416:12.2": "vaggaṁ me bhaṇato suṇa;",
  "ja416:12.3": "Kukku ca puna gandhāro,",
  "ja416:12.4": "dveva vuttā mahesināti.",
  "ja416:13.1": "Sattakanipātaṁ niṭṭhitaṁ."
}