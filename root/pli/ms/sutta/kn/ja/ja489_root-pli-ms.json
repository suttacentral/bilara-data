{
  "ja489:0.1": "Jātaka",
  "ja489:0.2": "Pakiṇṇakanipāta",
  "ja489:0.3": "Sālikedāravagga",
  "ja489:0.4": "6. Surucijātaka",
  "ja489:1.1": "“Mahesī surucino bhariyā,",
  "ja489:1.2": "Ānītā paṭhamaṁ ahaṁ;",
  "ja489:1.3": "Dasa vassasahassāni,",
  "ja489:1.4": "Yaṁ maṁ surucimānayi.",
  "ja489:2.1": "Sāhaṁ brāhmaṇa rājānaṁ,",
  "ja489:2.2": "vedehaṁ mithilaggahaṁ;",
  "ja489:2.3": "Nābhijānāmi kāyena,",
  "ja489:2.4": "vācāya uda cetasā;",
  "ja489:2.5": "Suruciṁ atimaññittha,",
  "ja489:2.6": "āvi vā yadi vā raho.",
  "ja489:3.1": "Etena saccavajjena,",
  "ja489:3.2": "putto uppajjataṁ ise;",
  "ja489:3.3": "Musā me bhaṇamānāya,",
  "ja489:3.4": "muddhā phalatu sattadhā. ",
  "ja489:4.1": "Bhattu mama sassu mātā,",
  "ja489:4.2": "pitā cāpi ca sassuro;",
  "ja489:4.3": "Te maṁ brahme vinetāro,",
  "ja489:4.4": "yāva aṭṭhaṁsu jīvitaṁ.",
  "ja489:5.1": "Sāhaṁ ahiṁsāratinī,",
  "ja489:5.2": "kāmasā dhammacārinī;",
  "ja489:5.3": "Sakkaccaṁ te upaṭṭhāsiṁ,",
  "ja489:5.4": "rattindivamatanditā.",
  "ja489:6.1": "Etena saccavajjena,",
  "ja489:6.2": "putto uppajjataṁ ise;",
  "ja489:6.3": "Musā me bhaṇamānāya,",
  "ja489:6.4": "muddhā phalatu sattadhā. ",
  "ja489:7.1": "Soḷasitthisahassāni,",
  "ja489:7.2": "sahabhariyāni brāhmaṇa;",
  "ja489:7.3": "Tāsu issā vā kodho vā,",
  "ja489:7.4": "nāhu mayhaṁ kudācanaṁ.",
  "ja489:8.1": "Hitena tāsaṁ nandāmi,",
  "ja489:8.2": "na ca me kāci appiyā;",
  "ja489:8.3": "Attānaṁvānukampāmi,",
  "ja489:8.4": "sadā sabbā sapattiyo.",
  "ja489:9.1": "Etena saccavajjena,",
  "ja489:9.2": "putto uppajjataṁ ise;",
  "ja489:9.3": "Musā me bhaṇamānāya,",
  "ja489:9.4": "muddhā phalatu sattadhā. ",
  "ja489:10.1": "Dāse kammakare pesse,",
  "ja489:10.2": "ye caññe anujīvino;",
  "ja489:10.3": "Pesemi sahadhammena,",
  "ja489:10.4": "sadā pamuditindriyā.",
  "ja489:11.1": "Etena saccavajjena,",
  "ja489:11.2": "putto uppajjataṁ ise;",
  "ja489:11.3": "Musā me bhaṇamānāya,",
  "ja489:11.4": "muddhā phalatu sattadhā. ",
  "ja489:12.1": "Samaṇe brāhmaṇe cāpi,",
  "ja489:12.2": "aññe cāpi vanibbake;",
  "ja489:12.3": "Tappemi annapānena,",
  "ja489:12.4": "sadā payatapāṇinī.",
  "ja489:13.1": "Etena saccavajjena,",
  "ja489:13.2": "putto uppajjataṁ ise;",
  "ja489:13.3": "Musā me bhaṇamānāya,",
  "ja489:13.4": "muddhā phalatu sattadhā. ",
  "ja489:14.1": "Cātuddasiṁ pañcaddasiṁ,",
  "ja489:14.2": "Yā ca pakkhassa aṭṭhamī;",
  "ja489:14.3": "Pāṭihāriyapakkhañca,",
  "ja489:14.4": "Aṭṭhaṅgasusamāgataṁ;",
  "ja489:14.5": "Uposathaṁ upavasāmi,",
  "ja489:14.6": "Sadā sīlesu saṁvutā.",
  "ja489:15.1": "Etena saccavajjena,",
  "ja489:15.2": "putto uppajjataṁ ise;",
  "ja489:15.3": "Musā me bhaṇamānāya,",
  "ja489:15.4": "muddhā phalatu sattadhā”. ",
  "ja489:16.1": "“Sabbeva te dhammaguṇā,",
  "ja489:16.2": "rājaputti yasassini;",
  "ja489:16.3": "Saṁvijjanti tayi bhadde,",
  "ja489:16.4": "ye tvaṁ kittesi attani.",
  "ja489:17.1": "Khattiyo jātisampanno,",
  "ja489:17.2": "abhijāto yasassimā;",
  "ja489:17.3": "Dhammarājā videhānaṁ,",
  "ja489:17.4": "putto uppajjate tava”.",
  "ja489:18.1": "“Dummī rajojalladharo,",
  "ja489:18.2": "aghe vehāyasaṁ ṭhito;",
  "ja489:18.3": "Manuññaṁ bhāsase vācaṁ,",
  "ja489:18.4": "yaṁ mayhaṁ hadayaṅgamaṁ.",
  "ja489:19.1": "Devatānusi saggamhā,",
  "ja489:19.2": "isi vāsi mahiddhiko;",
  "ja489:19.3": "Ko vāsi tvaṁ anuppatto,",
  "ja489:19.4": "attānaṁ me pavedaya”.",
  "ja489:20.1": "“Yaṁ devasaṅghā vandanti,",
  "ja489:20.2": "sudhammāyaṁ samāgatā;",
  "ja489:20.3": "Sohaṁ sakko sahassakkho,",
  "ja489:20.4": "āgatosmi tavantike.",
  "ja489:21.1": "Itthiyo jīvalokasmiṁ,",
  "ja489:21.2": "yā hoti samacārinī;",
  "ja489:21.3": "Medhāvinī sīlavatī,",
  "ja489:21.4": "sassudevā patibbatā.",
  "ja489:22.1": "Tādisāya sumedhāya,",
  "ja489:22.2": "sucikammāya nāriyā;",
  "ja489:22.3": "Devā dassanamāyanti,",
  "ja489:22.4": "mānusiyā amānusā.",
  "ja489:23.1": "Tvañca bhadde suciṇṇena,",
  "ja489:23.2": "pubbe sucaritena ca;",
  "ja489:23.3": "Idha rājakule jātā,",
  "ja489:23.4": "sabbakāmasamiddhinī.",
  "ja489:24.1": "Ayañca te rājaputti,",
  "ja489:24.2": "ubhayattha kaṭaggaho;",
  "ja489:24.3": "Devalokūpapattī ca,",
  "ja489:24.4": "kittī ca idha jīvite.",
  "ja489:25.1": "Ciraṁ sumedhe sukhinī,",
  "ja489:25.2": "dhammamattani pālaya;",
  "ja489:25.3": "Esāhaṁ tidivaṁ yāmi,",
  "ja489:25.4": "piyaṁ me tava dassanan”ti.",
  "ja489:26.1": "Surucijātakaṁ chaṭṭhaṁ."
}