{
  "ja250:0.1": "Jātaka",
  "ja250:0.2": "Dukanipāta",
  "ja250:0.3": "Siṅgālavagga",
  "ja250:0.4": "10. Kapijātaka",
  "ja250:1.1": "“Ayaṁ isī upasamasaṁyame rato,",
  "ja250:1.2": "Sa tiṭṭhati sisirabhayena aṭṭito;",
  "ja250:1.3": "Handa ayaṁ pavisatumaṁ agārakaṁ,",
  "ja250:1.4": "Vinetu sītaṁ darathañca kevalaṁ”.",
  "ja250:2.1": "“Nāyaṁ isī upasamasaṁyame rato,",
  "ja250:2.2": "Kapī ayaṁ dumavarasākhagocaro;",
  "ja250:2.3": "So dūsako rosako cāpi jammo,",
  "ja250:2.4": "Sacevajemampi dūseyyagāran”ti. ",
  "ja250:3.1": "Kapijātakaṁ dasamaṁ.",
  "ja250:3.2": "Siṅgālavaggo dasamo.",
  "ja250:4.1": "Tassuddānaṁ",
  "ja250:5.1": "Atha rājā siṅgālavaro sunakho,",
  "ja250:5.2": "Tathā kosiya icchati kālaghaso;",
  "ja250:5.3": "Atha dānavaroṭṭhapi sārathinā,",
  "ja250:5.4": "Punambavanañca sisirakapi dasāti.",
  "ja250:6.1": "Atha vagguddānaṁ",
  "ja250:7.1": "Daḷhañca vaggaṁ aparena santhavaṁ,",
  "ja250:7.2": "Kalyāṇavaggāsadiso ca rūhakaṁ;",
  "ja250:7.3": "Nataṁdaḷha bīraṇathambhakaṁ puna,",
  "ja250:7.4": "Kāsāvupāhana siṅgālakena dasāti.",
  "ja250:8.1": "Dukanipātaṁ niṭṭhitaṁ."
}