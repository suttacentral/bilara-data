{
  "ja473:0.1": "Jātaka",
  "ja473:0.2": "Dvādasakanipāta",
  "ja473:0.3": "Cūḷakuṇālavagga",
  "ja473:0.4": "10. Mittāmittajātaka",
  "ja473:1.1": "“Kāni kammāni kubbānaṁ,",
  "ja473:1.2": "kathaṁ viññū parakkame;",
  "ja473:1.3": "Amittaṁ jāneyya medhāvī,",
  "ja473:1.4": "disvā sutvā ca paṇḍito”.",
  "ja473:2.1": "“Na naṁ umhayate disvā,",
  "ja473:2.2": "na ca naṁ paṭinandati;",
  "ja473:2.3": "Cakkhūni cassa na dadāti,",
  "ja473:2.4": "paṭilomañca vattati.",
  "ja473:3.1": "Amitte tassa bhajati,",
  "ja473:3.2": "mitte tassa na sevati;",
  "ja473:3.3": "Vaṇṇakāme nivāreti,",
  "ja473:3.4": "akkosante pasaṁsati.",
  "ja473:4.1": "Guyhañca tassa nakkhāti,",
  "ja473:4.2": "tassa guyhaṁ na gūhati;",
  "ja473:4.3": "Kammaṁ tassa na vaṇṇeti,",
  "ja473:4.4": "paññassa nappasaṁsati.",
  "ja473:5.1": "Abhave nandati tassa,",
  "ja473:5.2": "bhave tassa na nandati;",
  "ja473:5.3": "Accheraṁ bhojanaṁ laddhā,",
  "ja473:5.4": "tassa nuppajjate sati;",
  "ja473:5.5": "Tato naṁ nānukampati,",
  "ja473:5.6": "aho sopi labheyyito.",
  "ja473:6.1": "Iccete soḷasākārā,",
  "ja473:6.2": "amittasmiṁ patiṭṭhitā;",
  "ja473:6.3": "Yehi amittaṁ jāneyya,",
  "ja473:6.4": "disvā sutvā ca paṇḍito”.",
  "ja473:7.1": "“Kāni kammāni kubbānaṁ,",
  "ja473:7.2": "kathaṁ viññū parakkame;",
  "ja473:7.3": "Mittaṁ jāneyya medhāvī,",
  "ja473:7.4": "disvā sutvā ca paṇḍito.",
  "ja473:8.1": "Pavutthaṁ tassa sarati,",
  "ja473:8.2": "āgataṁ abhinandati;",
  "ja473:8.3": "Tato kelāyito hoti,",
  "ja473:8.4": "vācāya paṭinandati.",
  "ja473:9.1": "Mitte tasseva bhajati,",
  "ja473:9.2": "amitte tassa na sevati;",
  "ja473:9.3": "Akkosante nivāreti,",
  "ja473:9.4": "vaṇṇakāme pasaṁsati.",
  "ja473:10.1": "Guyhañca tassa akkhāti,",
  "ja473:10.2": "tassa guyhañca gūhati;",
  "ja473:10.3": "Kammañca tassa vaṇṇeti,",
  "ja473:10.4": "paññaṁ tassa pasaṁsati.",
  "ja473:11.1": "Bhave ca nandati tassa,",
  "ja473:11.2": "abhave tassa na nandati;",
  "ja473:11.3": "Accheraṁ bhojanaṁ laddhā,",
  "ja473:11.4": "tassa uppajjate sati;",
  "ja473:11.5": "Tato naṁ anukampati,",
  "ja473:11.6": "aho sopi labheyyito.",
  "ja473:12.1": "Iccete soḷasākārā,",
  "ja473:12.2": "Mittasmiṁ suppatiṭṭhitā;",
  "ja473:12.3": "Yehi mittañca jāneyya,",
  "ja473:12.4": "Disvā sutvā ca paṇḍito”ti. ",
  "ja473:13.1": "Mittāmittajātakaṁ dasamaṁ.",
  "ja473:14.1": "Dvādasakanipātaṁ niṭṭhitaṁ.",
  "ja473:15.1": "Tassuddānaṁ",
  "ja473:16.1": "Lahucitta sasāla kasanti puna,",
  "ja473:16.2": "Atha kāma dasakhaluṭṭhānavaro;",
  "ja473:16.3": "Atha kaṇha sukosiya meṇḍavaro,",
  "ja473:16.4": "Padumo puna mittavarena dasāti."
}