{
  "ja496:0.1": "Jātaka",
  "ja496:0.2": "Pakiṇṇakanipāta",
  "ja496:0.3": "Sālikedāravagga",
  "ja496:0.4": "13. Bhikkhāparamparajātaka",
  "ja496:1.1": "“Sukhumālarūpaṁ disvā,",
  "ja496:1.2": "raṭṭhā vivanamāgataṁ;",
  "ja496:1.3": "Kūṭāgāravarūpetaṁ,",
  "ja496:1.4": "mahāsayanamupāsitaṁ.",
  "ja496:2.1": "Tassa te pemakenāhaṁ,",
  "ja496:2.2": "adāsiṁ vaḍḍhamodanaṁ;",
  "ja496:2.3": "Sālīnaṁ vicitaṁ bhattaṁ,",
  "ja496:2.4": "suciṁ maṁsūpasecanaṁ.",
  "ja496:3.1": "Taṁ tvaṁ bhattaṁ paṭiggayha,",
  "ja496:3.2": "brāhmaṇassa adāsayi;",
  "ja496:3.3": "Attānaṁ anasitvāna,",
  "ja496:3.4": "koyaṁ dhammo namatthu te”.",
  "ja496:4.1": "“Ācariyo brāhmaṇo mayhaṁ,",
  "ja496:4.2": "kiccākiccesu byāvaṭo;",
  "ja496:4.3": "Garu ca āmantanīyo ca,",
  "ja496:4.4": "dātumarahāmi bhojanaṁ”.",
  "ja496:5.1": "“Brāhmaṇaṁ dāni pucchāmi,",
  "ja496:5.2": "gotamaṁ rājapūjitaṁ;",
  "ja496:5.3": "Rājā te bhattaṁ pādāsi,",
  "ja496:5.4": "suciṁ maṁsūpasecanaṁ.",
  "ja496:6.1": "Taṁ tvaṁ bhattaṁ paṭiggayha,",
  "ja496:6.2": "isissa bhojanaṁ adā;",
  "ja496:6.3": "Akhettaññūsi dānassa,",
  "ja496:6.4": "koyaṁ dhammo namatthu te”.",
  "ja496:7.1": "“Bharāmi puttadāre ca,",
  "ja496:7.2": "gharesu gadhito ahaṁ;",
  "ja496:7.3": "Bhuñje mānusake kāme,",
  "ja496:7.4": "anusāsāmi rājino.",
  "ja496:8.1": "Āraññikassa isino,",
  "ja496:8.2": "cirarattaṁ tapassino;",
  "ja496:8.3": "Vuḍḍhassa bhāvitattassa,",
  "ja496:8.4": "dātumarahāmi bhojanaṁ”.",
  "ja496:9.1": "“Isiñca dāni pucchāmi,",
  "ja496:9.2": "kisaṁ dhamanisanthataṁ;",
  "ja496:9.3": "Parūḷhakacchanakhalomaṁ,",
  "ja496:9.4": "paṅkadantaṁ rajassiraṁ.",
  "ja496:10.1": "Eko araññe viharasi,",
  "ja496:10.2": "nāvakaṅkhasi jīvitaṁ;",
  "ja496:10.3": "Bhikkhu kena tayā seyyo,",
  "ja496:10.4": "yassa tvaṁ bhojanaṁ adā”.",
  "ja496:11.1": "“Khaṇantālukalambāni,",
  "ja496:11.2": "bilālitakkalāni ca;",
  "ja496:11.3": "Dhunaṁ sāmākanīvāraṁ,",
  "ja496:11.4": "saṅghāriyaṁ pasāriyaṁ.",
  "ja496:12.1": "Sākaṁ bhisaṁ madhuṁ maṁsaṁ,",
  "ja496:12.2": "Badarāmalakāni ca;",
  "ja496:12.3": "Tāni āharitvā bhuñjāmi,",
  "ja496:12.4": "Atthi me so pariggaho.",
  "ja496:13.1": "Pacanto apacantassa,",
  "ja496:13.2": "amamassa sakiñcano;",
  "ja496:13.3": "Anādānassa sādāno,",
  "ja496:13.4": "dātumarahāmi bhojanaṁ”.",
  "ja496:14.1": "“Bhikkhuñca dāni pucchāmi,",
  "ja496:14.2": "tuṇhīmāsīna subbataṁ;",
  "ja496:14.3": "Isi te bhattaṁ pādāsi,",
  "ja496:14.4": "suciṁ maṁsūpasecanaṁ.",
  "ja496:15.1": "Taṁ tvaṁ bhattaṁ paṭiggayha,",
  "ja496:15.2": "tuṇhī bhuñjasi ekako;",
  "ja496:15.3": "Nāññaṁ kañci nimantesi,",
  "ja496:15.4": "koyaṁ dhammo namatthu te”.",
  "ja496:16.1": "“Na pacāmi na pācemi,",
  "ja496:16.2": "na chindāmi na chedaye;",
  "ja496:16.3": "Taṁ maṁ akiñcanaṁ ñatvā,",
  "ja496:16.4": "sabbapāpehi ārataṁ.",
  "ja496:17.1": "Vāmena bhikkhamādāya,",
  "ja496:17.2": "dakkhiṇena kamaṇḍaluṁ;",
  "ja496:17.3": "Isi me bhattaṁ pādāsi,",
  "ja496:17.4": "suciṁ maṁsūpasecanaṁ.",
  "ja496:18.1": "Ete hi dātumarahanti,",
  "ja496:18.2": "samamā sapariggahā;",
  "ja496:18.3": "Paccanīkamahaṁ maññe,",
  "ja496:18.4": "yo dātāraṁ nimantaye”.",
  "ja496:19.1": "“Atthāya vata me ajja,",
  "ja496:19.2": "idhāgacchi rathesabho;",
  "ja496:19.3": "Sohaṁ ajja pajānāmi,",
  "ja496:19.4": "yattha dinnaṁ mahapphalaṁ.",
  "ja496:20.1": "Raṭṭhesu giddhā rājāno,",
  "ja496:20.2": "Kiccākiccesu brāhmaṇā;",
  "ja496:20.3": "Isī mūlaphale giddhā,",
  "ja496:20.4": "Vippamuttā ca bhikkhavo”ti. ",
  "ja496:21.1": "Bhikkhāparamparajātakaṁ terasamaṁ.",
  "ja496:22.1": "Pakiṇṇakanipātaṁ niṭṭhitaṁ.",
  "ja496:23.1": "Tassuddānaṁ",
  "ja496:24.1": "Suva kinnara mukka kharājinaso,",
  "ja496:24.2": "Bhisajāta mahesi kapotavaro;",
  "ja496:24.3": "Atha mora satacchaka vāṇijako,",
  "ja496:24.4": "Atha rāja sabrāhmaṇa bhikkhaparanti."
}