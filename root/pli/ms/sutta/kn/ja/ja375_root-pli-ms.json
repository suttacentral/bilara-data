{
  "ja375:0.1": "Jātaka",
  "ja375:0.2": "Pañcakanipāta",
  "ja375:0.3": "Aḍḍhavagga",
  "ja375:0.4": "5. Kapotajātaka",
  "ja375:1.1": "“Idāni khomhi sukhito arogo,",
  "ja375:1.2": "Nikkaṇṭako nippatito kapoto;",
  "ja375:1.3": "Kāhāmi dānī hadayassa tuṭṭhiṁ,",
  "ja375:1.4": "Tathāhimaṁ maṁsasākaṁ baleti”.",
  "ja375:2.1": "“Kāyaṁ balākā sikhinī,",
  "ja375:2.2": "corī laṅghipitāmahā;",
  "ja375:2.3": "Oraṁ balāke āgaccha,",
  "ja375:2.4": "caṇḍo me vāyaso sakhā”.",
  "ja375:3.1": "“Alañhi te jagghitāye,",
  "ja375:3.2": "Mamaṁ disvāna edisaṁ;",
  "ja375:3.3": "Vilūnaṁ sūdaputtena,",
  "ja375:3.4": "Piṭṭhamaṇḍena makkhitaṁ”.",
  "ja375:4.1": "“Sunhāto suvilittosi,",
  "ja375:4.2": "annapānena tappito;",
  "ja375:4.3": "Kaṇṭhe ca te veḷuriyo,",
  "ja375:4.4": "agamā nu kajaṅgalaṁ”.",
  "ja375:5.1": "“Mā te mitto amitto vā,",
  "ja375:5.2": "agamāsi kajaṅgalaṁ;",
  "ja375:5.3": "Piñchāni tattha lāyitvā,",
  "ja375:5.4": "kaṇṭhe bandhanti vaṭṭanaṁ”.",
  "ja375:6.1": "“Punapāpajjasī samma,",
  "ja375:6.2": "Sīlañhi tava tādisaṁ;",
  "ja375:6.3": "Na hi mānusakā bhogā,",
  "ja375:6.4": "Subhuñjā honti pakkhinā”ti. ",
  "ja375:7.1": "Kapotajātakaṁ pañcamaṁ.",
  "ja375:7.2": "Aḍḍhavaggo tatiyo.",
  "ja375:8.1": "Tassuddānaṁ",
  "ja375:9.1": "Atha vaṇṇa sasīla hiri labhate,",
  "ja375:9.2": "Sumukhā visa sāḷiyamittavaro;",
  "ja375:9.3": "Atha cakka palāsa sarāja sato,",
  "ja375:9.4": "Yava bāla kapotaka pannarasāti.",
  "ja375:10.1": "Atha vagguddānaṁ",
  "ja375:11.1": "Jīnañca vaṇṇaṁ asamaṁvaguppari,",
  "ja375:11.2": "Sudesitā jātakanti santi vīsati;",
  "ja375:11.3": "Mahesino brahmacarittamutta-",
  "ja375:11.4": "Mavoca gāthā atthavatī subyañjanāti.",
  "ja375:12.1": "Pañcakanipātaṁ niṭṭhitaṁ."
}