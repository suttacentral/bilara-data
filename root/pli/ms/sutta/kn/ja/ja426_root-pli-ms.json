{
  "ja426:0.1": "Jātaka",
  "ja426:0.2": "Aṭṭhakanipāta",
  "ja426:0.3": "Kaccānivagga",
  "ja426:0.4": "10. Dīpijātaka",
  "ja426:1.1": "“Khamanīyaṁ yāpanīyaṁ,",
  "ja426:1.2": "kacci mātula te sukhaṁ;",
  "ja426:1.3": "Sukhaṁ te ammā avaca,",
  "ja426:1.4": "sukhakāmāva te mayaṁ”.",
  "ja426:2.1": "“Naṅguṭṭhaṁ me avakkamma,",
  "ja426:2.2": "Heṭhayitvāna eḷike;",
  "ja426:2.3": "Sājja mātulavādena,",
  "ja426:2.4": "Muñcitabbā nu maññasi”.",
  "ja426:3.1": "“Puratthāmukho nisinnosi,",
  "ja426:3.2": "Ahaṁ te mukhamāgatā;",
  "ja426:3.3": "Pacchato tuyhaṁ naṅguṭṭhaṁ,",
  "ja426:3.4": "Kathaṁ khvāhaṁ avakkamiṁ”.",
  "ja426:4.1": "“Yāvatā caturo dīpā,",
  "ja426:4.2": "sasamuddā sapabbatā;",
  "ja426:4.3": "Tāvatā mayhaṁ naṅguṭṭhaṁ,",
  "ja426:4.4": "kathaṁ kho tvaṁ vivajjayi”.",
  "ja426:5.1": "“Pubbeva metamakkhiṁsu,",
  "ja426:5.2": "mātā pitā ca bhātaro;",
  "ja426:5.3": "Dīghaṁ duṭṭhassa naṅguṭṭhaṁ,",
  "ja426:5.4": "sāmhi vehāyasāgatā”.",
  "ja426:6.1": "“Tañca disvāna āyantiṁ,",
  "ja426:6.2": "antalikkhasmi eḷike;",
  "ja426:6.3": "Migasaṅgho palāyittha,",
  "ja426:6.4": "bhakkho me nāsito tayā”.",
  "ja426:7.1": "“Iccevaṁ vilapantiyā,",
  "ja426:7.2": "eḷakiyā ruhagghaso;",
  "ja426:7.3": "Galakaṁ anvāvamaddi,",
  "ja426:7.4": "natthi duṭṭhe subhāsitaṁ.",
  "ja426:8.1": "Neva duṭṭhe nayo atthi,",
  "ja426:8.2": "Na dhammo na subhāsitaṁ;",
  "ja426:8.3": "Nikkamaṁ duṭṭhe yuñjetha,",
  "ja426:8.4": "So ca sabbhiṁ na rañjatī”ti. ",
  "ja426:9.1": "Dīpijātakaṁ dasamaṁ.",
  "ja426:10.1": "Aṭṭhakanipātaṁ niṭṭhitaṁ.",
  "ja426:11.1": "Tassuddānaṁ",
  "ja426:12.1": "Parisuddhā manāvilā vatthadharā,",
  "ja426:12.2": "Bakarājassa kāyuraṁ daṇḍavaro;",
  "ja426:12.3": "Atha aṅgāra cetiya devilinā,",
  "ja426:12.4": "Atha āditta gaṅgā daseḷakināti."
}