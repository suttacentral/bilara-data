{
  "ja532:0.1": "Jātaka",
  "ja532:0.2": "Sattatinipāta",
  "ja532:0.3": "Kusavagga",
  "ja532:0.4": "2. Soṇanandajātaka",
  "ja532:1.1": "“Devatā nusi gandhabbo,",
  "ja532:1.2": "adu sakko purindado;",
  "ja532:1.3": "Manussabhūto iddhimā,",
  "ja532:1.4": "kathaṁ jānemu taṁ mayaṁ”.",
  "ja532:2.1": "“Nāpi devo na gandhabbo,",
  "ja532:2.2": "nāpi sakko purindado;",
  "ja532:2.3": "Manussabhūto iddhimā,",
  "ja532:2.4": "evaṁ jānāhi bhāradha”.",
  "ja532:3.1": "“Katarūpamidaṁ bhoto,",
  "ja532:3.2": "veyyāvaccaṁ anappakaṁ;",
  "ja532:3.3": "Devamhi vassamānamhi,",
  "ja532:3.4": "anovassaṁ bhavaṁ akā.",
  "ja532:4.1": "Tato vātātape ghore,",
  "ja532:4.2": "sītacchāyaṁ bhavaṁ akā;",
  "ja532:4.3": "Tato amittamajjhesu,",
  "ja532:4.4": "saratāṇaṁ bhavaṁ akā.",
  "ja532:5.1": "Tato phītāni raṭṭhāni,",
  "ja532:5.2": "vasino te bhavaṁ akā;",
  "ja532:5.3": "Tato ekasataṁ khatye,",
  "ja532:5.4": "anuyante bhavaṁ akā.",
  "ja532:6.1": "Patītāssu mayaṁ bhoto,",
  "ja532:6.2": "Vada taṁ bhañjamicchasi;",
  "ja532:6.3": "Hatthiyānaṁ assarathaṁ,",
  "ja532:6.4": "Nāriyo ca alaṅkatā;",
  "ja532:6.5": "Nivesanāni rammāni,",
  "ja532:6.6": "Mayaṁ bhoto dadāmase.",
  "ja532:7.1": "Atha vaṅge vā magadhe,",
  "ja532:7.2": "mayaṁ bhoto dadāmase;",
  "ja532:7.3": "Atha vā assakāvantī,",
  "ja532:7.4": "sumanā damma te mayaṁ.",
  "ja532:8.1": "Upaḍḍhaṁ vāpi rajjassa,",
  "ja532:8.2": "mayaṁ bhoto dadāmase;",
  "ja532:8.3": "Sace te attho rajjena,",
  "ja532:8.4": "anusāsa yadicchasi”.",
  "ja532:9.1": "“Na me atthopi rajjena,",
  "ja532:9.2": "nagarena dhanena vā;",
  "ja532:9.3": "Athopi janapadena,",
  "ja532:9.4": "attho mayhaṁ na vijjati.",
  "ja532:10.1": "Bhotova raṭṭhe vijite,",
  "ja532:10.2": "araññe atthi assamo;",
  "ja532:10.3": "Pitā mayhaṁ janettī ca,",
  "ja532:10.4": "ubho sammanti assame.",
  "ja532:11.1": "Tesāhaṁ pubbācariyesu,",
  "ja532:11.2": "Puññaṁ na labhāmi kātave;",
  "ja532:11.3": "Bhavantaṁ ajjhāvaraṁ katvā,",
  "ja532:11.4": "Soṇaṁ yācemu saṁvaraṁ”.",
  "ja532:12.1": "“Karomi te taṁ vacanaṁ,",
  "ja532:12.2": "yaṁ maṁ bhaṇasi brāhmaṇa;",
  "ja532:12.3": "Etañca kho no akkhāhi,",
  "ja532:12.4": "kīvanto hontu yācakā”.",
  "ja532:13.1": "“Parosataṁ jānapadā,",
  "ja532:13.2": "mahāsālā ca brāhmaṇā;",
  "ja532:13.3": "Ime ca khattiyā sabbe,",
  "ja532:13.4": "abhijātā yasassino;",
  "ja532:13.5": "Bhavañca rājā manojo,",
  "ja532:13.6": "alaṁ hessanti yācakā”.",
  "ja532:14.1": "“Hatthī asse ca yojentu,",
  "ja532:14.2": "rathaṁ sannayha sārathi;",
  "ja532:14.3": "Ābandhanāni gaṇhātha,",
  "ja532:14.4": "pādāsussārayaddhaje;",
  "ja532:14.5": "Assamaṁ taṁ gamissāmi,",
  "ja532:14.6": "yattha sammati kosiyo”.",
  "ja532:15.1": "Tato ca rājā pāyāsi,",
  "ja532:15.2": "senāya caturaṅginī;",
  "ja532:15.3": "Agamā assamaṁ rammaṁ,",
  "ja532:15.4": "yattha sammati kosiyo.",
  "ja532:16.1": "“Kassa kādambayo kājo,",
  "ja532:16.2": "vehāsaṁ caturaṅgulaṁ;",
  "ja532:16.3": "Aṁsaṁ asamphusaṁ eti,",
  "ja532:16.4": "udahārāya gacchato”.",
  "ja532:17.1": "“Ahaṁ soṇo mahārāja,",
  "ja532:17.2": "tāpaso sahitabbato;",
  "ja532:17.3": "Bharāmi mātāpitaro,",
  "ja532:17.4": "rattindivamatandito.",
  "ja532:18.1": "Vane phalañca mūlañca,",
  "ja532:18.2": "āharitvā disampati;",
  "ja532:18.3": "Posemi mātāpitaro,",
  "ja532:18.4": "pubbe katamanussaraṁ”.",
  "ja532:19.1": "“Icchāma assamaṁ gantuṁ,",
  "ja532:19.2": "yattha sammati kosiyo;",
  "ja532:19.3": "Maggaṁ no soṇa akkhāhi,",
  "ja532:19.4": "yena gacchemu assamaṁ”.",
  "ja532:20.1": "“Ayaṁ ekapadī rāja,",
  "ja532:20.2": "yenetaṁ meghasannibhaṁ;",
  "ja532:20.3": "Koviḷārehi sañchannaṁ,",
  "ja532:20.4": "ettha sammati kosiyo”.",
  "ja532:21.1": "Idaṁ vatvāna pakkāmi,",
  "ja532:21.2": "taramāno mahāisi;",
  "ja532:21.3": "Vehāse antalikkhasmiṁ,",
  "ja532:21.4": "anusāsitvāna khattiye.",
  "ja532:22.1": "Assamaṁ parimajjitvā,",
  "ja532:22.2": "paññāpetvāna āsanaṁ;",
  "ja532:22.3": "Paṇṇasālaṁ pavisitvā,",
  "ja532:22.4": "pitaraṁ paṭibodhayi.",
  "ja532:23.1": "“Ime āyanti rājāno,",
  "ja532:23.2": "abhijātā yasassino;",
  "ja532:23.3": "Assamā nikkhamitvāna,",
  "ja532:23.4": "nisīda tvaṁ mahāise”.",
  "ja532:24.1": "Tassa taṁ vacanaṁ sutvā,",
  "ja532:24.2": "taramāno mahāisi;",
  "ja532:24.3": "Assamā nikkhamitvāna,",
  "ja532:24.4": "sadvāramhi upāvisi.",
  "ja532:25.1": "Tañca disvāna āyantaṁ,",
  "ja532:25.2": "jalantaṁriva tejasā;",
  "ja532:25.3": "Khatyasaṅghaparibyūḷhaṁ,",
  "ja532:25.4": "kosiyo etadabravi.",
  "ja532:26.1": "Kassa bherī mudiṅgā ca,",
  "ja532:26.2": "saṅkhā paṇavadindimā;",
  "ja532:26.3": "Purato paṭipannāni,",
  "ja532:26.4": "hāsayantā rathesabhaṁ.",
  "ja532:27.1": "Kassa kañcanapaṭṭena,",
  "ja532:27.2": "puthunā vijjuvaṇṇinā;",
  "ja532:27.3": "Yuvā kalāpasannaddho,",
  "ja532:27.4": "ko eti siriyā jalaṁ.",
  "ja532:28.1": "Ukkāmukhapahaṭṭhaṁva,",
  "ja532:28.2": "khadiraṅgārasannibhaṁ;",
  "ja532:28.3": "Mukhañca rucirā bhāti,",
  "ja532:28.4": "ko eti siriyā jalaṁ.",
  "ja532:29.1": "Kassa paggahitaṁ chattaṁ,",
  "ja532:29.2": "sasalākaṁ manoramaṁ;",
  "ja532:29.3": "Ādiccaraṁsāvaraṇaṁ,",
  "ja532:29.4": "ko eti siriyā jalaṁ.",
  "ja532:30.1": "Kassa aṅgaṁ pariggayha,",
  "ja532:30.2": "vāḷabījanimuttamaṁ;",
  "ja532:30.3": "Caranti varapuññassa,",
  "ja532:30.4": "hatthikkhandhena āyato.",
  "ja532:31.1": "Kassa setāni chattāni,",
  "ja532:31.2": "ājānīyā ca vammitā;",
  "ja532:31.3": "Samantā parikīrenti,",
  "ja532:31.4": "ko eti siriyā jalaṁ.",
  "ja532:32.1": "Kassa ekasataṁ khatyā,",
  "ja532:32.2": "anuyantā yasassino;",
  "ja532:32.3": "Samantānupariyanti,",
  "ja532:32.4": "ko eti siriyā jalaṁ.",
  "ja532:33.1": "Hatthi assaratha patti,",
  "ja532:33.2": "senā ca caturaṅginī;",
  "ja532:33.3": "Samantānupariyanti,",
  "ja532:33.4": "ko eti siriyā jalaṁ.",
  "ja532:34.1": "Kassesā mahatī senā,",
  "ja532:34.2": "piṭṭhito anuvattati;",
  "ja532:34.3": "Akkhobhaṇī apariyantā,",
  "ja532:34.4": "sāgarasseva ūmiyo.",
  "ja532:35.1": "Rājābhirājā manojo,",
  "ja532:35.2": "indova jayataṁ pati;",
  "ja532:35.3": "Nandassajjhāvaraṁ eti,",
  "ja532:35.4": "assamaṁ brahmacārinaṁ.",
  "ja532:36.1": "Tassesā mahatī senā,",
  "ja532:36.2": "piṭṭhito anuvattati;",
  "ja532:36.3": "Akkhobhaṇī apariyantā,",
  "ja532:36.4": "sāgarasseva ūmiyo.",
  "ja532:37.1": "Anulittā candanena,",
  "ja532:37.2": "kāsikuttamadhārino;",
  "ja532:37.3": "Sabbe pañjalikā hutvā,",
  "ja532:37.4": "isīnaṁ ajjhupāgamuṁ.",
  "ja532:38.1": "“Kacci nu bhoto kusalaṁ,",
  "ja532:38.2": "kacci bhoto anāmayaṁ;",
  "ja532:38.3": "Kacci uñchena yāpetha,",
  "ja532:38.4": "kacci mūlaphalā bahū.",
  "ja532:39.1": "Kacci ḍaṁsā makasā ca,",
  "ja532:39.2": "appameva sarīsapā;",
  "ja532:39.3": "Vane vāḷamigākiṇṇe,",
  "ja532:39.4": "kacci hiṁsā na vijjati”.",
  "ja532:40.1": "“Kusalañceva no rāja,",
  "ja532:40.2": "atho rāja anāmayaṁ;",
  "ja532:40.3": "Atho uñchena yāpema,",
  "ja532:40.4": "atho mūlaphalā bahū.",
  "ja532:41.1": "Atho ḍaṁsā makasā ca,",
  "ja532:41.2": "appameva sarīsapā;",
  "ja532:41.3": "Vane vāḷamigākiṇṇe,",
  "ja532:41.4": "hiṁsā mayhaṁ na vijjati.",
  "ja532:42.1": "Bahūni vassapūgāni,",
  "ja532:42.2": "assame sammataṁ idha;",
  "ja532:42.3": "Nābhijānāmi uppannaṁ,",
  "ja532:42.4": "ābādhaṁ amanoramaṁ.",
  "ja532:43.1": "Svāgataṁ te mahārāja,",
  "ja532:43.2": "atho te adurāgataṁ;",
  "ja532:43.3": "Issarosi anuppatto,",
  "ja532:43.4": "yaṁ idhatthi pavedaya.",
  "ja532:44.1": "Tindukāni piyālāni,",
  "ja532:44.2": "madhuke kāsumāriyo;",
  "ja532:44.3": "Phalāni khuddakappāni,",
  "ja532:44.4": "bhuñja rāja varaṁ varaṁ.",
  "ja532:45.1": "Idampi pānīyaṁ sītaṁ,",
  "ja532:45.2": "ābhataṁ girigabbharā;",
  "ja532:45.3": "Tato piva mahārāja,",
  "ja532:45.4": "sace tvaṁ abhikaṅkhasi”.",
  "ja532:46.1": "“Paṭiggahitaṁ yaṁ dinnaṁ,",
  "ja532:46.2": "sabbassa agghiyaṁ kataṁ;",
  "ja532:46.3": "Nandassāpi nisāmetha,",
  "ja532:46.4": "vacanaṁ so pavakkhati.",
  "ja532:47.1": "Ajjhāvaramhā nandassa,",
  "ja532:47.2": "bhoto santikamāgatā;",
  "ja532:47.3": "Suṇātu bhavaṁ vacanaṁ,",
  "ja532:47.4": "nandassa parisāya ca”.",
  "ja532:48.1": "“Parosataṁ jānapadā,",
  "ja532:48.2": "mahāsālā ca brāhmaṇā;",
  "ja532:48.3": "Ime ca khattiyā sabbe,",
  "ja532:48.4": "abhijātā yasassino;",
  "ja532:48.5": "Bhavañca rājā manojo,",
  "ja532:48.6": "anumaññantu me vaco.",
  "ja532:49.1": "Ye ca santi samītāro,",
  "ja532:49.2": "yakkhāni idha massame;",
  "ja532:49.3": "Araññe bhūtabhabyāni,",
  "ja532:49.4": "suṇantu vacanaṁ mama.",
  "ja532:50.1": "Namo katvāna bhūtānaṁ,",
  "ja532:50.2": "isiṁ vakkhāmi subbataṁ;",
  "ja532:50.3": "So tyāhaṁ dakkhiṇā bāhu,",
  "ja532:50.4": "tava kosiya sammato.",
  "ja532:51.1": "Pitaraṁ me janettiñca,",
  "ja532:51.2": "bhattukāmassa me sato;",
  "ja532:51.3": "Vīra puññamidaṁ ṭhānaṁ,",
  "ja532:51.4": "mā maṁ kosiya vāraya.",
  "ja532:52.1": "Sabbhi hetaṁ upaññātaṁ,",
  "ja532:52.2": "mametaṁ upanissaja;",
  "ja532:52.3": "Uṭṭhānapāricariyāya,",
  "ja532:52.4": "dīgharattaṁ tayā kataṁ;",
  "ja532:52.5": "Mātāpitūsu puññāni,",
  "ja532:52.6": "mama lokadado bhava.",
  "ja532:53.1": "Tatheva santi manujā,",
  "ja532:53.2": "dhamme dhammapadaṁ vidū;",
  "ja532:53.3": "Maggo saggassa lokassa,",
  "ja532:53.4": "yathā jānāsi tvaṁ ise.",
  "ja532:54.1": "Uṭṭhānapāricariyāya,",
  "ja532:54.2": "mātāpitusukhāvahaṁ;",
  "ja532:54.3": "Taṁ maṁ puññā nivāreti,",
  "ja532:54.4": "ariyamaggāvaro naro”.",
  "ja532:55.1": "“Suṇantu bhonto vacanaṁ,",
  "ja532:55.2": "bhāturajjhāvarā mama;",
  "ja532:55.3": "Kulavaṁsaṁ mahārāja,",
  "ja532:55.4": "porāṇaṁ parihāpayaṁ;",
  "ja532:55.5": "Adhammacārī jeṭṭhesu,",
  "ja532:55.6": "nirayaṁ sopapajjati.",
  "ja532:56.1": "Ye ca dhammassa kusalā,",
  "ja532:56.2": "porāṇassa disampati;",
  "ja532:56.3": "Cārittena ca sampannā,",
  "ja532:56.4": "na te gacchanti duggatiṁ.",
  "ja532:57.1": "Mātā pitā ca bhātā ca,",
  "ja532:57.2": "bhaginī ñātibandhavā;",
  "ja532:57.3": "Sabbe jeṭṭhassa te bhārā,",
  "ja532:57.4": "evaṁ jānāhi bhāradha.",
  "ja532:58.1": "Ādiyitvā garuṁ bhāraṁ,",
  "ja532:58.2": "nāviko viya ussahe;",
  "ja532:58.3": "Dhammañca nappamajjāmi,",
  "ja532:58.4": "jeṭṭho casmi rathesabha”.",
  "ja532:59.1": "“Adhigamā tame ñāṇaṁ,",
  "ja532:59.2": "Jālaṁva jātavedato;",
  "ja532:59.3": "Evameva no bhavaṁ dhammaṁ,",
  "ja532:59.4": "Kosiyo pavidaṁsayi.",
  "ja532:60.1": "Yathā udayamādicco,",
  "ja532:60.2": "Vāsudevo pabhaṅkaro;",
  "ja532:60.3": "Pāṇīnaṁ pavidaṁseti,",
  "ja532:60.4": "Rūpaṁ kalyāṇapāpakaṁ;",
  "ja532:60.5": "Evameva no bhavaṁ dhammaṁ,",
  "ja532:60.6": "Kosiyo pavidaṁsayi”.",
  "ja532:61.1": "“Evaṁ me yācamānassa,",
  "ja532:61.2": "añjaliṁ nāvabujjhatha;",
  "ja532:61.3": "Tava paddhacaro hessaṁ,",
  "ja532:61.4": "vuṭṭhito paricārako”.",
  "ja532:62.1": "“Addhā nanda vijānāsi,",
  "ja532:62.2": "Saddhammaṁ sabbhi desitaṁ;",
  "ja532:62.3": "Ariyo ariyasamācāro,",
  "ja532:62.4": "Bāḷhaṁ tvaṁ mama ruccasi.",
  "ja532:63.1": "Bhavantaṁ vadāmi bhotiñca,",
  "ja532:63.2": "suṇātha vacanaṁ mama;",
  "ja532:63.3": "Nāyaṁ bhāro bhāramato,",
  "ja532:63.4": "ahu mayhaṁ kudācanaṁ.",
  "ja532:64.1": "Taṁ maṁ upaṭṭhitaṁ santaṁ,",
  "ja532:64.2": "mātāpitusukhāvahaṁ;",
  "ja532:64.3": "Nando ajjhāvaraṁ katvā,",
  "ja532:64.4": "upaṭṭhānāya yācati.",
  "ja532:65.1": "Yo ve icchati kāmena,",
  "ja532:65.2": "santānaṁ brahmacārinaṁ;",
  "ja532:65.3": "Nandaṁ vo varatha eko,",
  "ja532:65.4": "kaṁ nando upatiṭṭhatu”.",
  "ja532:66.1": "“Tayā tāta anuññātā,",
  "ja532:66.2": "soṇa taṁ nissitā mayaṁ;",
  "ja532:66.3": "Upaghātuṁ labhe nandaṁ,",
  "ja532:66.4": "muddhani brahmacārinaṁ.",
  "ja532:67.1": "Assatthasseva taruṇaṁ,",
  "ja532:67.2": "pavāḷaṁ māluteritaṁ;",
  "ja532:67.3": "Cirassaṁ nandaṁ disvāna,",
  "ja532:67.4": "hadayaṁ me pavedhati.",
  "ja532:68.1": "Yadā suttāpi supine,",
  "ja532:68.2": "nandaṁ passāmi āgataṁ;",
  "ja532:68.3": "Udaggā sumanā homi,",
  "ja532:68.4": "nando no āgato ayaṁ.",
  "ja532:69.1": "Yadā ca paṭibujjhitvā,",
  "ja532:69.2": "nandaṁ passāmi nāgataṁ;",
  "ja532:69.3": "Bhiyyo āvisatī soko,",
  "ja532:69.4": "domanassañcanappakaṁ.",
  "ja532:70.1": "Sāhaṁ ajja cirassampi,",
  "ja532:70.2": "nandaṁ passāmi āgataṁ;",
  "ja532:70.3": "Bhattucca mayhañca piyo,",
  "ja532:70.4": "nando no pāvisī gharaṁ.",
  "ja532:71.1": "Pitupi nando suppiyo,",
  "ja532:71.2": "Yaṁ nando nappavase gharā;",
  "ja532:71.3": "Labhatū tāta nando taṁ,",
  "ja532:71.4": "Maṁ nando upatiṭṭhatu”.",
  "ja532:72.1": "“Anukampikā patiṭṭhā ca,",
  "ja532:72.2": "pubbe rasadadī ca no;",
  "ja532:72.3": "Maggo saggassa lokassa,",
  "ja532:72.4": "mātā taṁ varate ise.",
  "ja532:73.1": "Pubbe rasadadī gottī,",
  "ja532:73.2": "mātā puññūpasaṁhitā;",
  "ja532:73.3": "Maggo saggassa lokassa,",
  "ja532:73.4": "mātā taṁ varate ise”.",
  "ja532:74.1": "“Ākaṅkhamānā puttaphalaṁ,",
  "ja532:74.2": "devatāya namassati;",
  "ja532:74.3": "Nakkhattāni ca pucchati,",
  "ja532:74.4": "utusaṁvaccharāni ca.",
  "ja532:75.1": "Tassā utumhi nhātāya,",
  "ja532:75.2": "hoti gabbhassa vokkamo;",
  "ja532:75.3": "Tena dohaḷinī hoti,",
  "ja532:75.4": "suhadā tena vuccati.",
  "ja532:76.1": "Saṁvaccharaṁ vā ūnaṁ vā,",
  "ja532:76.2": "pariharitvā vijāyati;",
  "ja532:76.3": "Tena sā janayantīti,",
  "ja532:76.4": "janetti tena vuccati.",
  "ja532:77.1": "Thanakhīrena gītena,",
  "ja532:77.2": "aṅgapāvuraṇena ca;",
  "ja532:77.3": "Rodantaṁ puttaṁ toseti,",
  "ja532:77.4": "tosentī tena vuccati.",
  "ja532:78.1": "Tato vātātape ghore,",
  "ja532:78.2": "mamaṁ katvā udikkhati;",
  "ja532:78.3": "Dārakaṁ appajānantaṁ,",
  "ja532:78.4": "posentī tena vuccati.",
  "ja532:79.1": "Yañca mātudhanaṁ hoti,",
  "ja532:79.2": "yañca hoti pituddhanaṁ;",
  "ja532:79.3": "Ubhayampetassa gopeti,",
  "ja532:79.4": "api puttassa no siyā.",
  "ja532:80.1": "Evaṁ putta aduṁ putta,",
  "ja532:80.2": "iti mātā vihaññati;",
  "ja532:80.3": "Pamattaṁ paradāresu,",
  "ja532:80.4": "nisīthe pattayobbane;",
  "ja532:80.5": "Sāyaṁ puttaṁ anāyantaṁ,",
  "ja532:80.6": "iti mātā vihaññati.",
  "ja532:81.1": "Evaṁ kicchā bhato poso,",
  "ja532:81.2": "mātu aparicārako;",
  "ja532:81.3": "Mātari micchā caritvāna,",
  "ja532:81.4": "nirayaṁ sopapajjati.",
  "ja532:82.1": "Evaṁ kicchā bhato poso,",
  "ja532:82.2": "pitu aparicārako;",
  "ja532:82.3": "Pitari micchā caritvāna,",
  "ja532:82.4": "nirayaṁ sopapajjati.",
  "ja532:83.1": "Dhanāpi dhanakāmānaṁ,",
  "ja532:83.2": "Nassati iti me sutaṁ;",
  "ja532:83.3": "Mātaraṁ aparicaritvāna,",
  "ja532:83.4": "Kicchaṁ vā so nigacchati.",
  "ja532:84.1": "Dhanāpi dhanakāmānaṁ,",
  "ja532:84.2": "Nassati iti me sutaṁ;",
  "ja532:84.3": "Pitaraṁ aparicaritvāna,",
  "ja532:84.4": "Kicchaṁ vā so nigacchati.",
  "ja532:85.1": "Ānando ca pamodo ca,",
  "ja532:85.2": "sadā hasitakīḷitaṁ;",
  "ja532:85.3": "Mātaraṁ paricaritvāna,",
  "ja532:85.4": "labbhametaṁ vijānato.",
  "ja532:86.1": "Ānando ca pamodo ca,",
  "ja532:86.2": "sadā hasitakīḷitaṁ;",
  "ja532:86.3": "Pitaraṁ paricaritvāna,",
  "ja532:86.4": "labbhametaṁ vijānato.",
  "ja532:87.1": "Dānañca piyavācā ca,",
  "ja532:87.2": "atthacariyā ca yā idha;",
  "ja532:87.3": "Samānattatā ca dhammesu,",
  "ja532:87.4": "tattha tattha yathārahaṁ;",
  "ja532:87.5": "Ete kho saṅgahā loke,",
  "ja532:87.6": "rathassāṇīva yāyato.",
  "ja532:88.1": "Ete ca saṅgahā nāssu,",
  "ja532:88.2": "na mātā puttakāraṇā;",
  "ja532:88.3": "Labhetha mānaṁ pūjaṁ vā,",
  "ja532:88.4": "pitā vā puttakāraṇā.",
  "ja532:89.1": "Yasmā ca saṅgahā ete,",
  "ja532:89.2": "sammapekkhanti paṇḍitā;",
  "ja532:89.3": "Tasmā mahattaṁ papponti,",
  "ja532:89.4": "pāsaṁsā ca bhavanti te.",
  "ja532:90.1": "Brahmāti mātāpitaro,",
  "ja532:90.2": "pubbācariyāti vuccare;",
  "ja532:90.3": "Āhuneyyā ca puttānaṁ,",
  "ja532:90.4": "pajāya anukampakā.",
  "ja532:91.1": "Tasmā hi ne namasseyya,",
  "ja532:91.2": "sakkareyya ca paṇḍito;",
  "ja532:91.3": "Annena atho pānena,",
  "ja532:91.4": "vatthena sayanena ca;",
  "ja532:91.5": "Ucchādanena nhāpanena,",
  "ja532:91.6": "pādānaṁ dhovanena ca.",
  "ja532:92.1": "Tāya naṁ pāricariyāya,",
  "ja532:92.2": "Mātāpitūsu paṇḍitā;",
  "ja532:92.3": "Idheva naṁ pasaṁsanti,",
  "ja532:92.4": "Pecca sagge pamodatī”ti. ",
  "ja532:93.1": "Soṇanandajātakaṁ dutiyaṁ.",
  "ja532:94.1": "Sattatinipātaṁ niṭṭhitaṁ.",
  "ja532:95.1": "Tassuddānaṁ",
  "ja532:96.1": "Atha sattatimamhi nipātavare,",
  "ja532:96.2": "Sabhāvantu kusāvatirājavaro;",
  "ja532:96.3": "Atha soṇasunandavaro ca puna,",
  "ja532:96.4": "Abhivāsitasattatimamhi suteti."
}