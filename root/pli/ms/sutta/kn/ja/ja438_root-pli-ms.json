{
  "ja438:0.1": "Jātaka",
  "ja438:0.2": "Navakanipāta",
  "ja438:0.3": "Gijjhavagga",
  "ja438:0.4": "12. Daddarajātaka",
  "ja438:1.1": "“Yo te puttake akhādi,",
  "ja438:1.2": "dinnabhatto adūsake;",
  "ja438:1.3": "Tasmiṁ dāṭhaṁ nipātehi,",
  "ja438:1.4": "mā te muccittha jīvato”.",
  "ja438:2.1": "“Ākiṇṇaluddo puriso,",
  "ja438:2.2": "dhāticelaṁva makkhito;",
  "ja438:2.3": "Padesaṁ taṁ na passāmi,",
  "ja438:2.4": "yattha dāṭhaṁ nipātaye.",
  "ja438:3.1": "Akataññussa posassa,",
  "ja438:3.2": "niccaṁ vivaradassino;",
  "ja438:3.3": "Sabbañce pathaviṁ dajjā,",
  "ja438:3.4": "neva naṁ abhirādhaye”.",
  "ja438:4.1": "“Kiṁ nu subāhu taramānarūpo,",
  "ja438:4.2": "Paccāgatosi saha māṇavena;",
  "ja438:4.3": "Kiṁ kiccamatthaṁ idhamatthi tuyhaṁ,",
  "ja438:4.4": "Akkhāhi me pucchito etamatthaṁ”.",
  "ja438:5.1": "“Yo te sakhā daddaro sādhurūpo,",
  "ja438:5.2": "Tassa vadhaṁ parisaṅkāmi ajja;",
  "ja438:5.3": "Purisassa kammāyatanāni sutvā,",
  "ja438:5.4": "Nāhaṁ sukhiṁ daddaraṁ ajja maññe”.",
  "ja438:6.1": "“Kānissa kammāyatanāni assu,",
  "ja438:6.2": "Purisassa vuttisamodhānatāya;",
  "ja438:6.3": "Kaṁ vā paṭiññaṁ purisassa sutvā,",
  "ja438:6.4": "Parisaṅkasi daddaraṁ māṇavena”.",
  "ja438:7.1": "“Ciṇṇā kaliṅgā caritā vaṇijjā,",
  "ja438:7.2": "Vettācaro saṅkupathopi ciṇṇo;",
  "ja438:7.3": "Naṭehi ciṇṇaṁ saha vākurehi,",
  "ja438:7.4": "Daṇḍena yuddhampi samajjamajjhe.",
  "ja438:8.1": "Baddhā kulīkā mitamāḷhakena,",
  "ja438:8.2": "Akkhā jitā saṁyamo abbhatīto;",
  "ja438:8.3": "Abbāhitaṁ pubbakaṁ aḍḍharattaṁ,",
  "ja438:8.4": "Hatthā daḍḍhā piṇḍapaṭiggahena.",
  "ja438:9.1": "Tānissa kammāyatanāni assu,",
  "ja438:9.2": "Purisassa vuttisamodhānatāya;",
  "ja438:9.3": "Yathā ayaṁ dissati lomapiṇḍo,",
  "ja438:9.4": "Gāvo hatā kiṁ pana daddarassā”ti. ",
  "ja438:10.1": "Daddarajātakaṁ dvādasamaṁ.",
  "ja438:11.1": "Navakanipātaṁ niṭṭhitaṁ.",
  "ja438:12.1": "Tassuddānaṁ",
  "ja438:13.1": "Varagijjha samajjana haṁsavaro,",
  "ja438:13.2": "Nidhisavhaya hārita pāṭaliko;",
  "ja438:13.3": "Ajarāmara dhaṅka titikkha kuto,",
  "ja438:13.4": "Atha dvādasa pekkhana daddaribhīti."
}