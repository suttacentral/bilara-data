{
  "thag1.106:0.1": "Theragāthā 1.106",
  "thag1.106:0.2": "Ekakanipāta",
  "thag1.106:0.3": "Ekādasamavagga",
  "thag1.106:0.4": "Suhemantattheragāthā",
  "thag1.106:1.1": "“Sataliṅgassa atthassa,",
  "thag1.106:1.2": "satalakkhaṇadhārino;",
  "thag1.106:1.3": "Ekaṅgadassī dummedho,",
  "thag1.106:1.4": "satadassī ca paṇḍito”ti.",
  "thag1.106:1.5": "… Suhemanto thero …."
}