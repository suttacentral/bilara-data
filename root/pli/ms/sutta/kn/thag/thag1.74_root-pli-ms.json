{
  "thag1.74:0.1": "Theragāthā 1.74",
  "thag1.74:0.2": "Ekakanipāta",
  "thag1.74:0.3": "Aṭṭhamavagga",
  "thag1.74:0.4": "Suyāmanattheragāthā",
  "thag1.74:1.1": "“Kāmacchando ca byāpādo,",
  "thag1.74:1.2": "Thinamiddhañca bhikkhuno;",
  "thag1.74:1.3": "Uddhaccaṁ vicikicchā ca,",
  "thag1.74:1.4": "Sabbasova na vijjatī”ti.",
  "thag1.74:1.5": "… Suyāmano thero …."
}