{
  "thag1.102:0.1": "Theragāthā 1.102",
  "thag1.102:0.2": "Ekakanipāta",
  "thag1.102:0.3": "Ekādasamavagga",
  "thag1.102:0.4": "Setucchattheragāthā",
  "thag1.102:1.1": "“Mānena vañcitāse,",
  "thag1.102:1.2": "Saṅkhāresu saṅkilissamānāse;",
  "thag1.102:1.3": "Lābhālābhena mathitā,",
  "thag1.102:1.4": "Samādhiṁ nādhigacchantī”ti.",
  "thag1.102:1.5": "… Setuccho thero …."
}