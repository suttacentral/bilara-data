{
  "thag2.43:0.1": "Theragāthā 2.43",
  "thag2.43:0.2": "Dukanipāta",
  "thag2.43:0.3": "Pañcamavagga",
  "thag2.43:0.4": "Brahmālittheragāthā",
  "thag2.43:1.1": "“Kassindriyāni samathaṅgatāni,",
  "thag2.43:1.2": "Assā yathā sārathinā sudantā;",
  "thag2.43:1.3": "Pahīnamānassa anāsavassa,",
  "thag2.43:1.4": "Devāpi kassa pihayanti tādinoti.",
  "thag2.43:2.1": "Mayhindriyāni samathaṅgatāni,",
  "thag2.43:2.2": "Assā yathā sārathinā sudantā;",
  "thag2.43:2.3": "Pahīnamānassa anāsavassa,",
  "thag2.43:2.4": "Devāpi mayhaṁ pihayanti tādino”ti.",
  "thag2.43:2.5": "… Brahmāli thero …."
}