{
  "thag2.23:0.1": "Theragāthā 2.23",
  "thag2.23:0.2": "Dukanipāta",
  "thag2.23:0.3": "Tatiyavagga",
  "thag2.23:0.4": "Sobhitattheragāthā",
  "thag2.23:1.1": "“Satimā paññavā bhikkhu,",
  "thag2.23:1.2": "āraddhabalavīriyo;",
  "thag2.23:1.3": "Pañca kappasatānāhaṁ,",
  "thag2.23:1.4": "ekarattiṁ anussariṁ.",
  "thag2.23:2.1": "Cattāro satipaṭṭhāne,",
  "thag2.23:2.2": "satta aṭṭha ca bhāvayaṁ;",
  "thag2.23:2.3": "Pañca kappasatānāhaṁ,",
  "thag2.23:2.4": "ekarattiṁ anussarin”ti.",
  "thag2.23:2.5": "… Sobhito thero …."
}