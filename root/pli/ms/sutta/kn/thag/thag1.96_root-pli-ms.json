{
  "thag1.96:0.1": "Theragāthā 1.96",
  "thag1.96:0.2": "Ekakanipāta",
  "thag1.96:0.3": "Dasamavagga",
  "thag1.96:0.4": "Khaṇḍasumanattheragāthā",
  "thag1.96:1.1": "“Ekapupphaṁ cajitvāna,",
  "thag1.96:1.2": "asīti vassakoṭiyo;",
  "thag1.96:1.3": "Saggesu paricāretvā,",
  "thag1.96:1.4": "sesakenamhi nibbuto”ti.",
  "thag1.96:1.5": "… Khaṇḍasumano thero …."
}