{
  "thag2.6:0.1": "Theragāthā 2.6",
  "thag2.6:0.2": "Dukanipāta",
  "thag2.6:0.3": "Paṭhamavagga",
  "thag2.6:0.4": "Meḷajinattheragāthā",
  "thag2.6:1.1": "“Yadāhaṁ dhammamassosiṁ,",
  "thag2.6:1.2": "bhāsamānassa satthuno;",
  "thag2.6:1.3": "Na kaṅkhamabhijānāmi,",
  "thag2.6:1.4": "sabbaññūaparājite.",
  "thag2.6:2.1": "Satthavāhe mahāvīre,",
  "thag2.6:2.2": "sārathīnaṁ varuttame;",
  "thag2.6:2.3": "Magge paṭipadāyaṁ vā,",
  "thag2.6:2.4": "kaṅkhā mayhaṁ na vijjatī”ti.",
  "thag2.6:2.5": "… Meḷajino thero …."
}