{
  "thag1.107:0.1": "Theragāthā 1.107",
  "thag1.107:0.2": "Ekakanipāta",
  "thag1.107:0.3": "Ekādasamavagga",
  "thag1.107:0.4": "Dhammasavattheragāthā",
  "thag1.107:1.1": "“Pabbajiṁ tulayitvāna,",
  "thag1.107:1.2": "agārasmānagāriyaṁ;",
  "thag1.107:1.3": "Tisso vijjā anuppattā,",
  "thag1.107:1.4": "kataṁ buddhassa sāsanan”ti.",
  "thag1.107:1.5": "… Dhammasavo thero …."
}