{
  "thag1.36:0.1": "Theragāthā 1.36",
  "thag1.36:0.2": "Ekakanipāta",
  "thag1.36:0.3": "Catutthavagga",
  "thag1.36:0.4": "Kumāputtattheragāthā",
  "thag1.36:1.1": "“Sādhu sutaṁ sādhu caritakaṁ,",
  "thag1.36:1.2": "Sādhu sadā aniketavihāro;",
  "thag1.36:1.3": "Atthapucchanaṁ padakkhiṇakammaṁ,",
  "thag1.36:1.4": "Etaṁ sāmaññamakiñcanassā”ti.",
  "thag1.36:1.5": "… Kumāputto thero …."
}