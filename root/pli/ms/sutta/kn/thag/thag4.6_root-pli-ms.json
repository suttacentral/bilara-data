{
  "thag4.6:0.1": "Theragāthā 4.6",
  "thag4.6:0.2": "Catukkanipāta",
  "thag4.6:0.3": "Paṭhamavagga",
  "thag4.6:0.4": "Senakattheragāthā",
  "thag4.6:1.1": "“Svāgataṁ vata me āsi,",
  "thag4.6:1.2": "gayāyaṁ gayaphagguyā;",
  "thag4.6:1.3": "Yaṁ addasāsiṁ sambuddhaṁ,",
  "thag4.6:1.4": "desentaṁ dhammamuttamaṁ.",
  "thag4.6:2.1": "Mahappabhaṁ gaṇācariyaṁ,",
  "thag4.6:2.2": "aggapattaṁ vināyakaṁ;",
  "thag4.6:2.3": "Sadevakassa lokassa,",
  "thag4.6:2.4": "jinaṁ atuladassanaṁ.",
  "thag4.6:3.1": "Mahānāgaṁ mahāvīraṁ,",
  "thag4.6:3.2": "mahājutimanāsavaṁ;",
  "thag4.6:3.3": "Sabbāsavaparikkhīṇaṁ,",
  "thag4.6:3.4": "satthāramakutobhayaṁ.",
  "thag4.6:4.1": "Cirasaṅkiliṭṭhaṁ vata maṁ,",
  "thag4.6:4.2": "diṭṭhisandānabandhitaṁ;",
  "thag4.6:4.3": "Vimocayi so bhagavā,",
  "thag4.6:4.4": "sabbaganthehi senakan”ti.",
  "thag4.6:4.5": "… Senako thero …."
}