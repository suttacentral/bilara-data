{
  "thag1.23:0.1": "Theragāthā 1.23",
  "thag1.23:0.2": "Ekakanipāta",
  "thag1.23:0.3": "Tatiyavagga",
  "thag1.23:0.4": "Gosālattheragāthā",
  "thag1.23:1.1": "“Ahaṁ kho veḷugumbasmiṁ,",
  "thag1.23:1.2": "bhutvāna madhupāyasaṁ;",
  "thag1.23:1.3": "Padakkhiṇaṁ sammasanto,",
  "thag1.23:1.4": "khandhānaṁ udayabbayaṁ;",
  "thag1.23:1.5": "Sānuṁ paṭigamissāmi,",
  "thag1.23:1.6": "vivekamanubrūhayan”ti.",
  "thag1.23:1.7": "… Gosālo thero …."
}