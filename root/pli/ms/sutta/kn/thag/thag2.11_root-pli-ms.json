{
  "thag2.11:0.1": "Theragāthā 2.11",
  "thag2.11:0.2": "Dukanipāta",
  "thag2.11:0.3": "Dutiyavagga",
  "thag2.11:0.4": "Mahācundattheragāthā",
  "thag2.11:1.1": "“Sussūsā sutavaddhanī,",
  "thag2.11:1.2": "sutaṁ paññāya vaddhanaṁ;",
  "thag2.11:1.3": "Paññāya atthaṁ jānāti,",
  "thag2.11:1.4": "ñāto attho sukhāvaho.",
  "thag2.11:2.1": "Sevetha pantāni senāsanāni,",
  "thag2.11:2.2": "Careyya saṁyojanavippamokkhaṁ;",
  "thag2.11:2.3": "Sace ratiṁ nādhigaccheyya tattha,",
  "thag2.11:2.4": "Saṅghe vase rakkhitatto satimā”ti.",
  "thag2.11:2.5": "… Mahācundo thero …."
}