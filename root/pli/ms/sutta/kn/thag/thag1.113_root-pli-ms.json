{
  "thag1.113:0.1": "Theragāthā 1.113",
  "thag1.113:0.2": "Ekakanipāta",
  "thag1.113:0.3": "Dvādasamavagga",
  "thag1.113:0.4": "Vanavacchattheragāthā",
  "thag1.113:1.1": "“Acchodikā puthusilā,",
  "thag1.113:1.2": "gonaṅgulamigāyutā;",
  "thag1.113:1.3": "Ambusevālasañchannā,",
  "thag1.113:1.4": "te selā ramayanti man”ti.",
  "thag1.113:1.5": "… Vanavaccho thero …."
}