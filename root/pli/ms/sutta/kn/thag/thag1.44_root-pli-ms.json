{
  "thag1.44:0.1": "Theragāthā 1.44",
  "thag1.44:0.2": "Ekakanipāta",
  "thag1.44:0.3": "Pañcamavagga",
  "thag1.44:0.4": "Sānuttheragāthā",
  "thag1.44:1.1": "“Mataṁ vā amma rodanti,",
  "thag1.44:1.2": "Yo vā jīvaṁ na dissati;",
  "thag1.44:1.3": "Jīvantaṁ maṁ amma passantī,",
  "thag1.44:1.4": "Kasmā maṁ amma rodasī”ti.",
  "thag1.44:1.5": "… Sānutthero …."
}