{
  "thag1.35:0.1": "Theragāthā 1.35",
  "thag1.35:0.2": "Ekakanipāta",
  "thag1.35:0.3": "Catutthavagga",
  "thag1.35:0.4": "Sāmaññakānittheragāthā",
  "thag1.35:1.1": "“Sukhaṁ sukhattho labhate tadācaraṁ,",
  "thag1.35:1.2": "Kittiñca pappoti yasassa vaḍḍhati;",
  "thag1.35:1.3": "Yo ariyamaṭṭhaṅgikamañjasaṁ ujuṁ,",
  "thag1.35:1.4": "Bhāveti maggaṁ amatassa pattiyā”ti.",
  "thag1.35:1.5": "… Sāmaññakānitthero …."
}