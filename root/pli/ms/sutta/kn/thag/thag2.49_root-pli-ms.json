{
  "thag2.49:0.1": "Theragāthā 2.49 ",
  "thag2.49:0.2": "Dukanipāta ",
  "thag2.49:0.3": "Pañcamavagga ",
  "thag2.49:0.4": "Sandhitattheragāthā ",
  "thag2.49:1.1": "“Assatthe haritobhāse, ",
  "thag2.49:1.2": "saṁvirūḷhamhi pādape; ",
  "thag2.49:1.3": "Ekaṁ buddhagataṁ saññaṁ, ",
  "thag2.49:1.4": "alabhitthaṁ patissato. ",
  "thag2.49:2.1": "Ekatiṁse ito kappe, ",
  "thag2.49:2.2": "yaṁ saññamalabhiṁ tadā; ",
  "thag2.49:2.3": "Tassā saññāya vāhasā, ",
  "thag2.49:2.4": "patto me āsavakkhayo”ti. ",
  "thag2.49:2.5": "… Sandhito thero … ",
  "thag2.49:2.6": "vaggo pañcamo. ",
  "thag2.49:3.0": "Tassuddānaṁ ",
  "thag2.49:3.1": "Kumārakassapo thero, ",
  "thag2.49:3.2": "dhammapālo ca brahmāli; ",
  "thag2.49:3.3": "Mogharājā visākho ca, ",
  "thag2.49:3.4": "cūḷako ca anūpamo; ",
  "thag2.49:3.5": "Vajjito sandhito thero, ",
  "thag2.49:3.6": "kilesarajavāhanoti. ",
  "thag2.49:3.7": "Dukanipāto niṭṭhito. ",
  "thag2.49:4.0": "Tatruddānaṁ ",
  "thag2.49:4.1": "Gāthādukanipātamhi, ",
  "thag2.49:4.2": "navuti ceva aṭṭha ca; ",
  "thag2.49:4.3": "Therā ekūnapaññāsaṁ, ",
  "thag2.49:4.4": "bhāsitā nayakovidāti. "
}