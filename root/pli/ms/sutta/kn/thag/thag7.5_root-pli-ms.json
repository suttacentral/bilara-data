{
  "thag7.5:0.1": "Theragāthā 7.5 ",
  "thag7.5:0.2": "Sattakanipāta ",
  "thag7.5:0.3": "Paṭhamavagga ",
  "thag7.5:0.4": "Sarabhaṅgattheragāthā ",
  "thag7.5:1.1": "“Sare hatthehi bhañjitvā, ",
  "thag7.5:1.2": "katvāna kuṭimacchisaṁ; ",
  "thag7.5:1.3": "Tena me sarabhaṅgoti, ",
  "thag7.5:1.4": "nāmaṁ sammutiyā ahu. ",
  "thag7.5:2.1": "Na mayhaṁ kappate ajja, ",
  "thag7.5:2.2": "sare hatthehi bhañjituṁ; ",
  "thag7.5:2.3": "Sikkhāpadā no paññattā, ",
  "thag7.5:2.4": "gotamena yasassinā. ",
  "thag7.5:3.1": "Sakalaṁ samattaṁ rogaṁ, ",
  "thag7.5:3.2": "Sarabhaṅgo nāddasaṁ pubbe; ",
  "thag7.5:3.3": "Soyaṁ rogo diṭṭho, ",
  "thag7.5:3.4": "Vacanakarenātidevassa. ",
  "thag7.5:4.1": "Yeneva maggena gato vipassī, ",
  "thag7.5:4.2": "Yeneva maggena sikhī ca vessabhū; ",
  "thag7.5:4.3": "Kakusandhakoṇāgamano ca kassapo, ",
  "thag7.5:4.4": "Tenañjasena agamāsi gotamo. ",
  "thag7.5:5.1": "Vītataṇhā anādānā, ",
  "thag7.5:5.2": "satta buddhā khayogadhā; ",
  "thag7.5:5.3": "Yehāyaṁ desito dhammo, ",
  "thag7.5:5.4": "dhammabhūtehi tādibhi. ",
  "thag7.5:6.1": "Cattāri ariyasaccāni, ",
  "thag7.5:6.2": "anukampāya pāṇinaṁ; ",
  "thag7.5:6.3": "Dukkhaṁ samudayo maggo, ",
  "thag7.5:6.4": "nirodho dukkhasaṅkhayo. ",
  "thag7.5:7.1": "Yasmiṁ nivattate dukkhaṁ, ",
  "thag7.5:7.2": "saṁsārasmiṁ anantakaṁ; ",
  "thag7.5:7.3": "Bhedā imassa kāyassa, ",
  "thag7.5:7.4": "jīvitassa ca saṅkhayā; ",
  "thag7.5:7.5": "Añño punabbhavo natthi, ",
  "thag7.5:7.6": "suvimuttomhi sabbadhī”ti. ",
  "thag7.5:7.7": "… Sarabhaṅgo thero … ",
  "thag7.5:7.8": "Sattakanipāto niṭṭhito. ",
  "thag7.5:8.0": "Tatruddānaṁ ",
  "thag7.5:8.1": "Sundarasamuddo thero, ",
  "thag7.5:8.2": "thero lakuṇḍabhaddiyo; ",
  "thag7.5:8.3": "Bhaddo thero ca sopāko, ",
  "thag7.5:8.4": "sarabhaṅgo mahāisi; ",
  "thag7.5:8.5": "Sattake pañcakā therā, ",
  "thag7.5:8.6": "gāthāyo pañcatiṁsatīti. "
}