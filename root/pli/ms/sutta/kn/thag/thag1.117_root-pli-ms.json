{
  "thag1.117:0.1": "Theragāthā 1.117",
  "thag1.117:0.2": "Ekakanipāta",
  "thag1.117:0.3": "Dvādasamavagga",
  "thag1.117:0.4": "Yasattheragāthā",
  "thag1.117:1.1": "“Suvilitto suvasano,",
  "thag1.117:1.2": "sabbābharaṇabhūsito;",
  "thag1.117:1.3": "Tisso vijjā ajjhagamiṁ,",
  "thag1.117:1.4": "kataṁ buddhassa sāsanan”ti.",
  "thag1.117:1.5": "… Yaso thero …."
}