{
  "thag2.39:0.1": "Theragāthā 2.39",
  "thag2.39:0.2": "Dukanipāta",
  "thag2.39:0.3": "Catutthavagga",
  "thag2.39:0.4": "Usabhattheragāthā",
  "thag2.39:1.1": "“Ambapallavasaṅkāsaṁ,",
  "thag2.39:1.2": "aṁse katvāna cīvaraṁ;",
  "thag2.39:1.3": "Nisinno hatthigīvāyaṁ,",
  "thag2.39:1.4": "gāmaṁ piṇḍāya pāvisiṁ.",
  "thag2.39:2.1": "Hatthikkhandhato oruyha,",
  "thag2.39:2.2": "saṁvegaṁ alabhiṁ tadā;",
  "thag2.39:2.3": "Sohaṁ ditto tadā santo,",
  "thag2.39:2.4": "patto me āsavakkhayo”ti.",
  "thag2.39:2.5": "… Usabho thero …."
}