{
  "thag1.47:0.1": "Theragāthā 1.47",
  "thag1.47:0.2": "Ekakanipāta",
  "thag1.47:0.3": "Pañcamavagga",
  "thag1.47:0.4": "Ujjayattheragāthā",
  "thag1.47:1.1": "“Namo te buddha vīratthu,",
  "thag1.47:1.2": "vippamuttosi sabbadhi;",
  "thag1.47:1.3": "Tuyhāpadāne viharaṁ,",
  "thag1.47:1.4": "viharāmi anāsavo”ti.",
  "thag1.47:1.5": "… Ujjayo thero …."
}