{
  "thag2.35:0.1": "Theragāthā 2.35",
  "thag2.35:0.2": "Dukanipāta",
  "thag2.35:0.3": "Catutthavagga",
  "thag2.35:0.4": "Sambulakaccānattheragāthā",
  "thag2.35:1.1": "“Devo ca vassati, devo ca gaḷagaḷāyati,",
  "thag2.35:1.2": "Ekako cāhaṁ bherave bile viharāmi;",
  "thag2.35:1.3": "Tassa mayhaṁ ekakassa bherave bile viharato,",
  "thag2.35:1.4": "Natthi bhayaṁ vā chambhitattaṁ vā lomahaṁso vā.",
  "thag2.35:2.1": "Dhammatā mamesā yassa me,",
  "thag2.35:2.2": "Ekakassa bherave bile;",
  "thag2.35:2.3": "Viharato natthi bhayaṁ vā,",
  "thag2.35:2.4": "Chambhitattaṁ vā lomahaṁso vā”ti.",
  "thag2.35:2.5": "… Sambulakaccāno thero …."
}