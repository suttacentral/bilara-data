{
  "thag1.73:0.1": "Theragāthā 1.73",
  "thag1.73:0.2": "Ekakanipāta",
  "thag1.73:0.3": "Aṭṭhamavagga",
  "thag1.73:0.4": "Māṇavattheragāthā",
  "thag1.73:1.1": "“Jiṇṇañca disvā dukhitañca byādhitaṁ,",
  "thag1.73:1.2": "Matañca disvā gatamāyusaṅkhayaṁ;",
  "thag1.73:1.3": "Tato ahaṁ nikkhamitūna pabbajiṁ,",
  "thag1.73:1.4": "Pahāya kāmāni manoramānī”ti.",
  "thag1.73:1.5": "… Māṇavo thero …."
}