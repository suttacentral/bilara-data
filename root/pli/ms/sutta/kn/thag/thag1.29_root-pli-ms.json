{
  "thag1.29:0.1": "Theragāthā 1.29",
  "thag1.29:0.2": "Ekakanipāta",
  "thag1.29:0.3": "Tatiyavagga",
  "thag1.29:0.4": "Hāritattheragāthā",
  "thag1.29:1.1": "“Samunnamayamattānaṁ,",
  "thag1.29:1.2": "usukārova tejanaṁ;",
  "thag1.29:1.3": "Cittaṁ ujuṁ karitvāna,",
  "thag1.29:1.4": "avijjaṁ bhinda hāritā”ti.",
  "thag1.29:1.5": "… Hārito thero …."
}