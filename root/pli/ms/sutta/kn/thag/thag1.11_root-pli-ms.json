{
  "thag1.11:0.1": "Theragāthā 1.11",
  "thag1.11:0.2": "Ekakanipāta",
  "thag1.11:0.3": "Dutiyavagga",
  "thag1.11:0.4": "Cūḷavacchattheragāthā",
  "thag1.11:1.1": "“Pāmojjabahulo bhikkhu,",
  "thag1.11:1.2": "dhamme buddhappavedite;",
  "thag1.11:1.3": "Adhigacche padaṁ santaṁ,",
  "thag1.11:1.4": "saṅkhārūpasamaṁ sukhan”ti.",
  "thag1.11:1.5": "… Cūḷavaccho thero …."
}