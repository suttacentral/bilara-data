{
  "thag1.100:0.1": "Theragāthā 1.100 ",
  "thag1.100:0.2": "Ekakanipāta ",
  "thag1.100:0.3": "Dasamavagga ",
  "thag1.100:0.4": "(Dutiya) Devasabhattheragāthā ",
  "thag1.100:1.1": "“Sammappadhānasampanno, ",
  "thag1.100:1.2": "Satipaṭṭhānagocaro; ",
  "thag1.100:1.3": "Vimuttikusumasañchanno, ",
  "thag1.100:1.4": "Parinibbissatyanāsavo”ti. ",
  "thag1.100:1.5": "… Devasabho thero …. ",
  "thag1.100:1.6": "vaggo dasamo. ",
  "thag1.100:2.0": "Tassuddānaṁ ",
  "thag1.100:2.1": "Paripuṇṇako ca vijayo, ",
  "thag1.100:2.2": "erako mettajī muni; ",
  "thag1.100:2.3": "Cakkhupālo khaṇḍasumano, ",
  "thag1.100:2.4": "tisso ca abhayo tathā; ",
  "thag1.100:2.5": "Uttiyo ca mahāpañño, ",
  "thag1.100:2.6": "thero devasabhopi cāti. "
}