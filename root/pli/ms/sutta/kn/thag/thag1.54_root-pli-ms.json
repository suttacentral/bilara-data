{
  "thag1.54:0.1": "Theragāthā 1.54",
  "thag1.54:0.2": "Ekakanipāta",
  "thag1.54:0.3": "Chaṭṭhavagga",
  "thag1.54:0.4": "Uttiyattheragāthā",
  "thag1.54:1.1": "“Vassati devo yathā sugītaṁ,",
  "thag1.54:1.2": "Channā me kuṭikā sukhā nivātā;",
  "thag1.54:1.3": "Tassaṁ viharāmi adutiyo,",
  "thag1.54:1.4": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.54:1.5": "… Uttiyo thero …."
}