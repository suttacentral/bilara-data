{
  "thag1.33:0.1": "Theragāthā 1.33",
  "thag1.33:0.2": "Ekakanipāta",
  "thag1.33:0.3": "Catutthavagga",
  "thag1.33:0.4": "Sopākattheragāthā",
  "thag1.33:1.1": "“Yathāpi ekaputtasmiṁ,",
  "thag1.33:1.2": "piyasmiṁ kusalī siyā;",
  "thag1.33:1.3": "Evaṁ sabbesu pāṇesu,",
  "thag1.33:1.4": "sabbattha kusalo siyā”ti.",
  "thag1.33:1.5": "… Sopāko thero …."
}