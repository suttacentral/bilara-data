{
  "thag1.115:0.1": "Theragāthā 1.115",
  "thag1.115:0.2": "Ekakanipāta",
  "thag1.115:0.3": "Dvādasamavagga",
  "thag1.115:0.4": "Mahānāmattheragāthā",
  "thag1.115:1.1": "“Esāvahiyyase pabbatena,",
  "thag1.115:1.2": "Bahukuṭajasallakikena;",
  "thag1.115:1.3": "Nesādakena girinā,",
  "thag1.115:1.4": "Yasassinā paricchadenā”ti.",
  "thag1.115:1.5": "… Mahānāmo thero …."
}