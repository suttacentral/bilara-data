{
  "thag1.50:0.1": "Theragāthā 1.50 ",
  "thag1.50:0.2": "Ekakanipāta ",
  "thag1.50:0.3": "Pañcamavagga ",
  "thag1.50:0.4": "Vimalattheragāthā ",
  "thag1.50:1.1": "“Dharaṇī ca siñcati vāti, ",
  "thag1.50:1.2": "Māluto vijjutā carati nabhe; ",
  "thag1.50:1.3": "Upasamanti vitakkā, ",
  "thag1.50:1.4": "Cittaṁ susamāhitaṁ mamā”ti. ",
  "thag1.50:1.5": "… Vimalo thero … ",
  "thag1.50:1.6": "vaggo pañcamo. ",
  "thag1.50:2.0": "Tassuddānaṁ ",
  "thag1.50:2.1": "Sirīvaḍḍho revato thero, ",
  "thag1.50:2.2": "sumaṅgalo sānusavhayo; ",
  "thag1.50:2.3": "Ramaṇīyavihārī ca, ",
  "thag1.50:2.4": "samiddhiujjayasañjayā; ",
  "thag1.50:2.5": "Rāmaṇeyyo ca so thero, ",
  "thag1.50:2.6": "vimalo ca raṇañjahoti. "
}