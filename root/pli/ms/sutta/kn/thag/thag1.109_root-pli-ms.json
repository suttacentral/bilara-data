{
  "thag1.109:0.1": "Theragāthā 1.109",
  "thag1.109:0.2": "Ekakanipāta",
  "thag1.109:0.3": "Ekādasamavagga",
  "thag1.109:0.4": "Saṅgharakkhitattheragāthā",
  "thag1.109:1.1": "“Na nūnāyaṁ paramahitānukampino,",
  "thag1.109:1.2": "Rahogato anuvigaṇeti sāsanaṁ;",
  "thag1.109:1.3": "Tathāhayaṁ viharati pākatindriyo,",
  "thag1.109:1.4": "Migī yathā taruṇajātikā vane”ti.",
  "thag1.109:1.5": "… Saṅgharakkhito thero …."
}