{
  "thag1.1:0.1": "Theragāthā 1.1",
  "thag1.1:0.2": "Ekakanipāta",
  "thag1.1:0.3": "Paṭhamavagga",
  "thag1.1:0.4": "Subhūtittheragāthā",
  "thag1.1:1.0.1": "Nidānagāthā",
  "thag1.1:1.0.2": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "thag1.1:1.1": "Sīhānaṁva nadantānaṁ,",
  "thag1.1:1.2": "dāṭhīnaṁ girigabbhare;",
  "thag1.1:1.3": "Suṇātha bhāvitattānaṁ,",
  "thag1.1:1.4": "gāthā atthūpanāyikā.",
  "thag1.1:2.1": "Yathānāmā yathāgottā,",
  "thag1.1:2.2": "yathādhammavihārino;",
  "thag1.1:2.3": "Yathādhimuttā sappaññā,",
  "thag1.1:2.4": "vihariṁsu atanditā.",
  "thag1.1:3.1": "Tattha tattha vipassitvā,",
  "thag1.1:3.2": "phusitvā accutaṁ padaṁ;",
  "thag1.1:3.3": "Katantaṁ paccavekkhantā,",
  "thag1.1:3.4": "imamatthamabhāsisuṁ.",
  "thag1.1:4.0": "Subhūtittheragāthā",
  "thag1.1:4.1": "“Channā me kuṭikā sukhā nivātā,",
  "thag1.1:4.2": "Vassa deva yathāsukhaṁ;",
  "thag1.1:4.3": "Cittaṁ me susamāhitaṁ vimuttaṁ,",
  "thag1.1:4.4": "Ātāpī viharāmi vassa devā”ti.",
  "thag1.1:4.5": "Itthaṁ sudaṁ āyasmā subhūtitthero gāthaṁ abhāsitthāti."
}