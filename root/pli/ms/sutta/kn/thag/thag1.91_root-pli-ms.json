{
  "thag1.91:0.1": "Theragāthā 1.91",
  "thag1.91:0.2": "Ekakanipāta",
  "thag1.91:0.3": "Dasamavagga",
  "thag1.91:0.4": "Paripuṇṇakattheragāthā",
  "thag1.91:1.1": "“Na tathā mataṁ satarasaṁ,",
  "thag1.91:1.2": "Sudhannaṁ yaṁ mayajja paribhuttaṁ;",
  "thag1.91:1.3": "Aparimitadassinā gotamena,",
  "thag1.91:1.4": "Buddhena desito dhammo”ti.",
  "thag1.91:1.5": "… Paripuṇṇako thero …."
}