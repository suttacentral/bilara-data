{
  "thag1.19:0.1": "Theragāthā 1.19",
  "thag1.19:0.2": "Ekakanipāta",
  "thag1.19:0.3": "Dutiyavagga",
  "thag1.19:0.4": "Kulattheragāthā",
  "thag1.19:1.1": "“Udakañhi nayanti nettikā,",
  "thag1.19:1.2": "Usukārā namayanti tejanaṁ;",
  "thag1.19:1.3": "Dāruṁ namayanti tacchakā,",
  "thag1.19:1.4": "Attānaṁ damayanti subbatā”ti.",
  "thag1.19:1.5": "… Kulo thero …."
}