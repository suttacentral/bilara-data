{
  "thag1.34:0.1": "Theragāthā 1.34",
  "thag1.34:0.2": "Ekakanipāta",
  "thag1.34:0.3": "Catutthavagga",
  "thag1.34:0.4": "Posiyattheragāthā",
  "thag1.34:1.1": "“Anāsannavarā etā,",
  "thag1.34:1.2": "niccameva vijānatā;",
  "thag1.34:1.3": "Gāmā araññamāgamma,",
  "thag1.34:1.4": "tato gehaṁ upāvisi;",
  "thag1.34:1.5": "Tato uṭṭhāya pakkāmi,",
  "thag1.34:1.6": "anāmantetvā posiyo”ti.",
  "thag1.34:1.7": "… Posiyo thero …."
}