{
  "thag2.1:0.1": "Theragāthā 2.1",
  "thag2.1:0.2": "Dukanipāta",
  "thag2.1:0.3": "Paṭhamavagga",
  "thag2.1:0.4": "Uttarattheragāthā",
  "thag2.1:1.1": "“Natthi koci bhavo nicco,",
  "thag2.1:1.2": "saṅkhārā vāpi sassatā;",
  "thag2.1:1.3": "Uppajjanti ca te khandhā,",
  "thag2.1:1.4": "cavanti aparāparaṁ.",
  "thag2.1:2.1": "Etamādīnavaṁ ñatvā,",
  "thag2.1:2.2": "bhavenamhi anatthiko;",
  "thag2.1:2.3": "Nissaṭo sabbakāmehi,",
  "thag2.1:2.4": "patto me āsavakkhayo”ti.",
  "thag2.1:2.5": "Itthaṁ sudaṁ āyasmā uttaro thero gāthāyo abhāsitthāti."
}