{
  "thag3.9:0.1": "Theragāthā 3.9",
  "thag3.9:0.2": "Tikanipāta",
  "thag3.9:0.3": "Paṭhamavagga",
  "thag3.9:0.4": "Yasojattheragāthā",
  "thag3.9:1.1": "“Kālapabbaṅgasaṅkāso,",
  "thag3.9:1.2": "kiso dhamanisanthato;",
  "thag3.9:1.3": "Mattaññū annapānamhi,",
  "thag3.9:1.4": "adīnamānaso naro”.",
  "thag3.9:2.1": "“Phuṭṭho ḍaṁsehi makasehi,",
  "thag3.9:2.2": "araññasmiṁ brahāvane;",
  "thag3.9:2.3": "Nāgo saṅgāmasīseva,",
  "thag3.9:2.4": "sato tatrādhivāsaye.",
  "thag3.9:3.1": "Yathā brahmā tathā eko,",
  "thag3.9:3.2": "yathā devo tathā duve;",
  "thag3.9:3.3": "Yathā gāmo tathā tayo,",
  "thag3.9:3.4": "kolāhalaṁ tatuttarin”ti.",
  "thag3.9:3.5": "… Yasojo thero …."
}