{
  "thag1.108:0.1": "Theragāthā 1.108",
  "thag1.108:0.2": "Ekakanipāta",
  "thag1.108:0.3": "Ekādasamavagga",
  "thag1.108:0.4": "Dhammasavapituttheragāthā",
  "thag1.108:1.1": "“Sa vīsavassasatiko,",
  "thag1.108:1.2": "pabbajiṁ anagāriyaṁ;",
  "thag1.108:1.3": "Tisso vijjā anuppattā,",
  "thag1.108:1.4": "kataṁ buddhassa sāsanan”ti.",
  "thag1.108:1.5": "… Dhammasavapitu thero …."
}