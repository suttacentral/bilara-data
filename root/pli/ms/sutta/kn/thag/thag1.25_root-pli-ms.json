{
  "thag1.25:0.1": "Theragāthā 1.25",
  "thag1.25:0.2": "Ekakanipāta",
  "thag1.25:0.3": "Tatiyavagga",
  "thag1.25:0.4": "Nandiyattheragāthā",
  "thag1.25:1.1": "“Obhāsajātaṁ phalagaṁ,",
  "thag1.25:1.2": "cittaṁ yassa abhiṇhaso;",
  "thag1.25:1.3": "Tādisaṁ bhikkhumāsajja,",
  "thag1.25:1.4": "kaṇha dukkhaṁ nigacchasī”ti.",
  "thag1.25:1.5": "… Nandiyo thero …."
}