{
  "thag1.62:0.1": "Theragāthā 1.62",
  "thag1.62:0.2": "Ekakanipāta",
  "thag1.62:0.3": "Sattamavagga",
  "thag1.62:0.4": "Vajjiputtattheragāthā",
  "thag1.62:1.1": "“Ekakā mayaṁ araññe viharāma,",
  "thag1.62:1.2": "Apaviddhaṁva vanasmiṁ dārukaṁ;",
  "thag1.62:1.3": "Tassa me bahukā pihayanti,",
  "thag1.62:1.4": "Nerayikā viya saggagāminan”ti.",
  "thag1.62:1.5": "… Vajjiputto thero …."
}