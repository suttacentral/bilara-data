{
  "thag1.79:0.1": "Theragāthā 1.79",
  "thag1.79:0.2": "Ekakanipāta",
  "thag1.79:0.3": "Aṭṭhamavagga",
  "thag1.79:0.4": "Rakkhitattheragāthā",
  "thag1.79:1.1": "“Sabbo rāgo pahīno me,",
  "thag1.79:1.2": "sabbo doso samūhato;",
  "thag1.79:1.3": "Sabbo me vigato moho,",
  "thag1.79:1.4": "sītibhūtosmi nibbuto”ti.",
  "thag1.79:1.5": "… Rakkhito thero …."
}