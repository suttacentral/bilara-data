{
  "thag1.53:0.1": "Theragāthā 1.53",
  "thag1.53:0.2": "Ekakanipāta",
  "thag1.53:0.3": "Chaṭṭhavagga",
  "thag1.53:0.4": "Valliyattheragāthā",
  "thag1.53:1.1": "“Vassati devo yathā sugītaṁ,",
  "thag1.53:1.2": "Channā me kuṭikā sukhā nivātā;",
  "thag1.53:1.3": "Tassaṁ viharāmi appamatto,",
  "thag1.53:1.4": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.53:1.5": "… Valliyo thero …."
}