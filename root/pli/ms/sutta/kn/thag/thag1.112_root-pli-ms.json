{
  "thag1.112:0.1": "Theragāthā 1.112",
  "thag1.112:0.2": "Ekakanipāta",
  "thag1.112:0.3": "Dvādasamavagga",
  "thag1.112:0.4": "Vacchagottattheragāthā",
  "thag1.112:1.1": "“Tevijjohaṁ mahājhāyī,",
  "thag1.112:1.2": "cetosamathakovido;",
  "thag1.112:1.3": "Sadattho me anuppatto,",
  "thag1.112:1.4": "kataṁ buddhassa sāsanan”ti.",
  "thag1.112:1.5": "… Vacchagotto thero …."
}