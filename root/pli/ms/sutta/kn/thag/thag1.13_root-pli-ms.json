{
  "thag1.13:0.1": "Theragāthā 1.13",
  "thag1.13:0.2": "Ekakanipāta",
  "thag1.13:0.3": "Dutiyavagga",
  "thag1.13:0.4": "Vanavacchattheragāthā",
  "thag1.13:1.1": "“Nīlabbhavaṇṇā rucirā,",
  "thag1.13:1.2": "sītavārī sucindharā;",
  "thag1.13:1.3": "Indagopakasañchannā,",
  "thag1.13:1.4": "te selā ramayanti man”ti.",
  "thag1.13:1.5": "… Vanavaccho thero …."
}