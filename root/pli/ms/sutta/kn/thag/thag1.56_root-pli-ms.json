{
  "thag1.56:0.1": "Theragāthā 1.56",
  "thag1.56:0.2": "Ekakanipāta",
  "thag1.56:0.3": "Chaṭṭhavagga",
  "thag1.56:0.4": "Kuṭivihārittheragāthā",
  "thag1.56:1.1": "“Ko kuṭikāyaṁ bhikkhu kuṭikāyaṁ,",
  "thag1.56:1.2": "Vītarāgo susamāhitacitto;",
  "thag1.56:1.3": "Evaṁ jānāhi āvuso,",
  "thag1.56:1.4": "Amoghā te kuṭikā katā”ti.",
  "thag1.56:1.5": "… Kuṭivihāritthero …."
}