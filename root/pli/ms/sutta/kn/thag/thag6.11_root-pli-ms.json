{
  "thag6.11:0.1": "Theragāthā 6.11",
  "thag6.11:0.2": "Chakkanipāta",
  "thag6.11:0.3": "Paṭhamavagga",
  "thag6.11:0.4": "Nhātakamunittheragāthā",
  "thag6.11:1.1": "“Vātarogābhinīto tvaṁ,",
  "thag6.11:1.2": "viharaṁ kānane vane;",
  "thag6.11:1.3": "Paviddhagocare lūkhe,",
  "thag6.11:1.4": "kathaṁ bhikkhu karissasi”.",
  "thag6.11:2.1": "“Pītisukhena vipulena,",
  "thag6.11:2.2": "pharitvāna samussayaṁ;",
  "thag6.11:2.3": "Lūkhampi abhisambhonto,",
  "thag6.11:2.4": "viharissāmi kānane.",
  "thag6.11:3.1": "Bhāvento satta bojjhaṅge,",
  "thag6.11:3.2": "indriyāni balāni ca;",
  "thag6.11:3.3": "Jhānasokhummasampanno,",
  "thag6.11:3.4": "viharissaṁ anāsavo.",
  "thag6.11:4.1": "Vippamuttaṁ kilesehi,",
  "thag6.11:4.2": "suddhacittaṁ anāvilaṁ;",
  "thag6.11:4.3": "Abhiṇhaṁ paccavekkhanto,",
  "thag6.11:4.4": "viharissaṁ anāsavo.",
  "thag6.11:5.1": "Ajjhattañca bahiddhā ca,",
  "thag6.11:5.2": "ye me vijjiṁsu āsavā;",
  "thag6.11:5.3": "Sabbe asesā ucchinnā,",
  "thag6.11:5.4": "na ca uppajjare puna.",
  "thag6.11:6.1": "Pañcakkhandhā pariññātā,",
  "thag6.11:6.2": "tiṭṭhanti chinnamūlakā;",
  "thag6.11:6.3": "Dukkhakkhayo anuppatto,",
  "thag6.11:6.4": "natthi dāni punabbhavo”ti.",
  "thag6.11:6.5": "… Nhātakamunitthero …."
}