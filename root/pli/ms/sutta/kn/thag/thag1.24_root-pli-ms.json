{
  "thag1.24:0.1": "Theragāthā 1.24",
  "thag1.24:0.2": "Ekakanipāta",
  "thag1.24:0.3": "Tatiyavagga",
  "thag1.24:0.4": "Sugandhattheragāthā",
  "thag1.24:1.1": "“Anuvassiko pabbajito,",
  "thag1.24:1.2": "passa dhammasudhammataṁ;",
  "thag1.24:1.3": "Tisso vijjā anuppattā,",
  "thag1.24:1.4": "kataṁ buddhassa sāsanan”ti.",
  "thag1.24:1.5": "… Sugandho thero …."
}