{
  "thag1.39:0.1": "Theragāthā 1.39",
  "thag1.39:0.2": "Ekakanipāta",
  "thag1.39:0.3": "Catutthavagga",
  "thag1.39:0.4": "Tissattheragāthā",
  "thag1.39:1.1": "“Sattiyā viya omaṭṭho,",
  "thag1.39:1.2": "ḍayhamānova matthake;",
  "thag1.39:1.3": "Kāmarāgappahānāya,",
  "thag1.39:1.4": "sato bhikkhu paribbaje”ti.",
  "thag1.39:1.5": "… Tisso thero …."
}