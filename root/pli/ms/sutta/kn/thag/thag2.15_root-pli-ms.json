{
  "thag2.15:0.1": "Theragāthā 2.15",
  "thag2.15:0.2": "Dukanipāta",
  "thag2.15:0.3": "Dutiyavagga",
  "thag2.15:0.4": "Sabbamittattheragāthā",
  "thag2.15:1.1": "“Jano janamhi sambaddho,",
  "thag2.15:1.2": "janamevassito jano;",
  "thag2.15:1.3": "Jano janena heṭhīyati,",
  "thag2.15:1.4": "heṭheti ca jano janaṁ.",
  "thag2.15:2.1": "Ko hi tassa janenattho,",
  "thag2.15:2.2": "janena janitena vā;",
  "thag2.15:2.3": "Janaṁ ohāya gacchaṁ taṁ,",
  "thag2.15:2.4": "heṭhayitvā bahuṁ janan”ti.",
  "thag2.15:2.5": "… Sabbamitto thero …."
}