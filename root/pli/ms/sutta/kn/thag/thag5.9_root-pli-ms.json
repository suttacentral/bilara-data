{
  "thag5.9:0.1": "Theragāthā 5.9",
  "thag5.9:0.2": "Pañcakanipāta",
  "thag5.9:0.3": "Paṭhamavagga",
  "thag5.9:0.4": "Vijitasenattheragāthā",
  "thag5.9:1.1": "“Olaggessāmi te citta,",
  "thag5.9:1.2": "āṇidvāreva hatthinaṁ;",
  "thag5.9:1.3": "Na taṁ pāpe niyojessaṁ,",
  "thag5.9:1.4": "kāmajāla sarīraja.",
  "thag5.9:2.1": "Tvaṁ olaggo na gacchasi,",
  "thag5.9:2.2": "Dvāravivaraṁ gajova alabhanto;",
  "thag5.9:2.3": "Na ca cittakali punappunaṁ,",
  "thag5.9:2.4": "Pasakka pāparato carissasi.",
  "thag5.9:3.1": "Yathā kuñjaraṁ adantaṁ,",
  "thag5.9:3.2": "Navaggahamaṅkusaggaho;",
  "thag5.9:3.3": "Balavā āvatteti akāmaṁ,",
  "thag5.9:3.4": "Evaṁ āvattayissaṁ taṁ.",
  "thag5.9:4.1": "Yathā varahayadamakusalo,",
  "thag5.9:4.2": "Sārathi pavaro dameti ājaññaṁ;",
  "thag5.9:4.3": "Evaṁ damayissaṁ taṁ,",
  "thag5.9:4.4": "Patiṭṭhito pañcasu balesu.",
  "thag5.9:5.1": "Satiyā taṁ nibandhissaṁ,",
  "thag5.9:5.2": "Payutto te damessāmi;",
  "thag5.9:5.3": "Vīriyadhuraniggahito,",
  "thag5.9:5.4": "Na yito dūraṁ gamissase cittā”ti.",
  "thag5.9:5.5": "… Vijitaseno thero …."
}