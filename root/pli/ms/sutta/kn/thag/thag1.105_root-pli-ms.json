{
  "thag1.105:0.1": "Theragāthā 1.105",
  "thag1.105:0.2": "Ekakanipāta",
  "thag1.105:0.3": "Ekādasamavagga",
  "thag1.105:0.4": "Malitavambhattheragāthā",
  "thag1.105:1.1": "“Ukkaṇṭhitopi na vase,",
  "thag1.105:1.2": "ramamānopi pakkame;",
  "thag1.105:1.3": "Na tvevānatthasaṁhitaṁ,",
  "thag1.105:1.4": "vase vāsaṁ vicakkhaṇo”ti.",
  "thag1.105:1.5": "… Malitavambho thero …."
}