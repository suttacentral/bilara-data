{
  "thag1.12:0.1": "Theragāthā 1.12",
  "thag1.12:0.2": "Ekakanipāta",
  "thag1.12:0.3": "Dutiyavagga",
  "thag1.12:0.4": "Mahāvacchattheragāthā",
  "thag1.12:1.1": "“Paññābalī sīlavatūpapanno,",
  "thag1.12:1.2": "Samāhito jhānarato satīmā;",
  "thag1.12:1.3": "Yadatthiyaṁ bhojanaṁ bhuñjamāno,",
  "thag1.12:1.4": "Kaṅkhetha kālaṁ idha vītarāgo”ti.",
  "thag1.12:1.5": "… Mahāvaccho thero …."
}