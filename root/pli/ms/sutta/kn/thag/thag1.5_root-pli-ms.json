{
  "thag1.5:0.1": "Theragāthā 1.5",
  "thag1.5:0.2": "Ekakanipāta",
  "thag1.5:0.3": "Paṭhamavagga",
  "thag1.5:0.4": "Dabbattheragāthā",
  "thag1.5:1.1": "“Yo duddamiyo damena danto,",
  "thag1.5:1.2": "Dabbo santusito vitiṇṇakaṅkho;",
  "thag1.5:1.3": "Vijitāvī apetabheravo hi,",
  "thag1.5:1.4": "Dabbo so parinibbuto ṭhitatto”ti.",
  "thag1.5:1.5": "Itthaṁ sudaṁ āyasmā dabbo thero gāthaṁ abhāsitthāti."
}