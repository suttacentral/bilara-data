{
  "thag2.12:0.1": "Theragāthā 2.12",
  "thag2.12:0.2": "Dukanipāta",
  "thag2.12:0.3": "Dutiyavagga",
  "thag2.12:0.4": "Jotidāsattheragāthā",
  "thag2.12:1.1": "“Ye kho te veṭhamissena,",
  "thag2.12:1.2": "nānattena ca kammunā;",
  "thag2.12:1.3": "Manusse uparundhanti,",
  "thag2.12:1.4": "pharusūpakkamā janā;",
  "thag2.12:1.5": "Tepi tattheva kīranti,",
  "thag2.12:1.6": "na hi kammaṁ panassati.",
  "thag2.12:2.1": "Yaṁ karoti naro kammaṁ,",
  "thag2.12:2.2": "Kalyāṇaṁ yadi pāpakaṁ;",
  "thag2.12:2.3": "Tassa tasseva dāyādo,",
  "thag2.12:2.4": "Yaṁ yaṁ kammaṁ pakubbatī”ti.",
  "thag2.12:2.5": "… Jotidāso thero …."
}