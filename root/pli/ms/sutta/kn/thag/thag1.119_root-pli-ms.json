{
  "thag1.119:0.1": "Theragāthā 1.119",
  "thag1.119:0.2": "Ekakanipāta",
  "thag1.119:0.3": "Dvādasamavagga",
  "thag1.119:0.4": "Vajjiputtattheragāthā",
  "thag1.119:1.1": "“Rukkhamūlagahanaṁ pasakkiya,",
  "thag1.119:1.2": "Nibbānaṁ hadayasmiṁ opiya;",
  "thag1.119:1.3": "Jhāya gotama mā ca pamādo,",
  "thag1.119:1.4": "Kiṁ te biḷibiḷikā karissatī”ti.",
  "thag1.119:1.5": "… Vajjiputto thero …."
}