{
  "thag16.5:0.1": "Theragāthā 16.5",
  "thag16.5:0.2": "Vīsatinipāta",
  "thag16.5:0.3": "Paṭhamavagga",
  "thag16.5:0.4": "Mālukyaputtattheragāthā",
  "thag16.5:1.1": "“Rūpaṁ disvā sati muṭṭhā,",
  "thag16.5:1.2": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:1.3": "Sārattacitto vedeti,",
  "thag16.5:1.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:2.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:2.2": "anekā rūpasambhavā;",
  "thag16.5:2.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:2.4": "cittamassūpahaññati;",
  "thag16.5:2.5": "Evamācinato dukkhaṁ,",
  "thag16.5:2.6": "ārā nibbāna vuccati.",
  "thag16.5:3.1": "Saddaṁ sutvā sati muṭṭhā,",
  "thag16.5:3.2": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:3.3": "Sārattacitto vedeti,",
  "thag16.5:3.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:4.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:4.2": "anekā saddasambhavā;",
  "thag16.5:4.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:4.4": "cittamassūpahaññati;",
  "thag16.5:4.5": "Evamācinato dukkhaṁ,",
  "thag16.5:4.6": "ārā nibbāna vuccati.",
  "thag16.5:5.1": "Gandhaṁ ghatvā sati muṭṭhā,",
  "thag16.5:5.2": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:5.3": "Sārattacitto vedeti,",
  "thag16.5:5.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:6.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:6.2": "anekā gandhasambhavā;",
  "thag16.5:6.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:6.4": "cittamassūpahaññati;",
  "thag16.5:6.5": "Evamācinato dukkhaṁ,",
  "thag16.5:6.6": "ārā nibbāna vuccati.",
  "thag16.5:7.1": "Rasaṁ bhotvā sati muṭṭhā,",
  "thag16.5:7.2": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:7.3": "Sārattacitto vedeti,",
  "thag16.5:7.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:8.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:8.2": "anekā rasasambhavā;",
  "thag16.5:8.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:8.4": "cittamassūpahaññati;",
  "thag16.5:8.5": "Evamācinato dukkhaṁ,",
  "thag16.5:8.6": "ārā nibbāna vuccati.",
  "thag16.5:9.1": "Phassaṁ phussa sati muṭṭhā,",
  "thag16.5:9.2": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:9.3": "Sārattacitto vedeti,",
  "thag16.5:9.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:10.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:10.2": "anekā phassasambhavā;",
  "thag16.5:10.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:10.4": "cittamassūpahaññati;",
  "thag16.5:10.5": "Evamācinato dukkhaṁ,",
  "thag16.5:10.6": "ārā nibbāna vuccati.",
  "thag16.5:11.1": "Dhammaṁ ñatvā sati muṭṭhā,",
  "thag16.5:11.2": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:11.3": "Sārattacitto vedeti,",
  "thag16.5:11.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:12.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:12.2": "anekā dhammasambhavā;",
  "thag16.5:12.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:12.4": "cittamassūpahaññati;",
  "thag16.5:12.5": "Evamācinato dukkhaṁ,",
  "thag16.5:12.6": "ārā nibbāna vuccati.",
  "thag16.5:13.1": "Na so rajjati rūpesu,",
  "thag16.5:13.2": "rūpaṁ disvā patissato;",
  "thag16.5:13.3": "Virattacitto vedeti,",
  "thag16.5:13.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:14.1": "Yathāssa passato rūpaṁ,",
  "thag16.5:14.2": "sevato cāpi vedanaṁ;",
  "thag16.5:14.3": "Khīyati nopacīyati,",
  "thag16.5:14.4": "evaṁ so caratī sato;",
  "thag16.5:14.5": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:14.6": "santike nibbāna vuccati.",
  "thag16.5:15.1": "Na so rajjati saddesu,",
  "thag16.5:15.2": "saddaṁ sutvā patissato;",
  "thag16.5:15.3": "Virattacitto vedeti,",
  "thag16.5:15.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:16.1": "Yathāssa suṇato saddaṁ,",
  "thag16.5:16.2": "sevato cāpi vedanaṁ;",
  "thag16.5:16.3": "Khīyati nopacīyati,",
  "thag16.5:16.4": "evaṁ so caratī sato;",
  "thag16.5:16.5": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:16.6": "santike nibbāna vuccati.",
  "thag16.5:17.1": "Na so rajjati gandhesu,",
  "thag16.5:17.2": "gandhaṁ ghatvā patissato;",
  "thag16.5:17.3": "Virattacitto vedeti,",
  "thag16.5:17.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:18.1": "Yathāssa ghāyato gandhaṁ,",
  "thag16.5:18.2": "sevato cāpi vedanaṁ;",
  "thag16.5:18.3": "Khīyati nopacīyati,",
  "thag16.5:18.4": "evaṁ so caratī sato;",
  "thag16.5:18.5": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:18.6": "santike nibbāna vuccati.",
  "thag16.5:19.1": "Na so rajjati rasesu,",
  "thag16.5:19.2": "rasaṁ bhotvā patissato;",
  "thag16.5:19.3": "Virattacitto vedeti,",
  "thag16.5:19.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:20.1": "Yathāssa sāyarato rasaṁ,",
  "thag16.5:20.2": "sevato cāpi vedanaṁ;",
  "thag16.5:20.3": "Khīyati nopacīyati,",
  "thag16.5:20.4": "evaṁ so caratī sato;",
  "thag16.5:20.5": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:20.6": "santike nibbāna vuccati.",
  "thag16.5:21.1": "Na so rajjati phassesu,",
  "thag16.5:21.2": "phassaṁ phussa patissato;",
  "thag16.5:21.3": "Virattacitto vedeti,",
  "thag16.5:21.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:22.1": "Yathāssa phusato phassaṁ,",
  "thag16.5:22.2": "sevato cāpi vedanaṁ;",
  "thag16.5:22.3": "Khīyati nopacīyati,",
  "thag16.5:22.4": "evaṁ so caratī sato;",
  "thag16.5:22.5": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:22.6": "santike nibbāna vuccati.",
  "thag16.5:23.1": "Na so rajjati dhammesu,",
  "thag16.5:23.2": "dhammaṁ ñatvā patissato;",
  "thag16.5:23.3": "Virattacitto vedeti,",
  "thag16.5:23.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:24.1": "Yathāssa vijānato dhammaṁ,",
  "thag16.5:24.2": "sevato cāpi vedanaṁ;",
  "thag16.5:24.3": "Khīyati nopacīyati,",
  "thag16.5:24.4": "evaṁ so caratī sato;",
  "thag16.5:24.5": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:24.6": "santike nibbāna vuccati”.",
  "thag16.5:24.7": "… Mālukyaputto thero …."
}