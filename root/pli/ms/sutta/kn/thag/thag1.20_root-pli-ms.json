{
  "thag1.20:0.1": "Theragāthā 1.20 ",
  "thag1.20:0.2": "Ekakanipāta ",
  "thag1.20:0.3": "Dutiyavagga ",
  "thag1.20:0.4": "Ajitattheragāthā ",
  "thag1.20:1.1": "“Maraṇe me bhayaṁ natthi, ",
  "thag1.20:1.2": "nikanti natthi jīvite; ",
  "thag1.20:1.3": "Sandehaṁ nikkhipissāmi, ",
  "thag1.20:1.4": "sampajāno paṭissato”ti. ",
  "thag1.20:1.5": "… Ajito thero … ",
  "thag1.20:1.6": "vaggo dutiyo. ",
  "thag1.20:2.0": "Tassuddānaṁ ",
  "thag1.20:2.1": "Cūḷavaccho mahāvaccho, ",
  "thag1.20:2.2": "vanavaccho ca sīvako; ",
  "thag1.20:2.3": "Kuṇḍadhāno ca belaṭṭhi, ",
  "thag1.20:2.4": "dāsako ca tatopari; ",
  "thag1.20:2.5": "Siṅgālapitiko thero, ",
  "thag1.20:2.6": "kulo ca ajito dasāti. "
}