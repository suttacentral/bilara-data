{
  "thag1.65:0.1": "Theragāthā 1.65",
  "thag1.65:0.2": "Ekakanipāta",
  "thag1.65:0.3": "Sattamavagga",
  "thag1.65:0.4": "Ukkhepakatavacchattheragāthā",
  "thag1.65:1.1": "“Ukkhepakatavacchassa,",
  "thag1.65:1.2": "Saṅkalitaṁ bahūhi vassehi;",
  "thag1.65:1.3": "Taṁ bhāsati gahaṭṭhānaṁ,",
  "thag1.65:1.4": "Sunisinno uḷārapāmojjo”ti.",
  "thag1.65:1.5": "… Ukkhepakatavaccho thero …."
}