{
  "thag1.28:0.1": "Theragāthā 1.28",
  "thag1.28:0.2": "Ekakanipāta",
  "thag1.28:0.3": "Tatiyavagga",
  "thag1.28:0.4": "Jambugāmikaputtattheragāthā",
  "thag1.28:1.1": "“Kacci no vatthapasuto,",
  "thag1.28:1.2": "Kacci no bhūsanārato;",
  "thag1.28:1.3": "Kacci sīlamayaṁ gandhaṁ,",
  "thag1.28:1.4": "Kiṁ tvaṁ vāyasi netarā pajā”ti.",
  "thag1.28:1.5": "… Jambugāmikaputto thero …."
}