{
  "thag1.7:0.1": "Theragāthā 1.7",
  "thag1.7:0.2": "Ekakanipāta",
  "thag1.7:0.3": "Paṭhamavagga",
  "thag1.7:0.4": "Bhalliyattheragāthā",
  "thag1.7:1.1": "“Yopānudī maccurājassa senaṁ,",
  "thag1.7:1.2": "Naḷasetuṁva sudubbalaṁ mahogho;",
  "thag1.7:1.3": "Vijitāvī apetabheravo hi,",
  "thag1.7:1.4": "Danto so parinibbuto ṭhitatto”ti.",
  "thag1.7:1.5": "Itthaṁ sudaṁ āyasmā bhalliyo thero gāthaṁ abhāsitthāti."
}