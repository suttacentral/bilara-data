{
  "thag1.64:0.1": "Theragāthā 1.64",
  "thag1.64:0.2": "Ekakanipāta",
  "thag1.64:0.3": "Sattamavagga",
  "thag1.64:0.4": "Vimalakoṇḍaññattheragāthā",
  "thag1.64:1.1": "“Dumavhayāya uppanno,",
  "thag1.64:1.2": "jāto paṇḍaraketunā;",
  "thag1.64:1.3": "Ketuhā ketunāyeva,",
  "thag1.64:1.4": "mahāketuṁ padhaṁsayī”ti.",
  "thag1.64:1.5": "… Vimalakoṇḍañño thero …."
}