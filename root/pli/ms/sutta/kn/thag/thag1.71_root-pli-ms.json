{
  "thag1.71:0.1": "Theragāthā 1.71",
  "thag1.71:0.2": "Ekakanipāta",
  "thag1.71:0.3": "Aṭṭhamavagga",
  "thag1.71:0.4": "Vacchapālattheragāthā",
  "thag1.71:1.1": "“Susukhumanipuṇatthadassinā,",
  "thag1.71:1.2": "Matikusalena nivātavuttinā;",
  "thag1.71:1.3": "Saṁsevitavuddhasīlinā,",
  "thag1.71:1.4": "Nibbānaṁ na hi tena dullabhan”ti.",
  "thag1.71:1.5": "… Vacchapālo thero …."
}