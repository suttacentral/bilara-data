{
  "thag2.10:0.1": "Theragāthā 2.10 ",
  "thag2.10:0.2": "Dukanipāta ",
  "thag2.10:0.3": "Paṭhamavagga ",
  "thag2.10:0.4": "Vasabhattheragāthā ",
  "thag2.10:1.1": "“Pubbe hanati attānaṁ, ",
  "thag2.10:1.2": "pacchā hanati so pare; ",
  "thag2.10:1.3": "Suhataṁ hanti attānaṁ, ",
  "thag2.10:1.4": "vītaṁseneva pakkhimā. ",
  "thag2.10:2.1": "Na brāhmaṇo bahivaṇṇo, ",
  "thag2.10:2.2": "anto vaṇṇo hi brāhmaṇo; ",
  "thag2.10:2.3": "Yasmiṁ pāpāni kammāni, ",
  "thag2.10:2.4": "sa ve kaṇho sujampatī”ti. ",
  "thag2.10:2.5": "… Vasabho thero … ",
  "thag2.10:2.6": "vaggo paṭhamo. ",
  "thag2.10:3.0": "Tassuddānaṁ ",
  "thag2.10:3.1": "Uttaro ceva piṇḍolo, ",
  "thag2.10:3.2": "valliyo tīriyo isi; ",
  "thag2.10:3.3": "Ajino ca meḷajino, ",
  "thag2.10:3.4": "rādho surādho gotamo; ",
  "thag2.10:3.5": "Vasabhena ime honti, ",
  "thag2.10:3.6": "dasa therā mahiddhikāti. "
}