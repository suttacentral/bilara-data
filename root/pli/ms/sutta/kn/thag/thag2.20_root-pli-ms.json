{
  "thag2.20:0.1": "Theragāthā 2.20 ",
  "thag2.20:0.2": "Dukanipāta ",
  "thag2.20:0.3": "Dutiyavagga ",
  "thag2.20:0.4": "Sirimattheragāthā ",
  "thag2.20:1.1": "“Pare ca naṁ pasaṁsanti, ",
  "thag2.20:1.2": "attā ce asamāhito; ",
  "thag2.20:1.3": "Moghaṁ pare pasaṁsanti, ",
  "thag2.20:1.4": "attā hi asamāhito. ",
  "thag2.20:2.1": "Pare ca naṁ garahanti, ",
  "thag2.20:2.2": "attā ce susamāhito; ",
  "thag2.20:2.3": "Moghaṁ pare garahanti, ",
  "thag2.20:2.4": "attā hi susamāhito”ti. ",
  "thag2.20:2.5": "… Sirimā thero …. ",
  "thag2.20:2.6": "vaggo dutiyo. ",
  "thag2.20:3.0": "Tassuddānaṁ ",
  "thag2.20:3.1": "Cundo ca jotidāso ca, ",
  "thag2.20:3.2": "thero heraññakāni ca; ",
  "thag2.20:3.3": "Somamitto sabbamitto, ",
  "thag2.20:3.4": "kāḷo tisso ca kimilo; ",
  "thag2.20:3.5": "Nando ca sirimā ceva, ",
  "thag2.20:3.6": "dasa therā mahiddhikāti. "
}