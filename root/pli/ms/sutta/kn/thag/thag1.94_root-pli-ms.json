{
  "thag1.94:0.1": "Theragāthā 1.94",
  "thag1.94:0.2": "Ekakanipāta",
  "thag1.94:0.3": "Dasamavagga",
  "thag1.94:0.4": "Mettajittheragāthā",
  "thag1.94:1.1": "“Namo hi tassa bhagavato,",
  "thag1.94:1.2": "sakyaputtassa sirīmato;",
  "thag1.94:1.3": "Tenāyaṁ aggappattena,",
  "thag1.94:1.4": "aggadhammo sudesito”ti.",
  "thag1.94:1.5": "… Mettaji thero …."
}