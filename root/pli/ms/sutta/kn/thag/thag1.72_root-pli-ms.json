{
  "thag1.72:0.1": "Theragāthā 1.72",
  "thag1.72:0.2": "Ekakanipāta",
  "thag1.72:0.3": "Aṭṭhamavagga",
  "thag1.72:0.4": "Ātumattheragāthā",
  "thag1.72:1.1": "“Yathā kaḷīro susu vaḍḍhitaggo,",
  "thag1.72:1.2": "Dunnikkhamo hoti pasākhajāto;",
  "thag1.72:1.3": "Evaṁ ahaṁ bhariyāyānitāya,",
  "thag1.72:1.4": "Anumaññaṁ maṁ pabbajitomhi dānī”ti.",
  "thag1.72:1.5": "… Ātumo thero …."
}