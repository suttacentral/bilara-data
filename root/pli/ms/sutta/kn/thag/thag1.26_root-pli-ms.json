{
  "thag1.26:0.1": "Theragāthā 1.26",
  "thag1.26:0.2": "Ekakanipāta",
  "thag1.26:0.3": "Tatiyavagga",
  "thag1.26:0.4": "Abhayattheragāthā",
  "thag1.26:1.1": "“Sutvā subhāsitaṁ vācaṁ,",
  "thag1.26:1.2": "buddhassādiccabandhuno;",
  "thag1.26:1.3": "Paccabyadhiṁ hi nipuṇaṁ,",
  "thag1.26:1.4": "vālaggaṁ usunā yathā”ti.",
  "thag1.26:1.5": "… Abhayo thero …."
}