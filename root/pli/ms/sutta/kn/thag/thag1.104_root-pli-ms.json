{
  "thag1.104:0.1": "Theragāthā 1.104",
  "thag1.104:0.2": "Ekakanipāta",
  "thag1.104:0.3": "Ekādasamavagga",
  "thag1.104:0.4": "Khitakattheragāthā",
  "thag1.104:1.1": "“Lahuko vata me kāyo,",
  "thag1.104:1.2": "Phuṭṭho ca pītisukhena vipulena;",
  "thag1.104:1.3": "Tūlamiva eritaṁ mālutena,",
  "thag1.104:1.4": "Pilavatīva me kāyo”ti.",
  "thag1.104:1.5": "… Khitako thero …."
}