{
  "thag1.27:0.1": "Theragāthā 1.27",
  "thag1.27:0.2": "Ekakanipāta",
  "thag1.27:0.3": "Tatiyavagga",
  "thag1.27:0.4": "Lomasakaṅgiyattheragāthā",
  "thag1.27:1.1": "“Dabbaṁ kusaṁ poṭakilaṁ,",
  "thag1.27:1.2": "usīraṁ muñjapabbajaṁ;",
  "thag1.27:1.3": "Urasā panudissāmi,",
  "thag1.27:1.4": "vivekamanubrūhayan”ti.",
  "thag1.27:1.5": "… Lomasakaṅgiyo thero …."
}