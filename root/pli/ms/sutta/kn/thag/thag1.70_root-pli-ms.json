{
  "thag1.70:0.1": "Theragāthā 1.70 ",
  "thag1.70:0.2": "Ekakanipāta ",
  "thag1.70:0.3": "Sattamavagga ",
  "thag1.70:0.4": "Puṇṇattheragāthā ",
  "thag1.70:1.1": "“Sīlameva idha aggaṁ, ",
  "thag1.70:1.2": "paññavā pana uttamo; ",
  "thag1.70:1.3": "Manussesu ca devesu, ",
  "thag1.70:1.4": "sīlapaññāṇato jayan”ti. ",
  "thag1.70:1.5": "… Puṇṇo thero … ",
  "thag1.70:1.6": "vaggo sattamo. ",
  "thag1.70:2.0": "Tassuddānaṁ ",
  "thag1.70:2.1": "Vappo ca vajjiputto ca, ",
  "thag1.70:2.2": "pakkho vimalakoṇḍañño; ",
  "thag1.70:2.3": "Ukkhepakatavaccho ca, ",
  "thag1.70:2.4": "meghiyo ekadhammiko; ",
  "thag1.70:2.5": "Ekudāniyachannā ca, ",
  "thag1.70:2.6": "puṇṇatthero mahabbaloti. "
}