{
  "thag1.8:0.1": "Theragāthā 1.8",
  "thag1.8:0.2": "Ekakanipāta",
  "thag1.8:0.3": "Paṭhamavagga",
  "thag1.8:0.4": "Vīrattheragāthā",
  "thag1.8:1.1": "“Yo duddamiyo damena danto,",
  "thag1.8:1.2": "Vīro santusito vitiṇṇakaṅkho;",
  "thag1.8:1.3": "Vijitāvī apetalomahaṁso,",
  "thag1.8:1.4": "Vīro so parinibbuto ṭhitatto”ti.",
  "thag1.8:1.5": "Itthaṁ sudaṁ āyasmā vīro thero gāthaṁ abhāsitthāti."
}