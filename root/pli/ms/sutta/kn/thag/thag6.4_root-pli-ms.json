{
  "thag6.4:0.1": "Theragāthā 6.4",
  "thag6.4:0.2": "Chakkanipāta",
  "thag6.4:0.3": "Paṭhamavagga",
  "thag6.4:0.4": "Kullattheragāthā",
  "thag6.4:1.1": "“Kullo sivathikaṁ gantvā,",
  "thag6.4:1.2": "addasa itthimujjhitaṁ;",
  "thag6.4:1.3": "Apaviddhaṁ susānasmiṁ,",
  "thag6.4:1.4": "khajjantiṁ kimihī phuṭaṁ.",
  "thag6.4:2.1": "Āturaṁ asuciṁ pūtiṁ,",
  "thag6.4:2.2": "passa kulla samussayaṁ;",
  "thag6.4:2.3": "Uggharantaṁ paggharantaṁ,",
  "thag6.4:2.4": "bālānaṁ abhinanditaṁ.",
  "thag6.4:3.1": "Dhammādāsaṁ gahetvāna,",
  "thag6.4:3.2": "ñāṇadassanapattiyā;",
  "thag6.4:3.3": "Paccavekkhiṁ imaṁ kāyaṁ,",
  "thag6.4:3.4": "tucchaṁ santarabāhiraṁ.",
  "thag6.4:4.1": "Yathā idaṁ tathā etaṁ,",
  "thag6.4:4.2": "yathā etaṁ tathā idaṁ;",
  "thag6.4:4.3": "Yathā adho tathā uddhaṁ,",
  "thag6.4:4.4": "yathā uddhaṁ tathā adho.",
  "thag6.4:5.1": "Yathā divā tathā rattiṁ,",
  "thag6.4:5.2": "yathā rattiṁ tathā divā;",
  "thag6.4:5.3": "Yathā pure tathā pacchā,",
  "thag6.4:5.4": "yathā pacchā tathā pure.",
  "thag6.4:6.1": "Pañcaṅgikena turiyena,",
  "thag6.4:6.2": "Na ratī hoti tādisī;",
  "thag6.4:6.3": "Yathā ekaggacittassa,",
  "thag6.4:6.4": "Sammā dhammaṁ vipassato”ti.",
  "thag6.4:6.5": "… Kullo thero …."
}