{
  "thag1.58:0.1": "Theragāthā 1.58",
  "thag1.58:0.2": "Ekakanipāta",
  "thag1.58:0.3": "Chaṭṭhavagga",
  "thag1.58:0.4": "Ramaṇīyakuṭikattheragāthā",
  "thag1.58:1.1": "“Ramaṇīyā me kuṭikā,",
  "thag1.58:1.2": "Saddhādeyyā manoramā;",
  "thag1.58:1.3": "Na me attho kumārīhi,",
  "thag1.58:1.4": "Yesaṁ attho tahiṁ gacchatha nāriyo”ti.",
  "thag1.58:1.5": "… Ramaṇīyakuṭiko thero …."
}