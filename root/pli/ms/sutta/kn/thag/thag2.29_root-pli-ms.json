{
  "thag2.29:0.1": "Theragāthā 2.29",
  "thag2.29:0.2": "Dukanipāta",
  "thag2.29:0.3": "Tatiyavagga",
  "thag2.29:0.4": "Bhāradvājattheragāthā",
  "thag2.29:1.1": "“Nadanti evaṁ sappaññā,",
  "thag2.29:1.2": "sīhāva girigabbhare;",
  "thag2.29:1.3": "Vīrā vijitasaṅgāmā,",
  "thag2.29:1.4": "jetvā māraṁ savāhiniṁ.",
  "thag2.29:2.1": "Satthā ca pariciṇṇo me,",
  "thag2.29:2.2": "dhammo saṅgho ca pūjito;",
  "thag2.29:2.3": "Ahañca vitto sumano,",
  "thag2.29:2.4": "puttaṁ disvā anāsavan”ti.",
  "thag2.29:2.5": "… Bhāradvājo thero …."
}