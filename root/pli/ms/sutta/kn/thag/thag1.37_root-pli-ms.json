{
  "thag1.37:0.1": "Theragāthā 1.37",
  "thag1.37:0.2": "Ekakanipāta",
  "thag1.37:0.3": "Catutthavagga",
  "thag1.37:0.4": "Kumāputtasahāyakattheragāthā",
  "thag1.37:1.1": "“Nānājanapadaṁ yanti,",
  "thag1.37:1.2": "Vicarantā asaññatā;",
  "thag1.37:1.3": "Samādhiñca virādhenti,",
  "thag1.37:1.4": "Kiṁsu raṭṭhacariyā karissati;",
  "thag1.37:1.5": "Tasmā vineyya sārambhaṁ,",
  "thag1.37:1.6": "Jhāyeyya apurakkhato”ti.",
  "thag1.37:1.7": "… Kumāputtattherassa sahāyako thero …."
}