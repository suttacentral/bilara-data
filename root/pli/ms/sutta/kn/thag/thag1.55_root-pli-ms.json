{
  "thag1.55:0.1": "Theragāthā 1.55",
  "thag1.55:0.2": "Ekakanipāta",
  "thag1.55:0.3": "Chaṭṭhavagga",
  "thag1.55:0.4": "Añjanavaniyattheragāthā",
  "thag1.55:1.1": "“Āsandiṁ kuṭikaṁ katvā,",
  "thag1.55:1.2": "ogayha añjanaṁ vanaṁ;",
  "thag1.55:1.3": "Tisso vijjā anuppattā,",
  "thag1.55:1.4": "kataṁ buddhassa sāsanan”ti.",
  "thag1.55:1.5": "… Añjanavaniyo thero …."
}