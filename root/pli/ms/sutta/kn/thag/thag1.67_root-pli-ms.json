{
  "thag1.67:0.1": "Theragāthā 1.67",
  "thag1.67:0.2": "Ekakanipāta",
  "thag1.67:0.3": "Sattamavagga",
  "thag1.67:0.4": "Ekadhammasavanīyattheragāthā",
  "thag1.67:1.1": "“Kilesā jhāpitā mayhaṁ,",
  "thag1.67:1.2": "bhavā sabbe samūhatā;",
  "thag1.67:1.3": "Vikkhīṇo jātisaṁsāro,",
  "thag1.67:1.4": "natthi dāni punabbhavo”ti.",
  "thag1.67:1.5": "… Ekadhammasavanīyo thero …."
}