{
  "pli-tv-bu-vb-as1-7:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-as1-7:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-as1-7:0.3": "Adhikaraṇasamatha",
  "pli-tv-bu-vb-as1-7:1.1": "Ime kho panāyasmanto satta adhikaraṇasamathā dhammā uddesaṃ āgacchanti.",
  "pli-tv-bu-vb-as1-7:2.1": "Uppannuppannānaṃ adhikaraṇānaṃ samathāya vūpasamāya",
  "pli-tv-bu-vb-as1-7:2.2": "sammukhāvinayo dātabbo,",
  "pli-tv-bu-vb-as1-7:2.3": "sativinayo dātabbo,",
  "pli-tv-bu-vb-as1-7:2.4": "amūḷhavinayo dātabbo,",
  "pli-tv-bu-vb-as1-7:2.5": "paṭiññāya kāretabbaṃ,",
  "pli-tv-bu-vb-as1-7:2.6": "yebhuyyasikā,",
  "pli-tv-bu-vb-as1-7:2.7": "tassapāpiyasikā,",
  "pli-tv-bu-vb-as1-7:2.8": "tiṇavatthārakoti.",
  "pli-tv-bu-vb-as1-7:3.1": "Uddiṭṭhā kho, āyasmanto, satta adhikaraṇasamathā dhammā.",
  "pli-tv-bu-vb-as1-7:3.2": "Tatthāyasmante pucchāmi—",
  "pli-tv-bu-vb-as1-7:3.3": "“kaccittha parisuddhā”?",
  "pli-tv-bu-vb-as1-7:3.4": "Dutiyampi pucchāmi—",
  "pli-tv-bu-vb-as1-7:3.5": "“kaccittha parisuddhā”?",
  "pli-tv-bu-vb-as1-7:3.6": "Tatiyampi pucchāmi—",
  "pli-tv-bu-vb-as1-7:3.7": "“kaccittha parisuddhā”?",
  "pli-tv-bu-vb-as1-7:3.8": "Parisuddhetthāyasmanto, tasmā tuṇhī, evametaṃ dhārayāmīti.",
  "pli-tv-bu-vb-as1-7:4.1": "Adhikaraṇasamathā niṭṭhitā.",
  "pli-tv-bu-vb-as1-7:5.1": "Uddiṭṭhaṃ kho, āyasmanto, nidānaṃ;",
  "pli-tv-bu-vb-as1-7:5.2": "uddiṭṭhā cattāro pārājikā dhammā;",
  "pli-tv-bu-vb-as1-7:5.3": "uddiṭṭhā terasa saṃghādisesā dhammā;",
  "pli-tv-bu-vb-as1-7:5.4": "uddiṭṭhā dve aniyatā dhammā;",
  "pli-tv-bu-vb-as1-7:5.5": "uddiṭṭhā tiṃsa nissaggiyā pācittiyā dhammā;",
  "pli-tv-bu-vb-as1-7:5.6": "uddiṭṭhā dvenavuti pācittiyā dhammā;",
  "pli-tv-bu-vb-as1-7:5.7": "uddiṭṭhā cattāro pāṭidesanīyā dhammā;",
  "pli-tv-bu-vb-as1-7:5.8": "uddiṭṭhā sekhiyā dhammā;",
  "pli-tv-bu-vb-as1-7:5.9": "uddiṭṭhā satta adhikaraṇasamathā dhammā.",
  "pli-tv-bu-vb-as1-7:5.10": "Ettakaṃ tassa bhagavato suttāgataṃ suttapariyāpannaṃ anvaddhamāsaṃ uddesaṃ āgacchati.",
  "pli-tv-bu-vb-as1-7:5.11": "Tattha sabbeheva samaggehi sammodamānehi avivadamānehi sikkhitabbanti.",
  "pli-tv-bu-vb-as1-7:6.1": "Mahāvibhaṅgo niṭṭhito."
}