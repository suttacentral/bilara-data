{
  "pli-tv-bu-vb-sk12:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk12:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk12:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk12:0.4": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk12:0.5": "12. Dutiyaujjagghikasikkhāpada",
  "pli-tv-bu-vb-sk12:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk12:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū mahāhasitaṃ hasantā antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk12:1.3": "“Na ujjagghikāya antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk12:1.4": "Na ujjagghikāya antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk12:1.5": "Yo anādariyaṃ paṭicca mahāhasitaṃ hasanto antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk12:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk12:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk12:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk12:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk12:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk12:1.11": "hasanīyasmiṃ vatthusmiṃ mihitamattaṃ karoti,",
  "pli-tv-bu-vb-sk12:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk12:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk12:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk12:1.15": "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
}