{
  "pli-tv-bu-vb-sk47:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk47:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk47:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk47:0.4": "Kabaḷavagga",
  "pli-tv-bu-vb-sk47:0.5": "47. Hatthaniddhunakasikkhāpada",
  "pli-tv-bu-vb-sk47:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk47:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū hatthaniddhunakaṃ bhuñjanti …",
  "pli-tv-bu-vb-sk47:1.3": "“Na hatthaniddhunakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk47:1.4": "Na hatthaniddhunakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk47:1.5": "Yo anādariyaṃ paṭicca hatthaniddhunakaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk47:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk47:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk47:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk47:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk47:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk47:1.11": "kacavaraṃ chaḍḍento hatthaṃ niddhunāti,",
  "pli-tv-bu-vb-sk47:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk47:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk47:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk47:1.15": "Sattamasikkhāpadaṃ niṭṭhitaṃ."
}