{
  "pli-tv-bu-vb-sk45:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk45:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk45:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk45:0.4": "Kabaḷavagga",
  "pli-tv-bu-vb-sk45:0.5": "45. Kabaḷāvacchedakasikkhāpada",
  "pli-tv-bu-vb-sk45:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk45:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū kabaḷāvacchedakaṃ bhuñjanti …",
  "pli-tv-bu-vb-sk45:1.3": "“Na kabaḷāvacchedakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk45:1.4": "Na kabaḷāvacchedakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk45:1.5": "Yo anādariyaṃ paṭicca kabaḷāvacchedakaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk45:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk45:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk45:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk45:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk45:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk45:1.11": "khajjake,",
  "pli-tv-bu-vb-sk45:1.12": "phalāphale,",
  "pli-tv-bu-vb-sk45:1.13": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk45:1.14": "āpadāsu,",
  "pli-tv-bu-vb-sk45:1.15": "ummattakassa,",
  "pli-tv-bu-vb-sk45:1.16": "ādikammikassāti.",
  "pli-tv-bu-vb-sk45:1.17": "Pañcamasikkhāpadaṃ niṭṭhitaṃ."
}