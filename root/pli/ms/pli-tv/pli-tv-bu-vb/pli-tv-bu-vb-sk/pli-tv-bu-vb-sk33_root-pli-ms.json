{
  "pli-tv-bu-vb-sk33:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk33:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk33:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk33:0.4": "Sakkaccavagga",
  "pli-tv-bu-vb-sk33:0.5": "33. Sapadānasikkhāpada",
  "pli-tv-bu-vb-sk33:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk33:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū tahaṃ tahaṃ omasitvā piṇḍapātaṃ bhuñjanti …",
  "pli-tv-bu-vb-sk33:1.3": "“Sapadānaṃ piṇḍapātaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk33:1.4": "Sapadānaṃ piṇḍapāto bhuñjitabbo.",
  "pli-tv-bu-vb-sk33:1.5": "Yo anādariyaṃ paṭicca tahaṃ tahaṃ omasitvā piṇḍapātaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk33:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk33:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk33:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk33:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk33:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk33:1.11": "aññesaṃ dento omasati,",
  "pli-tv-bu-vb-sk33:1.12": "aññassa bhājane ākiranto omasati,",
  "pli-tv-bu-vb-sk33:1.13": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk33:1.14": "āpadāsu,",
  "pli-tv-bu-vb-sk33:1.15": "ummattakassa,",
  "pli-tv-bu-vb-sk33:1.16": "ādikammikassāti.",
  "pli-tv-bu-vb-sk33:1.17": "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
}