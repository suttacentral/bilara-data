{
  "pli-tv-bu-vb-sk13:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk13:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk13:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk13:0.4": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk13:0.5": "13. Uccasaddasikkhāpada",
  "pli-tv-bu-vb-sk13:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk13:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū uccāsaddaṃ mahāsaddaṃ karontā antaraghare gacchanti …",
  "pli-tv-bu-vb-sk13:1.3": "“Appasaddo antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk13:1.4": "Appasaddena antaraghare gantabbaṃ.",
  "pli-tv-bu-vb-sk13:1.5": "Yo anādariyaṃ paṭicca uccāsaddaṃ mahāsaddaṃ karonto antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk13:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk13:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk13:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk13:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk13:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk13:1.11": "āpadāsu,",
  "pli-tv-bu-vb-sk13:1.12": "ummattakassa,",
  "pli-tv-bu-vb-sk13:1.13": "ādikammikassāti.",
  "pli-tv-bu-vb-sk13:1.14": "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
}