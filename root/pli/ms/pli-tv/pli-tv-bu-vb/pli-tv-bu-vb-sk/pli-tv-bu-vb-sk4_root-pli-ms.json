{
  "pli-tv-bu-vb-sk4:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk4:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk4:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk4:0.4": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk4:0.5": "4. Dutiyasuppaṭicchannasikkhāpada",
  "pli-tv-bu-vb-sk4:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk4:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū kāyaṃ vivaritvā antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk4:1.3": "“Suppaṭicchanno antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk4:1.4": "Suppaṭicchannena antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk4:1.5": "Yo anādariyaṃ paṭicca kāyaṃ vivaritvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk4:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk4:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk4:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk4:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk4:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk4:1.11": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk4:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk4:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk4:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk4:1.15": "Catutthasikkhāpadaṃ niṭṭhitaṃ."
}