{
  "pli-tv-bu-vb-sk7:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk7:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk7:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk7:0.4": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk7:0.5": "7. Okkhittacakkhusikkhāpada",
  "pli-tv-bu-vb-sk7:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk7:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū tahaṃ tahaṃ olokentā antaraghare gacchanti …",
  "pli-tv-bu-vb-sk7:1.3": "“Okkhittacakkhu antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk7:1.4": "Okkhittacakkhunā antaraghare gantabbaṃ yugamattaṃ pekkhantena.",
  "pli-tv-bu-vb-sk7:1.5": "Yo anādariyaṃ paṭicca tahaṃ tahaṃ olokento antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk7:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk7:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk7:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk7:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk7:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk7:1.11": "āpadāsu,",
  "pli-tv-bu-vb-sk7:1.12": "ummattakassa,",
  "pli-tv-bu-vb-sk7:1.13": "ādikammikassāti.",
  "pli-tv-bu-vb-sk7:1.14": "Sattamasikkhāpadaṃ niṭṭhitaṃ."
}