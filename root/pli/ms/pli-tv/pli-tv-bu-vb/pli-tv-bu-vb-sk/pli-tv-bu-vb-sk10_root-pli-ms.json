{
  "pli-tv-bu-vb-sk10:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk10:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk10:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk10:0.4": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk10:0.5": "10. Dutiyaukkhittakasikkhāpada",
  "pli-tv-bu-vb-sk10:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk10:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū ukkhittakāya antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk10:1.3": "“Na ukkhittakāya antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk10:1.4": "Na ukkhittakāya antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk10:1.5": "Yo anādariyaṃ paṭicca ekato vā ubhato vā ukkhipitvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk10:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk10:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk10:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk10:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk10:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk10:1.11": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk10:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk10:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk10:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk10:1.15": "Dasamasikkhāpadaṃ niṭṭhitaṃ.",
  "pli-tv-bu-vb-sk10:1.16": "Parimaṇḍalavaggo paṭhamo."
}