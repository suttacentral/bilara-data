{
  "pli-tv-bu-vb-sk31:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk31:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk31:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk31:0.4": "Sakkaccavagga",
  "pli-tv-bu-vb-sk31:0.5": "31. Sakkaccasikkhāpada",
  "pli-tv-bu-vb-sk31:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk31:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū asakkaccaṃ piṇḍapātaṃ bhuñjanti abhuñjitukāmā viya …",
  "pli-tv-bu-vb-sk31:1.3": "“Sakkaccaṃ piṇḍapātaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk31:1.4": "Sakkaccaṃ piṇḍapāto bhuñjitabbo.",
  "pli-tv-bu-vb-sk31:1.5": "Yo anādariyaṃ paṭicca asakkaccaṃ piṇḍapātaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk31:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk31:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk31:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk31:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk31:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk31:1.11": "āpadāsu,",
  "pli-tv-bu-vb-sk31:1.12": "ummattakassa,",
  "pli-tv-bu-vb-sk31:1.13": "ādikammikassāti.",
  "pli-tv-bu-vb-sk31:1.14": "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
}