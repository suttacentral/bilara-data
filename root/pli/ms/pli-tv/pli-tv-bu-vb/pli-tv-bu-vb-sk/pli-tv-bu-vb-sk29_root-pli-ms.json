{
  "pli-tv-bu-vb-sk29:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk29:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk29:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk29:0.4": "Khambhakatavagga",
  "pli-tv-bu-vb-sk29:0.5": "29. Samasūpakapaṭiggahaṇasikkhāpada",
  "pli-tv-bu-vb-sk29:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk29:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū piṇḍapātaṃ paṭiggaṇhantā sūpaññeva bahuṃ paṭiggaṇhanti …",
  "pli-tv-bu-vb-sk29:1.3": "“Samasūpakaṃ piṇḍapātaṃ paṭiggahessāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk29:1.4": "Sūpo nāma",
  "pli-tv-bu-vb-sk29:1.5": "dve sūpā— muggasūpo, māsasūpo hatthahāriyo.",
  "pli-tv-bu-vb-sk29:1.6": "Samasūpako piṇḍapāto paṭiggahetabbo.",
  "pli-tv-bu-vb-sk29:1.7": "Yo anādariyaṃ paṭicca sūpaññeva bahuṃ paṭiggaṇhāti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk29:1.8": "Anāpatti—",
  "pli-tv-bu-vb-sk29:1.9": "asañcicca,",
  "pli-tv-bu-vb-sk29:1.10": "assatiyā,",
  "pli-tv-bu-vb-sk29:1.11": "ajānantassa,",
  "pli-tv-bu-vb-sk29:1.12": "gilānassa,",
  "pli-tv-bu-vb-sk29:1.13": "rasarase,",
  "pli-tv-bu-vb-sk29:1.14": "ñātakānaṃ,",
  "pli-tv-bu-vb-sk29:1.15": "pavāritānaṃ,",
  "pli-tv-bu-vb-sk29:1.16": "aññassatthāya,",
  "pli-tv-bu-vb-sk29:1.17": "attano dhanena,",
  "pli-tv-bu-vb-sk29:1.18": "āpadāsu,",
  "pli-tv-bu-vb-sk29:1.19": "ummattakassa,",
  "pli-tv-bu-vb-sk29:1.20": "ādikammikassāti.",
  "pli-tv-bu-vb-sk29:1.21": "Navamasikkhāpadaṃ niṭṭhitaṃ."
}