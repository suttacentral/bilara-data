{
  "pli-tv-bu-vb-sk44:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk44:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk44:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk44:0.4": "Kabaḷavagga",
  "pli-tv-bu-vb-sk44:0.5": "44. Piṇḍukkhepakasikkhāpada",
  "pli-tv-bu-vb-sk44:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk44:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū piṇḍukkhepakaṃ bhuñjanti …pe…",
  "pli-tv-bu-vb-sk44:1.3": "“Na piṇḍukkhepakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk44:1.4": "Na piṇḍukkhepakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk44:1.5": "Yo anādariyaṃ paṭicca piṇḍukkhepakaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk44:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk44:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk44:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk44:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk44:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk44:1.11": "khajjake,",
  "pli-tv-bu-vb-sk44:1.12": "phalāphale,",
  "pli-tv-bu-vb-sk44:1.13": "āpadāsu,",
  "pli-tv-bu-vb-sk44:1.14": "ummattakassa,",
  "pli-tv-bu-vb-sk44:1.15": "ādikammikassāti.",
  "pli-tv-bu-vb-sk44:1.16": "Catutthasikkhāpadaṃ niṭṭhitaṃ."
}