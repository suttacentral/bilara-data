{
  "pli-tv-bu-vb-sk11:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk11:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk11:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk11:0.4": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk11:0.5": "11. Ujjagghikasikkhāpada",
  "pli-tv-bu-vb-sk11:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk11:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū mahāhasitaṃ hasantā antaraghare gacchanti …",
  "pli-tv-bu-vb-sk11:1.3": "“Na ujjagghikāya antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk11:1.4": "Na ujjagghikāya antaraghare gantabbaṃ.",
  "pli-tv-bu-vb-sk11:1.5": "Yo anādariyaṃ paṭicca mahāhasitaṃ hasanto antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk11:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk11:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk11:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk11:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk11:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk11:1.11": "hasanīyasmiṃ vatthusmiṃ mihitamattaṃ karoti,",
  "pli-tv-bu-vb-sk11:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk11:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk11:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk11:1.15": "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
}