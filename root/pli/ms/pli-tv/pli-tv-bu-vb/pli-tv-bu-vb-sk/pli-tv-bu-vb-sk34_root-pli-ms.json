{
  "pli-tv-bu-vb-sk34:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk34:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk34:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk34:0.4": "Sakkaccavagga",
  "pli-tv-bu-vb-sk34:0.5": "34. Samasūpakasikkhāpada",
  "pli-tv-bu-vb-sk34:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk34:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū piṇḍapātaṃ bhuñjantā sūpaññeva bahuṃ bhuñjanti …",
  "pli-tv-bu-vb-sk34:1.3": "“Samasūpakaṃ piṇḍapātaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk34:1.4": "Sūpo nāma",
  "pli-tv-bu-vb-sk34:1.5": "dve sūpā—",
  "pli-tv-bu-vb-sk34:1.6": "muggasūpo, māsasūpo hatthahāriyo.",
  "pli-tv-bu-vb-sk34:1.7": "Samasūpako piṇḍapāto bhuñjitabbo.",
  "pli-tv-bu-vb-sk34:1.8": "Yo anādariyaṃ paṭicca sūpaññeva bahuṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk34:1.9": "Anāpatti—",
  "pli-tv-bu-vb-sk34:1.10": "asañcicca,",
  "pli-tv-bu-vb-sk34:1.11": "assatiyā,",
  "pli-tv-bu-vb-sk34:1.12": "ajānantassa,",
  "pli-tv-bu-vb-sk34:1.13": "gilānassa,",
  "pli-tv-bu-vb-sk34:1.14": "rasarase,",
  "pli-tv-bu-vb-sk34:1.15": "ñātakānaṃ,",
  "pli-tv-bu-vb-sk34:1.16": "pavāritānaṃ,",
  "pli-tv-bu-vb-sk34:1.17": "attano dhanena,",
  "pli-tv-bu-vb-sk34:1.18": "āpadāsu,",
  "pli-tv-bu-vb-sk34:1.19": "ummattakassa,",
  "pli-tv-bu-vb-sk34:1.20": "ādikammikassāti.",
  "pli-tv-bu-vb-sk34:1.21": "Catutthasikkhāpadaṃ niṭṭhitaṃ."
}