{
  "pli-tv-bu-vb-sk24:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk24:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk24:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk24:0.4": "Khambhakatavagga",
  "pli-tv-bu-vb-sk24:0.5": "24. Dutiyaoguṇṭhitasikkhāpada",
  "pli-tv-bu-vb-sk24:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk24:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū sasīsaṃ pārupitvā antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk24:1.3": "“Na oguṇṭhito antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk24:1.4": "Na oguṇṭhitena antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk24:1.5": "Yo anādariyaṃ paṭicca sasīsaṃ pārupitvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk24:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk24:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk24:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk24:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk24:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk24:1.11": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk24:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk24:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk24:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk24:1.15": "Catutthasikkhāpadaṃ niṭṭhitaṃ."
}