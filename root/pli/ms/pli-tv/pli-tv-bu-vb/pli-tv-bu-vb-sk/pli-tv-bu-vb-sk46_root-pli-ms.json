{
  "pli-tv-bu-vb-sk46:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk46:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk46:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk46:0.4": "Kabaḷavagga",
  "pli-tv-bu-vb-sk46:0.5": "46. Avagaṇḍakārakasikkhāpada",
  "pli-tv-bu-vb-sk46:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk46:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū avagaṇḍakārakaṃ bhuñjanti …pe…",
  "pli-tv-bu-vb-sk46:1.3": "“Na avagaṇḍakārakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk46:1.4": "Na avagaṇḍakārakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk46:1.5": "Yo anādariyaṃ paṭicca ekato vā ubhato vā gaṇḍaṃ katvā bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk46:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk46:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk46:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk46:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk46:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk46:1.11": "phalāphale,",
  "pli-tv-bu-vb-sk46:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk46:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk46:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk46:1.15": "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
}