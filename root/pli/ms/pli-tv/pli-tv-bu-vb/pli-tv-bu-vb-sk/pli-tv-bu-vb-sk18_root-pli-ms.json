{
  "pli-tv-bu-vb-sk18:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk18:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk18:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk18:0.4": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk18:0.5": "18. Dutiyabāhuppacālakasikkhāpada",
  "pli-tv-bu-vb-sk18:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk18:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū bāhuppacālakaṃ antaraghare nisīdanti bāhuṃ olambentā …",
  "pli-tv-bu-vb-sk18:1.3": "“Na bāhuppacālakaṃ antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk18:1.4": "Na bāhuppacālakaṃ antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk18:1.5": "Bāhuṃ paggahetvā nisīditabbaṃ.",
  "pli-tv-bu-vb-sk18:1.6": "Yo anādariyaṃ paṭicca bāhuppacālakaṃ antaraghare nisīdati bāhuṃ olambento, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk18:1.7": "Anāpatti—",
  "pli-tv-bu-vb-sk18:1.8": "asañcicca,",
  "pli-tv-bu-vb-sk18:1.9": "assatiyā,",
  "pli-tv-bu-vb-sk18:1.10": "ajānantassa,",
  "pli-tv-bu-vb-sk18:1.11": "gilānassa,",
  "pli-tv-bu-vb-sk18:1.12": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk18:1.13": "āpadāsu,",
  "pli-tv-bu-vb-sk18:1.14": "ummattakassa,",
  "pli-tv-bu-vb-sk18:1.15": "ādikammikassāti.",
  "pli-tv-bu-vb-sk18:1.16": "Aṭṭhamasikkhāpadaṃ niṭṭhitaṃ."
}