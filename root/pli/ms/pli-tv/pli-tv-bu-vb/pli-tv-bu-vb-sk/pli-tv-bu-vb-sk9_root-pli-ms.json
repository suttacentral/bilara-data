{
  "pli-tv-bu-vb-sk9:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk9:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk9:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk9:0.4": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk9:0.5": "9. Ukkhittakasikkhāpada",
  "pli-tv-bu-vb-sk9:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk9:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū ukkhittakāya antaraghare gacchanti …",
  "pli-tv-bu-vb-sk9:1.3": "“Na ukkhittakāya antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk9:1.4": "Na ukkhittakāya antaraghare gantabbaṃ.",
  "pli-tv-bu-vb-sk9:1.5": "Yo anādariyaṃ paṭicca ekato vā ubhato vā ukkhipitvā antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk9:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk9:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk9:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk9:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk9:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk9:1.11": "ummattakassa,",
  "pli-tv-bu-vb-sk9:1.12": "ādikammikassāti.",
  "pli-tv-bu-vb-sk9:1.13": "Navamasikkhāpadaṃ niṭṭhitaṃ."
}