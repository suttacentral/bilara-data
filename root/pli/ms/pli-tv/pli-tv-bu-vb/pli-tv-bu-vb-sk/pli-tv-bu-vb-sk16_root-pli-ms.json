{
  "pli-tv-bu-vb-sk16:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk16:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk16:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk16:0.4": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk16:0.5": "16. Dutiyakāyappacālakasikkhāpada",
  "pli-tv-bu-vb-sk16:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk16:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū kāyappacālakaṃ antaraghare nisīdanti, kāyaṃ olambentā …",
  "pli-tv-bu-vb-sk16:1.3": "“Na kāyappacālakaṃ antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk16:1.4": "Na kāyappacālakaṃ antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk16:1.5": "Kāyaṃ paggahetvā nisīditabbaṃ.",
  "pli-tv-bu-vb-sk16:1.6": "Yo anādariyaṃ paṭicca kāyappacālakaṃ antaraghare nisīdati kāyaṃ olambento, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk16:1.7": "Anāpatti—",
  "pli-tv-bu-vb-sk16:1.8": "asañcicca,",
  "pli-tv-bu-vb-sk16:1.9": "assatiyā,",
  "pli-tv-bu-vb-sk16:1.10": "ajānantassa,",
  "pli-tv-bu-vb-sk16:1.11": "gilānassa,",
  "pli-tv-bu-vb-sk16:1.12": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk16:1.13": "āpadāsu,",
  "pli-tv-bu-vb-sk16:1.14": "ummattakassa,",
  "pli-tv-bu-vb-sk16:1.15": "ādikammikassāti.",
  "pli-tv-bu-vb-sk16:1.16": "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
}