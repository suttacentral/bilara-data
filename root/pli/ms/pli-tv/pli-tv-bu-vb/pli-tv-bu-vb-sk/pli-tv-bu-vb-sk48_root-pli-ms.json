{
  "pli-tv-bu-vb-sk48:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk48:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk48:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk48:0.4": "Kabaḷavagga",
  "pli-tv-bu-vb-sk48:0.5": "48. Sitthāvakārakasikkhāpada",
  "pli-tv-bu-vb-sk48:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk48:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū sitthāvakārakaṃ bhuñjanti …pe…",
  "pli-tv-bu-vb-sk48:1.3": "“Na sitthāvakārakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk48:1.4": "Na sitthāvakārakaṃ bhuñjitabbaṃ.",
  "pli-tv-bu-vb-sk48:1.5": "Yo anādariyaṃ paṭicca sitthāvakārakaṃ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk48:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk48:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk48:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk48:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk48:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk48:1.11": "kacavaraṃ chaḍḍento sitthaṃ chaḍḍayati,",
  "pli-tv-bu-vb-sk48:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk48:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk48:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk48:1.15": "Aṭṭhamasikkhāpadaṃ niṭṭhitaṃ."
}