{
  "pli-tv-bu-vb-sk30:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk30:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk30:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk30:0.4": "Khambhakatavagga",
  "pli-tv-bu-vb-sk30:0.5": "30. Samatittikasikkhāpada",
  "pli-tv-bu-vb-sk30:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk30:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū thūpīkataṃ piṇḍapātaṃ paṭiggaṇhanti …",
  "pli-tv-bu-vb-sk30:1.3": "“Samatittikaṃ piṇḍapātaṃ paṭiggahessāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk30:1.4": "Samatittiko piṇḍapāto paṭiggahetabbo.",
  "pli-tv-bu-vb-sk30:1.5": "Yo anādariyaṃ paṭicca thūpīkataṃ piṇḍapātaṃ paṭiggaṇhāti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk30:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk30:1.7": "asañcicca, assatiyā, ajānantassa, āpadāsu,",
  "pli-tv-bu-vb-sk30:1.8": "ummattakassa,",
  "pli-tv-bu-vb-sk30:1.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk30:1.10": "Dasamasikkhāpadaṃ niṭṭhitaṃ.",
  "pli-tv-bu-vb-sk30:1.11": "Khambhakatavaggo tatiyo."
}