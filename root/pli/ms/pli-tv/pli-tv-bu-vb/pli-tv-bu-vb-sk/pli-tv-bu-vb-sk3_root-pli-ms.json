{
  "pli-tv-bu-vb-sk3:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk3:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk3:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk3:0.4": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk3:0.5": "3. Suppaṭicchannasikkhāpada",
  "pli-tv-bu-vb-sk3:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk3:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū kāyaṃ vivaritvā antaraghare gacchanti …",
  "pli-tv-bu-vb-sk3:1.3": "“Suppaṭicchanno antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk3:1.4": "Suppaṭicchannena antaraghare gantabbaṃ.",
  "pli-tv-bu-vb-sk3:1.5": "Yo anādariyaṃ paṭicca kāyaṃ vivaritvā antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk3:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk3:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk3:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk3:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk3:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk3:1.11": "āpadāsu,",
  "pli-tv-bu-vb-sk3:1.12": "ummattakassa,",
  "pli-tv-bu-vb-sk3:1.13": "ādikammikassāti.",
  "pli-tv-bu-vb-sk3:1.14": "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
}