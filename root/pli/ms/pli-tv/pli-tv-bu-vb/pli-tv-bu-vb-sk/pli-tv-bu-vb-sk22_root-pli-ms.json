{
  "pli-tv-bu-vb-sk22:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk22:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk22:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk22:0.4": "Khambhakatavagga",
  "pli-tv-bu-vb-sk22:0.5": "22. Dutiyakhambhakatasikkhāpada",
  "pli-tv-bu-vb-sk22:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk22:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū khambhakatā antaraghare nisīdanti …",
  "pli-tv-bu-vb-sk22:1.3": "“Na khambhakato antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk22:1.4": "Na khambhakatena antaraghare nisīditabbaṃ.",
  "pli-tv-bu-vb-sk22:1.5": "Yo anādariyaṃ paṭicca ekato vā ubhato vā khambhaṃ katvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk22:1.6": "Anāpatti—",
  "pli-tv-bu-vb-sk22:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk22:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk22:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk22:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk22:1.11": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk22:1.12": "āpadāsu,",
  "pli-tv-bu-vb-sk22:1.13": "ummattakassa,",
  "pli-tv-bu-vb-sk22:1.14": "ādikammikassāti.",
  "pli-tv-bu-vb-sk22:1.15": "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
}