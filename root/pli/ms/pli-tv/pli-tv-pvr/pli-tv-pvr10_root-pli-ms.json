{
  "pli-tv-pvr10:0.1": "Parivāra",
  "pli-tv-pvr10:0.2": "Gāthāsaṅgaṇika",
  "pli-tv-pvr10:0.3": "1. Sattanagaresupaññattasikkhāpada",
  "pli-tv-pvr10:1.1": "“Ekaṃsaṃ cīvaraṃ katvā,",
  "pli-tv-pvr10:1.2": "paggaṇhitvāna añjaliṃ;",
  "pli-tv-pvr10:1.3": "Āsīsamānarūpova,",
  "pli-tv-pvr10:1.4": "kissa tvaṃ idha māgato”.",
  "pli-tv-pvr10:2.1": "“Dvīsu vinayesu ye paññattā,",
  "pli-tv-pvr10:2.2": "Uddesaṃ āgacchanti uposathesu;",
  "pli-tv-pvr10:2.3": "Kati te sikkhāpadā honti,",
  "pli-tv-pvr10:2.4": "Katisu nagaresu paññattā”.",
  "pli-tv-pvr10:3.1": "“Bhaddako te ummaṅgo,",
  "pli-tv-pvr10:3.2": "yoniso paripucchasi;",
  "pli-tv-pvr10:3.3": "Taggha te ahamakkhissaṃ,",
  "pli-tv-pvr10:3.4": "yathāsi kusalo tathā”.",
  "pli-tv-pvr10:4.1": "“Dvīsu vinayesu ye paññattā,",
  "pli-tv-pvr10:4.2": "Uddesaṃ āgacchanti uposathesu;",
  "pli-tv-pvr10:4.3": "Aḍḍhuḍḍhasatāni te honti,",
  "pli-tv-pvr10:4.4": "Sattasu nagaresu paññattā”.",
  "pli-tv-pvr10:5.1": "“Katamesu sattasu nagaresu paññattā,",
  "pli-tv-pvr10:5.2": "Iṅgha me tvaṃ byākara naṃ;",
  "pli-tv-pvr10:5.3": "Taṃ vacanapathaṃ nisāmayitvā,",
  "pli-tv-pvr10:5.4": "Paṭipajjema hitāya no siyā”.",
  "pli-tv-pvr10:6.1": "“Vesāliyaṃ rājagahe,",
  "pli-tv-pvr10:6.2": "sāvatthiyañca āḷaviyaṃ;",
  "pli-tv-pvr10:6.3": "Kosambiyañca sakkesu,",
  "pli-tv-pvr10:6.4": "bhaggesu ceva paññattā”.",
  "pli-tv-pvr10:7.1": "“Kati vesāliyaṃ paññattā,",
  "pli-tv-pvr10:7.2": "kati rājagahe katā;",
  "pli-tv-pvr10:7.3": "Sāvatthiyaṃ kati honti,",
  "pli-tv-pvr10:7.4": "kati āḷaviyaṃ katā”.",
  "pli-tv-pvr10:8.1": "“Kati kosambiyaṃ paññattā,",
  "pli-tv-pvr10:8.2": "kati sakkesu vuccanti;",
  "pli-tv-pvr10:8.3": "Kati bhaggesu paññattā,",
  "pli-tv-pvr10:8.4": "taṃ me akkhāhi pucchito”.",
  "pli-tv-pvr10:9.1": "“Dasa vesāliyaṃ paññattā,",
  "pli-tv-pvr10:9.2": "Ekavīsa rājagahe katā;",
  "pli-tv-pvr10:9.3": "Chaūna tīṇisatāni,",
  "pli-tv-pvr10:9.4": "Sabbe sāvatthiyaṃ katā.",
  "pli-tv-pvr10:10.1": "Cha āḷaviyaṃ paññattā,",
  "pli-tv-pvr10:10.2": "aṭṭha kosambiyaṃ katā;",
  "pli-tv-pvr10:10.3": "Aṭṭha sakkesu vuccanti,",
  "pli-tv-pvr10:10.4": "tayo bhaggesu paññattā.",
  "pli-tv-pvr10:11.1": "Ye vesāliyaṃ paññattā,",
  "pli-tv-pvr10:11.2": "te suṇohi yathātathaṃ;",
  "pli-tv-pvr10:11.3": "Methunaviggahuttari,",
  "pli-tv-pvr10:11.4": "atirekañca kāḷakaṃ.",
  "pli-tv-pvr10:12.1": "Bhūtaṃ paramparabhattaṃ,",
  "pli-tv-pvr10:12.2": "dantaponena acelako;",
  "pli-tv-pvr10:12.3": "Bhikkhunīsu ca akkoso,",
  "pli-tv-pvr10:12.4": "dasete vesāliyaṃ katā.",
  "pli-tv-pvr10:13.1": "Ye rājagahe paññattā,",
  "pli-tv-pvr10:13.2": "Te suṇohi yathātathaṃ;",
  "pli-tv-pvr10:13.3": "Adinnādānaṃ rājagahe,",
  "pli-tv-pvr10:13.4": "Dve anuddhaṃsanā dvepi ca bhedā.",
  "pli-tv-pvr10:14.1": "Antaravāsakaṃ rūpiyaṃ suttaṃ,",
  "pli-tv-pvr10:14.2": "Ujjhāpanena ca pācitapiṇḍaṃ;",
  "pli-tv-pvr10:14.3": "Gaṇabhojanaṃ vikāle ca,",
  "pli-tv-pvr10:14.4": "Cārittaṃ nahānaṃ ūnavīsati.",
  "pli-tv-pvr10:15.1": "Cīvaraṃ datvā vosāsanti,",
  "pli-tv-pvr10:15.2": "Ete rājagahe katā;",
  "pli-tv-pvr10:15.3": "Giraggacariyā tattheva,",
  "pli-tv-pvr10:15.4": "Chandadānena ekavīsati.",
  "pli-tv-pvr10:16.1": "Ye sāvatthiyaṃ paññattā,",
  "pli-tv-pvr10:16.2": "Te suṇohi yathātathaṃ;",
  "pli-tv-pvr10:16.3": "Pārājikāni cattāri,",
  "pli-tv-pvr10:16.4": "Saṃghādisesā bhavanti soḷasa.",
  "pli-tv-pvr10:17.1": "Aniyatā ca dve honti,",
  "pli-tv-pvr10:17.2": "nissaggiyā catuvīsati;",
  "pli-tv-pvr10:17.3": "Chapaññāsasatañceva,",
  "pli-tv-pvr10:17.4": "khuddakāni pavuccanti.",
  "pli-tv-pvr10:18.1": "Dasayeva ca gārayhā,",
  "pli-tv-pvr10:18.2": "dvesattati ca sekhiyā;",
  "pli-tv-pvr10:18.3": "Chaūna tīṇisatāni,",
  "pli-tv-pvr10:18.4": "sabbe sāvatthiyaṃ katā.",
  "pli-tv-pvr10:19.1": "Ye āḷaviyaṃ paññattā,",
  "pli-tv-pvr10:19.2": "te suṇohi yathātathaṃ;",
  "pli-tv-pvr10:19.3": "Kuṭikosiyaseyyā ca,",
  "pli-tv-pvr10:19.4": "khaṇane gaccha devate;",
  "pli-tv-pvr10:19.5": "Sappāṇakañca siñcanti,",
  "pli-tv-pvr10:19.6": "cha ete āḷaviyaṃ katā.",
  "pli-tv-pvr10:20.1": "Ye kosambiyaṃ paññattā,",
  "pli-tv-pvr10:20.2": "te suṇohi yathātathaṃ;",
  "pli-tv-pvr10:20.3": "Mahāvihāro dovacassaṃ,",
  "pli-tv-pvr10:20.4": "aññaṃ dvāraṃ surāya ca;",
  "pli-tv-pvr10:20.5": "Anādariyaṃ sahadhammo,",
  "pli-tv-pvr10:20.6": "payopānena aṭṭhamaṃ.",
  "pli-tv-pvr10:21.1": "Ye sakkesu paññattā,",
  "pli-tv-pvr10:21.2": "te suṇohi yathātathaṃ;",
  "pli-tv-pvr10:21.3": "Eḷakalomāni patto ca,",
  "pli-tv-pvr10:21.4": "ovādo ceva bhesajjaṃ.",
  "pli-tv-pvr10:22.1": "Sūci āraññiko ceva,",
  "pli-tv-pvr10:22.2": "aṭṭhete kāpilavatthave;",
  "pli-tv-pvr10:22.3": "Udakasuddhiyā ovādo,",
  "pli-tv-pvr10:22.4": "bhikkhunīsu pavuccanti.",
  "pli-tv-pvr10:23.1": "Ye bhaggesu paññattā,",
  "pli-tv-pvr10:23.2": "te suṇohi yathātathaṃ;",
  "pli-tv-pvr10:23.3": "Samādahitvā visibbenti,",
  "pli-tv-pvr10:23.4": "sāmisena sasitthakaṃ.",
  "pli-tv-pvr10:24.1": "Pārājikāni cattāri,",
  "pli-tv-pvr10:24.2": "saṃghādisesāni bhavanti;",
  "pli-tv-pvr10:24.3": "Satta ca nissaggiyāni,",
  "pli-tv-pvr10:24.4": "aṭṭha dvattiṃsa khuddakā.",
  "pli-tv-pvr10:25.1": "Dve gārayhā tayo sekkhā,",
  "pli-tv-pvr10:25.2": "chappaññāsa sikkhāpadā;",
  "pli-tv-pvr10:25.3": "Chasu nagaresu paññattā,",
  "pli-tv-pvr10:25.4": "buddhenādiccabandhunā.",
  "pli-tv-pvr10:26.1": "Chaūna tīṇisatāni,",
  "pli-tv-pvr10:26.2": "sabbe sāvatthiyaṃ katā;",
  "pli-tv-pvr10:26.3": "Kāruṇikena buddhena,",
  "pli-tv-pvr10:26.4": "gotamena yasassinā”.",
  "pli-tv-pvr10:26.5": "2. Catuvipatti",
  "pli-tv-pvr10:27.1": "“Yaṃ taṃ pucchimha akittayi no,",
  "pli-tv-pvr10:27.2": "Taṃ taṃ byākataṃ anaññathā;",
  "pli-tv-pvr10:27.3": "Aññaṃ taṃ pucchāmi tadiṅgha brūhi,",
  "pli-tv-pvr10:27.4": "Garuka lahukañcāpi sāvasesaṃ;",
  "pli-tv-pvr10:27.5": "Anavasesaṃ duṭṭhullañca aduṭṭhullaṃ,",
  "pli-tv-pvr10:27.6": "Ye ca yāvatatiyakā.",
  "pli-tv-pvr10:28.1": "Sādhāraṇaṃ asādhāraṇaṃ,",
  "pli-tv-pvr10:28.2": "Vibhattiyo ca yehi samathehi sammanti;",
  "pli-tv-pvr10:28.3": "Sabbānipetāni viyākarohi,",
  "pli-tv-pvr10:28.4": "Handa vākyaṃ suṇoma te”.",
  "pli-tv-pvr10:29.1": "“Ekatiṃsā ye garukā,",
  "pli-tv-pvr10:29.2": "Aṭṭhettha anavasesā;",
  "pli-tv-pvr10:29.3": "Ye garukā te duṭṭhullā,",
  "pli-tv-pvr10:29.4": "Ye duṭṭhullā sā sīlavipatti;",
  "pli-tv-pvr10:29.5": "Pārājikaṃ saṃghādiseso,",
  "pli-tv-pvr10:29.6": "‘Sīlavipattī’ti vuccati.",
  "pli-tv-pvr10:30.1": "Thullaccayaṃ pācittiyā,",
  "pli-tv-pvr10:30.2": "Pāṭidesanīyaṃ dukkaṭaṃ;",
  "pli-tv-pvr10:30.3": "Dubbhāsitaṃ yo cāyaṃ,",
  "pli-tv-pvr10:30.4": "Akkosati hasādhippāyo;",
  "pli-tv-pvr10:30.5": "Ayaṃ sā ācāravipattisammatā.",
  "pli-tv-pvr10:31.1": "Viparītadiṭṭhiṃ gaṇhanti,",
  "pli-tv-pvr10:31.2": "Asaddhammehi purakkhatā;",
  "pli-tv-pvr10:31.3": "Abbhācikkhanti sambuddhaṃ,",
  "pli-tv-pvr10:31.4": "Duppaññā mohapārutā;",
  "pli-tv-pvr10:31.5": "Ayaṃ sā diṭṭhivipattisammatā.",
  "pli-tv-pvr10:32.1": "Ājīvahetu ājīvakāraṇā pāpiccho icchāpakato asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapati, ājīvahetu ājīvakāraṇā sañcarittaṃ samāpajjati, ājīvahetu ājīvakāraṇā—",
  "pli-tv-pvr10:32.2": "‘yo te vihāre vasati, so bhikkhu arahā’ti bhaṇati, ājīvahetu ājīvakāraṇā bhikkhu paṇītabhojanāni attano atthāya viññāpetvā bhuñjati, ājīvahetu ājīvakāraṇā bhikkhunī paṇītabhojanāni attano atthāya viññāpetvā bhuñjati, ājīvahetu ājīvakāraṇā sūpaṃ vā odanaṃ vā agilāno attano atthāya viññāpetvā bhuñjati.",
  "pli-tv-pvr10:32.3": "Ayaṃ sā ājīvavipatti sammatā.",
  "pli-tv-pvr10:33.1": "Ekādasa yāvatatiyakā,",
  "pli-tv-pvr10:33.2": "Te suṇohi yathātathaṃ;",
  "pli-tv-pvr10:33.3": "Ukkhittānuvattikā,",
  "pli-tv-pvr10:33.4": "Aṭṭha yāvatatiyakā;",
  "pli-tv-pvr10:33.5": "Ariṭṭho caṇḍakāḷī ca,",
  "pli-tv-pvr10:33.6": "Ime te yāvatatiyakā”.",
  "pli-tv-pvr10:33.7": "3. Chedanakādi",
  "pli-tv-pvr10:34.1": "“Kati chedanakāni?",
  "pli-tv-pvr10:34.2": "Kati bhedanakāni?",
  "pli-tv-pvr10:34.3": "Kati uddālanakāni?",
  "pli-tv-pvr10:34.4": "Kati anaññapācittiyāni?",
  "pli-tv-pvr10:34.5": "Kati bhikkhusammutiyo?",
  "pli-tv-pvr10:34.6": "Kati sāmīciyo?",
  "pli-tv-pvr10:34.7": "Kati paramāni?",
  "pli-tv-pvr10:35.1": "Kati jānanti paññattā,",
  "pli-tv-pvr10:35.2": "buddhenādiccabandhunā”.",
  "pli-tv-pvr10:36.1": "“Cha chedanakāni.",
  "pli-tv-pvr10:36.2": "Ekaṃ bhedanakaṃ.",
  "pli-tv-pvr10:36.3": "Ekaṃ uddālanakaṃ.",
  "pli-tv-pvr10:36.4": "Cattāri anaññapācittiyāni.",
  "pli-tv-pvr10:36.5": "Catasso bhikkhusammutiyo.",
  "pli-tv-pvr10:36.6": "Satta sāmīciyo.",
  "pli-tv-pvr10:36.7": "Cuddasa paramāni.",
  "pli-tv-pvr10:37.1": "Sodasa jānanti paññattā,",
  "pli-tv-pvr10:37.2": "buddhenādiccabandhunā”.",
  "pli-tv-pvr10:37.3": "4. Asādhāraṇādi",
  "pli-tv-pvr10:38.1": "“Vīsaṃ dve satāni,",
  "pli-tv-pvr10:38.2": "Bhikkhūnaṃ sikkhāpadāni;",
  "pli-tv-pvr10:38.3": "Uddesaṃ āgacchanti uposathesu,",
  "pli-tv-pvr10:38.4": "Tīṇi satāni cattāri;",
  "pli-tv-pvr10:38.5": "Bhikkhunīnaṃ sikkhāpadāni,",
  "pli-tv-pvr10:38.6": "Uddesaṃ āgacchanti uposathesu.",
  "pli-tv-pvr10:39.1": "Chacattārīsā bhikkhūnaṃ,",
  "pli-tv-pvr10:39.2": "bhikkhunīhi asādhāraṇā;",
  "pli-tv-pvr10:39.3": "Sataṃ tiṃsā ca bhikkhunīnaṃ,",
  "pli-tv-pvr10:39.4": "bhikkhūhi asādhāraṇā.",
  "pli-tv-pvr10:40.1": "Sataṃ sattati chacceva,",
  "pli-tv-pvr10:40.2": "ubhinnaṃ asādhāraṇā;",
  "pli-tv-pvr10:40.3": "Sataṃ sattati cattāri,",
  "pli-tv-pvr10:40.4": "ubhinnaṃ samasikkhatā.",
  "pli-tv-pvr10:41.1": "Vīsaṃ dve satāni,",
  "pli-tv-pvr10:41.2": "Bhikkhūnaṃ sikkhāpadāni;",
  "pli-tv-pvr10:41.3": "Uddesaṃ āgacchanti uposathesu,",
  "pli-tv-pvr10:41.4": "Te suṇohi yathātathaṃ.",
  "pli-tv-pvr10:42.1": "Pārājikāni cattāri,",
  "pli-tv-pvr10:42.2": "Saṃghādisesāni bhavanti terasa;",
  "pli-tv-pvr10:42.3": "Aniyatā dve honti.",
  "pli-tv-pvr10:43.1": "Nissaggiyāni tiṃseva,",
  "pli-tv-pvr10:43.2": "dvenavuti ca khuddakā;",
  "pli-tv-pvr10:43.3": "Cattāro pāṭidesanīyā,",
  "pli-tv-pvr10:43.4": "pañcasattati sekhiyā.",
  "pli-tv-pvr10:44.1": "Vīsaṃ dve satāni cime honti,",
  "pli-tv-pvr10:44.2": "Bhikkhūnaṃ sikkhāpadāni;",
  "pli-tv-pvr10:44.3": "Uddesaṃ āgacchanti uposathesu.",
  "pli-tv-pvr10:45.1": "Tīṇi satāni cattāri,",
  "pli-tv-pvr10:45.2": "Bhikkhunīnaṃ sikkhāpadāni;",
  "pli-tv-pvr10:45.3": "Uddesaṃ āgacchanti uposathesu,",
  "pli-tv-pvr10:45.4": "Te suṇohi yathātathaṃ.",
  "pli-tv-pvr10:46.1": "Pārājikāni aṭṭha,",
  "pli-tv-pvr10:46.2": "Saṃghādisesāni bhavanti sattarasa;",
  "pli-tv-pvr10:46.3": "Nissaggiyāni tiṃseva,",
  "pli-tv-pvr10:46.4": "Sataṃ saṭṭhi cha ceva;",
  "pli-tv-pvr10:46.5": "Khuddakāni pavuccanti.",
  "pli-tv-pvr10:47.1": "Aṭṭha pāṭidesanīyā,",
  "pli-tv-pvr10:47.2": "Pañcasattati sekhiyā;",
  "pli-tv-pvr10:47.3": "Tīṇi satāni cattāri cime honti,",
  "pli-tv-pvr10:47.4": "Bhikkhunīnaṃ sikkhāpadāni;",
  "pli-tv-pvr10:47.5": "Uddesaṃ āgacchanti uposathesu.",
  "pli-tv-pvr10:48.1": "Chacattārīsā bhikkhūnaṃ,",
  "pli-tv-pvr10:48.2": "bhikkhunīhi asādhāraṇā;",
  "pli-tv-pvr10:48.3": "Te suṇohi yathātathaṃ.",
  "pli-tv-pvr10:49.1": "Cha saṃghādisesā,",
  "pli-tv-pvr10:49.2": "dve aniyatehi aṭṭha;",
  "pli-tv-pvr10:49.3": "Nissaggiyāni dvādasa,",
  "pli-tv-pvr10:49.4": "tehi te honti vīsati.",
  "pli-tv-pvr10:50.1": "Dvevīsati khuddakā,",
  "pli-tv-pvr10:50.2": "Caturo pāṭidesanīyā;",
  "pli-tv-pvr10:50.3": "Chacattārīsā cime honti,",
  "pli-tv-pvr10:50.4": "Bhikkhūnaṃ bhikkhunīhi asādhāraṇā.",
  "pli-tv-pvr10:51.1": "Sataṃ tiṃsā ca bhikkhunīnaṃ,",
  "pli-tv-pvr10:51.2": "bhikkhūhi asādhāraṇā;",
  "pli-tv-pvr10:51.3": "Te suṇohi yathātathaṃ.",
  "pli-tv-pvr10:52.1": "Pārājikāni cattāri,",
  "pli-tv-pvr10:52.2": "saṃghamhā dasa nissare;",
  "pli-tv-pvr10:52.3": "Nissaggiyāni dvādasa,",
  "pli-tv-pvr10:52.4": "channavuti ca khuddakā;",
  "pli-tv-pvr10:52.5": "Aṭṭha pāṭidesanīyā.",
  "pli-tv-pvr10:53.1": "Sataṃ tiṃsā cime honti,",
  "pli-tv-pvr10:53.2": "Bhikkhunīnaṃ bhikkhūhi asādhāraṇā;",
  "pli-tv-pvr10:53.3": "Sataṃ sattati chacceva,",
  "pli-tv-pvr10:53.4": "Ubhinnaṃ asādhāraṇā;",
  "pli-tv-pvr10:53.5": "Te suṇohi yathātathaṃ.",
  "pli-tv-pvr10:54.1": "Pārājikāni cattāri,",
  "pli-tv-pvr10:54.2": "Saṃghādisesāni bhavanti soḷasa;",
  "pli-tv-pvr10:54.3": "Aniyatā dve honti,",
  "pli-tv-pvr10:54.4": "Nissaggiyāni catuvīsati;",
  "pli-tv-pvr10:54.5": "Sataṃ aṭṭhārasā ceva,",
  "pli-tv-pvr10:54.6": "Khuddakāni pavuccanti;",
  "pli-tv-pvr10:54.7": "Dvādasa pāṭidesanīyā.",
  "pli-tv-pvr10:55.1": "Sataṃ sattati chaccevime honti,",
  "pli-tv-pvr10:55.2": "Ubhinnaṃ asādhāraṇā;",
  "pli-tv-pvr10:55.3": "Sataṃ sattati cattāri,",
  "pli-tv-pvr10:55.4": "Ubhinnaṃ samasikkhatā;",
  "pli-tv-pvr10:55.5": "Te suṇohi yathātathaṃ.",
  "pli-tv-pvr10:56.1": "Pārājikāni cattāri,",
  "pli-tv-pvr10:56.2": "Saṃghādisesāni bhavanti satta;",
  "pli-tv-pvr10:56.3": "Nissaggiyāni aṭṭhārasa,",
  "pli-tv-pvr10:56.4": "Samasattati khuddakā;",
  "pli-tv-pvr10:56.5": "Pañcasattati sekhiyāni.",
  "pli-tv-pvr10:57.1": "Sataṃ sattati cattāri cime honti,",
  "pli-tv-pvr10:57.2": "Ubhinnaṃ samasikkhatā;",
  "pli-tv-pvr10:57.3": "Aṭṭheva pārājikā ye durāsadā,",
  "pli-tv-pvr10:57.4": "Tālavatthusamūpamā.",
  "pli-tv-pvr10:58.1": "Paṇḍupalāso puthusilā,",
  "pli-tv-pvr10:58.2": "sīsacchinnova so naro;",
  "pli-tv-pvr10:58.3": "Tālova matthakacchinno,",
  "pli-tv-pvr10:58.4": "aviruḷhī bhavanti te.",
  "pli-tv-pvr10:59.1": "Tevīsati saṃghādisesā,",
  "pli-tv-pvr10:59.2": "Dve aniyatā;",
  "pli-tv-pvr10:59.3": "Dve cattārīsa nissaggiyā,",
  "pli-tv-pvr10:59.4": "Aṭṭhāsītisataṃ pācittiyā;",
  "pli-tv-pvr10:59.5": "Dvādasa pāṭidesanīyā.",
  "pli-tv-pvr10:60.1": "Pañcasattati sekhiyā,",
  "pli-tv-pvr10:60.2": "tīhi samathehi sammanti;",
  "pli-tv-pvr10:60.3": "Sammukhā ca paṭiññāya,",
  "pli-tv-pvr10:60.4": "tiṇavatthārakena ca.",
  "pli-tv-pvr10:61.1": "Dve uposathā dve pavāraṇā,",
  "pli-tv-pvr10:61.2": "Cattāri kammāni jinena desitā;",
  "pli-tv-pvr10:61.3": "Pañceva uddesā caturo bhavanti,",
  "pli-tv-pvr10:61.4": "Anaññathā āpattikkhandhā ca bhavanti satta.",
  "pli-tv-pvr10:62.1": "Adhikaraṇāni cattāri,",
  "pli-tv-pvr10:62.2": "Sattahi samathehi sammanti;",
  "pli-tv-pvr10:62.3": "Dvīhi catūhi tīhi,",
  "pli-tv-pvr10:62.4": "Kiccaṃ ekena sammati”.",
  "pli-tv-pvr10:62.5": "5. Pārājikādiāpatti",
  "pli-tv-pvr10:63.1": "“Pārājikan”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:63.2": "taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:63.3": "Cuto paraddho bhaṭṭho ca,",
  "pli-tv-pvr10:63.4": "saddhammā hi niraṅkato;",
  "pli-tv-pvr10:63.5": "Saṃvāsopi tahiṃ natthi,",
  "pli-tv-pvr10:63.6": "tenetaṃ iti vuccati.",
  "pli-tv-pvr10:64.1": "“Saṃghādiseso”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:64.2": "taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:64.3": "Saṃghova deti parivāsaṃ,",
  "pli-tv-pvr10:64.4": "mūlāya paṭikassati;",
  "pli-tv-pvr10:64.5": "Mānattaṃ deti abbheti,",
  "pli-tv-pvr10:64.6": "tenetaṃ iti vuccati.",
  "pli-tv-pvr10:65.1": "“Aniyato”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:65.2": "taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:65.3": "Aniyato na niyato,",
  "pli-tv-pvr10:65.4": "anekaṃsikataṃ padaṃ;",
  "pli-tv-pvr10:65.5": "Tiṇṇamaññataraṃ ṭhānaṃ,",
  "pli-tv-pvr10:65.6": "“aniyato”ti pavuccati.",
  "pli-tv-pvr10:66.1": "“Thullaccayan”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:66.2": "taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:66.3": "Ekassa mūle yo deseti,",
  "pli-tv-pvr10:66.4": "yo ca taṃ paṭigaṇhati;",
  "pli-tv-pvr10:66.5": "Accayo tena samo natthi,",
  "pli-tv-pvr10:66.6": "tenetaṃ iti vuccati.",
  "pli-tv-pvr10:67.1": "“Nissaggiyan”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:67.2": "taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:67.3": "Saṃghamajjhe gaṇamajjhe,",
  "pli-tv-pvr10:67.4": "ekasseva ca ekato;",
  "pli-tv-pvr10:67.5": "Nissajjitvāna deseti,",
  "pli-tv-pvr10:67.6": "tenetaṃ iti vuccati.",
  "pli-tv-pvr10:68.1": "“Pācittiyan”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:68.2": "Taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:68.3": "Pāteti kusalaṃ dhammaṃ,",
  "pli-tv-pvr10:68.4": "Ariyamaggaṃ aparajjhati;",
  "pli-tv-pvr10:68.5": "Cittasammohanaṭṭhānaṃ,",
  "pli-tv-pvr10:68.6": "Tenetaṃ iti vuccati.",
  "pli-tv-pvr10:69.1": "“Pāṭidesanīyan”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:69.2": "Taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:69.3": "Bhikkhu aññātako santo,",
  "pli-tv-pvr10:69.4": "Kicchā laddhāya bhojanaṃ;",
  "pli-tv-pvr10:69.5": "Sāmaṃ gahetvā bhuñjeyya,",
  "pli-tv-pvr10:69.6": "“Gārayhan”ti pavuccati.",
  "pli-tv-pvr10:70.1": "Nimantanāsu bhuñjantā chandāya,",
  "pli-tv-pvr10:70.2": "Vosāsati tattha bhikkhuniṃ;",
  "pli-tv-pvr10:70.3": "Anivāretvā tahiṃ bhuñje,",
  "pli-tv-pvr10:70.4": "“Gārayhan”ti pavuccati.",
  "pli-tv-pvr10:71.1": "Saddhācittaṃ kulaṃ gantvā,",
  "pli-tv-pvr10:71.2": "appabhogaṃ anāḷiyaṃ;",
  "pli-tv-pvr10:71.3": "Agilāno tahiṃ bhuñje,",
  "pli-tv-pvr10:71.4": "“gārayhan”ti pavuccati.",
  "pli-tv-pvr10:72.1": "Yo ce araññe viharanto,",
  "pli-tv-pvr10:72.2": "sāsaṅke sabhayānake;",
  "pli-tv-pvr10:72.3": "Aviditaṃ tahiṃ bhuñje,",
  "pli-tv-pvr10:72.4": "“gārayhan”ti pavuccati.",
  "pli-tv-pvr10:73.1": "Bhikkhunī aññātikā santā,",
  "pli-tv-pvr10:73.2": "Yaṃ paresaṃ mamāyitaṃ;",
  "pli-tv-pvr10:73.3": "Sappi telaṃ madhuṃ phāṇitaṃ,",
  "pli-tv-pvr10:73.4": "Macchamaṃsaṃ atho khīraṃ;",
  "pli-tv-pvr10:73.5": "Dadhiṃ sayaṃ viññāpeyya bhikkhunī,",
  "pli-tv-pvr10:73.6": "Gārayhapattā sugatassa sāsane.",
  "pli-tv-pvr10:74.1": "“Dukkaṭan”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:74.2": "taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:74.3": "Aparaddhaṃ viraddhañca,",
  "pli-tv-pvr10:74.4": "khalitaṃ yañca dukkaṭaṃ.",
  "pli-tv-pvr10:75.1": "Yaṃ manusso kare pāpaṃ,",
  "pli-tv-pvr10:75.2": "āvi vā yadi vā raho;",
  "pli-tv-pvr10:75.3": "“Dukkaṭan”ti pavedenti,",
  "pli-tv-pvr10:75.4": "tenetaṃ iti vuccati.",
  "pli-tv-pvr10:76.1": "“Dubbhāsitan”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:76.2": "taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:76.3": "Dubbhāsitaṃ durābhaṭṭhaṃ,",
  "pli-tv-pvr10:76.4": "saṃkiliṭṭhañca yaṃ padaṃ;",
  "pli-tv-pvr10:76.5": "Yañca viññū garahanti,",
  "pli-tv-pvr10:76.6": "tenetaṃ iti vuccati.",
  "pli-tv-pvr10:77.1": "“Sekhiyan”ti yaṃ vuttaṃ,",
  "pli-tv-pvr10:77.2": "taṃ suṇohi yathātathaṃ;",
  "pli-tv-pvr10:77.3": "Sekkhassa sikkhamānassa,",
  "pli-tv-pvr10:77.4": "ujumaggānusārino.",
  "pli-tv-pvr10:78.1": "Ādi cetaṃ caraṇañca,",
  "pli-tv-pvr10:78.2": "mukhaṃ saññamasaṃvaro;",
  "pli-tv-pvr10:78.3": "Sikkhā etādisī natthi,",
  "pli-tv-pvr10:78.4": "tenetaṃ iti vuccati.",
  "pli-tv-pvr10:79.1": "Channamativassati,",
  "pli-tv-pvr10:79.2": "vivaṭaṃ nātivassati;",
  "pli-tv-pvr10:79.3": "Tasmā channaṃ vivaretha,",
  "pli-tv-pvr10:79.4": "evaṃ taṃ nātivassati.",
  "pli-tv-pvr10:80.1": "Gati migānaṃ pavanaṃ,",
  "pli-tv-pvr10:80.2": "Ākāso pakkhinaṃ gati;",
  "pli-tv-pvr10:80.3": "Vibhavo gati dhammānaṃ,",
  "pli-tv-pvr10:80.4": "Nibbānaṃ arahato gatīti.",
  "pli-tv-pvr10:81.1": "Gāthāsaṅgaṇikaṃ niṭṭhitaṃ.",
  "pli-tv-pvr10:82.1": "Tassuddānaṃ",
  "pli-tv-pvr10:83.1": "Sattanagaresu paññattā,",
  "pli-tv-pvr10:83.2": "vipatti caturopi ca;",
  "pli-tv-pvr10:83.3": "Bhikkhūnaṃ bhikkhunīnañca,",
  "pli-tv-pvr10:83.4": "sādhāraṇā asādhāraṇā;",
  "pli-tv-pvr10:83.5": "Sāsanaṃ anuggahāya,",
  "pli-tv-pvr10:83.6": "gāthāsaṅgaṇikaṃ idanti.",
  "pli-tv-pvr10:84.1": "Gāthāsaṅgaṇikaṃ niṭṭhitaṃ."
}