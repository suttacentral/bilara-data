{
  "pli-tv-bi-vb-pc62:0.1": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc62:0.2": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc62:0.3": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc62:0.4": "Gabbhinivagga",
  "pli-tv-bi-vb-pc62:0.5": "62. Pāyantisikkhāpada",
  "pli-tv-bi-vb-pc62:1.1": "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc62:1.2": "Tena kho pana samayena bhikkhuniyo pāyantiṃ vuṭṭhāpenti.",
  "pli-tv-bi-vb-pc62:1.3": "Sā piṇḍāya carati.",
  "pli-tv-bi-vb-pc62:1.4": "Manussā evamāhaṃsu—",
  "pli-tv-bi-vb-pc62:1.5": "“dethāyyāya bhikkhaṃ, sadutiyikā ayyā”ti.",
  "pli-tv-bi-vb-pc62:1.6": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc62:1.7": "“kathañhi nāma bhikkhuniyo pāyantiṃ vuṭṭhāpessantī”ti.",
  "pli-tv-bi-vb-pc62:1.8": "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ.",
  "pli-tv-bi-vb-pc62:1.9": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc62:1.10": "“kathañhi nāma bhikkhuniyo pāyantiṃ vuṭṭhāpessantī”ti …pe…",
  "pli-tv-bi-vb-pc62:1.11": "“saccaṃ kira, bhikkhave, bhikkhuniyo pāyantiṃ vuṭṭhāpentī”ti?",
  "pli-tv-bi-vb-pc62:1.12": "“Saccaṃ, bhagavā”ti.",
  "pli-tv-bi-vb-pc62:1.13": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc62:1.14": "kathañhi nāma, bhikkhave, bhikkhuniyo pāyantiṃ vuṭṭhāpessanti.",
  "pli-tv-bi-vb-pc62:1.15": "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc62:1.16": "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—",
  "pli-tv-bi-vb-pc62:1.17": "“Yā pana bhikkhunī pāyantiṃ vuṭṭhāpeyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc62:2.1": "Yā panāti",
  "pli-tv-bi-vb-pc62:2.2": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc62:2.3": "bhikkhunīti",
  "pli-tv-bi-vb-pc62:2.4": "…pe… ayaṃ imasmiṃ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc62:2.5": "Pāyantī nāma",
  "pli-tv-bi-vb-pc62:2.6": "mātā vā hoti dhāti vā.",
  "pli-tv-bi-vb-pc62:2.7": "Vuṭṭhāpeyyāti",
  "pli-tv-bi-vb-pc62:2.8": "upasampādeyya.",
  "pli-tv-bi-vb-pc62:2.9": "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.10": "Ñattiyā dukkaṭaṃ.",
  "pli-tv-bi-vb-pc62:2.11": "Dvīhi kammavācāhi dukkaṭā.",
  "pli-tv-bi-vb-pc62:2.12": "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc62:2.13": "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.14": "Pāyantiyā pāyantisaññā vuṭṭhāpeti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc62:2.15": "Pāyantiyā vematikā vuṭṭhāpeti, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.16": "Pāyantiyā apāyantisaññā vuṭṭhāpeti, anāpatti.",
  "pli-tv-bi-vb-pc62:2.17": "Apāyantiyā pāyantisaññā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.18": "Apāyantiyā vematikā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.19": "Apāyantiyā apāyantisaññā, anāpatti.",
  "pli-tv-bi-vb-pc62:2.20": "Anāpatti—",
  "pli-tv-bi-vb-pc62:2.21": "pāyantiṃ apāyantisaññā vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc62:2.22": "apāyantiṃ apāyantisaññā vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc62:2.23": "ummattikāya,",
  "pli-tv-bi-vb-pc62:2.24": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc62:2.25": "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
}