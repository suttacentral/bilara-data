{
  "pli-tv-bi-vb-as1-7:0.1": "Theravāda Vinaya",
  "pli-tv-bi-vb-as1-7:0.2": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-as1-7:0.3": "Adhikaraṇasamatha",
  "pli-tv-bi-vb-as1-7:1.1": "Ime kho panāyyāyo satta adhikaraṇasamathā dhammā uddesaṃ āgacchanti.",
  "pli-tv-bi-vb-as1-7:1.2": "Uppannuppannānaṃ adhikaraṇānaṃ samathāya vūpasamāya",
  "pli-tv-bi-vb-as1-7:1.3": "sammukhāvinayo dātabbo,",
  "pli-tv-bi-vb-as1-7:1.4": "sativinayo dātabbo,",
  "pli-tv-bi-vb-as1-7:1.5": "amūḷhavinayo dātabbo,",
  "pli-tv-bi-vb-as1-7:1.6": "paṭiññāya kāretabbaṃ,",
  "pli-tv-bi-vb-as1-7:1.7": "yebhuyyasikā,",
  "pli-tv-bi-vb-as1-7:1.8": "tassapāpiyasikā,",
  "pli-tv-bi-vb-as1-7:1.9": "tiṇavatthārakoti.",
  "pli-tv-bi-vb-as1-7:3.1": "Uddiṭṭhā kho, ayyāyo, satta adhikaraṇasamathā dhammā.",
  "pli-tv-bi-vb-as1-7:3.2": "Tatthāyyāyo pucchāmi—",
  "pli-tv-bi-vb-as1-7:3.3": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as1-7:3.4": "Dutiyampi pucchāmi—",
  "pli-tv-bi-vb-as1-7:3.5": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as1-7:3.6": "Tatiyampi pucchāmi—",
  "pli-tv-bi-vb-as1-7:3.7": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as1-7:3.8": "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṃ dhārayāmīti.",
  "pli-tv-bi-vb-as1-7:4.1": "Adhikaraṇasamathā niṭṭhitā.",
  "pli-tv-bi-vb-as1-7:5.1": "Uddiṭṭhaṃ kho, ayyāyo, nidānaṃ.",
  "pli-tv-bi-vb-as1-7:5.2": "Uddiṭṭhā aṭṭha pārājikā dhammā.",
  "pli-tv-bi-vb-as1-7:5.3": "Uddiṭṭhā sattarasa saṃghādisesā dhammā.",
  "pli-tv-bi-vb-as1-7:5.4": "Uddiṭṭhā tiṃsa nissaggiyā pācittiyā dhammā.",
  "pli-tv-bi-vb-as1-7:5.5": "Uddiṭṭhā chasaṭṭhisatā pācittiyā dhammā.",
  "pli-tv-bi-vb-as1-7:5.6": "Uddiṭṭhā aṭṭha pāṭidesanīyā dhammā.",
  "pli-tv-bi-vb-as1-7:5.7": "Uddiṭṭhā sekhiyā dhammā.",
  "pli-tv-bi-vb-as1-7:5.8": "Uddiṭṭhā satta adhikaraṇasamathā dhammā.",
  "pli-tv-bi-vb-as1-7:5.9": "Ettakaṃ tassa bhagavato suttāgataṃ suttapariyāpannaṃ anvaddhamāsaṃ uddesaṃ āgacchati.",
  "pli-tv-bi-vb-as1-7:5.10": "Tattha sabbāheva samaggāhi sammodamānāhi avivadamānāhi sikkhitabbanti.",
  "pli-tv-bi-vb-as1-7:6.1": "Bhikkhunivibhaṅgo niṭṭhito.",
  "pli-tv-bi-vb-as1-7:7.1": "Pācittiyapāḷi niṭṭhitā."
}