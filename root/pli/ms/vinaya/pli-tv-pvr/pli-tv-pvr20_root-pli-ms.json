{
  "pli-tv-pvr20:0.1": "Parivāra",
  "pli-tv-pvr20:0.2": "20. Sedamocanagāthā",
  "pli-tv-pvr20:0.3": "1. Avippavāsapañhā",
  "pli-tv-pvr20:1.1": "Asaṁvāso bhikkhūhi ca bhikkhunīhi ca,",
  "pli-tv-pvr20:1.2": "Sambhogo ekacco tahiṁ na labbhati;",
  "pli-tv-pvr20:1.3": "Avippavāsena anāpatti,",
  "pli-tv-pvr20:1.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:2.1": "Avissajjiyaṁ avebhaṅgiyaṁ,",
  "pli-tv-pvr20:2.2": "Pañca vuttā mahesinā;",
  "pli-tv-pvr20:2.3": "Vissajjantassa paribhuñjantassa anāpatti,",
  "pli-tv-pvr20:2.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:3.1": "Dasa puggale na vadāmi,",
  "pli-tv-pvr20:3.2": "Ekādasa vivajjiya;",
  "pli-tv-pvr20:3.3": "Vuḍḍhaṁ vandantassa āpatti,",
  "pli-tv-pvr20:3.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:4.1": "Na ukkhittako na ca pana pārivāsiko,",
  "pli-tv-pvr20:4.2": "Na saṅghabhinno na ca pana pakkhasaṅkanto;",
  "pli-tv-pvr20:4.3": "Samānasaṁvāsakabhūmiyā ṭhito,",
  "pli-tv-pvr20:4.4": "Kathaṁ nu sikkhāya asādhāraṇo siyā;",
  "pli-tv-pvr20:4.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:5.1": "Upeti dhammaṁ paripucchamāno,",
  "pli-tv-pvr20:5.2": "Kusalaṁ atthūpasañhitaṁ;",
  "pli-tv-pvr20:5.3": "Na jīvati na mato na nibbuto,",
  "pli-tv-pvr20:5.4": "Taṁ puggalaṁ katamaṁ vadanti buddhā;",
  "pli-tv-pvr20:5.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:6.1": "Ubbhakkhake na vadāmi,",
  "pli-tv-pvr20:6.2": "Adho nābhiṁ vivajjiya;",
  "pli-tv-pvr20:6.3": "Methunadhammapaccayā,",
  "pli-tv-pvr20:6.4": "Kathaṁ pārājiko siyā;",
  "pli-tv-pvr20:6.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:7.1": "Bhikkhu saññācikāya kuṭiṁ karoti,",
  "pli-tv-pvr20:7.2": "Adesitavatthukaṁ pamāṇātikkantaṁ;",
  "pli-tv-pvr20:7.3": "Sārambhaṁ aparikkamanaṁ anāpatti,",
  "pli-tv-pvr20:7.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:8.1": "Bhikkhu saññācikāya kuṭiṁ karoti,",
  "pli-tv-pvr20:8.2": "Desitavatthukaṁ pamāṇikaṁ;",
  "pli-tv-pvr20:8.3": "Anārambhaṁ saparikkamanaṁ āpatti,",
  "pli-tv-pvr20:8.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:9.1": "Na kāyikaṁ kiñci payogamācare,",
  "pli-tv-pvr20:9.2": "Na cāpi vācāya pare bhaṇeyya;",
  "pli-tv-pvr20:9.3": "Āpajjeyya garukaṁ chejjavatthuṁ,",
  "pli-tv-pvr20:9.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:10.1": "Na kāyikaṁ vācasikañca kiñci,",
  "pli-tv-pvr20:10.2": "Manasāpi santo na kareyya pāpaṁ;",
  "pli-tv-pvr20:10.3": "So nāsito kinti sunāsito bhave,",
  "pli-tv-pvr20:10.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:11.1": "Anālapanto manujena kenaci,",
  "pli-tv-pvr20:11.2": "Vācāgiraṁ no ca pare bhaṇeyya;",
  "pli-tv-pvr20:11.3": "Āpajjeyya vācasikaṁ na kāyikaṁ,",
  "pli-tv-pvr20:11.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:12.1": "Sikkhāpadā buddhavarena vaṇṇitā,",
  "pli-tv-pvr20:12.2": "Saṅghādisesā caturo bhaveyyuṁ;",
  "pli-tv-pvr20:12.3": "Āpajjeyya ekapayogena sabbe,",
  "pli-tv-pvr20:12.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:13.1": "Ubho ekato upasampannā,",
  "pli-tv-pvr20:13.2": "Ubhinnaṁ hatthato cīvaraṁ paṭiggaṇheyya;",
  "pli-tv-pvr20:13.3": "Siyā āpattiyo nānā,",
  "pli-tv-pvr20:13.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:14.1": "Caturo janā saṁvidhāya,",
  "pli-tv-pvr20:14.2": "Garubhaṇḍaṁ avāharuṁ;",
  "pli-tv-pvr20:14.3": "Tayo pārājikā eko na pārājiko,",
  "pli-tv-pvr20:14.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:15.0": "2. Pārājikādipañhā",
  "pli-tv-pvr20:15.1": "Itthī ca abbhantare siyā,",
  "pli-tv-pvr20:15.2": "Bhikkhu ca bahiddhā siyā;",
  "pli-tv-pvr20:15.3": "Chiddaṁ tasmiṁ ghare natthi,",
  "pli-tv-pvr20:15.4": "Methunadhammapaccayā;",
  "pli-tv-pvr20:15.5": "Kathaṁ pārājiko siyā,",
  "pli-tv-pvr20:15.6": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:16.1": "Telaṁ madhuṁ phāṇitañcāpi sappiṁ,",
  "pli-tv-pvr20:16.2": "Sāmaṁ gahetvāna nikkhipeyya;",
  "pli-tv-pvr20:16.3": "Avītivatte sattāhe,",
  "pli-tv-pvr20:16.4": "Sati paccaye paribhuñjantassa āpatti;",
  "pli-tv-pvr20:16.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:17.1": "Nissaggiyena āpatti,",
  "pli-tv-pvr20:17.2": "Suddhakena pācittiyaṁ;",
  "pli-tv-pvr20:17.3": "Āpajjantassa ekato,",
  "pli-tv-pvr20:17.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:18.1": "Bhikkhū siyā vīsatiyā samāgatā,",
  "pli-tv-pvr20:18.2": "Kammaṁ kareyyuṁ samaggasaññino;",
  "pli-tv-pvr20:18.3": "Bhikkhu siyā dvādasayojane ṭhito,",
  "pli-tv-pvr20:18.4": "Kammañca taṁ kuppeyya vaggapaccayā;",
  "pli-tv-pvr20:18.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:19.1": "Padavītihāramattena vācāya bhaṇitena ca,",
  "pli-tv-pvr20:19.2": "Sabbāni garukāni sappaṭikammāni;",
  "pli-tv-pvr20:19.3": "Catusaṭṭhi āpattiyo āpajjeyya ekato,",
  "pli-tv-pvr20:19.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:20.1": "Nivattho antaravāsakena,",
  "pli-tv-pvr20:20.2": "Diguṇaṁ saṅghāṭiṁ pāruto;",
  "pli-tv-pvr20:20.3": "Sabbāni tāni nissaggiyāni honti,",
  "pli-tv-pvr20:20.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:21.1": "Na cāpi ñatti na ca pana kammavācā,",
  "pli-tv-pvr20:21.2": "Na cehi bhikkhūti jino avoca;",
  "pli-tv-pvr20:21.3": "Saraṇagamanampi na tassa atthi,",
  "pli-tv-pvr20:21.4": "Upasampadā cassa akuppā;",
  "pli-tv-pvr20:21.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:22.1": "Itthiṁ hane na mātaraṁ,",
  "pli-tv-pvr20:22.2": "Purisañca na pitaraṁ hane;",
  "pli-tv-pvr20:22.3": "Haneyya anariyaṁ mando,",
  "pli-tv-pvr20:22.4": "Tena cānantaraṁ phuse;",
  "pli-tv-pvr20:22.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:23.1": "Itthiṁ hane ca mātaraṁ,",
  "pli-tv-pvr20:23.2": "Purisañca pitaraṁ hane;",
  "pli-tv-pvr20:23.3": "Mātaraṁ pitaraṁ hantvā,",
  "pli-tv-pvr20:23.4": "Na tenānantaraṁ phuse;",
  "pli-tv-pvr20:23.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:24.1": "Acodayitvā assārayitvā,",
  "pli-tv-pvr20:24.2": "Asammukhībhūtassa kareyya kammaṁ;",
  "pli-tv-pvr20:24.3": "Katañca kammaṁ sukataṁ bhaveyya,",
  "pli-tv-pvr20:24.4": "Kārako ca saṅgho anāpattiko siyā;",
  "pli-tv-pvr20:24.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:25.1": "Codayitvā sārayitvā,",
  "pli-tv-pvr20:25.2": "Sammukhībhūtassa kareyya kammaṁ;",
  "pli-tv-pvr20:25.3": "Katañca kammaṁ akataṁ bhaveyya,",
  "pli-tv-pvr20:25.4": "Kārako ca saṅgho sāpattiko siyā;",
  "pli-tv-pvr20:25.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:26.1": "Chindantassa āpatti,",
  "pli-tv-pvr20:26.2": "Chindantassa anāpatti;",
  "pli-tv-pvr20:26.3": "Chādentassa āpatti,",
  "pli-tv-pvr20:26.4": "Chādentassa anāpatti;",
  "pli-tv-pvr20:26.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:27.1": "Saccaṁ bhaṇanto garukaṁ,",
  "pli-tv-pvr20:27.2": "Musā ca lahu bhāsato;",
  "pli-tv-pvr20:27.3": "Musā bhaṇanto garukaṁ,",
  "pli-tv-pvr20:27.4": "Saccañca lahu bhāsato;",
  "pli-tv-pvr20:27.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:28.0": "3. Pācittiyādipañhā",
  "pli-tv-pvr20:28.1": "Adhiṭṭhitaṁ rajanāya rattaṁ,",
  "pli-tv-pvr20:28.2": "Kappakatampi santaṁ;",
  "pli-tv-pvr20:28.3": "Paribhuñjantassa āpatti,",
  "pli-tv-pvr20:28.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:29.1": "Atthaṅgate sūriye bhikkhu maṁsāni khādati,",
  "pli-tv-pvr20:29.2": "Na ummattako na ca pana khittacitto;",
  "pli-tv-pvr20:29.3": "Na cāpi so vedanāṭṭo bhaveyya,",
  "pli-tv-pvr20:29.4": "Na cassa hoti āpatti;",
  "pli-tv-pvr20:29.5": "So ca dhammo sugatena desito,",
  "pli-tv-pvr20:29.6": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:30.1": "Na rattacitto na ca pana theyyacitto,",
  "pli-tv-pvr20:30.2": "Na cāpi so paraṁ maraṇāya cetayi;",
  "pli-tv-pvr20:30.3": "Salākaṁ dentassa hoti chejjaṁ,",
  "pli-tv-pvr20:30.4": "Paṭiggaṇhantassa thullaccayaṁ;",
  "pli-tv-pvr20:30.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:31.1": "Na cāpi āraññakaṁ sāsaṅkasammataṁ,",
  "pli-tv-pvr20:31.2": "Na cāpi saṅghena sammuti dinnā;",
  "pli-tv-pvr20:31.3": "Na cassa kathinaṁ atthataṁ tattheva,",
  "pli-tv-pvr20:31.4": "Cīvaraṁ nikkhipitvā gaccheyya aḍḍhayojanaṁ;",
  "pli-tv-pvr20:31.5": "Tattheva aruṇaṁ uggacchantassa anāpatti,",
  "pli-tv-pvr20:31.6": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:32.1": "Kāyikāni na vācasikāni,",
  "pli-tv-pvr20:32.2": "Sabbāni nānāvatthukāni;",
  "pli-tv-pvr20:32.3": "Apubbaṁ acarimaṁ āpajjeyya ekato,",
  "pli-tv-pvr20:32.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:33.1": "Vācasikāni na kāyikāni,",
  "pli-tv-pvr20:33.2": "Sabbāni nānāvatthukāni;",
  "pli-tv-pvr20:33.3": "Apubbaṁ acarimaṁ āpajjeyya ekato,",
  "pli-tv-pvr20:33.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:34.1": "Tissitthiyo methunaṁ taṁ na seve,",
  "pli-tv-pvr20:34.2": "Tayo purise tayo anariyapaṇḍake;",
  "pli-tv-pvr20:34.3": "Na cācare methunaṁ byañjanasmiṁ,",
  "pli-tv-pvr20:34.4": "Chejjaṁ siyā methunadhammapaccayā;",
  "pli-tv-pvr20:34.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:35.1": "Mātaraṁ cīvaraṁ yāce,",
  "pli-tv-pvr20:35.2": "No ca saṅghe pariṇataṁ;",
  "pli-tv-pvr20:35.3": "Kenassa hoti āpatti,",
  "pli-tv-pvr20:35.4": "Anāpatti ca ñātake;",
  "pli-tv-pvr20:35.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:36.1": "Kuddho ārādhako hoti,",
  "pli-tv-pvr20:36.2": "Kuddho hoti garahiyo;",
  "pli-tv-pvr20:36.3": "Atha ko nāma so dhammo,",
  "pli-tv-pvr20:36.4": "Yena kuddho pasaṁsiyo;",
  "pli-tv-pvr20:36.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:37.1": "Tuṭṭho ārādhako hoti,",
  "pli-tv-pvr20:37.2": "Tuṭṭho hoti garahiyo;",
  "pli-tv-pvr20:37.3": "Atha ko nāma so dhammo,",
  "pli-tv-pvr20:37.4": "Yena tuṭṭho garahiyo;",
  "pli-tv-pvr20:37.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:38.1": "Saṅghādisesaṁ thullaccayaṁ,",
  "pli-tv-pvr20:38.2": "Pācittiyaṁ pāṭidesanīyaṁ;",
  "pli-tv-pvr20:38.3": "Dukkaṭaṁ āpajjeyya ekato,",
  "pli-tv-pvr20:38.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:39.1": "Ubho paripuṇṇavīsativassā,",
  "pli-tv-pvr20:39.2": "Ubhinnaṁ ekupajjhāyo;",
  "pli-tv-pvr20:39.3": "Ekācariyo ekā kammavācā,",
  "pli-tv-pvr20:39.4": "Eko upasampanno eko anupasampanno;",
  "pli-tv-pvr20:39.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:40.1": "Akappakataṁ nāpi rajanāya rattaṁ,",
  "pli-tv-pvr20:40.2": "Tena nivattho yena kāmaṁ vajeyya;",
  "pli-tv-pvr20:40.3": "Na cassa hoti āpatti,",
  "pli-tv-pvr20:40.4": "So ca dhammo sugatena desito;",
  "pli-tv-pvr20:40.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:41.1": "Na deti na paṭiggaṇhāti,",
  "pli-tv-pvr20:41.2": "Paṭiggaho tena na vijjati;",
  "pli-tv-pvr20:41.3": "Āpajjati garukaṁ na lahukaṁ,",
  "pli-tv-pvr20:41.4": "Tañca paribhogapaccayā;",
  "pli-tv-pvr20:41.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:42.1": "Na deti na paṭiggaṇhāti,",
  "pli-tv-pvr20:42.2": "Paṭiggaho tena na vijjati;",
  "pli-tv-pvr20:42.3": "Āpajjati lahukaṁ na garukaṁ,",
  "pli-tv-pvr20:42.4": "Tañca paribhogapaccayā;",
  "pli-tv-pvr20:42.5": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:43.1": "Āpajjati garukaṁ sāvasesaṁ,",
  "pli-tv-pvr20:43.2": "Chādeti anādariyaṁ paṭicca;",
  "pli-tv-pvr20:43.3": "Na bhikkhunī no ca phuseyya vajjaṁ,",
  "pli-tv-pvr20:43.4": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:44.1": "Sedamocanagāthā niṭṭhitā.",
  "pli-tv-pvr20:45.1": "Tassuddānaṁ",
  "pli-tv-pvr20:46.1": "Asaṁvāso avissajji,",
  "pli-tv-pvr20:46.2": "Dasa ca anukkhittako;",
  "pli-tv-pvr20:46.3": "Upeti dhammaṁ ubbhakkhakaṁ,",
  "pli-tv-pvr20:46.4": "Tato saññācikā ca dve.",
  "pli-tv-pvr20:47.1": "Na kāyikañca garukaṁ,",
  "pli-tv-pvr20:47.2": "na kāyikaṁ na vācasikaṁ;",
  "pli-tv-pvr20:47.3": "Anālapanto sikkhā ca,",
  "pli-tv-pvr20:47.4": "ubho ca caturo janā.",
  "pli-tv-pvr20:48.1": "Itthī telañca nissaggi,",
  "pli-tv-pvr20:48.2": "bhikkhu ca padavītiyo;",
  "pli-tv-pvr20:48.3": "Nivattho ca na ca ñatti,",
  "pli-tv-pvr20:48.4": "na mātaraṁ pitaraṁ hane.",
  "pli-tv-pvr20:49.1": "Acodayitvā codayitvā,",
  "pli-tv-pvr20:49.2": "chindantaṁ saccameva ca;",
  "pli-tv-pvr20:49.3": "Adhiṭṭhitañcatthaṅgate,",
  "pli-tv-pvr20:49.4": "na rattaṁ na cāraññakaṁ.",
  "pli-tv-pvr20:50.1": "Kāyikā vācasikā ca,",
  "pli-tv-pvr20:50.2": "tissitthī cāpi mātaraṁ;",
  "pli-tv-pvr20:50.3": "Kuddho ārādhako tuṭṭho,",
  "pli-tv-pvr20:50.4": "saṅghādisesā ca ubho.",
  "pli-tv-pvr20:51.1": "Akappakataṁ na deti,",
  "pli-tv-pvr20:51.2": "Na detāpajjatī garuṁ;",
  "pli-tv-pvr20:51.3": "Sedamocanikā gāthā,",
  "pli-tv-pvr20:51.4": "Pañhā viññūhi vibhāvitāti."
}