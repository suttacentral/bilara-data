{
  "pli-tv-pvr3:0.1": "Parivāra",
  "pli-tv-pvr3:0.2": "3. Samuṭṭhānasīsasaṅkhepa",
  "pli-tv-pvr3:0.3": "1. Samuṭṭhānassuddāna",
  "pli-tv-pvr3:1.1": "Aniccā sabbe saṅkhārā,",
  "pli-tv-pvr3:1.2": "dukkhānattā ca saṅkhatā;",
  "pli-tv-pvr3:1.3": "Nibbānañceva paññatti,",
  "pli-tv-pvr3:1.4": "anattā iti nicchayā.",
  "pli-tv-pvr3:2.1": "Buddhacande anuppanne,",
  "pli-tv-pvr3:2.2": "buddhādicce anuggate;",
  "pli-tv-pvr3:2.3": "Tesaṁ sabhāgadhammānaṁ,",
  "pli-tv-pvr3:2.4": "nāmamattaṁ na nāyati.",
  "pli-tv-pvr3:3.1": "Dukkaraṁ vividhaṁ katvā,",
  "pli-tv-pvr3:3.2": "pūrayitvāna pāramī;",
  "pli-tv-pvr3:3.3": "Uppajjanti mahāvīrā,",
  "pli-tv-pvr3:3.4": "cakkhubhūtā sabrahmake.",
  "pli-tv-pvr3:4.1": "Te desayanti saddhammaṁ,",
  "pli-tv-pvr3:4.2": "dukkhahāniṁ sukhāvahaṁ;",
  "pli-tv-pvr3:4.3": "Aṅgīraso sakyamuni,",
  "pli-tv-pvr3:4.4": "sabbabhūtānukampako.",
  "pli-tv-pvr3:5.1": "Sabbasattuttamo sīho,",
  "pli-tv-pvr3:5.2": "piṭake tīṇi desayi;",
  "pli-tv-pvr3:5.3": "Suttantamabhidhammañca,",
  "pli-tv-pvr3:5.4": "vinayañca mahāguṇaṁ.",
  "pli-tv-pvr3:6.1": "Evaṁ nīyati saddhammo,",
  "pli-tv-pvr3:6.2": "vinayo yadi tiṭṭhati;",
  "pli-tv-pvr3:6.3": "Ubhato ca vibhaṅgāni,",
  "pli-tv-pvr3:6.4": "khandhakā yā ca mātikā.",
  "pli-tv-pvr3:7.1": "Mālā suttaguṇeneva,",
  "pli-tv-pvr3:7.2": "parivārena ganthitā;",
  "pli-tv-pvr3:7.3": "Tasseva parivārassa,",
  "pli-tv-pvr3:7.4": "samuṭṭhānaṁ niyato kataṁ.",
  "pli-tv-pvr3:8.1": "Sambhedaṁ nidānañcaññaṁ,",
  "pli-tv-pvr3:8.2": "sutte dissanti upari;",
  "pli-tv-pvr3:8.3": "Tasmā sikkhe parivāraṁ,",
  "pli-tv-pvr3:8.4": "dhammakāmo supesaloti.",
  "pli-tv-pvr3:9.0": "2. Terasasamuṭṭhāna",
  "pli-tv-pvr3:9.1": "Vibhaṅge dvīsu paññattaṁ,",
  "pli-tv-pvr3:9.2": "uddisanti uposathe;",
  "pli-tv-pvr3:9.3": "Pavakkhāmi samuṭṭhānaṁ,",
  "pli-tv-pvr3:9.4": "yathāñāyaṁ suṇātha me.",
  "pli-tv-pvr3:10.1": "Pārājikaṁ yaṁ paṭhamaṁ,",
  "pli-tv-pvr3:10.2": "dutiyañca tato paraṁ;",
  "pli-tv-pvr3:10.3": "Sañcarittānubhāsanañca,",
  "pli-tv-pvr3:10.4": "atirekañca cīvaraṁ.",
  "pli-tv-pvr3:11.1": "Lomāni padasodhammo,",
  "pli-tv-pvr3:11.2": "bhūtaṁ saṁvidhānena ca;",
  "pli-tv-pvr3:11.3": "Theyyadesanacorī ca,",
  "pli-tv-pvr3:11.4": "ananuññātāya terasa.",
  "pli-tv-pvr3:12.1": "Terasete samuṭṭhāna-",
  "pli-tv-pvr3:12.2": "nayā viññūhi cintitā;",
  "pli-tv-pvr3:12.3": "Ekekasmiṁ samuṭṭhāne,",
  "pli-tv-pvr3:12.4": "sadisā idha dissare.",
  "pli-tv-pvr3:13.0": "2.1. Paṭhamapārājikasamuṭṭhāna",
  "pli-tv-pvr3:13.1": "Methunaṁ sukkasaṁsaggo,",
  "pli-tv-pvr3:13.2": "aniyatā paṭhamikā;",
  "pli-tv-pvr3:13.3": "Pubbūpaparipācitā,",
  "pli-tv-pvr3:13.4": "raho bhikkhuniyā saha.",
  "pli-tv-pvr3:14.1": "Sabhojane raho dve ca,",
  "pli-tv-pvr3:14.2": "aṅguli udake hasaṁ;",
  "pli-tv-pvr3:14.3": "Pahāre uggire ceva,",
  "pli-tv-pvr3:14.4": "tepaññāsā ca sekhiyā.",
  "pli-tv-pvr3:15.1": "Adhakkhagāmāvassutā,",
  "pli-tv-pvr3:15.2": "talamaṭṭhañca suddhikā;",
  "pli-tv-pvr3:15.3": "Vassaṁvuṭṭhā ca ovādaṁ,",
  "pli-tv-pvr3:15.4": "nānubandhe pavattiniṁ.",
  "pli-tv-pvr3:16.1": "Chasattati ime sikkhā,",
  "pli-tv-pvr3:16.2": "kāyamānasikā katā;",
  "pli-tv-pvr3:16.3": "Sabbe ekasamuṭṭhānā,",
  "pli-tv-pvr3:16.4": "paṭhamaṁ pārājikaṁ yathā.",
  "pli-tv-pvr3:17.1": "Paṭhamapārājikasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:18.0": "2.2. Dutiyapārājikasamuṭṭhāna",
  "pli-tv-pvr3:18.1": "Adinnaṁ viggahuttari,",
  "pli-tv-pvr3:18.2": "duṭṭhullā attakāminaṁ;",
  "pli-tv-pvr3:18.3": "Amūlā aññabhāgiyā,",
  "pli-tv-pvr3:18.4": "aniyatā dutiyikā.",
  "pli-tv-pvr3:19.1": "Acchinde pariṇāmane,",
  "pli-tv-pvr3:19.2": "musā omasapesuṇā;",
  "pli-tv-pvr3:19.3": "Duṭṭhullā pathavīkhaṇe,",
  "pli-tv-pvr3:19.4": "bhūtaṁ aññāya ujjhāpe.",
  "pli-tv-pvr3:20.1": "Nikkaḍḍhanaṁ siñcanañca,",
  "pli-tv-pvr3:20.2": "āmisahetu bhuttāvī;",
  "pli-tv-pvr3:20.3": "Ehi anādari bhiṁsā,",
  "pli-tv-pvr3:20.4": "apanidhe ca jīvitaṁ.",
  "pli-tv-pvr3:21.1": "Jānaṁ sappāṇakaṁ kammaṁ,",
  "pli-tv-pvr3:21.2": "ūnasaṁvāsanāsanā;",
  "pli-tv-pvr3:21.3": "Sahadhammikavilekhā,",
  "pli-tv-pvr3:21.4": "moho amūlakena ca.",
  "pli-tv-pvr3:22.1": "Kukkuccaṁ dhammikaṁ cīvaraṁ datvā,",
  "pli-tv-pvr3:22.2": "Pariṇāmeyya puggale;",
  "pli-tv-pvr3:22.3": "Kiṁ te akālaṁ acchinde,",
  "pli-tv-pvr3:22.4": "Duggahī nirayena ca.",
  "pli-tv-pvr3:23.1": "Gaṇaṁ vibhaṅgaṁ dubbalaṁ,",
  "pli-tv-pvr3:23.2": "kathināphāsupassayaṁ;",
  "pli-tv-pvr3:23.3": "Akkosacaṇḍī maccharī,",
  "pli-tv-pvr3:23.4": "gabbhinī ca pāyantiyā.",
  "pli-tv-pvr3:24.1": "Dvevassaṁ sikkhā saṅghena,",
  "pli-tv-pvr3:24.2": "tayo ceva gihīgatā;",
  "pli-tv-pvr3:24.3": "Kumāribhūtā tisso ca,",
  "pli-tv-pvr3:24.4": "ūnadvādasasammatā.",
  "pli-tv-pvr3:25.1": "Alaṁ tāva sokāvāsaṁ,",
  "pli-tv-pvr3:25.2": "chandā anuvassā ca dve;",
  "pli-tv-pvr3:25.3": "Sikkhāpadā sattatime,",
  "pli-tv-pvr3:25.4": "samuṭṭhānā tikā katā.",
  "pli-tv-pvr3:26.1": "Kāyacittena na vācā,",
  "pli-tv-pvr3:26.2": "vācācittaṁ na kāyikaṁ;",
  "pli-tv-pvr3:26.3": "Tīhi dvārehi jāyanti,",
  "pli-tv-pvr3:26.4": "pārājikaṁ dutiyaṁ yathā.",
  "pli-tv-pvr3:27.1": "Dutiyapārājikasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:28.0": "2.3. Sañcarittasamuṭṭhāna",
  "pli-tv-pvr3:28.1": "Sañcarī kuṭi vihāro,",
  "pli-tv-pvr3:28.2": "dhovanañca paṭiggaho;",
  "pli-tv-pvr3:28.3": "Viññattuttari abhihaṭṭhuṁ,",
  "pli-tv-pvr3:28.4": "ubhinnaṁ dūtakena ca.",
  "pli-tv-pvr3:29.1": "Kosiyā suddhadvebhāgā,",
  "pli-tv-pvr3:29.2": "chabbassāni nisīdanaṁ;",
  "pli-tv-pvr3:29.3": "Riñcanti rūpikā ceva,",
  "pli-tv-pvr3:29.4": "ubho nānappakārakā.",
  "pli-tv-pvr3:30.1": "Ūnabandhanavassikā,",
  "pli-tv-pvr3:30.2": "suttaṁ vikappanena ca;",
  "pli-tv-pvr3:30.3": "Dvāradānasibbāni ca,",
  "pli-tv-pvr3:30.4": "pūvapaccayajoti ca.",
  "pli-tv-pvr3:31.1": "Ratanaṁ sūci mañco ca,",
  "pli-tv-pvr3:31.2": "tūlaṁ nisīdanakaṇḍu ca;",
  "pli-tv-pvr3:31.3": "Vassikā ca sugatena,",
  "pli-tv-pvr3:31.4": "viññatti aññaṁ cetāpanā.",
  "pli-tv-pvr3:32.1": "Dve saṅghikā mahājanikā dve,",
  "pli-tv-pvr3:32.2": "Puggalalahukā garu;",
  "pli-tv-pvr3:32.3": "Dve vighāsā sāṭikā ca,",
  "pli-tv-pvr3:32.4": "Samaṇacīvarena ca.",
  "pli-tv-pvr3:33.1": "Samapaññāsime dhammā,",
  "pli-tv-pvr3:33.2": "chahi ṭhānehi jāyare;",
  "pli-tv-pvr3:33.3": "Kāyato na vācācittā,",
  "pli-tv-pvr3:33.4": "vācato na kāyamanā.",
  "pli-tv-pvr3:34.1": "Kāyavācā na ca cittā,",
  "pli-tv-pvr3:34.2": "kāyacittā na vācikā;",
  "pli-tv-pvr3:34.3": "Vācācittā na kāyena,",
  "pli-tv-pvr3:34.4": "tīhi dvārehi jāyare;",
  "pli-tv-pvr3:34.5": "Chasamuṭṭhānikā ete,",
  "pli-tv-pvr3:34.6": "sañcarittena sādisā.",
  "pli-tv-pvr3:35.1": "Sañcarittasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:36.0": "2.4. Samanubhāsanāsamuṭṭhāna",
  "pli-tv-pvr3:36.1": "Bhedānuvattadubbaca-",
  "pli-tv-pvr3:36.2": "dūsaduṭṭhulladiṭṭhi ca;",
  "pli-tv-pvr3:36.3": "Chandaṁ ujjagghikā dve ca,",
  "pli-tv-pvr3:36.4": "dve ca saddā na byāhare.",
  "pli-tv-pvr3:37.1": "Chamā nīcāsane ṭhānaṁ,",
  "pli-tv-pvr3:37.2": "pacchato uppathena ca;",
  "pli-tv-pvr3:37.3": "Vajjānuvattigahaṇā,",
  "pli-tv-pvr3:37.4": "osāre paccācikkhanā.",
  "pli-tv-pvr3:38.1": "Kismiṁ saṁsaṭṭhā dve vadhi,",
  "pli-tv-pvr3:38.2": "Visibbe dukkhitāya ca;",
  "pli-tv-pvr3:38.3": "Puna saṁsaṭṭhā na vūpasame,",
  "pli-tv-pvr3:38.4": "Ārāmañca pavāraṇā.",
  "pli-tv-pvr3:39.1": "Anvaddhaṁ saha jīviniṁ dve,",
  "pli-tv-pvr3:39.2": "Cīvaraṁ anubandhanā;",
  "pli-tv-pvr3:39.3": "Sattatiṁsa ime dhammā,",
  "pli-tv-pvr3:39.4": "Kāyavācāya cittato;",
  "pli-tv-pvr3:39.5": "Sabbe ekasamuṭṭhānā,",
  "pli-tv-pvr3:39.6": "Samanubhāsanā yathā.",
  "pli-tv-pvr3:40.1": "Samanubhāsanāsamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:41.0": "2.5. Kathinasamuṭṭhāna",
  "pli-tv-pvr3:41.1": "Ubbhataṁ kathinaṁ tīṇi,",
  "pli-tv-pvr3:41.2": "paṭhamaṁ pattabhesajjaṁ;",
  "pli-tv-pvr3:41.3": "Accekaṁ cāpi sāsaṅkaṁ,",
  "pli-tv-pvr3:41.4": "pakkamantena vā duve.",
  "pli-tv-pvr3:42.1": "Upassayaṁ paramparā,",
  "pli-tv-pvr3:42.2": "anatirittaṁ nimantanā;",
  "pli-tv-pvr3:42.3": "Vikappaṁ rañño vikāle,",
  "pli-tv-pvr3:42.4": "vosāsāraññakena ca.",
  "pli-tv-pvr3:43.1": "Ussayāsannicayañca,",
  "pli-tv-pvr3:43.2": "pure pacchā vikāle ca;",
  "pli-tv-pvr3:43.3": "Pañcāhikā saṅkamanī,",
  "pli-tv-pvr3:43.4": "dvepi āvasathena ca.",
  "pli-tv-pvr3:44.1": "Pasākhe āsane ceva,",
  "pli-tv-pvr3:44.2": "tiṁsa ekūnakā ime;",
  "pli-tv-pvr3:44.3": "Kāyavācā na ca cittā,",
  "pli-tv-pvr3:44.4": "tīhi dvārehi jāyare;",
  "pli-tv-pvr3:44.5": "Dvisamuṭṭhānikā sabbe,",
  "pli-tv-pvr3:44.6": "kathinena sahā samā.",
  "pli-tv-pvr3:45.1": "Kathinasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:46.0": "2.6. Eḷakalomasamuṭṭhāna",
  "pli-tv-pvr3:46.1": "Eḷakalomā dve seyyā,",
  "pli-tv-pvr3:46.2": "āhacca piṇḍabhojanaṁ;",
  "pli-tv-pvr3:46.3": "Gaṇavikālasannidhi,",
  "pli-tv-pvr3:46.4": "dantaponena celakā.",
  "pli-tv-pvr3:47.1": "Uyyuttaṁ senaṁ uyyodhi,",
  "pli-tv-pvr3:47.2": "surā orena nhāyanā;",
  "pli-tv-pvr3:47.3": "Dubbaṇṇe dve desanikā,",
  "pli-tv-pvr3:47.4": "lasuṇupatiṭṭhe naccanā.",
  "pli-tv-pvr3:48.1": "Nhānamattharaṇaṁ seyyā,",
  "pli-tv-pvr3:48.2": "antoraṭṭhe tathā bahi;",
  "pli-tv-pvr3:48.3": "Antovassaṁ cittāgāraṁ,",
  "pli-tv-pvr3:48.4": "āsandi suttakantanā.",
  "pli-tv-pvr3:49.1": "Veyyāvaccaṁ sahatthā ca,",
  "pli-tv-pvr3:49.2": "abhikkhukāvāsena ca;",
  "pli-tv-pvr3:49.3": "Chattaṁ yānañca saṅghāṇiṁ,",
  "pli-tv-pvr3:49.4": "alaṅkāraṁ gandhavāsitaṁ.",
  "pli-tv-pvr3:50.1": "Bhikkhunī sikkhamānā ca,",
  "pli-tv-pvr3:50.2": "sāmaṇerī gihiniyā;",
  "pli-tv-pvr3:50.3": "Asaṅkaccikā āpatti,",
  "pli-tv-pvr3:50.4": "cattārīsā catuttari.",
  "pli-tv-pvr3:51.1": "Kāyena na vācācittena,",
  "pli-tv-pvr3:51.2": "kāyacittena na vācato;",
  "pli-tv-pvr3:51.3": "Dvisamuṭṭhānikā sabbe,",
  "pli-tv-pvr3:51.4": "samā eḷakalomikāti.",
  "pli-tv-pvr3:52.1": "Eḷakalomasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:53.0": "2.7. Padasodhammasamuṭṭhāna",
  "pli-tv-pvr3:53.1": "Padaññatra asammatā,",
  "pli-tv-pvr3:53.2": "tathā atthaṅgatena ca;",
  "pli-tv-pvr3:53.3": "Tiracchānavijjā dve vuttā,",
  "pli-tv-pvr3:53.4": "anokāso ca pucchanā.",
  "pli-tv-pvr3:54.1": "Satta sikkhāpadā ete,",
  "pli-tv-pvr3:54.2": "vācā na kāyacittato;",
  "pli-tv-pvr3:54.3": "Vācācittena jāyanti,",
  "pli-tv-pvr3:54.4": "na tu kāyena jāyare;",
  "pli-tv-pvr3:54.5": "Dvisamuṭṭhānikā sabbe,",
  "pli-tv-pvr3:54.6": "padasodhammasadisā.",
  "pli-tv-pvr3:55.1": "Padasodhammasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:56.0": "2.8. Addhānasamuṭṭhāna",
  "pli-tv-pvr3:56.1": "Addhānanāvaṁ paṇītaṁ,",
  "pli-tv-pvr3:56.2": "mātugāmena saṁhare;",
  "pli-tv-pvr3:56.3": "Dhaññaṁ nimantitā ceva,",
  "pli-tv-pvr3:56.4": "aṭṭha ca pāṭidesanī.",
  "pli-tv-pvr3:57.1": "Sikkhā pannarasa ete,",
  "pli-tv-pvr3:57.2": "kāyā na vācā na manā;",
  "pli-tv-pvr3:57.3": "Kāyavācāhi jāyanti,",
  "pli-tv-pvr3:57.4": "na te cittena jāyare.",
  "pli-tv-pvr3:58.1": "Kāyacittena jāyanti,",
  "pli-tv-pvr3:58.2": "na te jāyanti vācato;",
  "pli-tv-pvr3:58.3": "Kāyavācāhi cittena,",
  "pli-tv-pvr3:58.4": "samuṭṭhānā catubbidhā;",
  "pli-tv-pvr3:58.5": "Paññattā buddhañāṇena,",
  "pli-tv-pvr3:58.6": "addhānena sahā samā.",
  "pli-tv-pvr3:59.1": "Addhānasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:60.0": "2.9. Theyyasatthasamuṭṭhāna",
  "pli-tv-pvr3:60.1": "Theyyasatthaṁ upassuti,",
  "pli-tv-pvr3:60.2": "sūpaviññāpanena ca;",
  "pli-tv-pvr3:60.3": "Rattichannañca okāsaṁ,",
  "pli-tv-pvr3:60.4": "ete byūhena sattamā.",
  "pli-tv-pvr3:61.1": "Kāyacittena jāyanti,",
  "pli-tv-pvr3:61.2": "na te jāyanti vācato;",
  "pli-tv-pvr3:61.3": "Tīhi dvārehi jāyanti,",
  "pli-tv-pvr3:61.4": "dvisamuṭṭhānikā ime;",
  "pli-tv-pvr3:61.5": "Theyyasatthasamuṭṭhānā,",
  "pli-tv-pvr3:61.6": "desitādiccabandhunā.",
  "pli-tv-pvr3:62.1": "Theyyasatthasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:63.0": "2.10. Dhammadesanāsamuṭṭhāna",
  "pli-tv-pvr3:63.1": "Chattapāṇissa saddhammaṁ,",
  "pli-tv-pvr3:63.2": "na desenti tathāgatā;",
  "pli-tv-pvr3:63.3": "Evameva daṇḍapāṇissa,",
  "pli-tv-pvr3:63.4": "satthaāvudhapāṇinaṁ.",
  "pli-tv-pvr3:64.1": "Pādukupāhanā yānaṁ,",
  "pli-tv-pvr3:64.2": "seyyapallatthikāya ca;",
  "pli-tv-pvr3:64.3": "Veṭhitoguṇṭhito ceva,",
  "pli-tv-pvr3:64.4": "ekādasamanūnakā.",
  "pli-tv-pvr3:65.1": "Vācācittena jāyanti,",
  "pli-tv-pvr3:65.2": "na te jāyanti kāyato;",
  "pli-tv-pvr3:65.3": "Sabbe ekasamuṭṭhānā,",
  "pli-tv-pvr3:65.4": "samakā dhammadesane.",
  "pli-tv-pvr3:66.1": "Dhammadesanāsamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:67.0": "2.11. Bhūtārocanasamuṭṭhāna",
  "pli-tv-pvr3:67.1": "Bhūtaṁ kāyena jāyati,",
  "pli-tv-pvr3:67.2": "na vācā na ca cittato;",
  "pli-tv-pvr3:67.3": "Vācato ca samuṭṭhāti,",
  "pli-tv-pvr3:67.4": "na kāyā na ca cittato.",
  "pli-tv-pvr3:68.1": "Kāyavācāya jāyati,",
  "pli-tv-pvr3:68.2": "na tu jāyati cittato;",
  "pli-tv-pvr3:68.3": "Bhūtārocanakā nāma,",
  "pli-tv-pvr3:68.4": "tīhi ṭhānehi jāyati.",
  "pli-tv-pvr3:69.1": "Bhūtārocanasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:70.0": "2.12. Corivuṭṭhāpanasamuṭṭhāna",
  "pli-tv-pvr3:70.1": "Corī vācāya cittena,",
  "pli-tv-pvr3:70.2": "na taṁ jāyati kāyato;",
  "pli-tv-pvr3:70.3": "Jāyati tīhi dvārehi,",
  "pli-tv-pvr3:70.4": "corivuṭṭhāpanaṁ idaṁ;",
  "pli-tv-pvr3:70.5": "Akataṁ dvisamuṭṭhānaṁ,",
  "pli-tv-pvr3:70.6": "dhammarājena bhāsitaṁ.",
  "pli-tv-pvr3:71.1": "Corivuṭṭhāpanasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:72.0": "2.13. Ananuññātasamuṭṭhāna",
  "pli-tv-pvr3:72.1": "Ananuññātaṁ vācāya,",
  "pli-tv-pvr3:72.2": "na kāyā na ca cittato;",
  "pli-tv-pvr3:72.3": "Jāyati kāyavācāya,",
  "pli-tv-pvr3:72.4": "na taṁ jāyati cittato.",
  "pli-tv-pvr3:73.1": "Jāyati vācācittena,",
  "pli-tv-pvr3:73.2": "na taṁ jāyati kāyato;",
  "pli-tv-pvr3:73.3": "Jāyati tīhi dvārehi,",
  "pli-tv-pvr3:73.4": "akataṁ catuṭhānikaṁ.",
  "pli-tv-pvr3:74.1": "Ananuññātasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:75.1": "Samuṭṭhānañhi saṅkhepaṁ,",
  "pli-tv-pvr3:75.2": "dasa tīṇi sudesitaṁ;",
  "pli-tv-pvr3:75.3": "Asammohakaraṁ ṭhānaṁ,",
  "pli-tv-pvr3:75.4": "nettidhammānulomikaṁ;",
  "pli-tv-pvr3:75.5": "Dhārayanto imaṁ viññū,",
  "pli-tv-pvr3:75.6": "samuṭṭhāne na muyhatīti.",
  "pli-tv-pvr3:76.1": "Samuṭṭhānasīsasaṅkhepo niṭṭhito."
}