  "pli-tv-pvr19:64.2": "Añjalisāmicena ca;",
  "pli-tv-pvr19:64.3": "Dasannaṁ dukkaṭaṁ hoti,",
  "pli-tv-pvr19:64.4": "Dasa cīvaradhāraṇā.",
  "pli-tv-pvr19:65.1": "Katinaṁ vassaṁvuṭṭhānaṁ,",
  "pli-tv-pvr19:65.2": "dātabbaṁ idha cīvaraṁ;",
  "pli-tv-pvr19:65.3": "Katinaṁ sante dātabbaṁ,",
  "pli-tv-pvr19:65.4": "katinañceva na dātabbaṁ.",
  "pli-tv-pvr19:66.1": "Pañcannaṁ vassaṁvuṭṭhānaṁ,",
  "pli-tv-pvr19:66.2": "dātabbaṁ idha cīvaraṁ;",
  "pli-tv-pvr19:66.3": "Sattannaṁ sante dātabbaṁ,",
  "pli-tv-pvr19:66.4": "soḷasannaṁ na dātabbaṁ.",
  "pli-tv-pvr19:67.1": "Katisataṁ rattisataṁ,",
  "pli-tv-pvr19:67.2": "āpattiyo chādayitvāna;",
  "pli-tv-pvr19:67.3": "Kati rattiyo vasitvāna,",
  "pli-tv-pvr19:67.4": "mucceyya pārivāsiko.",
  "pli-tv-pvr19:68.1": "Dasasataṁ rattisataṁ,",
  "pli-tv-pvr19:68.2": "āpattiyo chādayitvāna;",
  "pli-tv-pvr19:68.3": "Dasa rattiyo vasitvāna,",
  "pli-tv-pvr19:68.4": "mucceyya pārivāsiko.",
  "pli-tv-pvr19:69.1": "Kati kammadosā vuttā,",
  "pli-tv-pvr19:69.2": "buddhenādiccabandhunā;",
  "pli-tv-pvr19:69.3": "Campāyaṁ vinayavatthusmiṁ,",
  "pli-tv-pvr19:69.4": "sabbeva adhammikā kati.",
  "pli-tv-pvr19:70.1": "Dvādasa kammadosā vuttā,",
  "pli-tv-pvr19:70.2": "buddhenādiccabandhunā;",
  "pli-tv-pvr19:70.3": "Campāyaṁ vinayavatthusmiṁ,",
  "pli-tv-pvr19:70.4": "sabbeva adhammikā katā.",
  "pli-tv-pvr19:71.1": "Kati kammasampattiyo vuttā,",
  "pli-tv-pvr19:71.2": "Buddhenādiccabandhunā;",
  "pli-tv-pvr19:71.3": "Campāyaṁ vinayavatthusmiṁ,",
  "pli-tv-pvr19:71.4": "Sabbeva dhammikā kati.",
  "pli-tv-pvr19:72.1": "Catasso kammasampattiyo vuttā,",
  "pli-tv-pvr19:72.2": "Buddhenādiccabandhunā;",
  "pli-tv-pvr19:72.3": "Campāyaṁ vinayavatthusmiṁ,",
  "pli-tv-pvr19:72.4": "Sabbeva dhammikā katā.",
  "pli-tv-pvr19:73.1": "Kati kammāni vuttāni,",
  "pli-tv-pvr19:73.2": "buddhenādiccabandhunā;",
  "pli-tv-pvr19:73.3": "Campāyaṁ vinayavatthusmiṁ,",
  "pli-tv-pvr19:73.4": "dhammikā adhammikā kati.",
  "pli-tv-pvr19:74.1": "Cha kammāni vuttāni,",
  "pli-tv-pvr19:74.2": "buddhenādiccabandhunā;",
  "pli-tv-pvr19:74.3": "Campāyaṁ vinayavatthusmiṁ,",
  "pli-tv-pvr19:74.4": "ekettha dhammikā katā;",
  "pli-tv-pvr19:74.5": "Pañca adhammikā vuttā,",
  "pli-tv-pvr19:74.6": "buddhenādiccabandhunā.",
  "pli-tv-pvr19:75.1": "Kati kammāni vuttāni,",
  "pli-tv-pvr19:75.2": "buddhenādiccabandhunā;",
  "pli-tv-pvr19:75.3": "Campāyaṁ vinayavatthusmiṁ,",
  "pli-tv-pvr19:75.4": "dhammikā adhammikā kati.",
  "pli-tv-pvr19:76.1": "Cattāri kammāni vuttāni,",
  "pli-tv-pvr19:76.2": "buddhenādiccabandhunā;",
  "pli-tv-pvr19:76.3": "Campāyaṁ vinayavatthusmiṁ,",
  "pli-tv-pvr19:76.4": "ekettha dhammikā katā;",
  "pli-tv-pvr19:76.5": "Tayo adhammikā vuttā,",
  "pli-tv-pvr19:76.6": "buddhenādiccabandhunā.",
  "pli-tv-pvr19:77.1": "Yaṁ desitaṁnantajinena tādinā,",
  "pli-tv-pvr19:77.2": "Āpattikkhandhāni vivekadassinā;",
  "pli-tv-pvr19:77.3": "Katettha sammanti vinā samathehi,",
  "pli-tv-pvr19:77.4": "Pucchāmi taṁ brūhi vibhaṅgakovida.",
  "pli-tv-pvr19:78.1": "Yaṁ desitaṁnantajinena tādinā,",
  "pli-tv-pvr19:78.2": "Āpattikkhandhāni vivekadassinā;",
  "pli-tv-pvr19:78.3": "Ekettha sammati vinā samathehi,",
  "pli-tv-pvr19:78.4": "Etaṁ te akkhāmi vibhaṅgakovida.",
  "pli-tv-pvr19:79.1": "Kati āpāyikā vuttā,",
  "pli-tv-pvr19:79.2": "buddhenādiccabandhunā;",
  "pli-tv-pvr19:79.3": "Vinayaṁ paṭijānantassa,",
  "pli-tv-pvr19:79.4": "vinayāni suṇoma te.",
  "pli-tv-pvr19:80.1": "Chaūnadiyaḍḍhasatā vuttā,",
  "pli-tv-pvr19:80.2": "Buddhenādiccabandhunā;",
  "pli-tv-pvr19:80.3": "Āpāyikā nerayikā,",
  "pli-tv-pvr19:80.4": "Kappaṭṭhā saṅghabhedakā;",
  "pli-tv-pvr19:80.5": "Vinayaṁ paṭijānantassa,",
  "pli-tv-pvr19:80.6": "Vinayāni suṇohi me.",
  "pli-tv-pvr19:81.1": "Kati nāpāyikā vuttā,",
  "pli-tv-pvr19:81.2": "buddhenādiccabandhunā;",
  "pli-tv-pvr19:81.3": "Vinayaṁ paṭijānantassa,",
  "pli-tv-pvr19:81.4": "vinayāni suṇoma te.",
