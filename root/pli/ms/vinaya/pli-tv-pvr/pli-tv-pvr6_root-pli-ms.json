{
  "pli-tv-pvr6:0.1": "Parivāra",
  "pli-tv-pvr6:0.2": "6. Khandhakapucchāvāra",
  "pli-tv-pvr6:1.1": "Upasampadaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:1.2": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:1.3": "Upasampadaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:1.4": "Samukkaṭṭhapadānaṁ dve āpattiyo. ",
  "pli-tv-pvr6:2.1": "Uposathaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:2.2": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:2.3": "Uposathaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:2.4": "Samukkaṭṭhapadānaṁ tisso āpattiyo. ",
  "pli-tv-pvr6:3.1": "Vassūpanāyikaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
