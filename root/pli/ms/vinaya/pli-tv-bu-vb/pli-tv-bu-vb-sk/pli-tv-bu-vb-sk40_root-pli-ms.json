{
  "pli-tv-bu-vb-sk40:0.1": "Theravāda Vinaya ",
  "pli-tv-bu-vb-sk40:0.2": "Mahāvibhaṅga ",
  "pli-tv-bu-vb-sk40:0.3": "Sekhiyakaṇḍa ",
  "pli-tv-bu-vb-sk40:0.4": "Sakkaccavagga ",
  "pli-tv-bu-vb-sk40:0.5": "40. Ālopasikkhāpada ",
  "pli-tv-bu-vb-sk40:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme. ",
  "pli-tv-bu-vb-sk40:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū dīghaṁ ālopaṁ karonti …pe…. ",
  "pli-tv-bu-vb-sk40:1.3.1": "“Parimaṇḍalaṁ ālopaṁ karissāmīti sikkhā karaṇīyā”ti. ",
  "pli-tv-bu-vb-sk40:1.4": "Parimaṇḍalo ālopo kātabbo. ",
  "pli-tv-bu-vb-sk40:1.5": "Yo anādariyaṁ paṭicca dīghaṁ ālopaṁ karoti, āpatti dukkaṭassa. ",
  "pli-tv-bu-vb-sk40:1.6.1": "Anāpatti—",
  "pli-tv-bu-vb-sk40:1.7": "asañcicca, ",
  "pli-tv-bu-vb-sk40:1.8": "assatiyā, ",
  "pli-tv-bu-vb-sk40:1.9": "ajānantassa, ",
  "pli-tv-bu-vb-sk40:1.10": "gilānassa, ",
  "pli-tv-bu-vb-sk40:1.11": "khajjake, ",
  "pli-tv-bu-vb-sk40:1.12": "phalāphale, ",
  "pli-tv-bu-vb-sk40:1.13": "uttaribhaṅge, ",
  "pli-tv-bu-vb-sk40:1.14": "āpadāsu, ",
  "pli-tv-bu-vb-sk40:1.15": "ummattakassa, ",
  "pli-tv-bu-vb-sk40:1.16": "ādikammikassāti. ",
  "pli-tv-bu-vb-sk40:1.17": "Dasamasikkhāpadaṁ niṭṭhitaṁ. ",
  "pli-tv-bu-vb-sk40:1.18": "Sakkaccavaggo catuttho. "
}