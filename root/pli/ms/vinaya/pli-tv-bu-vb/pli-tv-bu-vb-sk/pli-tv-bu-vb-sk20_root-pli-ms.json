{
  "pli-tv-bu-vb-sk20:0.1": "Theravāda Vinaya ",
  "pli-tv-bu-vb-sk20:0.2": "Mahāvibhaṅga ",
  "pli-tv-bu-vb-sk20:0.3": "Sekhiyakaṇḍa ",
  "pli-tv-bu-vb-sk20:0.4": "Ujjagghikavagga ",
  "pli-tv-bu-vb-sk20:0.5": "20. Dutiyasīsappacālakasikkhāpada ",
  "pli-tv-bu-vb-sk20:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme. ",
  "pli-tv-bu-vb-sk20:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū sīsappacālakaṁ antaraghare nisīdanti sīsaṁ olambentā …pe…. ",
  "pli-tv-bu-vb-sk20:1.3.1": "“Na sīsappacālakaṁ antaraghare nisīdissāmīti sikkhā karaṇīyā”ti. ",
  "pli-tv-bu-vb-sk20:1.4": "Na sīsappacālakaṁ antaraghare nisīditabbaṁ. ",
  "pli-tv-bu-vb-sk20:1.5": "Sīsaṁ paggahetvā nisīditabbaṁ. ",
  "pli-tv-bu-vb-sk20:1.6": "Yo anādariyaṁ paṭicca sīsappacālakaṁ antaraghare nisīdati sīsaṁ olambento, āpatti dukkaṭassa. ",
  "pli-tv-bu-vb-sk20:1.7.1": "Anāpatti—",
  "pli-tv-bu-vb-sk20:1.8": "asañcicca, ",
  "pli-tv-bu-vb-sk20:1.9": "assatiyā, ",
  "pli-tv-bu-vb-sk20:1.10": "ajānantassa, ",
  "pli-tv-bu-vb-sk20:1.11": "gilānassa, ",
  "pli-tv-bu-vb-sk20:1.12": "vāsūpagatassa, ",
  "pli-tv-bu-vb-sk20:1.13": "āpadāsu, ",
  "pli-tv-bu-vb-sk20:1.14": "ummattakassa, ",
  "pli-tv-bu-vb-sk20:1.15": "ādikammikassāti. ",
  "pli-tv-bu-vb-sk20:1.16": "Dasamasikkhāpadaṁ niṭṭhitaṁ. ",
  "pli-tv-bu-vb-sk20:1.17": "Ujjagghikavaggo dutiyo. "
}