{
  "an8.15:0.1": "Aṅguttara Nikāya 8 ",
  "an8.15:0.2": "2. Mahāvagga ",
  "an8.15:0.3": "15. Malasutta ",
  "an8.15:1.1": "“Aṭṭhimāni, bhikkhave, malāni. ",
  "an8.15:1.2": "Katamāni aṭṭha? ",
  "an8.15:1.3": "Asajjhāyamalā, bhikkhave, mantā; ",
  "an8.15:1.4": "anuṭṭhānamalā, bhikkhave, gharā; ",
  "an8.15:1.5": "malaṃ, bhikkhave, vaṇṇassa kosajjaṃ; ",
  "an8.15:1.6": "pamādo, bhikkhave, rakkhato malaṃ; ",
  "an8.15:1.7": "malaṃ, bhikkhave, itthiyā duccaritaṃ; ",
  "an8.15:1.8": "maccheraṃ, bhikkhave, dadato malaṃ; ",
  "an8.15:1.9": "malā, bhikkhave, pāpakā akusalā dhammā asmiṃ loke paramhi ca; ",
  "an8.15:1.10": "tato, bhikkhave, malā malataraṃ avijjā paramaṃ malaṃ. ",
  "an8.15:1.11": "Imāni kho, bhikkhave, aṭṭha malānīti. ",
  "an8.15:2.1": "Asajjhāyamalā mantā, ",
  "an8.15:2.2": "anuṭṭhānamalā gharā; ",
  "an8.15:2.3": "Malaṃ vaṇṇassa kosajjaṃ, ",
  "an8.15:2.4": "pamādo rakkhato malaṃ. ",
  "an8.15:3.1": "Malitthiyā duccaritaṃ, ",
  "an8.15:3.2": "maccheraṃ dadato malaṃ; ",
  "an8.15:3.3": "Malā ve pāpakā dhammā, ",
  "an8.15:3.4": "asmiṃ loke paramhi ca; ",
  "an8.15:3.5": "Tato malā malataraṃ, ",
  "an8.15:3.6": "avijjā paramaṃ malan”ti. ",
  "an8.15:3.7": "Pañcamaṃ. "
}