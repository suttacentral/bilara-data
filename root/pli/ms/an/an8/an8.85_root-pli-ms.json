{
  "an8.85:0.1": "Aṅguttara Nikāya 8 ",
  "an8.85:0.2": "9. Sativagga ",
  "an8.85:0.3": "85. Samaṇasutta ",
  "an8.85:1.1": "“‘Samaṇo’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa. ",
  "an8.85:1.2": "‘Brāhmaṇo’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa. ",
  "an8.85:1.3": "‘Vedagū’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa. ",
  "an8.85:1.4": "‘Bhisakko’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa. ",
  "an8.85:1.5": "‘Nimmalo’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa. ",
  "an8.85:1.6": "‘Vimalo’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa. ",
  "an8.85:1.7": "‘Ñāṇī’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa. ",
  "an8.85:1.8": "‘Vimutto’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassāti. ",
  "an8.85:2.1": "Yaṃ samaṇena pattabbaṃ, ",
  "an8.85:2.2": "brāhmaṇena vusīmatā; ",
  "an8.85:2.3": "Yaṃ vedagunā pattabbaṃ, ",
  "an8.85:2.4": "bhisakkena anuttaraṃ. ",
  "an8.85:3.1": "Yaṃ nimmalena pattabbaṃ, ",
  "an8.85:3.2": "vimalena sucīmatā; ",
  "an8.85:3.3": "Yaṃ ñāṇinā ca pattabbaṃ, ",
  "an8.85:3.4": "vimuttena anuttaraṃ. ",
  "an8.85:4.1": "Sohaṃ vijitasaṅgāmo, ",
  "an8.85:4.2": "mutto mocemi bandhanā; ",
  "an8.85:4.3": "Nāgomhi paramadanto, ",
  "an8.85:4.4": "asekho parinibbuto”ti. ",
  "an8.85:4.5": "Pañcamaṃ. "
}