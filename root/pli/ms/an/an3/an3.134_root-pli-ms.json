{
  "an3.134:0.1": "Aṅguttara Nikāya 3 ",
  "an3.134:0.2": "14. Yodhājīvavagga ",
  "an3.134:0.3": "134. Parisāsutta ",
  "an3.134:1.1": "“Tisso imā, bhikkhave, parisā. ",
  "an3.134:1.2": "Katamā tisso? ",
  "an3.134:1.3": "Ukkācitavinītā parisā, paṭipucchāvinītā parisā, yāvatāvinītā parisā—",
  "an3.134:1.4": "imā kho, bhikkhave, tisso parisā”ti. ",
  "an3.134:1.5": "Dutiyaṃ. "
}