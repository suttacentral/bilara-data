{
  "an5.229:0.1": "Aṅguttara Nikāya 5 ",
  "an5.229:0.2": "23. Dīghacārikavagga ",
  "an5.229:0.3": "229. Paṭhamakaṇhasappasutta ",
  "an5.229:1.1": "“Pañcime, bhikkhave, ādīnavā kaṇhasappe. ",
  "an5.229:1.2": "Katame pañca? ",
  "an5.229:1.3": "Asuci, duggandho, sabhīru, sappaṭibhayo, mittadubbhī. ",
  "an5.229:1.4": "Ime kho, bhikkhave, pañca ādīnavā kaṇhasappe. ",
  "an5.229:2.1": "Evamevaṃ kho, bhikkhave, pañcime ādīnavā mātugāme. ",
  "an5.229:2.2": "Katame pañca? ",
  "an5.229:2.3": "Asuci, duggandho, sabhīru, sappaṭibhayo, mittadubbhī. ",
  "an5.229:2.4": "Ime kho, bhikkhave, pañca ādīnavā mātugāme”ti. ",
  "an5.229:2.5": "Navamaṃ. "
}