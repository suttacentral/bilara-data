{
  "dt1.1:0.1": "Dhātukathā",
  "dt1.1:0.2": "Uddesa",
  "dt1.1:0.3": "1. Nayamātikā",
  "dt1.1:1.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "dt1.1:2.1": "Saṅgaho asaṅgaho ",
  "dt1.1:3.1": "Saṅgahitena asaṅgahitaṁ ",
  "dt1.1:4.1": "Asaṅgahitena saṅgahitaṁ ",
  "dt1.1:5.1": "Saṅgahitena saṅgahitaṁ ",
  "dt1.1:6.1": "Asaṅgahitena asaṅgahitaṁ ",
  "dt1.1:7.1": "Sampayogo vippayogo ",
  "dt1.1:8.1": "Sampayuttena vippayuttaṁ ",
  "dt1.1:9.1": "Vippayuttena sampayuttaṁ ",
  "dt1.1:10.1": "Sampayuttena sampayuttaṁ ",
  "dt1.1:11.1": "Vippayuttena vippayuttaṁ ",
  "dt1.1:12.1": "Saṅgahitena sampayuttaṁ vippayuttaṁ ",
  "dt1.1:13.1": "Sampayuttena saṅgahitaṁ asaṅgahitaṁ ",
  "dt1.1:14.1": "Asaṅgahitena sampayuttaṁ vippayuttaṁ ",
  "dt1.1:15.1": "Vippayuttena saṅgahitaṁ asaṅgahitaṁ. "
}