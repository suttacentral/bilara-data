{
  "kv19.3:0.1": "Kathāvatthu",
  "kv19.3:0.2": "Mahāpaṇṇāsaka",
  "kv19.3:0.3": "Ekūnavīsatimavagga",
  "kv19.3:0.4": "Sāmaññaphalakathā",
  "kv19.3:1.1": "Sāmaññaphalaṁ asaṅkhatanti?",
  "kv19.3:1.2": "Āmantā.",
  "kv19.3:1.3": "Nibbānaṁ tāṇaṁ leṇaṁ saraṇaṁ parāyaṇaṁ accutaṁ amatanti?",
  "kv19.3:1.4": "Na hevaṁ vattabbe …pe…",
  "kv19.3:1.5": "sāmaññaphalaṁ asaṅkhataṁ, nibbānaṁ asaṅkhatanti?",
  "kv19.3:1.6": "Āmantā.",
  "kv19.3:1.7": "Dve asaṅkhatānīti?",
  "kv19.3:1.8": "Na hevaṁ vattabbe …pe…",
  "kv19.3:1.9": "dve asaṅkhatānīti?",
  "kv19.3:1.10": "Āmantā.",
  "kv19.3:1.11": "Dve tāṇāni …pe…",
  "kv19.3:1.12": "antarikā vāti?",
  "kv19.3:1.13": "Na hevaṁ vattabbe …pe….",
  "kv19.3:2.1": "Sāmaññaphalaṁ asaṅkhatanti?",
  "kv19.3:2.2": "Āmantā.",
  "kv19.3:2.3": "Sāmaññaṁ asaṅkhatanti?",
  "kv19.3:2.4": "Na hevaṁ vattabbe …pe…",
  "kv19.3:2.5": "sāmaññaṁ saṅkhatanti?",
  "kv19.3:2.6": "Āmantā.",
  "kv19.3:2.7": "Sāmaññaphalaṁ saṅkhatanti?",
  "kv19.3:2.8": "Na hevaṁ vattabbe …pe….",
  "kv19.3:3.1": "Sotāpattiphalaṁ asaṅkhatanti?",
  "kv19.3:3.2": "Āmantā.",
  "kv19.3:3.3": "Sotāpattimaggo asaṅkhatoti?",
  "kv19.3:3.4": "Na hevaṁ vattabbe …pe…",
  "kv19.3:3.5": "sotāpattimaggo saṅkhatoti?",
  "kv19.3:3.6": "Āmantā.",
  "kv19.3:3.7": "Sotāpattiphalaṁ saṅkhatanti?",
  "kv19.3:3.8": "Na hevaṁ vattabbe …pe….",
  "kv19.3:4.1": "Sakadāgāmiphalaṁ …pe…",
  "kv19.3:4.2": "anāgāmiphalaṁ …pe…",
  "kv19.3:4.3": "arahattaphalaṁ asaṅkhatanti?",
  "kv19.3:4.4": "Āmantā.",
  "kv19.3:4.5": "Arahattamaggo asaṅkhatoti?",
  "kv19.3:4.6": "Na hevaṁ vattabbe …pe…",
  "kv19.3:4.7": "arahattamaggo saṅkhatoti?",
  "kv19.3:4.8": "Āmantā.",
  "kv19.3:4.9": "Arahattaphalaṁ saṅkhatanti?",
  "kv19.3:4.10": "Na hevaṁ vattabbe …pe….",
  "kv19.3:5.1": "Sotāpattiphalaṁ asaṅkhataṁ, sakadāgāmiphalaṁ …pe…",
  "kv19.3:5.2": "anāgāmiphalaṁ …pe…",
  "kv19.3:5.3": "arahattaphalaṁ asaṅkhataṁ, nibbānaṁ asaṅkhatanti?",
  "kv19.3:5.4": "Āmantā.",
  "kv19.3:5.5": "Pañca asaṅkhatānīti?",
  "kv19.3:5.6": "Na hevaṁ vattabbe …pe…",
  "kv19.3:5.7": "pañca asaṅkhatānīti?",
  "kv19.3:5.8": "Āmantā.",
  "kv19.3:5.9": "Pañca tāṇāni …pe…",
  "kv19.3:5.10": "antarikā vāti?",
  "kv19.3:5.11": "Na hevaṁ vattabbe …pe….",
  "kv19.3:5.12": "Sāmaññaphalakathā niṭṭhitā."
}