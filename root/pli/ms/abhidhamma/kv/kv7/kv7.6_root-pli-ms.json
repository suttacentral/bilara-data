{
  "kv7.6:0.1": "Kathāvatthu",
  "kv7.6:0.2": "Mahāpaṇṇāsaka",
  "kv7.6:0.3": "Sattamavagga",
  "kv7.6:0.4": "Itodinnakathā",
  "kv7.6:1.1": "Ito dinnena tattha yāpentīti?",
  "kv7.6:1.2": "Āmantā.",
  "kv7.6:1.3": "Ito cīvaraṁ denti taṁ cīvaraṁ tattha paribhuñjantīti?",
  "kv7.6:1.4": "Na hevaṁ vattabbe …pe…",
  "kv7.6:1.5": "ito piṇḍapātaṁ denti, ito senāsanaṁ denti, ito gilānapaccayabhesajjaparikkhāraṁ denti, ito khādanīyaṁ denti, ito bhojanīyaṁ denti, ito pānīyaṁ denti;",
  "kv7.6:1.6": "taṁ pānīyaṁ tattha paribhuñjantīti?",
  "kv7.6:1.7": "Na hevaṁ vattabbe …pe….",
  "kv7.6:2.1": "Ito dinnena tattha yāpentīti?",
  "kv7.6:2.2": "Āmantā.",
  "kv7.6:2.3": "Añño aññassa kārako parakataṁ sukhadukkhaṁ añño karoti, añño paṭisaṁvedetīti?",
  "kv7.6:2.4": "Na hevaṁ vattabbe …pe….",
  "kv7.6:3.1": "Na vattabbaṁ—",
  "kv7.6:3.2": "“ito dinnena tattha yāpentī”ti?",
  "kv7.6:3.3": "Āmantā.",
  "kv7.6:3.4": "Nanu petā attano atthāya dānaṁ dentaṁ anumodenti, cittaṁ pasādenti, pītiṁ uppādenti, somanassaṁ paṭilabhantīti?",
  "kv7.6:3.5": "Āmantā.",
  "kv7.6:3.6": "Hañci petā attano atthāya dānaṁ dentaṁ anumodenti, cittaṁ pasādenti, pītiṁ uppādenti, somanassaṁ paṭilabhanti;",
  "kv7.6:3.7": "tena vata re vattabbe—",
  "kv7.6:3.8": "“ito dinnena tattha yāpentī”ti.",
  "kv7.6:4.1": "Na vattabbaṁ—",
  "kv7.6:4.2": "“ito dinnena tattha yāpentī”ti?",
  "kv7.6:4.3": "Āmantā.",
  "kv7.6:4.4": "Nanu vuttaṁ bhagavatā—",
  "kv7.6:5.1": "“Unname udakaṁ vuṭṭhaṁ,",
  "kv7.6:5.2": "yathāninnaṁ pavattati;",
  "kv7.6:5.3": "Evameva ito dinnaṁ,",
  "kv7.6:5.4": "petānaṁ upakappati.",
  "kv7.6:6.1": "Yathā vārivahā pūrā,",
  "kv7.6:6.2": "paripūrenti sāgaraṁ;",
  "kv7.6:6.3": "Evameva ito dinnaṁ,",
  "kv7.6:6.4": "petānaṁ upakappati.",
  "kv7.6:7.1": "Na hi tattha kasī atthi,",
  "kv7.6:7.2": "gorakkhettha na vijjati;",
  "kv7.6:7.3": "Vaṇijjā tādisī natthi,",
  "kv7.6:7.4": "hiraññena kayākayaṁ;",
  "kv7.6:7.5": "Ito dinnena yāpenti,",
  "kv7.6:7.6": "petā kālaṅkatā tahin”ti.",
  "kv7.6:8.1": "Attheva suttantoti?",
  "kv7.6:8.2": "Āmantā.",
  "kv7.6:8.3": "Tena hi ito dinnena tattha yāpentīti.",
  "kv7.6:9.1": "Na vattabbaṁ—",
  "kv7.6:9.2": "“ito dinnena tattha yāpentī”ti?",
  "kv7.6:9.3": "Āmantā.",
  "kv7.6:9.4": "Nanu vuttaṁ bhagavatā—",
  "kv7.6:9.5": "“pañcimāni, bhikkhave, ṭhānāni sampassantā mātāpitaro puttaṁ icchanti kule jāyamānaṁ.",
  "kv7.6:9.6": "Katamāni pañca?",
  "kv7.6:9.7": "Bhaṭo vā no bharissati, kiccaṁ vā no karissati, kulavaṁso ciraṁ ṭhassati, dāyajjaṁ paṭipajjissati, atha vā pana petānaṁ kālaṅkatānaṁ dakkhiṇaṁ anuppadassati—",
  "kv7.6:9.8": "imāni kho, bhikkhave, pañca ṭhānāni sampassantā mātāpitaro puttaṁ icchanti kule jāyamānanti.",
  "kv7.6:10.1": "Pañca ṭhānāni sampassaṁ,",
  "kv7.6:10.2": "puttaṁ icchanti paṇḍitā;",
  "kv7.6:10.3": "Bhaṭo vā no bharissati,",
  "kv7.6:10.4": "kiccaṁ vā no karissati.",
  "kv7.6:11.1": "Kulavaṁso ciraṁ tiṭṭhe,",
  "kv7.6:11.2": "dāyajjaṁ paṭipajjati;",
  "kv7.6:11.3": "Atha vā pana petānaṁ,",
  "kv7.6:11.4": "dakkhiṇaṁ anuppadassati.",
  "kv7.6:12.1": "Ṭhānānetāni sampassaṁ,",
  "kv7.6:12.2": "puttaṁ icchanti paṇḍitā;",
  "kv7.6:12.3": "Tasmā santo sappurisā,",
  "kv7.6:12.4": "kataññū katavedino.",
  "kv7.6:13.1": "Bharanti mātāpitaro,",
  "kv7.6:13.2": "pubbe katamanussaraṁ;",
  "kv7.6:13.3": "Karonti tesaṁ kiccāni,",
  "kv7.6:13.4": "yathā taṁ pubbakārinaṁ.",
  "kv7.6:14.1": "Ovādakārī bhaṭaposī,",
  "kv7.6:14.2": "kulavaṁsaṁ ahāpayaṁ;",
  "kv7.6:14.3": "Saddho sīlena sampanno,",
  "kv7.6:14.4": "putto hoti pasaṁsiyo”ti.",
  "kv7.6:15.1": "Attheva suttantoti?",
  "kv7.6:15.2": "Āmantā.",
  "kv7.6:15.3": "Tena hi ito dinnena tattha yāpentīti.",
  "kv7.6:15.4": "Ito dinnakathā niṭṭhitā."
}