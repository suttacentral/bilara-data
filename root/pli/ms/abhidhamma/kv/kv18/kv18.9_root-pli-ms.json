{
  "kv18.9:0.1": "Kathāvatthu",
  "kv18.9:0.2": "Mahāpaṇṇāsaka",
  "kv18.9:0.3": "Aṭṭhārasamavagga",
  "kv18.9:0.4": "Cakkhunārūpaṁpassatītikathā",
  "kv18.9:1.1": "Cakkhunā rūpaṁ passatīti?",
  "kv18.9:1.2": "Āmantā.",
  "kv18.9:1.3": "Rūpena rūpaṁ passatīti?",
  "kv18.9:1.4": "Na hevaṁ vattabbe …pe…",
  "kv18.9:1.5": "rūpena rūpaṁ passatīti?",
  "kv18.9:1.6": "Āmantā.",
  "kv18.9:1.7": "Rūpena rūpaṁ paṭivijānātīti?",
  "kv18.9:1.8": "Na hevaṁ vattabbe …pe…",
  "kv18.9:1.9": "rūpena rūpaṁ paṭivijānātīti?",
  "kv18.9:1.10": "Āmantā.",
  "kv18.9:1.11": "Rūpaṁ manoviññāṇanti?",
  "kv18.9:1.12": "Na hevaṁ vattabbe …pe…",
  "kv18.9:1.13": "cakkhunā rūpaṁ passatīti?",
  "kv18.9:1.14": "Āmantā.",
  "kv18.9:1.15": "Atthi cakkhussa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:1.16": "Na hevaṁ vattabbe …pe…",
  "kv18.9:1.17": "nanu natthi cakkhussa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:1.18": "Āmantā.",
  "kv18.9:1.19": "Hañci natthi cakkhussa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv18.9:1.20": "“cakkhunā rūpaṁ passatī”ti.",
  "kv18.9:2.1": "Sotena saddaṁ suṇātīti …pe…",
  "kv18.9:2.2": "ghānena gandhaṁ ghāyatīti …pe…",
  "kv18.9:2.3": "jivhāya rasaṁ sāyatīti …pe…",
  "kv18.9:2.4": "kāyena phoṭṭhabbaṁ phusatīti?",
  "kv18.9:2.5": "Āmantā.",
  "kv18.9:2.6": "Rūpena rūpaṁ phusatīti?",
  "kv18.9:2.7": "Na hevaṁ vattabbe …pe….",
  "kv18.9:3.1": "Rūpena rūpaṁ phusatīti?",
  "kv18.9:3.2": "Āmantā.",
  "kv18.9:3.3": "Rūpena rūpaṁ paṭivijānātīti?",
  "kv18.9:3.4": "Na hevaṁ vattabbe …pe…",
  "kv18.9:3.5": "rūpena rūpaṁ paṭivijānātīti?",
  "kv18.9:3.6": "Āmantā.",
  "kv18.9:3.7": "Rūpaṁ manoviññāṇanti?",
  "kv18.9:3.8": "Na hevaṁ vattabbe …pe…",
  "kv18.9:3.9": "kāyena phoṭṭhabbaṁ phusatīti?",
  "kv18.9:3.10": "Āmantā.",
  "kv18.9:3.11": "Atthi kāyassa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:3.12": "Na hevaṁ vattabbe …pe…",
  "kv18.9:3.13": "nanu natthi kāyassa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:3.14": "Āmantā.",
  "kv18.9:3.15": "Hañci natthi kāyassa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv18.9:3.16": "“kāyena phoṭṭhabbaṁ phusatī”ti …pe….",
  "kv18.9:4.1": "Na vattabbaṁ—",
  "kv18.9:4.2": "“cakkhunā rūpaṁ passatī”ti …pe…",
  "kv18.9:4.3": "“kāyena phoṭṭhabbaṁ phusatī”ti?",
  "kv18.9:4.4": "Āmantā.",
  "kv18.9:4.5": "Nanu vuttaṁ bhagavatā—",
  "kv18.9:4.6": "“idha, bhikkhave, bhikkhu cakkhunā rūpaṁ passati …pe…",
  "kv18.9:4.7": "kāyena phoṭṭhabbaṁ phusatī”ti.",
  "kv18.9:4.8": "Attheva suttantoti?",
  "kv18.9:4.9": "Āmantā.",
  "kv18.9:4.10": "Tena hi cakkhunā rūpaṁ passati …pe…",
  "kv18.9:4.11": "kāyena phoṭṭhabbaṁ phusatīti.",
  "kv18.9:4.12": "Cakkhunā rūpaṁ passatītikathā niṭṭhitā.",
  "kv18.9:4.13": "Aṭṭhārasamo vaggo.",
  "kv18.9:4.14": "Tassuddānaṁ",
  "kv18.9:5.1": "Buddho bhagavā manussaloke aṭṭhāsi,",
  "kv18.9:5.2": "buddhena bhagavatā dhammo desito,",
  "kv18.9:5.3": "natthi buddhassa bhagavato karuṇā,",
  "kv18.9:5.4": "buddhassa bhagavato uccārapassāvo ativiya aññe gandhajāte adhiggaṇhāti,",
  "kv18.9:5.5": "ekena ariyamaggena cattāri sāmaññaphalāni sacchikaroti,",
  "kv18.9:5.6": "jhānā jhānaṁ saṅkamati,",
  "kv18.9:5.7": "atthi jhānantarikā,",
  "kv18.9:5.8": "samāpanno saddaṁ suṇāti,",
  "kv18.9:5.9": "cakkhunā rūpaṁ passati …pe…",
  "kv18.9:5.10": "kāyena phoṭṭhabbaṁ phusati."
}