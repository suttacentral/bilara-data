{
  "kv8.5:0.1": "Kathāvatthu",
  "kv8.5:0.2": "Mahāpaṇṇāsaka",
  "kv8.5:0.3": "Aṭṭhamavagga",
  "kv8.5:0.4": "Rūpadhātukathā",
  "kv8.5:1.1": "Rūpino dhammā rūpadhātūti?",
  "kv8.5:1.2": "Āmantā.",
  "kv8.5:1.3": "Rūpaṁ bhavo gati sattāvāso saṁsāro yoni viññāṇaṭṭhiti attabhāvapaṭilābhoti?",
  "kv8.5:1.4": "Na hevaṁ vattabbe …pe…",
  "kv8.5:1.5": "atthi rūpūpagaṁ kammanti?",
  "kv8.5:1.6": "Na hevaṁ vattabbe …pe…",
  "kv8.5:1.7": "atthi rūpūpagā sattāti?",
  "kv8.5:1.8": "Na hevaṁ vattabbe …pe…",
  "kv8.5:1.9": "rūpe sattā jāyanti jīyanti mīyanti cavanti upapajjantīti?",
  "kv8.5:1.10": "Na hevaṁ vattabbe …pe…",
  "kv8.5:1.11": "rūpe atthi rūpaṁ vedanā saññā saṅkhārā viññāṇanti?",
  "kv8.5:1.12": "Na hevaṁ vattabbe …pe…",
  "kv8.5:1.13": "rūpaṁ pañcavokārabhavoti?",
  "kv8.5:1.14": "Na hevaṁ vattabbe …pe….",
  "kv8.5:2.1": "Rūpadhātu bhavo gati …pe…",
  "kv8.5:2.2": "attabhāvapaṭilābhoti?",
  "kv8.5:2.3": "Āmantā.",
  "kv8.5:2.4": "Rūpaṁ bhavo gati …pe…",
  "kv8.5:2.5": "attabhāvapaṭilābhoti?",
  "kv8.5:2.6": "Na hevaṁ vattabbe …pe…",
  "kv8.5:2.7": "atthi rūpadhātūpagaṁ kammanti?",
  "kv8.5:2.8": "Āmantā.",
  "kv8.5:2.9": "Atthi rūpūpagaṁ kammanti?",
  "kv8.5:2.10": "Na hevaṁ vattabbe …pe…",
  "kv8.5:2.11": "atthi rūpadhātūpagā sattāti?",
  "kv8.5:2.12": "Āmantā.",
  "kv8.5:2.13": "Atthi rūpūpagā sattāti?",
  "kv8.5:2.14": "Na hevaṁ vattabbe …pe….",
  "kv8.5:3.1": "Rūpadhātuyā sattā jāyanti jīyanti mīyanti cavanti upapajjantīti?",
  "kv8.5:3.2": "Āmantā.",
  "kv8.5:3.3": "Rūpe sattā jāyanti jīyanti mīyanti cavanti upapajjantīti?",
  "kv8.5:3.4": "Na hevaṁ vattabbe …pe…",
  "kv8.5:3.5": "rūpadhātuyā atthi rūpaṁ vedanā saññā saṅkhārā viññāṇanti?",
  "kv8.5:3.6": "Āmantā.",
  "kv8.5:3.7": "Rūpe atthi rūpaṁ vedanā saññā saṅkhārā viññāṇanti?",
  "kv8.5:3.8": "Na hevaṁ vattabbe …pe…",
  "kv8.5:3.9": "rūpadhātu pañcavokārabhavoti?",
  "kv8.5:3.10": "Āmantā.",
  "kv8.5:3.11": "Rūpaṁ pañcavokārabhavoti?",
  "kv8.5:3.12": "Na hevaṁ vattabbe …pe….",
  "kv8.5:4.1": "Rūpino dhammā rūpadhātu, kāmadhātuyā atthi rūpanti?",
  "kv8.5:4.2": "Āmantā.",
  "kv8.5:4.3": "Sāva kāmadhātu, sā rūpadhātūti?",
  "kv8.5:4.4": "Na hevaṁ vattabbe …pe…",
  "kv8.5:4.5": "sāva kāmadhātu, sā rūpadhātūti?",
  "kv8.5:4.6": "Āmantā.",
  "kv8.5:4.7": "Kāmabhavena samannāgato puggalo dvīhi bhavehi samannāgato hoti—",
  "kv8.5:4.8": "kāmabhavena ca rūpabhavena cāti?",
  "kv8.5:4.9": "Na hevaṁ vattabbe.",
  "kv8.5:4.10": "Rūpadhātukathā niṭṭhitā."
}