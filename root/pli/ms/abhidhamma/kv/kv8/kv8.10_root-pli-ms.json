{
  "kv8.10:0.1": "Kathāvatthu",
  "kv8.10:0.2": "Mahāpaṇṇāsaka",
  "kv8.10:0.3": "Aṭṭhamavagga",
  "kv8.10:0.4": "Jīvitindriyakathā",
  "kv8.10:1.1": "Natthi rūpajīvitindriyanti?",
  "kv8.10:1.2": "Āmantā.",
  "kv8.10:1.3": "Natthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanāti?",
  "kv8.10:1.4": "Na hevaṁ vattabbe …pe…",
  "kv8.10:1.5": "atthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanāti?",
  "kv8.10:1.6": "Āmantā.",
  "kv8.10:1.7": "Hañci atthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā, no ca vata re vattabbe—",
  "kv8.10:1.8": "“natthi rūpajīvitindriyan”ti.",
  "kv8.10:2.1": "Atthi arūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā, atthi arūpajīvitindriyanti?",
  "kv8.10:2.2": "Āmantā.",
  "kv8.10:2.3": "Atthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā, atthi rūpajīvitindriyanti?",
  "kv8.10:2.4": "Na hevaṁ vattabbe …pe…",
  "kv8.10:2.5": "atthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā, natthi rūpajīvitindriyanti?",
  "kv8.10:2.6": "Āmantā.",
  "kv8.10:2.7": "Atthi arūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā, natthi arūpajīvitindriyanti?",
  "kv8.10:2.8": "Na hevaṁ vattabbe …pe….",
  "kv8.10:3.1": "Arūpīnaṁ dhammānaṁ āyu arūpajīvitindriyanti?",
  "kv8.10:3.2": "Āmantā.",
  "kv8.10:3.3": "Rūpīnaṁ dhammānaṁ āyu rūpajīvitindriyanti?",
  "kv8.10:3.4": "Na hevaṁ vattabbe …pe…",
  "kv8.10:3.5": "rūpīnaṁ dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:3.6": "“rūpajīvitindriyan”ti?",
  "kv8.10:3.7": "Āmantā.",
  "kv8.10:3.8": "Arūpīnaṁ dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:3.9": "“arūpajīvitindriyan”ti?",
  "kv8.10:3.10": "Na hevaṁ vattabbe …pe….",
  "kv8.10:4.1": "Rūpīnaṁ dhammānaṁ āyu arūpajīvitindriyanti?",
  "kv8.10:4.2": "Āmantā.",
  "kv8.10:4.3": "Arūpīnaṁ dhammānaṁ āyu rūpajīvitindriyanti?",
  "kv8.10:4.4": "Na hevaṁ vattabbe …pe….",
  "kv8.10:5.1": "Arūpīnaṁ dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:5.2": "“rūpajīvitindriyan”ti?",
  "kv8.10:5.3": "Āmantā.",
  "kv8.10:5.4": "Rūpīnaṁ dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:5.5": "“arūpajīvitindriyan”ti?",
  "kv8.10:5.6": "Na hevaṁ vattabbe …pe….",
  "kv8.10:6.1": "Rūpīnañca arūpīnañca dhammānaṁ āyu arūpajīvitindriyanti?",
  "kv8.10:6.2": "Āmantā.",
  "kv8.10:6.3": "Rūpīnañca arūpīnañca dhammānaṁ āyu rūpajīvitindriyanti?",
  "kv8.10:6.4": "Na hevaṁ vattabbe …pe….",
  "kv8.10:7.1": "Rūpīnañca arūpīnañca dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:7.2": "“rūpajīvitindriyan”ti?",
  "kv8.10:7.3": "Āmantā.",
  "kv8.10:7.4": "Rūpīnañca arūpīnañca dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:7.5": "“arūpajīvitindriyan”ti?",
  "kv8.10:7.6": "Na hevaṁ vattabbe …pe…",
  "kv8.10:7.7": "natthi rūpajīvitindriyanti?",
  "kv8.10:7.8": "Āmantā.",
  "kv8.10:7.9": "Nirodhaṁ samāpannassa natthi jīvitindriyanti?",
  "kv8.10:7.10": "Na hevaṁ vattabbe …pe….",
  "kv8.10:8.1": "Nirodhaṁ samāpannassa atthi jīvitindriyanti?",
  "kv8.10:8.2": "Āmantā.",
  "kv8.10:8.3": "Hañci nirodhaṁ samāpannassa atthi jīvitindriyaṁ, no ca vata re vattabbe—",
  "kv8.10:8.4": "“natthi rūpajīvitindriyan”ti.",
  "kv8.10:9.1": "Nirodhaṁ samāpannassa atthi jīvitindriyanti?",
  "kv8.10:9.2": "Āmantā.",
  "kv8.10:9.3": "Katamakkhandhapariyāpannanti?",
  "kv8.10:9.4": "Saṅkhārakkhandhapariyāpannanti.",
  "kv8.10:9.5": "Nirodhaṁ samāpannassa atthi saṅkhārakkhandhoti?",
  "kv8.10:9.6": "Na hevaṁ vattabbe …pe….",
  "kv8.10:10.1": "Nirodhaṁ samāpannassa atthi saṅkhārakkhandhoti?",
  "kv8.10:10.2": "Āmantā.",
  "kv8.10:10.3": "Nirodhaṁ samāpannassa atthi vedanākkhandho …pe…",
  "kv8.10:10.4": "saññākkhandho …pe…",
  "kv8.10:10.5": "viññāṇakkhandhoti?",
  "kv8.10:10.6": "Na hevaṁ vattabbe …pe….",
  "kv8.10:11.1": "Nirodhaṁ samāpannassa atthi vedanākkhandho …pe…",
  "kv8.10:11.2": "saññākkhandho …pe…",
  "kv8.10:11.3": "viññāṇakkhandhoti?",
  "kv8.10:11.4": "Āmantā.",
  "kv8.10:11.5": "Na nirodhaṁ samāpannoti?",
  "kv8.10:11.6": "Na hevaṁ vattabbe …pe….",
  "kv8.10:12.1": "Natthi rūpajīvitindriyanti?",
  "kv8.10:12.2": "Āmantā.",
  "kv8.10:12.3": "Asaññasattānaṁ natthi jīvitindriyanti?",
  "kv8.10:12.4": "Na hevaṁ vattabbe …pe….",
  "kv8.10:13.1": "Asaññasattānaṁ atthi jīvitindriyanti?",
  "kv8.10:13.2": "Āmantā.",
  "kv8.10:13.3": "Hañci asaññasattānaṁ atthi jīvitindriyaṁ, no ca vata re vattabbe—",
  "kv8.10:13.4": "“natthi rūpajīvitindriyan”ti.",
  "kv8.10:13.5": "Asaññasattānaṁ atthi jīvitindriyanti?",
  "kv8.10:13.6": "Āmantā.",
  "kv8.10:13.7": "Katamakkhandhapariyāpannanti?",
  "kv8.10:13.8": "Saṅkhārakkhandhapariyāpannanti.",
  "kv8.10:13.9": "Asaññasattānaṁ atthi saṅkhārakkhandhoti?",
  "kv8.10:13.10": "Na hevaṁ vattabbe …pe….",
  "kv8.10:14.1": "Asaññasattānaṁ atthi saṅkhārakkhandhoti?",
  "kv8.10:14.2": "Āmantā.",
  "kv8.10:14.3": "Asaññasattānaṁ atthi vedanākkhandho …pe…",
  "kv8.10:14.4": "saññākkhandho …pe…",
  "kv8.10:14.5": "viññāṇakkhandhoti?",
  "kv8.10:14.6": "Na hevaṁ vattabbe …pe….",
  "kv8.10:15.1": "Asaññasattānaṁ atthi vedanākkhandho …pe…",
  "kv8.10:15.2": "saññākkhandho …pe…",
  "kv8.10:15.3": "viññāṇakkhandhoti?",
  "kv8.10:15.4": "Āmantā.",
  "kv8.10:15.5": "Pañcavokārabhavoti?",
  "kv8.10:15.6": "Na hevaṁ vattabbe …pe….",
  "kv8.10:16.1": "Upapattesiyena cittena samuṭṭhitaṁ jīvitindriyaṁ upapattesiye citte bhijjamāne ekadesaṁ bhijjatīti?",
  "kv8.10:16.2": "Āmantā.",
  "kv8.10:16.3": "Upapattesiyena cittena samuṭṭhito phasso upapattesiye citte bhijjamāne ekadeso bhijjatīti?",
  "kv8.10:16.4": "Na hevaṁ vattabbe …pe….",
  "kv8.10:17.1": "Upapattesiyena cittena samuṭṭhito phasso upapattesiye citte bhijjamāne anavaseso bhijjatīti?",
  "kv8.10:17.2": "Āmantā.",
  "kv8.10:17.3": "Upapattesiyena cittena samuṭṭhitaṁ jīvitindriyaṁ upapattesiye citte bhijjamāne anavasesaṁ bhijjatīti?",
  "kv8.10:17.4": "Na hevaṁ vattabbe …pe….",
  "kv8.10:18.1": "Dve jīvitindriyānīti?",
  "kv8.10:18.2": "Āmantā.",
  "kv8.10:18.3": "Dvīhi jīvitehi jīvati, dvīhi maraṇehi mīyatīti?",
  "kv8.10:18.4": "Āmantā.",
  "kv8.10:18.5": "Jīvitindriyakathā niṭṭhitā."
}