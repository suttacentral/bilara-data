{
  "kv6.2:0.1": "Kathāvatthu",
  "kv6.2:0.2": "Niyāmapaṇṇāsaka",
  "kv6.2:0.3": "Chaṭṭhavagga",
  "kv6.2:0.4": "Paṭiccasamuppādakathā",
  "kv6.2:1.1": "Paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:1.2": "Āmantā.",
  "kv6.2:1.3": "Nibbānaṁ tāṇaṁ leṇaṁ saraṇaṁ parāyanaṁ accutaṁ amatanti?",
  "kv6.2:1.4": "Na hevaṁ vattabbe …pe…",
  "kv6.2:1.5": "paṭiccasamuppādo asaṅkhato, nibbānaṁ asaṅkhatanti?",
  "kv6.2:1.6": "Āmantā.",
  "kv6.2:1.7": "Dve asaṅkhatānīti?",
  "kv6.2:1.8": "Na hevaṁ vattabbe …pe…",
  "kv6.2:1.9": "dve asaṅkhatānīti?",
  "kv6.2:1.10": "Āmantā.",
  "kv6.2:1.11": "Dve tāṇāni dve leṇāni dve saraṇāni dve parāyanāni dve accutāni dve amatāni dve nibbānānīti?",
  "kv6.2:1.12": "Na hevaṁ vattabbe …pe…",
  "kv6.2:1.13": "dve nibbānānīti?",
  "kv6.2:1.14": "Āmantā.",
  "kv6.2:1.15": "Atthi dvinnaṁ nibbānānaṁ uccanīcatā hīnapaṇītatā ukkaṁsāvakaṁso sīmā vā bhedo vā rāji vā antarikā vāti?",
  "kv6.2:1.16": "Na hevaṁ vattabbe …pe….",
  "kv6.2:2.1": "Paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.2": "Āmantā.",
  "kv6.2:2.3": "Avijjā asaṅkhatāti?",
  "kv6.2:2.4": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.5": "avijjā saṅkhatāti?",
  "kv6.2:2.6": "Āmantā.",
  "kv6.2:2.7": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.8": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.9": "paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.10": "Āmantā.",
  "kv6.2:2.11": "Avijjāpaccayā saṅkhārā asaṅkhatāti?",
  "kv6.2:2.12": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.13": "avijjāpaccayā saṅkhārā saṅkhatāti?",
  "kv6.2:2.14": "Āmantā.",
  "kv6.2:2.15": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.16": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.17": "paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.18": "Āmantā.",
  "kv6.2:2.19": "Saṅkhārapaccayā viññāṇaṁ asaṅkhatanti?",
  "kv6.2:2.20": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.21": "saṅkhārapaccayā viññāṇaṁ saṅkhatanti?",
  "kv6.2:2.22": "Āmantā.",
  "kv6.2:2.23": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.24": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.25": "paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.26": "Āmantā.",
  "kv6.2:2.27": "Viññāṇapaccayā nāmarūpaṁ asaṅkhatanti?",
  "kv6.2:2.28": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.29": "viññāṇapaccayā nāmarūpaṁ saṅkhatanti?",
  "kv6.2:2.30": "Āmantā.",
  "kv6.2:2.31": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.32": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.33": "paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.34": "Āmantā.",
  "kv6.2:2.35": "Jātipaccayā jarāmaraṇaṁ asaṅkhatanti?",
  "kv6.2:2.36": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.37": "jātipaccayā jarāmaraṇaṁ saṅkhatanti?",
  "kv6.2:2.38": "Āmantā.",
  "kv6.2:2.39": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.40": "Na hevaṁ vattabbe …pe….",
  "kv6.2:3.1": "Na vattabbaṁ—",
  "kv6.2:3.2": "“paṭiccasamuppādo asaṅkhato”ti?",
  "kv6.2:3.3": "Āmantā.",
  "kv6.2:3.4": "Nanu vuttaṁ bhagavatā—",
  "kv6.2:3.5": "“jātipaccayā, bhikkhave, jarāmaraṇaṁ.",
  "kv6.2:3.6": "Uppādā vā tathāgatānaṁ anuppādā vā tathāgatānaṁ ṭhitāva sā dhātu dhammaṭṭhitatā dhammaniyāmatā idappaccayatā.",
  "kv6.2:3.7": "Taṁ tathāgato abhisambujjhati abhisameti.",
  "kv6.2:3.8": "Abhisambujjhitvā abhisametvā ācikkhati deseti paññāpeti paṭṭhapeti vivarati vibhajati uttāniṁ karoti.",
  "kv6.2:3.9": "‘Passathā’ti cāha—",
  "kv6.2:3.10": "jātipaccayā, bhikkhave, jarāmaraṇaṁ.",
  "kv6.2:3.11": "Bhavapaccayā, bhikkhave, jāti …pe…",
  "kv6.2:3.12": "avijjāpaccayā, bhikkhave, saṅkhārā.",
  "kv6.2:3.13": "Uppādā vā tathāgatānaṁ anuppādā vā tathāgatānaṁ ṭhitāva sā dhātu …pe…",
  "kv6.2:3.14": "‘passathā’ti cāha—",
  "kv6.2:3.15": "avijjāpaccayā, bhikkhave, saṅkhārā.",
  "kv6.2:3.16": "Iti kho, bhikkhave, yā tatra tathatā avitathatā anaññathatā idappaccayatā—",
  "kv6.2:3.17": "ayaṁ vuccati, bhikkhave, paṭiccasamuppādo”ti.",
  "kv6.2:3.18": "Attheva suttantoti?",
  "kv6.2:3.19": "Āmantā.",
  "kv6.2:3.20": "Tena hi paṭiccasamuppādo asaṅkhatoti.",
  "kv6.2:4.1": "“Avijjāpaccayā saṅkhārā”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā, nibbānaṁ asaṅkhatanti?",
  "kv6.2:4.2": "Āmantā.",
  "kv6.2:4.3": "Dve asaṅkhatānīti?",
  "kv6.2:4.4": "Na hevaṁ vattabbe …pe…",
  "kv6.2:4.5": "dve asaṅkhatānīti?",
  "kv6.2:4.6": "Āmantā.",
  "kv6.2:4.7": "Dve tāṇāni …pe…",
  "kv6.2:4.8": "antarikā vāti?",
  "kv6.2:4.9": "Na hevaṁ vattabbe …pe….",
  "kv6.2:5.1": "“Avijjāpaccayā saṅkhārā”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā, “saṅkhārapaccayā viññāṇan”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā, nibbānaṁ asaṅkhatanti?",
  "kv6.2:5.2": "Āmantā.",
  "kv6.2:5.3": "Tīṇi asaṅkhatānīti?",
  "kv6.2:5.4": "Na hevaṁ vattabbe …pe….",
  "kv6.2:5.5": "Tīṇi asaṅkhatānīti?",
  "kv6.2:5.6": "Āmantā.",
  "kv6.2:5.7": "Tīṇi tāṇāni …pe…",
  "kv6.2:5.8": "antarikā vāti?",
  "kv6.2:5.9": "Na hevaṁ vattabbe …pe….",
  "kv6.2:6.1": "“Avijjāpaccayā saṅkhārā”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā, “saṅkhārapaccayā viññāṇan”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā …pe…",
  "kv6.2:6.2": "“jātipaccayā jarāmaraṇan”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā, nibbānaṁ asaṅkhatanti?",
  "kv6.2:6.3": "Āmantā.",
  "kv6.2:6.4": "Dvādasa asaṅkhatānīti?",
  "kv6.2:6.5": "Na hevaṁ vattabbe …pe…",
  "kv6.2:6.6": "dvādasa asaṅkhatānīti?",
  "kv6.2:6.7": "Āmantā.",
  "kv6.2:6.8": "Dvādasa tāṇāni dvādasa leṇāni …pe…",
  "kv6.2:6.9": "antarikā vāti?",
  "kv6.2:6.10": "Na hevaṁ vattabbe …pe….",
  "kv6.2:6.11": "Paṭiccasamuppādakathā niṭṭhitā."
}