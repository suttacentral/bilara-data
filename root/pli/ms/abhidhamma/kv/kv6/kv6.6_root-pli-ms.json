{
  "kv6.6:0.1": "Kathāvatthu",
  "kv6.6:0.2": "Mahāpaṇṇāsaka",
  "kv6.6:0.3": "Chaṭṭhavagga",
  "kv6.6:0.4": "Ākāsakathā",
  "kv6.6:1.1": "Ākāso asaṅkhatoti?",
  "kv6.6:1.2": "Āmantā.",
  "kv6.6:1.3": "Nibbānaṁ tāṇaṁ leṇaṁ saraṇaṁ parāyanaṁ accutaṁ amatanti?",
  "kv6.6:1.4": "Na hevaṁ vattabbe …pe…",
  "kv6.6:1.5": "ākāso asaṅkhato, nibbānaṁ asaṅkhatanti?",
  "kv6.6:1.6": "Āmantā.",
  "kv6.6:1.7": "Dve asaṅkhatānīti?",
  "kv6.6:1.8": "Na hevaṁ vattabbe …pe….",
  "kv6.6:1.9": "Dve asaṅkhatānīti?",
  "kv6.6:1.10": "Āmantā.",
  "kv6.6:1.11": "Dve tāṇāni …pe… antarikā vāti?",
  "kv6.6:1.12": "Na hevaṁ vattabbe …pe….",
  "kv6.6:2.1": "Ākāso asaṅkhatoti?",
  "kv6.6:2.2": "Āmantā.",
  "kv6.6:2.3": "Atthi keci anākāsaṁ ākāsaṁ karontīti?",
  "kv6.6:2.4": "Āmantā.",
  "kv6.6:2.5": "Atthi keci saṅkhataṁ asaṅkhataṁ karontīti?",
  "kv6.6:2.6": "Na hevaṁ vattabbe …pe…",
  "kv6.6:2.7": "atthi keci ākāsaṁ anākāsaṁ karontīti?",
  "kv6.6:2.8": "Āmantā.",
  "kv6.6:2.9": "Atthi keci asaṅkhataṁ saṅkhataṁ karontīti?",
  "kv6.6:2.10": "Na hevaṁ vattabbe …pe….",
  "kv6.6:3.1": "Ākāse pakkhino gacchanti, candimasūriyā gacchanti, tārakarūpāni gacchanti, iddhiṁ vikubbanti, bāhuṁ cālenti, pāṇiṁ cālenti, leḍḍuṁ khipanti, laguḷaṁ khipanti, iddhiṁ khipanti, usuṁ khipantīti?",
  "kv6.6:3.2": "Āmantā.",
  "kv6.6:3.3": "Asaṅkhate pakkhino gacchanti, candimasūriyā gacchanti, tārakarūpāni gacchanti, iddhiṁ vikubbanti, bāhuṁ cālenti, pāṇiṁ cālenti, leḍḍuṁ khipanti, laguḷaṁ khipanti, iddhiṁ khipanti, usuṁ khipantīti?",
  "kv6.6:3.4": "Na hevaṁ vattabbe …pe….",
  "kv6.6:4.1": "Ākāsaṁ parivāretvā gharāni karonti koṭṭhāni karontīti?",
  "kv6.6:4.2": "Āmantā.",
  "kv6.6:4.3": "Asaṅkhataṁ parivāretvā gharāni karonti koṭṭhāni karontīti?",
  "kv6.6:4.4": "Na hevaṁ vattabbe …pe….",
  "kv6.6:5.1": "Udapāne khaññamāne anākāso ākāso hotīti?",
  "kv6.6:5.2": "Āmantā.",
  "kv6.6:5.3": "Saṅkhataṁ asaṅkhataṁ hotīti?",
  "kv6.6:5.4": "Na hevaṁ vattabbe …pe….",
  "kv6.6:6.1": "Tucchaudapāne pūriyamāne, tucchakoṭṭhe pūriyamāne, tucchakumbhiyā pūriyamānāya ākāso antaradhāyatīti?",
  "kv6.6:6.2": "Āmantā.",
  "kv6.6:6.3": "Asaṅkhataṁ antaradhāyatīti?",
  "kv6.6:6.4": "Na hevaṁ vattabbe …pe….",
  "kv6.6:7.1": "Na vattabbaṁ—",
  "kv6.6:7.2": "“ākāso asaṅkhato”ti?",
  "kv6.6:7.3": "Āmantā.",
  "kv6.6:7.4": "Ākāso saṅkhatoti?",
  "kv6.6:7.5": "Na hevaṁ vattabbe …pe…",
  "kv6.6:7.6": "tena hi ākāso asaṅkhatoti.",
  "kv6.6:7.7": "Ākāsakathā niṭṭhitā."
}