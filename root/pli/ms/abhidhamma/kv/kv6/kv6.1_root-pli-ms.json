{
  "kv6.1:0.1": "Kathāvatthu",
  "kv6.1:0.2": "Niyāmapaṇṇāsaka",
  "kv6.1:0.3": "Chaṭṭhavagga",
  "kv6.1:0.4": "Niyāmakathā",
  "kv6.1:1.1": "Niyāmo asaṅkhatoti?",
  "kv6.1:1.2": "Āmantā.",
  "kv6.1:1.3": "Nibbānaṁ tāṇaṁ leṇaṁ saraṇaṁ parāyanaṁ accutaṁ amatanti?",
  "kv6.1:1.4": "Na hevaṁ vattabbe …pe….",
  "kv6.1:2.1": "Niyāmo asaṅkhato, nibbānaṁ asaṅkhatanti?",
  "kv6.1:2.2": "Āmantā.",
  "kv6.1:2.3": "Dve asaṅkhatānīti?",
  "kv6.1:2.4": "Na hevaṁ vattabbe …pe….",
  "kv6.1:3.1": "Dve asaṅkhatānīti?",
  "kv6.1:3.2": "Āmantā.",
  "kv6.1:3.3": "Dve tāṇāni dve leṇāni dve saraṇāni dve parāyanāni dve accutāni dve amatāni dve nibbānānīti?",
  "kv6.1:3.4": "Na hevaṁ vattabbe …pe….",
  "kv6.1:4.1": "Dve nibbānānīti?",
  "kv6.1:4.2": "Āmantā.",
  "kv6.1:4.3": "Atthi dvinnaṁ nibbānānaṁ uccanīcatā hīnapaṇītatā ukkaṁsāvakaṁso sīmā vā bhedo vā rāji vā antarikā vāti?",
  "kv6.1:4.4": "Na hevaṁ vattabbe …pe….",
  "kv6.1:5.1": "Niyāmo asaṅkhatoti?",
  "kv6.1:5.2": "Āmantā.",
  "kv6.1:5.3": "Atthi keci niyāmaṁ okkamanti paṭilabhanti uppādenti samuppādenti uṭṭhāpenti samuṭṭhāpenti nibbattenti abhinibbattenti janenti sañjanentīti?",
  "kv6.1:5.4": "Āmantā.",
  "kv6.1:5.5": "Atthi keci asaṅkhataṁ okkamanti paṭilabhanti uppādenti samuppādenti uṭṭhāpenti samuṭṭhāpenti nibbattenti abhinibbattenti janenti sañjanentīti?",
  "kv6.1:5.6": "Na hevaṁ vattabbe …pe….",
  "kv6.1:6.1": "Niyāmo asaṅkhatoti?",
  "kv6.1:6.2": "Āmantā.",
  "kv6.1:6.3": "Maggo asaṅkhatoti?",
  "kv6.1:6.4": "Na hevaṁ vattabbe …pe….",
  "kv6.1:7.1": "Maggo saṅkhatoti?",
  "kv6.1:7.2": "Āmantā.",
  "kv6.1:7.3": "Niyāmo saṅkhatoti?",
  "kv6.1:7.4": "Na hevaṁ vattabbe …pe….",
  "kv6.1:8.1": "Sotāpattiniyāmo asaṅkhatoti?",
  "kv6.1:8.2": "Āmantā.",
  "kv6.1:8.3": "Sotāpattimaggo asaṅkhatoti?",
  "kv6.1:8.4": "Na hevaṁ vattabbe …pe….",
  "kv6.1:9.1": "Sotāpattimaggo saṅkhatoti?",
  "kv6.1:9.2": "Āmantā.",
  "kv6.1:9.3": "Sotāpattiniyāmo saṅkhatoti?",
  "kv6.1:9.4": "Na hevaṁ vattabbe …pe….",
  "kv6.1:10.1": "Sakadāgāminiyāmo …pe…",
  "kv6.1:10.2": "anāgāminiyāmo …pe…",
  "kv6.1:10.3": "arahattaniyāmo asaṅkhatoti?",
  "kv6.1:10.4": "Āmantā.",
  "kv6.1:10.5": "Arahattamaggo asaṅkhatoti?",
  "kv6.1:10.6": "Na hevaṁ vattabbe …pe…",
  "kv6.1:10.7": "arahattamaggo saṅkhatoti?",
  "kv6.1:10.8": "Āmantā.",
  "kv6.1:10.9": "Arahattaniyāmo saṅkhatoti?",
  "kv6.1:10.10": "Na hevaṁ vattabbe …pe….",
  "kv6.1:11.1": "Sotāpattiniyāmo asaṅkhato …pe…",
  "kv6.1:11.2": "arahattaniyāmo asaṅkhato, nibbānaṁ asaṅkhatanti?",
  "kv6.1:11.3": "Āmantā.",
  "kv6.1:11.4": "Pañca asaṅkhatānīti?",
  "kv6.1:11.5": "Na hevaṁ vattabbe …pe…",
  "kv6.1:11.6": "pañca asaṅkhatānīti?",
  "kv6.1:11.7": "Āmantā.",
  "kv6.1:11.8": "Pañca tāṇāni …pe…",
  "kv6.1:11.9": "antarikā vāti?",
  "kv6.1:11.10": "Na hevaṁ vattabbe …pe….",
  "kv6.1:12.1": "Niyāmo asaṅkhatoti?",
  "kv6.1:12.2": "Āmantā.",
  "kv6.1:12.3": "Micchattaniyāmo asaṅkhatoti?",
  "kv6.1:12.4": "Na hevaṁ vattabbe …pe…",
  "kv6.1:12.5": "micchattaniyāmo saṅkhatoti?",
  "kv6.1:12.6": "Āmantā.",
  "kv6.1:12.7": "Sammattaniyāmo saṅkhatoti?",
  "kv6.1:12.8": "Na hevaṁ vattabbe …pe….",
  "kv6.1:13.1": "Na vattabbaṁ—",
  "kv6.1:13.2": "“niyāmo asaṅkhato”ti?",
  "kv6.1:13.3": "Āmantā.",
  "kv6.1:13.4": "Niyāme uppajja niruddhe aniyato hotīti?",
  "kv6.1:13.5": "Na hevaṁ vattabbe …pe….",
  "kv6.1:13.6": "Tena hi niyāmo asaṅkhatoti.",
  "kv6.1:13.7": "Micchattaniyāme uppajja niruddhe aniyato hotīti?",
  "kv6.1:13.8": "Na hevaṁ vattabbe …pe…",
  "kv6.1:13.9": "tena hi micchattaniyāmo asaṅkhatoti.",
  "kv6.1:13.10": "Niyāmakathā niṭṭhitā."
}