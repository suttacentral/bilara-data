{
  "kv6.9:0.1": "Kathāvatthu",
  "kv6.9:0.2": "Mahāpaṇṇāsaka",
  "kv6.9:0.3": "Chaṭṭhavagga",
  "kv6.9:0.4": "Cakkhundriyaṁsanidassanantiādikathā",
  "kv6.9:1.1": "Cakkhundriyaṁ sanidassananti?",
  "kv6.9:1.2": "Āmantā.",
  "kv6.9:1.3": "Rūpaṁ rūpāyatanaṁ rūpadhātu …pe…",
  "kv6.9:1.4": "cakkhussa āpāthaṁ āgacchatīti?",
  "kv6.9:1.5": "Na hevaṁ vattabbe …pe….",
  "kv6.9:2.1": "Cakkhundriyaṁ sanidassananti?",
  "kv6.9:2.2": "Āmantā.",
  "kv6.9:2.3": "Cakkhuñca paṭicca cakkhundriyañca uppajjati cakkhuviññāṇanti?",
  "kv6.9:2.4": "Na hevaṁ vattabbe …pe….",
  "kv6.9:3.1": "Cakkhuñca paṭicca cakkhundriyañca uppajjati cakkhuviññāṇanti?",
  "kv6.9:3.2": "Āmantā.",
  "kv6.9:3.3": "“Cakkhuñca paṭicca cakkhundriyañca uppajjati cakkhuviññāṇan”ti—",
  "kv6.9:3.4": "attheva suttantoti?",
  "kv6.9:3.5": "Natthi.",
  "kv6.9:3.6": "“Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.9:3.7": "attheva suttantoti?",
  "kv6.9:3.8": "Āmantā.",
  "kv6.9:3.9": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.9:3.10": "attheva suttanto, no ca vata re vattabbe—",
  "kv6.9:3.11": "“cakkhuñca paṭicca cakkhundriyañca uppajjati cakkhuviññāṇan”ti.",
  "kv6.9:4.1": "Na vattabbaṁ—",
  "kv6.9:4.2": "“pañcindriyāni sanidassanānī”ti?",
  "kv6.9:4.3": "Āmantā.",
  "kv6.9:4.4": "Nanu passati cakkhuṁ sotaṁ ghānaṁ jivhaṁ kāyanti?",
  "kv6.9:4.5": "Āmantā.",
  "kv6.9:4.6": "Hañci passati cakkhuṁ sotaṁ ghānaṁ jivhaṁ kāyaṁ, tena vata re vattabbe—",
  "kv6.9:4.7": "“pañcindriyāni sanidassanānī”ti …pe….",
  "kv6.9:4.8": "Cakkhundriyaṁ sanidassanantiādikathā niṭṭhitā."
}