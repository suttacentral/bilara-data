{
  "kv6.8:0.1": "Kathāvatthu",
  "kv6.8:0.2": "Mahāpaṇṇāsaka",
  "kv6.8:0.3": "Chaṭṭhavagga",
  "kv6.8:0.4": "Pathavīdhātusanidassanātiādikathā",
  "kv6.8:1.1": "Pathavīdhātu sanidassanāti?",
  "kv6.8:1.2": "Āmantā.",
  "kv6.8:1.3": "Rūpaṁ rūpāyatanaṁ rūpadhātu nīlaṁ pītakaṁ lohitakaṁ odātaṁ cakkhuviññeyyaṁ cakkhusmiṁ paṭihaññati cakkhussa āpāthaṁ āgacchatīti?",
  "kv6.8:1.4": "Na hevaṁ vattabbe …pe….",
  "kv6.8:2.1": "Pathavīdhātu sanidassanāti?",
  "kv6.8:2.2": "Āmantā.",
  "kv6.8:2.3": "Cakkhuñca paṭicca pathavīdhātuñca uppajjati cakkhuviññāṇanti?",
  "kv6.8:2.4": "Na hevaṁ vattabbe …pe….",
  "kv6.8:3.1": "Cakkhuñca paṭicca pathavīdhātuñca uppajjati cakkhuviññāṇanti?",
  "kv6.8:3.2": "Āmantā.",
  "kv6.8:3.3": "“Cakkhuñca paṭicca pathavīdhātuñca uppajjati cakkhuviññāṇan”ti—",
  "kv6.8:3.4": "attheva suttantoti?",
  "kv6.8:3.5": "Natthi.",
  "kv6.8:3.6": "“Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.8:3.7": "attheva suttantoti?",
  "kv6.8:3.8": "Āmantā.",
  "kv6.8:3.9": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.8:3.10": "attheva suttanto, no ca vata re vattabbe—",
  "kv6.8:3.11": "“cakkhuñca paṭicca pathavīdhātuñca uppajjati cakkhuviññāṇan”ti.",
  "kv6.8:4.1": "Na vattabbaṁ—",
  "kv6.8:4.2": "“pathavīdhātu sanidassanā”ti?",
  "kv6.8:4.3": "Āmantā.",
  "kv6.8:4.4": "Nanu passati bhūmiṁ pāsāṇaṁ pabbatanti?",
  "kv6.8:4.5": "Āmantā.",
  "kv6.8:4.6": "Hañci passati bhūmiṁ pāsāṇaṁ pabbataṁ, tena vata re vattabbe—",
  "kv6.8:4.7": "“pathavīdhātu sanidassanā”ti …pe….",
  "kv6.8:5.1": "Na vattabbaṁ—",
  "kv6.8:5.2": "“āpodhātu sanidassanā”ti?",
  "kv6.8:5.3": "Āmantā.",
  "kv6.8:5.4": "Nanu passati udakanti?",
  "kv6.8:5.5": "Āmantā.",
  "kv6.8:5.6": "Hañci passati udakaṁ, tena vata re vattabbe—",
  "kv6.8:5.7": "“āpodhātu sanidassanāti …pe….",
  "kv6.8:6.1": "Na vattabbaṁ—",
  "kv6.8:6.2": "tejodhātu sanidassanāti?",
  "kv6.8:6.3": "Āmantā.",
  "kv6.8:6.4": "Nanu passati aggiṁ jalantanti?",
  "kv6.8:6.5": "Āmantā.",
  "kv6.8:6.6": "Hañci passati aggiṁ jalantaṁ, tena vata re vattabbe—",
  "kv6.8:6.7": "“tejodhātu sanidassanā”ti …pe….",
  "kv6.8:7.1": "Na vattabbaṁ—",
  "kv6.8:7.2": "“vāyodhātu sanidassanā”ti?",
  "kv6.8:7.3": "Āmantā.",
  "kv6.8:7.4": "Nanu passati vātena rukkhe sañcāliyamāneti?",
  "kv6.8:7.5": "Āmantā.",
  "kv6.8:7.6": "Hañci passati vātena rukkhe sañcāliyamāne, tena vata re vattabbe—",
  "kv6.8:7.7": "“vāyodhātu sanidassanā”ti …pe….",
  "kv6.8:7.8": "Pathavīdhātu sanidassanātiādikathā niṭṭhitā."
}