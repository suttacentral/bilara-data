{
  "kv17.9:0.1": "Kathāvatthu",
  "kv17.9:0.2": "Mahāpaṇṇāsaka",
  "kv17.9:0.3": "Sattarasamavagga",
  "kv17.9:0.4": "Navattabbaṁsaṅghassadinnaṁmahapphalantikathā",
  "kv17.9:1.1": "Na vattabbaṁ—",
  "kv17.9:1.2": "“saṅghassa dinnaṁ mahapphalan”ti?",
  "kv17.9:1.3": "Āmantā.",
  "kv17.9:1.4": "Nanu saṅgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṁ puññakkhettaṁ lokassāti?",
  "kv17.9:1.5": "Āmantā.",
  "kv17.9:1.6": "Hañci saṅgho āhuneyyo …pe… anuttaraṁ puññakkhettaṁ lokassa, tena vata re vattabbe—",
  "kv17.9:1.7": "“saṅghassa dinnaṁ mahapphalan”ti.",
  "kv17.9:2.1": "Na vattabbaṁ—",
  "kv17.9:2.2": "“saṅghassa dinnaṁ mahapphalan”ti?",
  "kv17.9:2.3": "Āmantā.",
  "kv17.9:2.4": "Nanu cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatāti?",
  "kv17.9:2.5": "Āmantā.",
  "kv17.9:2.6": "Hañci cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatā, tena vata re vattabbe—",
  "kv17.9:2.7": "“saṅghassa dinnaṁ mahapphalan”ti.",
  "kv17.9:3.1": "Na vattabbaṁ—",
  "kv17.9:3.2": "“saṅghassa dinnaṁ mahapphalan”ti?",
  "kv17.9:3.3": "Āmantā.",
  "kv17.9:3.4": "Nanu vuttaṁ bhagavatā—",
  "kv17.9:3.5": "“saṅghe, gotami, dehi, saṅghe te dinne ahañceva pūjito bhavissāmi saṅgho cā”ti.",
  "kv17.9:3.6": "Attheva suttantoti?",
  "kv17.9:3.7": "Āmantā.",
  "kv17.9:3.8": "Tena hi saṅghassa dinnaṁ mahapphalanti.",
  "kv17.9:4.1": "Na vattabbaṁ—",
  "kv17.9:4.2": "“saṅghassa dinnaṁ mahapphalan”ti?",
  "kv17.9:4.3": "Āmantā.",
  "kv17.9:4.4": "Nanu sakko devānamindo bhagavantaṁ etadavoca—",
  "kv17.9:5.1": "“Yajamānānaṁ manussānaṁ,",
  "kv17.9:5.2": "puññapekkhāna pāṇinaṁ;",
  "kv17.9:5.3": "Karotaṁ opadhikaṁ puññaṁ,",
  "kv17.9:5.4": "kattha dinnaṁ mahapphalan”ti.",
  "kv17.9:6.1": "“Cattāro ca paṭipannā,",
  "kv17.9:6.2": "cattāro ca phale ṭhitā;",
  "kv17.9:6.3": "Esa saṅgho ujubhūto,",
  "kv17.9:6.4": "paññāsīlasamāhito.",
  "kv17.9:7.1": "Yajamānānaṁ manussānaṁ,",
  "kv17.9:7.2": "puññapekkhāna pāṇinaṁ;",
  "kv17.9:7.3": "Karotaṁ opadhikaṁ puññaṁ,",
  "kv17.9:7.4": "saṅghe dinnaṁ mahapphalanti.",
  "kv17.9:8.1": "Eso hi saṅgho vipulo mahaggato,",
  "kv17.9:8.2": "Esappameyyo udadhīva sāgaro;",
  "kv17.9:8.3": "Ete hi seṭṭhā naravīrasāvakā,",
  "kv17.9:8.4": "Pabhaṅkarā dhammamudīrayanti.",
  "kv17.9:9.1": "Tesaṁ sudinnaṁ suhutaṁ suyiṭṭhaṁ,",
  "kv17.9:9.2": "Ye saṅghamuddissa dadanti dānaṁ;",
  "kv17.9:9.3": "Sā dakkhiṇā saṅghagatā patiṭṭhitā,",
  "kv17.9:9.4": "Mahapphalā lokavidūna vaṇṇitā.",
  "kv17.9:10.1": "Etādisaṁ yaññamanussarantā,",
  "kv17.9:10.2": "Ye vedajātā vicaranti loke;",
  "kv17.9:10.3": "Vineyya maccheramalaṁ samūlaṁ,",
  "kv17.9:10.4": "Aninditā saggamupenti ṭhānan”ti.",
  "kv17.9:11.1": "Attheva suttantoti?",
  "kv17.9:11.2": "Āmantā.",
  "kv17.9:11.3": "Tena hi saṅghassa dinnaṁ mahapphalanti.",
  "kv17.9:11.4": "Na vattabbaṁ saṅghassa dinnaṁ mahapphalantikathā niṭṭhitā."
}