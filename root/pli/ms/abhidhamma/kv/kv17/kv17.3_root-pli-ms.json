{
  "kv17.3:0.1": "Kathāvatthu",
  "kv17.3:0.2": "Mahāpaṇṇāsaka",
  "kv17.3:0.3": "Sattarasamavagga",
  "kv17.3:0.4": "Sabbamidaṅkammatotikathā",
  "kv17.3:1.1": "Sabbamidaṁ kammatoti?",
  "kv17.3:1.2": "Āmantā.",
  "kv17.3:1.3": "Kammampi kammatoti?",
  "kv17.3:1.4": "Na hevaṁ vattabbe …pe…",
  "kv17.3:1.5": "sabbamidaṁ kammatoti?",
  "kv17.3:1.6": "Āmantā.",
  "kv17.3:1.7": "Sabbamidaṁ pubbekatahetūti?",
  "kv17.3:1.8": "Na hevaṁ vattabbe …pe…",
  "kv17.3:1.9": "sabbamidaṁ kammatoti?",
  "kv17.3:1.10": "Āmantā.",
  "kv17.3:1.11": "Sabbamidaṁ kammavipākatoti?",
  "kv17.3:1.12": "Na hevaṁ vattabbe …pe….",
  "kv17.3:2.1": "Sabbamidaṁ kammavipākatoti?",
  "kv17.3:2.2": "Āmantā.",
  "kv17.3:2.3": "Kammavipākena pāṇaṁ haneyyāti?",
  "kv17.3:2.4": "Āmantā.",
  "kv17.3:2.5": "Pāṇātipāto saphaloti?",
  "kv17.3:2.6": "Āmantā.",
  "kv17.3:2.7": "Kammavipāko saphaloti?",
  "kv17.3:2.8": "Na hevaṁ vattabbe …pe…",
  "kv17.3:2.9": "kammavipāko aphaloti?",
  "kv17.3:2.10": "Āmantā.",
  "kv17.3:2.11": "Pāṇātipāto aphaloti?",
  "kv17.3:2.12": "Na hevaṁ vattabbe …pe….",
  "kv17.3:3.1": "Kammavipākena adinnaṁ ādiyeyya …pe…",
  "kv17.3:3.2": "musā bhaṇeyya …",
  "kv17.3:3.3": "pisuṇaṁ bhaṇeyya …",
  "kv17.3:3.4": "pharusaṁ bhaṇeyya …",
  "kv17.3:3.5": "samphaṁ palapeyya …",
  "kv17.3:3.6": "sandhiṁ chindeyya …",
  "kv17.3:3.7": "nillopaṁ hareyya …",
  "kv17.3:3.8": "ekāgārikaṁ kareyya …",
  "kv17.3:3.9": "paripanthe tiṭṭheyya …",
  "kv17.3:3.10": "paradāraṁ gaccheyya …",
  "kv17.3:3.11": "gāmaghātakaṁ kareyya …",
  "kv17.3:3.12": "nigamaghātakaṁ kareyya …",
  "kv17.3:3.13": "kammavipākena dānaṁ dadeyya …",
  "kv17.3:3.14": "cīvaraṁ dadeyya …",
  "kv17.3:3.15": "piṇḍapātaṁ dadeyya …",
  "kv17.3:3.16": "senāsanaṁ dadeyya …",
  "kv17.3:3.17": "gilānapaccayabhesajjaparikkhāraṁ dadeyyāti?",
  "kv17.3:3.18": "Āmantā.",
  "kv17.3:3.19": "Gilānapaccayabhesajjaparikkhāro saphaloti?",
  "kv17.3:3.20": "Āmantā.",
  "kv17.3:3.21": "Kammavipāko saphaloti?",
  "kv17.3:3.22": "Na hevaṁ vattabbe …pe…",
  "kv17.3:3.23": "kammavipāko aphaloti?",
  "kv17.3:3.24": "Āmantā.",
  "kv17.3:3.25": "Gilānapaccayabhesajjaparikkhāro aphaloti?",
  "kv17.3:3.26": "Na hevaṁ vattabbe …pe….",
  "kv17.3:4.1": "Na vattabbaṁ—",
  "kv17.3:4.2": "“sabbamidaṁ kammato”ti?",
  "kv17.3:4.3": "Āmantā.",
  "kv17.3:4.4": "Nanu vuttaṁ bhagavatā—",
  "kv17.3:5.1": "“Kammunā vattatī loko,",
  "kv17.3:5.2": "kammunā vattatī pajā;",
  "kv17.3:5.3": "Kammanibandhanā sattā,",
  "kv17.3:5.4": "rathassāṇīva yāyato.",
  "kv17.3:6.1": "Kammena kittiṁ labhate pasaṁsaṁ,",
  "kv17.3:6.2": "Kammena jāniñca vadhañca bandhaṁ;",
  "kv17.3:6.3": "Taṁ kammaṁ nānākaraṇaṁ viditvā,",
  "kv17.3:6.4": "Kasmā vade natthi kammanti loke”ti.",
  "kv17.3:7.1": "Attheva suttantoti?",
  "kv17.3:7.2": "Āmantā.",
  "kv17.3:7.3": "Tena hi “sabbamidaṁ kammato”ti.",
  "kv17.3:7.4": "Sabbamidaṁ kammatotikathā niṭṭhitā."
}