{
  "kv17.6:0.1": "Kathāvatthu",
  "kv17.6:0.2": "Mahāpaṇṇāsaka",
  "kv17.6:0.3": "Sattarasamavagga",
  "kv17.6:0.4": "Navattabbaṁsaṅghodakkhiṇaṁpaṭiggaṇhātikathā",
  "kv17.6:1.1": "Na vattabbaṁ—",
  "kv17.6:1.2": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti?",
  "kv17.6:1.3": "Āmantā.",
  "kv17.6:1.4": "Nanu saṅgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṁ puññakkhettaṁ lokassāti?",
  "kv17.6:1.5": "Āmantā.",
  "kv17.6:1.6": "Hañci saṅgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṁ puññakkhettaṁ lokassa, tena vata re vattabbe—",
  "kv17.6:1.7": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti.",
  "kv17.6:2.1": "Na vattabbaṁ—",
  "kv17.6:2.2": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti?",
  "kv17.6:2.3": "Āmantā.",
  "kv17.6:2.4": "Nanu cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatāti?",
  "kv17.6:2.5": "Āmantā.",
  "kv17.6:2.6": "Hañci cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatā, tena vata re vattabbe—",
  "kv17.6:2.7": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti.",
  "kv17.6:3.1": "Na vattabbaṁ—",
  "kv17.6:3.2": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti?",
  "kv17.6:3.3": "Āmantā.",
  "kv17.6:3.4": "Nanu atthi keci saṅghassa dānaṁ dentīti?",
  "kv17.6:3.5": "Āmantā.",
  "kv17.6:3.6": "Hañci atthi keci saṅghassa dānaṁ denti, tena vata re vattabbe—",
  "kv17.6:3.7": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti.",
  "kv17.6:3.8": "Nanu atthi keci saṅghassa cīvaraṁ denti …pe…",
  "kv17.6:3.9": "piṇḍapātaṁ denti …",
  "kv17.6:3.10": "senāsanaṁ denti …",
  "kv17.6:3.11": "gilānapaccayabhesajjaparikkhāraṁ denti …",
  "kv17.6:3.12": "khādanīyaṁ denti …",
  "kv17.6:3.13": "bhojanīyaṁ denti …pe…",
  "kv17.6:3.14": "pānīyaṁ dentīti?",
  "kv17.6:3.15": "Āmantā.",
  "kv17.6:3.16": "Hañci atthi keci saṅghassa pānīyaṁ denti, tena vata re vattabbe—",
  "kv17.6:3.17": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti.",
  "kv17.6:4.1": "Na vattabbaṁ—",
  "kv17.6:4.2": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti?",
  "kv17.6:4.3": "Āmantā.",
  "kv17.6:4.4": "Nanu vuttaṁ bhagavatā—",
  "kv17.6:5.1": "“Āhutiṁ jātavedova,",
  "kv17.6:5.2": "mahāmeghaṁva medanī;",
  "kv17.6:5.3": "Saṅgho samādhisampanno,",
  "kv17.6:5.4": "paṭiggaṇhāti dakkhiṇan”ti.",
  "kv17.6:6.1": "Attheva suttantoti?",
  "kv17.6:6.2": "Āmantā.",
  "kv17.6:6.3": "Tena hi saṅgho dakkhiṇaṁ paṭiggaṇhātīti.",
  "kv17.6:7.1": "Saṅgho dakkhiṇaṁ paṭiggaṇhātīti?",
  "kv17.6:7.2": "Āmantā.",
  "kv17.6:7.3": "Maggo paṭiggaṇhāti, phalaṁ paṭiggaṇhātīti?",
  "kv17.6:7.4": "Na hevaṁ vattabbe …pe….",
  "kv17.6:7.5": "Na vattabbaṁ saṅgho dakkhiṇaṁ paṭiggaṇhātītikathā niṭṭhitā."
}