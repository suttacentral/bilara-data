{
  "kv17.5:0.1": "Kathāvatthu",
  "kv17.5:0.2": "Mahāpaṇṇāsaka",
  "kv17.5:0.3": "Sattarasamavagga",
  "kv17.5:0.4": "Ṭhapetvāariyamaggantikathā",
  "kv17.5:1.1": "Ṭhapetvā ariyamaggaṁ avasesā saṅkhārā dukkhāti?",
  "kv17.5:1.2": "Āmantā.",
  "kv17.5:1.3": "Dukkhasamudayopi dukkhoti?",
  "kv17.5:1.4": "Na hevaṁ vattabbe …pe…",
  "kv17.5:1.5": "dukkhasamudayopi dukkhoti?",
  "kv17.5:1.6": "Āmantā.",
  "kv17.5:1.7": "Tīṇeva ariyasaccānīti?",
  "kv17.5:1.8": "Na hevaṁ vattabbe …pe…",
  "kv17.5:1.9": "tīṇeva ariyasaccānīti?",
  "kv17.5:1.10": "Āmantā.",
  "kv17.5:1.11": "Nanu cattāri ariyasaccāni vuttāni bhagavatā—",
  "kv17.5:1.12": "dukkhaṁ, dukkhasamudayo, dukkhanirodho, dukkhanirodhagāminī paṭipadāti?",
  "kv17.5:1.13": "Āmantā.",
  "kv17.5:1.14": "Hañci cattāri ariyasaccāni vuttāni bhagavatā—",
  "kv17.5:1.15": "dukkhaṁ, dukkhasamudayo, dukkhanirodho, dukkhanirodhagāminī paṭipadā;",
  "kv17.5:1.16": "no ca vata re vattabbe—",
  "kv17.5:1.17": "“tīṇeva ariyasaccānī”ti.",
  "kv17.5:2.1": "Dukkhasamudayopi dukkhoti?",
  "kv17.5:2.2": "Āmantā.",
  "kv17.5:2.3": "Kenaṭṭhenāti?",
  "kv17.5:2.4": "Aniccaṭṭhena.",
  "kv17.5:2.5": "Ariyamaggo aniccoti?",
  "kv17.5:2.6": "Āmantā.",
  "kv17.5:2.7": "Ariyamaggo dukkhoti?",
  "kv17.5:2.8": "Na hevaṁ vattabbe …pe….",
  "kv17.5:3.1": "Ariyamaggo anicco, so ca na dukkhoti?",
  "kv17.5:3.2": "Āmantā.",
  "kv17.5:3.3": "Dukkhasamudayo anicco, so ca na dukkhoti?",
  "kv17.5:3.4": "Na hevaṁ vattabbe …pe…",
  "kv17.5:3.5": "dukkhasamudayo anicco, so ca dukkhoti?",
  "kv17.5:3.6": "Āmantā.",
  "kv17.5:3.7": "Ariyamaggo anicco, so ca dukkhoti?",
  "kv17.5:3.8": "Na hevaṁ vattabbe …pe….",
  "kv17.5:4.1": "Na vattabbaṁ—",
  "kv17.5:4.2": "“ṭhapetvā ariyamaggaṁ avasesā saṅkhārā dukkhā”ti?",
  "kv17.5:4.3": "Āmantā.",
  "kv17.5:4.4": "Nanu sā dukkhanirodhagāminī paṭipadāti?",
  "kv17.5:4.5": "Āmantā.",
  "kv17.5:4.6": "Hañci sā dukkhanirodhagāminī paṭipadā, tena vata re vattabbe—",
  "kv17.5:4.7": "“ṭhapetvā ariyamaggaṁ avasesā saṅkhārā dukkhā”ti.",
  "kv17.5:4.8": "Ṭhapetvā ariyamaggantikathā niṭṭhitā."
}