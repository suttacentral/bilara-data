{
  "kv12.5:0.1": "Kathāvatthu",
  "kv12.5:0.2": "Mahāpaṇṇāsaka",
  "kv12.5:0.3": "Dvādasamavagga",
  "kv12.5:0.4": "Sattakkhattuparamakathā",
  "kv12.5:1.1": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:1.2": "Āmantā.",
  "kv12.5:1.3": "Mātā jīvitā voropitā …",
  "kv12.5:1.4": "pitā jīvitā voropito …",
  "kv12.5:1.5": "arahā jīvitā voropito …",
  "kv12.5:1.6": "duṭṭhena cittena tathāgatassa lohitaṁ uppāditaṁ …",
  "kv12.5:1.7": "saṅgho bhinnoti?",
  "kv12.5:1.8": "Na hevaṁ vattabbe …pe….",
  "kv12.5:2.1": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:2.2": "Āmantā.",
  "kv12.5:2.3": "Abhabbo antarā dhammaṁ abhisametunti?",
  "kv12.5:2.4": "Na hevaṁ vattabbe …pe….",
  "kv12.5:2.5": "Abhabbo antarā dhammaṁ abhisametunti?",
  "kv12.5:2.6": "Āmantā.",
  "kv12.5:2.7": "Mātā jīvitā voropitā …",
  "kv12.5:2.8": "pitā jīvitā voropito …",
  "kv12.5:2.9": "arahā jīvitā voropito …",
  "kv12.5:2.10": "duṭṭhena cittena tathāgatassa lohitaṁ uppāditaṁ …",
  "kv12.5:2.11": "saṅgho bhinnoti?",
  "kv12.5:2.12": "Na hevaṁ vattabbe …pe….",
  "kv12.5:3.1": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:3.2": "Āmantā.",
  "kv12.5:3.3": "Atthi so niyamo yena niyamena sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:3.4": "Na hevaṁ vattabbe …pe…",
  "kv12.5:3.5": "atthi te satipaṭṭhānā …pe…",
  "kv12.5:3.6": "sammappadhānā …",
  "kv12.5:3.7": "iddhipādā …",
  "kv12.5:3.8": "indriyā …",
  "kv12.5:3.9": "balā …",
  "kv12.5:3.10": "bojjhaṅgā yehi bojjhaṅgehi sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:3.11": "Na hevaṁ vattabbe …pe….",
  "kv12.5:4.1": "Natthi so niyamo yena niyamena sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:4.2": "Āmantā.",
  "kv12.5:4.3": "Hañci natthi so niyamo yena niyamena sattakkhattuparamo puggalo sattakkhattuparamatāniyato, no ca vata re vattabbe—",
  "kv12.5:4.4": "“sattakkhattuparamo puggalo sattakkhattuparamatāniyato”ti.",
  "kv12.5:5.1": "Natthi te satipaṭṭhānā …pe…",
  "kv12.5:5.2": "bojjhaṅgā yehi bojjhaṅgehi sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:5.3": "Āmantā.",
  "kv12.5:5.4": "Hañci natthi te bojjhaṅgā yehi bojjhaṅgehi sattakkhattuparamo puggalo sattakkhattuparamatāniyato, no ca vata re vattabbe—",
  "kv12.5:5.5": "“sattakkhattuparamo puggalo sattakkhattuparamatāniyato”ti.",
  "kv12.5:6.1": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:6.2": "Āmantā.",
  "kv12.5:6.3": "Sakadāgāminiyamenāti?",
  "kv12.5:6.4": "Na hevaṁ vattabbe …pe…",
  "kv12.5:6.5": "anāgāminiyamenāti?",
  "kv12.5:6.6": "Na hevaṁ vattabbe …pe…",
  "kv12.5:6.7": "arahattaniyamenāti?",
  "kv12.5:6.8": "Na hevaṁ vattabbe …pe….",
  "kv12.5:7.1": "Katamena niyamenāti?",
  "kv12.5:7.2": "Sotāpattiniyamenāti.",
  "kv12.5:7.3": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:7.4": "Āmantā.",
  "kv12.5:7.5": "Ye keci sotāpattiniyāmaṁ okkamanti, sabbe te sattakkhattuparamatāniyatāti?",
  "kv12.5:7.6": "Na hevaṁ vattabbe …pe….",
  "kv12.5:8.1": "Na vattabbaṁ—",
  "kv12.5:8.2": "“sattakkhattuparamo puggalo sattakkhattuparamatāniyato”ti?",
  "kv12.5:8.3": "Āmantā.",
  "kv12.5:8.4": "Nanu so sattakkhattuparamoti?",
  "kv12.5:8.5": "Āmantā.",
  "kv12.5:8.6": "Hañci so sattakkhattuparamo, tena vata re vattabbe—",
  "kv12.5:8.7": "“sattakkhattuparamo puggalo sattakkhattuparamatāniyato”ti.",
  "kv12.5:8.8": "Sattakkhattuparamakathā niṭṭhitā."
}