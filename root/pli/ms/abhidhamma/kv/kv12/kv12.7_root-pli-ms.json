{
  "kv12.7:0.1": "Kathāvatthu",
  "kv12.7:0.2": "Mahāpaṇṇāsaka",
  "kv12.7:0.3": "Dvādasamavagga",
  "kv12.7:0.4": "Ekabījīkathā",
  "kv12.7:1.1": "Na vattabbaṁ—",
  "kv12.7:1.2": "“ekabījī puggalo ekabījitāniyato”ti?",
  "kv12.7:1.3": "Āmantā.",
  "kv12.7:1.4": "Nanu so ekabījīti?",
  "kv12.7:1.5": "Āmantā.",
  "kv12.7:1.6": "Hañci so ekabījī, tena vata re vattabbe—",
  "kv12.7:1.7": "“ekabījī puggalo ekabījitāniyato”ti.",
  "kv12.7:1.8": "Ekabījīkathā niṭṭhitā."
}