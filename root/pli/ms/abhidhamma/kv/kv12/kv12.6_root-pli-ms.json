{
  "kv12.6:0.1": "Kathāvatthu",
  "kv12.6:0.2": "Mahāpaṇṇāsaka",
  "kv12.6:0.3": "Dvādasamavagga",
  "kv12.6:0.4": "Kolaṅkolakathā",
  "kv12.6:1.1": "Na vattabbaṁ—",
  "kv12.6:1.2": "“kolaṅkolo puggalo kolaṅkolatāniyato”ti?",
  "kv12.6:1.3": "Āmantā.",
  "kv12.6:1.4": "Nanu so kolaṅkoloti?",
  "kv12.6:1.5": "Āmantā.",
  "kv12.6:1.6": "Hañci so kolaṅkolo, tena vata re vattabbe—",
  "kv12.6:1.7": "“kolaṅkolo puggalo kolaṅkolatāniyato”ti.",
  "kv12.6:1.8": "Kolaṅkolakathā niṭṭhitā."
}