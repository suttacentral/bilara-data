{
  "kv22.3:0.1": "Kathāvatthu",
  "kv22.3:0.2": "Mahāpaṇṇāsaka",
  "kv22.3:0.3": "Bāvīsatimavagga",
  "kv22.3:0.4": "Āneñjakathā",
  "kv22.3:1.1": "Arahā āneñje ṭhito parinibbāyatīti?",
  "kv22.3:1.2": "Āmantā.",
  "kv22.3:1.3": "Nanu arahā pakaticitte ṭhito parinibbāyatīti?",
  "kv22.3:1.4": "Āmantā.",
  "kv22.3:1.5": "Hañci arahā pakaticitte ṭhito parinibbāyati, no ca vata re vattabbe—",
  "kv22.3:1.6": "“arahā āneñje ṭhito parinibbāyatī”ti.",
  "kv22.3:2.1": "Arahā āneñje ṭhito parinibbāyatīti?",
  "kv22.3:2.2": "Āmantā.",
  "kv22.3:2.3": "Arahā kiriyamaye citte ṭhito parinibbāyatīti?",
  "kv22.3:2.4": "Na hevaṁ vattabbe …pe…",
  "kv22.3:2.5": "nanu arahā vipākacitte ṭhito parinibbāyatīti?",
  "kv22.3:2.6": "Āmantā.",
  "kv22.3:2.7": "Hañci arahā vipākacitte ṭhito parinibbāyati, no ca vata re vattabbe—",
  "kv22.3:2.8": "“arahā āneñje ṭhito parinibbāyatī”ti.",
  "kv22.3:3.1": "Arahā āneñje ṭhito parinibbāyatīti?",
  "kv22.3:3.2": "Āmantā.",
  "kv22.3:3.3": "Arahā kiriyābyākate citte ṭhito parinibbāyatīti?",
  "kv22.3:3.4": "Na hevaṁ vattabbe …pe…",
  "kv22.3:3.5": "nanu arahā vipākābyākate citte ṭhito parinibbāyatīti?",
  "kv22.3:3.6": "Āmantā.",
  "kv22.3:3.7": "Hañci arahā vipākābyākate citte ṭhito parinibbāyati, no ca vata re vattabbe—",
  "kv22.3:3.8": "“arahā āneñje ṭhito parinibbāyatī”ti.",
  "kv22.3:4.1": "Arahā āneñje ṭhito parinibbāyatīti?",
  "kv22.3:4.2": "Āmantā.",
  "kv22.3:4.3": "Nanu bhagavā catutthajjhānā vuṭṭhahitvā samanantarā parinibbutoti?",
  "kv22.3:4.4": "Āmantā.",
  "kv22.3:4.5": "Hañci bhagavā catutthajjhānā vuṭṭhahitvā samanantarā parinibbuto, no ca vata re vattabbe—",
  "kv22.3:4.6": "“arahā āneñje ṭhito parinibbāyatī”ti.",
  "kv22.3:4.7": "Āneñjakathā niṭṭhitā."
}