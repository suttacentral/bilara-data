{
  "kv22.1:0.1": "Kathāvatthu",
  "kv22.1:0.2": "Khuddakaaḍḍhapaṇṇāsaka",
  "kv22.1:0.3": "Bāvīsatimavagga",
  "kv22.1:0.4": "Parinibbānakathā",
  "kv22.1:1.1": "Atthi kiñci saṁyojanaṁ appahāya parinibbānanti?",
  "kv22.1:1.2": "Āmantā.",
  "kv22.1:1.3": "Atthi kiñci sakkāyadiṭṭhiṁ appahāya …pe… anottappaṁ appahāya parinibbānanti?",
  "kv22.1:1.4": "Na hevaṁ vattabbe …pe….",
  "kv22.1:2.1": "Atthi kiñci saṁyojanaṁ appahāya parinibbānanti?",
  "kv22.1:2.2": "Āmantā.",
  "kv22.1:2.3": "Arahā sarāgo …pe… sakilesoti?",
  "kv22.1:2.4": "Na hevaṁ vattabbe …pe…",
  "kv22.1:2.5": "nanu arahā nirāgo …pe… nikkilesoti?",
  "kv22.1:2.6": "Āmantā.",
  "kv22.1:2.7": "Hañci arahā nirāgo …pe… nikkileso, no ca vata re vattabbe—",
  "kv22.1:2.8": "“atthi kiñci saṁyojanaṁ appahāya parinibbānan”ti.",
  "kv22.1:3.1": "Na vattabbaṁ—",
  "kv22.1:3.2": "“atthi kiñci saṁyojanaṁ appahāya parinibbānan”ti?",
  "kv22.1:3.3": "Āmantā.",
  "kv22.1:3.4": "Arahā sabbaṁ buddhavisayaṁ jānātīti?",
  "kv22.1:3.5": "Na hevaṁ vattabbe.",
  "kv22.1:3.6": "Tena hi atthi kiñci saṁyojanaṁ appahāya parinibbānanti.",
  "kv22.1:3.7": "Parinibbānakathā niṭṭhitā."
}