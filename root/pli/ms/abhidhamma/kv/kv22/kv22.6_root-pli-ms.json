{
  "kv22.6:0.1": "Kathāvatthu",
  "kv22.6:0.2": "Mahāpaṇṇāsaka",
  "kv22.6:0.3": "Bāvīsatimavagga",
  "kv22.6:0.4": "Abyākatakathā",
  "kv22.6:1.1": "Sabbaṁ supinagatassa cittaṁ abyākatanti?",
  "kv22.6:1.2": "Āmantā.",
  "kv22.6:1.3": "Supinantena pāṇaṁ haneyyāti?",
  "kv22.6:1.4": "Āmantā.",
  "kv22.6:1.5": "Hañci supinantena pāṇaṁ haneyya, no ca vata re vattabbe—",
  "kv22.6:1.6": "“sabbaṁ supinagatassa cittaṁ abyākatan”ti.",
  "kv22.6:2.1": "Supinantena adinnaṁ ādiyeyya …pe…",
  "kv22.6:2.2": "musā bhaṇeyya,",
  "kv22.6:2.3": "pisuṇaṁ bhaṇeyya,",
  "kv22.6:2.4": "pharusaṁ bhaṇeyya,",
  "kv22.6:2.5": "samphaṁ palapeyya,",
  "kv22.6:2.6": "sandhiṁ chindeyya,",
  "kv22.6:2.7": "nillopaṁ hareyya,",
  "kv22.6:2.8": "ekāgārikaṁ kareyya,",
  "kv22.6:2.9": "paripanthe tiṭṭheyya,",
  "kv22.6:2.10": "paradāraṁ gaccheyya,",
  "kv22.6:2.11": "gāmaghātakaṁ kareyya,",
  "kv22.6:2.12": "nigamaghātakaṁ kareyya,",
  "kv22.6:2.13": "supinantena methunaṁ dhammaṁ paṭiseveyya,",
  "kv22.6:2.14": "supinagatassa asuci mucceyya,",
  "kv22.6:2.15": "supinantena dānaṁ dadeyya,",
  "kv22.6:2.16": "cīvaraṁ dadeyya,",
  "kv22.6:2.17": "piṇḍapātaṁ dadeyya,",
  "kv22.6:2.18": "senāsanaṁ dadeyya,",
  "kv22.6:2.19": "gilānapaccayabhesajjaparikkhāraṁ dadeyya,",
  "kv22.6:2.20": "khādanīyaṁ dadeyya,",
  "kv22.6:2.21": "bhojanīyaṁ dadeyya,",
  "kv22.6:2.22": "pānīyaṁ dadeyya,",
  "kv22.6:2.23": "cetiyaṁ vandeyya,",
  "kv22.6:2.24": "cetiye mālaṁ āropeyya,",
  "kv22.6:2.25": "gandhaṁ āropeyya,",
  "kv22.6:2.26": "vilepanaṁ āropeyya …pe…",
  "kv22.6:2.27": "cetiyaṁ abhidakkhiṇaṁ kareyyāti?",
  "kv22.6:2.28": "Āmantā.",
  "kv22.6:2.29": "Hañci supinantena cetiyaṁ abhidakkhiṇaṁ kareyya, no ca vata re vattabbe—",
  "kv22.6:2.30": "“sabbaṁ supinagatassa cittaṁ abyākatan”ti.",
  "kv22.6:3.1": "Na vattabbaṁ—",
  "kv22.6:3.2": "“sabbaṁ supinagatassa cittaṁ abyākatan”ti?",
  "kv22.6:3.3": "Āmantā.",
  "kv22.6:3.4": "Nanu supinagatassa cittaṁ abbohāriyaṁ vuttaṁ bhagavatāti?",
  "kv22.6:3.5": "Āmantā.",
  "kv22.6:3.6": "Hañci supinagatassa cittaṁ abbohāriyaṁ vuttaṁ bhagavatā, tena vata re vattabbe—",
  "kv22.6:3.7": "“sabbaṁ supinagatassa cittaṁ abyākatan”ti.",
  "kv22.6:3.8": "Abyākatakathā niṭṭhitā."
}