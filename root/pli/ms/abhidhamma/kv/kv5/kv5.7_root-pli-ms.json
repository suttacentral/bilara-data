{
  "kv5.7:0.1": "Kathāvatthu",
  "kv5.7:0.2": "Mahāpaṇṇāsaka",
  "kv5.7:0.3": "Pañcamavagga",
  "kv5.7:0.4": "Cittārammaṇakathā",
  "kv5.7:1.1": "Cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇanti?",
  "kv5.7:1.2": "Āmantā.",
  "kv5.7:1.3": "Nanu atthi koci sarāgaṁ cittaṁ “sarāgaṁ cittan”ti pajānātīti?",
  "kv5.7:1.4": "Āmantā.",
  "kv5.7:1.5": "Hañci atthi koci sarāgaṁ cittaṁ “sarāgaṁ cittan”ti pajānāti, no ca vata re vattabbe—",
  "kv5.7:1.6": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:2.1": "Nanu atthi koci vītarāgaṁ cittaṁ …pe…",
  "kv5.7:2.2": "sadosaṁ cittaṁ …",
  "kv5.7:2.3": "vītadosaṁ cittaṁ …",
  "kv5.7:2.4": "samohaṁ cittaṁ …",
  "kv5.7:2.5": "vītamohaṁ cittaṁ …",
  "kv5.7:2.6": "saṅkhittaṁ cittaṁ …",
  "kv5.7:2.7": "vikkhittaṁ cittaṁ …",
  "kv5.7:2.8": "mahaggataṁ cittaṁ …",
  "kv5.7:2.9": "amahaggataṁ cittaṁ …",
  "kv5.7:2.10": "sauttaraṁ cittaṁ …",
  "kv5.7:2.11": "anuttaraṁ cittaṁ …",
  "kv5.7:2.12": "samāhitaṁ cittaṁ …",
  "kv5.7:2.13": "asamāhitaṁ cittaṁ …",
  "kv5.7:2.14": "vimuttaṁ cittaṁ …pe…",
  "kv5.7:2.15": "avimuttaṁ cittaṁ “avimuttaṁ cittan”ti pajānātīti?",
  "kv5.7:2.16": "Āmantā.",
  "kv5.7:2.17": "Hañci atthi koci avimuttaṁ cittaṁ “avimuttaṁ cittan”ti pajānāti, no ca vata re vattabbe—",
  "kv5.7:2.18": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:3.1": "Phassārammaṇe ñāṇaṁ vattabbaṁ—",
  "kv5.7:3.2": "“cetopariyāye ñāṇan”ti?",
  "kv5.7:3.3": "Āmantā.",
  "kv5.7:3.4": "Hañci phassārammaṇe ñāṇaṁ vattabbaṁ cetopariyāye ñāṇaṁ, no ca vata re vattabbe—",
  "kv5.7:3.5": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:3.6": "Vedanārammaṇe ñāṇaṁ …pe…",
  "kv5.7:3.7": "saññārammaṇe ñāṇaṁ …",
  "kv5.7:3.8": "cetanārammaṇe ñāṇaṁ …",
  "kv5.7:3.9": "cittārammaṇe ñāṇaṁ …",
  "kv5.7:3.10": "saddhārammaṇe ñāṇaṁ …",
  "kv5.7:3.11": "vīriyārammaṇe ñāṇaṁ …",
  "kv5.7:3.12": "satārammaṇe ñāṇaṁ …",
  "kv5.7:3.13": "samādhārammaṇe ñāṇaṁ …",
  "kv5.7:3.14": "paññārammaṇe ñāṇaṁ …",
  "kv5.7:3.15": "rāgārammaṇe ñāṇaṁ …",
  "kv5.7:3.16": "dosārammaṇe ñāṇaṁ …",
  "kv5.7:3.17": "mohārammaṇe ñāṇaṁ …pe…",
  "kv5.7:3.18": "anottappārammaṇe ñāṇaṁ vattabbaṁ—",
  "kv5.7:3.19": "“cetopariyāye ñāṇan”ti?",
  "kv5.7:3.20": "Āmantā.",
  "kv5.7:3.21": "Hañci anottappārammaṇe ñāṇaṁ vattabbaṁ cetopariyāye ñāṇaṁ, no ca vata re vattabbe—",
  "kv5.7:3.22": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:4.1": "Phassārammaṇe ñāṇaṁ na vattabbaṁ—",
  "kv5.7:4.2": "“cetopariyāye ñāṇan”ti?",
  "kv5.7:4.3": "Āmantā.",
  "kv5.7:4.4": "Phassapariyāye ñāṇanti?",
  "kv5.7:4.5": "Na hevaṁ vattabbe …pe…",
  "kv5.7:4.6": "vedanārammaṇe ñāṇaṁ …pe…",
  "kv5.7:4.7": "saññārammaṇe ñāṇaṁ …pe…",
  "kv5.7:4.8": "anottappārammaṇe ñāṇaṁ na vattabbaṁ—",
  "kv5.7:4.9": "“cetopariyāye ñāṇan”ti?",
  "kv5.7:4.10": "Āmantā.",
  "kv5.7:4.11": "Anottappapariyāye ñāṇanti?",
  "kv5.7:4.12": "Na hevaṁ vattabbe …pe….",
  "kv5.7:5.1": "Na vattabbaṁ—",
  "kv5.7:5.2": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti?",
  "kv5.7:5.3": "Āmantā.",
  "kv5.7:5.4": "Nanu cetopariyāye ñāṇanti?",
  "kv5.7:5.5": "Āmantā.",
  "kv5.7:5.6": "Hañci cetopariyāye ñāṇaṁ, tena vata re vattabbe—",
  "kv5.7:5.7": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:5.8": "Cittārammaṇakathā niṭṭhitā."
}