{
  "kv5.5:0.1": "Kathāvatthu",
  "kv5.5:0.2": "Mahāpaṇṇāsaka",
  "kv5.5:0.3": "Pañcamavagga",
  "kv5.5:0.4": "Paṭisambhidākathā",
  "kv5.5:1.1": "Sabbaṁ ñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.2": "Āmantā.",
  "kv5.5:1.3": "Sammutiñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.4": "Na hevaṁ vattabbe …pe…",
  "kv5.5:1.5": "sammutiñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.6": "Āmantā.",
  "kv5.5:1.7": "Ye keci sammutiṁ jānanti, sabbe te paṭisambhidāpattāti?",
  "kv5.5:1.8": "Na hevaṁ vattabbe …pe…",
  "kv5.5:1.9": "sabbaṁ ñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.10": "Āmantā.",
  "kv5.5:1.11": "Cetopariyāye ñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.12": "Na hevaṁ vattabbe …pe…",
  "kv5.5:1.13": "cetopariyāye ñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.14": "Āmantā.",
  "kv5.5:1.15": "Ye keci paracittaṁ jānanti, sabbe te paṭisambhidāpattāti?",
  "kv5.5:1.16": "Na hevaṁ vattabbe …pe….",
  "kv5.5:2.1": "Sabbaṁ ñāṇaṁ paṭisambhidāti?",
  "kv5.5:2.2": "Āmantā.",
  "kv5.5:2.3": "Sabbā paññā paṭisambhidāti?",
  "kv5.5:2.4": "Na hevaṁ vattabbe …pe…",
  "kv5.5:2.5": "sabbā paññā paṭisambhidāti?",
  "kv5.5:2.6": "Āmantā.",
  "kv5.5:2.7": "Pathavīkasiṇaṁ samāpattiṁ samāpannassa atthi paññā, sā paññā paṭisambhidāti?",
  "kv5.5:2.8": "Na hevaṁ vattabbe …pe…",
  "kv5.5:2.9": "āpokasiṇaṁ …pe…",
  "kv5.5:2.10": "tejokasiṇaṁ …pe…",
  "kv5.5:2.11": "vāyokasiṇaṁ …pe…",
  "kv5.5:2.12": "nīlakasiṇaṁ …pe…",
  "kv5.5:2.13": "pītakasiṇaṁ …pe…",
  "kv5.5:2.14": "lohitakasiṇaṁ …pe…",
  "kv5.5:2.15": "odātakasiṇaṁ …pe…",
  "kv5.5:2.16": "ākāsānañcāyatanaṁ …pe…",
  "kv5.5:2.17": "viññāṇañcāyatanaṁ …pe…",
  "kv5.5:2.18": "ākiñcaññāyatanaṁ …pe…",
  "kv5.5:2.19": "nevasaññānāsaññāyatanaṁ samāpannassa …pe…",
  "kv5.5:2.20": "dānaṁ dadantassa …pe…",
  "kv5.5:2.21": "cīvaraṁ dadantassa …pe…",
  "kv5.5:2.22": "piṇḍapātaṁ dadantassa …pe…",
  "kv5.5:2.23": "senāsanaṁ dadantassa …pe…",
  "kv5.5:2.24": "gilānapaccayabhesajjaparikkhāraṁ dadantassa atthi paññā, sā paññā paṭisambhidāti?",
  "kv5.5:2.25": "Na hevaṁ vattabbe …pe….",
  "kv5.5:3.1": "Na vattabbaṁ—",
  "kv5.5:3.2": "“sabbaṁ ñāṇaṁ paṭisambhidā”ti?",
  "kv5.5:3.3": "Āmantā.",
  "kv5.5:3.4": "Atthi lokuttarā paññā, sā paññā na paṭisambhidāti?",
  "kv5.5:3.5": "Na hevaṁ vattabbe …pe…",
  "kv5.5:3.6": "tena hi sabbaṁ ñāṇaṁ paṭisambhidāti.",
  "kv5.5:3.7": "Paṭisambhidākathā niṭṭhitā."
}