{
  "kv15.7:0.1": "Kathāvatthu",
  "kv15.7:0.2": "Mahāpaṇṇāsaka",
  "kv15.7:0.3": "Pannarasamavagga",
  "kv15.7:0.4": "Saññāvedayitakathā",
  "kv15.7:1.1": "Saññāvedayitanirodhasamāpatti lokuttarāti?",
  "kv15.7:1.2": "Āmantā.",
  "kv15.7:1.3": "Maggo phalaṁ nibbānaṁ, sotāpattimaggo sotāpattiphalaṁ …pe… bojjhaṅgoti?",
  "kv15.7:1.4": "Na hevaṁ vattabbe …pe….",
  "kv15.7:2.1": "Na vattabbaṁ—",
  "kv15.7:2.2": "“saññāvedayitanirodhasamāpatti lokuttarā”ti?",
  "kv15.7:2.3": "Āmantā.",
  "kv15.7:2.4": "Lokiyāti?",
  "kv15.7:2.5": "Na hevaṁ vattabbe.",
  "kv15.7:2.6": "Tena hi lokuttarāti.",
  "kv15.7:2.7": "Saññāvedayitakathā niṭṭhitā."
}