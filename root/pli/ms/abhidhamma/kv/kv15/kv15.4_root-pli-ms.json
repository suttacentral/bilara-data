{
  "kv15.4:0.1": "Kathāvatthu",
  "kv15.4:0.2": "Mahāpaṇṇāsaka",
  "kv15.4:0.3": "Pannarasamavagga",
  "kv15.4:0.4": "Khaṇalayamuhuttakathā",
  "kv15.4:1.1": "Khaṇo parinipphanno, layo parinipphanno, muhuttaṁ parinipphannanti?",
  "kv15.4:1.2": "Āmantā.",
  "kv15.4:1.3": "Rūpanti?",
  "kv15.4:1.4": "Na hevaṁ vattabbe …pe…",
  "kv15.4:1.5": "vedanā …",
  "kv15.4:1.6": "saññā …",
  "kv15.4:1.7": "saṅkhārā …",
  "kv15.4:1.8": "viññāṇanti?",
  "kv15.4:1.9": "Na hevaṁ vattabbe …pe….",
  "kv15.4:2.1": "Na vattabbaṁ—",
  "kv15.4:2.2": "“muhuttaṁ parinipphannan”ti?",
  "kv15.4:2.3": "Āmantā.",
  "kv15.4:2.4": "Nanu vuttaṁ bhagavatā—",
  "kv15.4:2.5": "“tīṇimāni, bhikkhave, kathāvatthūni.",
  "kv15.4:2.6": "Katamāni tīṇi?",
  "kv15.4:2.7": "Atītaṁ vā, bhikkhave, addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.4:2.8": "‘evaṁ ahosi atītamaddhānan’ti;",
  "kv15.4:2.9": "anāgataṁ vā, bhikkhave, addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.4:2.10": "‘evaṁ bhavissati anāgatamaddhānan’ti;",
  "kv15.4:2.11": "etarahi vā, bhikkhave, paccuppannaṁ addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.4:2.12": "‘evaṁ hoti etarahi paccuppannan’ti.",
  "kv15.4:2.13": "Imāni kho, bhikkhave, tīṇi kathāvatthūnī”ti.",
  "kv15.4:2.14": "Attheva suttantoti?",
  "kv15.4:2.15": "Āmantā.",
  "kv15.4:2.16": "Tena hi muhuttaṁ parinipphannanti.",
  "kv15.4:2.17": "Khaṇalayamuhuttakathā niṭṭhitā."
}