{
  "kv2.11:0.1": "Kathāvatthu",
  "kv2.11:0.2": "Mahāpaṇṇāsaka",
  "kv2.11:0.3": "Dutiyavagga",
  "kv2.11:0.4": "Nirodhakathā",
  "kv2.11:1.1": "Dve nirodhāti?",
  "kv2.11:1.2": "Āmantā.",
  "kv2.11:1.3": "Dve dukkhanirodhāti?",
  "kv2.11:1.4": "Na hevaṁ vattabbe …pe…",
  "kv2.11:1.5": "dve dukkhanirodhāti?",
  "kv2.11:1.6": "Āmantā.",
  "kv2.11:1.7": "Dve nirodhasaccānīti?",
  "kv2.11:1.8": "Na hevaṁ vattabbe …pe…",
  "kv2.11:1.9": "dve nirodhasaccānīti?",
  "kv2.11:1.10": "Āmantā.",
  "kv2.11:1.11": "Dve dukkhasaccānīti?",
  "kv2.11:1.12": "Na hevaṁ vattabbe …pe…",
  "kv2.11:1.13": "dve nirodhasaccānīti?",
  "kv2.11:1.14": "Āmantā.",
  "kv2.11:1.15": "Dve samudayasaccānīti?",
  "kv2.11:1.16": "Na hevaṁ vattabbe …pe…",
  "kv2.11:1.17": "dve nirodhasaccānīti?",
  "kv2.11:1.18": "Āmantā.",
  "kv2.11:1.19": "Dve maggasaccānīti?",
  "kv2.11:1.20": "Na hevaṁ vattabbe …pe….",
  "kv2.11:2.1": "Dve nirodhasaccānīti?",
  "kv2.11:2.2": "Āmantā.",
  "kv2.11:2.3": "Dve tāṇāni …pe…",
  "kv2.11:2.4": "dve leṇāni …",
  "kv2.11:2.5": "dve saraṇāni …",
  "kv2.11:2.6": "dve parāyaṇāni …",
  "kv2.11:2.7": "dve accutāni …",
  "kv2.11:2.8": "dve amatāni …",
  "kv2.11:2.9": "dve nibbānānīti?",
  "kv2.11:2.10": "Na hevaṁ vattabbe …pe….",
  "kv2.11:3.1": "Dve nibbānānīti?",
  "kv2.11:3.2": "Āmantā.",
  "kv2.11:3.3": "Atthi dvinnaṁ nibbānānaṁ uccanīcatā hīnapaṇītatā ukkaṁsāvakaṁso sīmā vā bhedo vā rāji vā antarikā vāti?",
  "kv2.11:3.4": "Na hevaṁ vattabbe …pe….",
  "kv2.11:4.1": "Dve nirodhāti?",
  "kv2.11:4.2": "Āmantā.",
  "kv2.11:4.3": "Nanu appaṭisaṅkhāniruddhe saṅkhāre paṭisaṅkhā nirodhentīti?",
  "kv2.11:4.4": "Āmantā.",
  "kv2.11:4.5": "Hañci appaṭisaṅkhāniruddhe saṅkhāre paṭisaṅkhā nirodhenti, no ca vata re vattabbe—",
  "kv2.11:4.6": "“dve nirodhā”ti.",
  "kv2.11:5.1": "Na vattabbaṁ—",
  "kv2.11:5.2": "“dve nirodhā”ti?",
  "kv2.11:5.3": "Āmantā.",
  "kv2.11:5.4": "Nanu appaṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā, paṭisaṅkhāniruddhāpi saṅkhārā accantabhaggāti?",
  "kv2.11:5.5": "Āmantā.",
  "kv2.11:5.6": "Hañci appaṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā, paṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā, tena vata re vattabbe—",
  "kv2.11:5.7": "“dve nirodhā”ti.",
  "kv2.11:6.1": "Dve nirodhāti?",
  "kv2.11:6.2": "Āmantā.",
  "kv2.11:6.3": "Paṭisaṅkhāniruddhāpi saṅkhārā ariyamaggaṁ āgamma niruddhāti?",
  "kv2.11:6.4": "Āmantā.",
  "kv2.11:6.5": "Appaṭisaṅkhāniruddhā saṅkhārā ariyamaggaṁ āgamma niruddhāti?",
  "kv2.11:6.6": "Na hevaṁ vattabbe …pe…",
  "kv2.11:7.1": "Dve nirodhāti?",
  "kv2.11:7.2": "Āmantā.",
  "kv2.11:7.3": "Paṭisaṅkhāniruddhā saṅkhārā na puna uppajjantīti?",
  "kv2.11:7.4": "Āmantā.",
  "kv2.11:7.5": "Appaṭisaṅkhāniruddhā saṅkhārā na puna uppajjantīti?",
  "kv2.11:7.6": "Na hevaṁ vattabbe …pe…",
  "kv2.11:7.7": "tena hi na vattabbaṁ—",
  "kv2.11:7.8": "“dve nirodhā”ti.",
  "kv2.11:7.9": "Nirodhakathā niṭṭhitā.",
  "kv2.11:7.10": "Dutiyo vaggo.",
  "kv2.11:7.11": "Tassuddānaṁ",
  "kv2.11:8.1": "Parūpahāro aññāṇaṁ,",
  "kv2.11:8.2": "kaṅkhā paravitāraṇā;",
  "kv2.11:8.3": "Vacībhedo dukkhāhāro,",
  "kv2.11:8.4": "cittaṭṭhiti ca kukkuḷā;",
  "kv2.11:8.5": "Anupubbābhisamayo,",
  "kv2.11:8.6": "vohāro ca nirodhakoti."
}