{
  "kv20.1:0.1": "Kathāvatthu",
  "kv20.1:0.2": "Niggahapaṇṇāsaka",
  "kv20.1:0.3": "Vīsatimavagga",
  "kv20.1:0.4": "Asañciccakathā",
  "kv20.1:1.1": "Asañcicca mātaraṁ jīvitā voropetvā ānantariko hotīti?",
  "kv20.1:1.2": "Āmantā.",
  "kv20.1:1.3": "Asañcicca pāṇaṁ hantvā pāṇātipātī hotīti?",
  "kv20.1:1.4": "Na hevaṁ vattabbe …pe…",
  "kv20.1:1.5": "asañcicca mātaraṁ jīvitā voropetvā ānantariko hotīti?",
  "kv20.1:1.6": "Āmantā.",
  "kv20.1:1.7": "Asañcicca adinnaṁ ādiyitvā …pe…",
  "kv20.1:1.8": "musā bhaṇitvā musāvādī hotīti?",
  "kv20.1:1.9": "Na hevaṁ vattabbe …pe….",
  "kv20.1:2.1": "Asañcicca pāṇaṁ hantvā pāṇātipātī na hotīti?",
  "kv20.1:2.2": "Āmantā.",
  "kv20.1:2.3": "Asañcicca mātaraṁ jīvitā voropetvā ānantariko na hotīti?",
  "kv20.1:2.4": "Na hevaṁ vattabbe …pe…",
  "kv20.1:2.5": "asañcicca adinnaṁ ādiyitvā …pe…",
  "kv20.1:2.6": "musā bhaṇitvā musāvādī na hotīti?",
  "kv20.1:2.7": "Āmantā.",
  "kv20.1:2.8": "Asañcicca mātaraṁ jīvitā voropetvā ānantariko na hotīti?",
  "kv20.1:2.9": "Na hevaṁ vattabbe …pe….",
  "kv20.1:3.1": "Asañcicca mātaraṁ jīvitā voropetvā ānantariko hotīti?",
  "kv20.1:3.2": "Āmantā.",
  "kv20.1:3.3": "“Asañcicca mātaraṁ jīvitā voropetvā ānantariko hotī”ti—",
  "kv20.1:3.4": "attheva suttantoti?",
  "kv20.1:3.5": "Natthi.",
  "kv20.1:3.6": "“Sañcicca mātaraṁ jīvitā voropetvā ānantariko hotī”ti—",
  "kv20.1:3.7": "attheva suttantoti?",
  "kv20.1:3.8": "Āmantā.",
  "kv20.1:3.9": "Hañci “sañcicca mātaraṁ jīvitā voropetvā ānantariko hotī”ti—",
  "kv20.1:3.10": "attheva suttanto, no ca vata re vattabbe—",
  "kv20.1:3.11": "“asañcicca mātaraṁ jīvitā voropetvā ānantariko hotī”ti.",
  "kv20.1:4.1": "Na vattabbaṁ—",
  "kv20.1:4.2": "“mātughātako ānantariko”ti?",
  "kv20.1:4.3": "Āmantā.",
  "kv20.1:4.4": "Nanu mātā jīvitā voropitāti?",
  "kv20.1:4.5": "Āmantā.",
  "kv20.1:4.6": "Hañci mātā jīvitā voropitā, tena vata re vattabbe—",
  "kv20.1:4.7": "“mātughātako ānantariko”ti.",
  "kv20.1:5.1": "Na vattabbaṁ—",
  "kv20.1:5.2": "“pitughātako ānantariko”ti?",
  "kv20.1:5.3": "Āmantā.",
  "kv20.1:5.4": "Nanu pitā jīvitā voropitoti?",
  "kv20.1:5.5": "Āmantā.",
  "kv20.1:5.6": "Hañci pitā jīvitā voropito, tena vata re vattabbe—",
  "kv20.1:5.7": "“pitughātako ānantariko”ti.",
  "kv20.1:6.1": "Na vattabbaṁ—",
  "kv20.1:6.2": "“arahantaghātako ānantariko”ti?",
  "kv20.1:6.3": "Āmantā.",
  "kv20.1:6.4": "Nanu arahā jīvitā voropitoti?",
  "kv20.1:6.5": "Āmantā.",
  "kv20.1:6.6": "Hañci arahā jīvitā voropito, tena vata re vattabbe—",
  "kv20.1:6.7": "“arahantaghātako ānantariko”ti.",
  "kv20.1:7.1": "Na vattabbaṁ—",
  "kv20.1:7.2": "“ruhiruppādako ānantariko”ti?",
  "kv20.1:7.3": "Āmantā.",
  "kv20.1:7.4": "Nanu tathāgatassa lohitaṁ uppāditanti?",
  "kv20.1:7.5": "Āmantā.",
  "kv20.1:7.6": "Hañci tathāgatassa lohitaṁ uppāditaṁ, tena vata re vattabbe—",
  "kv20.1:7.7": "“ruhiruppādako ānantariko”ti.",
  "kv20.1:8.1": "Saṅghabhedako ānantarikoti?",
  "kv20.1:8.2": "Āmantā.",
  "kv20.1:8.3": "Sabbe saṅghabhedakā ānantarikāti?",
  "kv20.1:8.4": "Na hevaṁ vattabbe …pe…",
  "kv20.1:8.5": "sabbe saṅghabhedakā ānantarikāti?",
  "kv20.1:8.6": "Āmantā.",
  "kv20.1:8.7": "Dhammasaññī saṅghabhedako ānantarikoti?",
  "kv20.1:8.8": "Na hevaṁ vattabbe …pe….",
  "kv20.1:9.1": "Dhammasaññī saṅghabhedako ānantarikoti?",
  "kv20.1:9.2": "Āmantā.",
  "kv20.1:9.3": "Nanu vuttaṁ bhagavatā—",
  "kv20.1:9.4": "“atthupāli, saṅghabhedako āpāyiko nerayiko kappaṭṭho atekiccho;",
  "kv20.1:9.5": "atthupāli, saṅghabhedako na āpāyiko na nerayiko na kappaṭṭho na atekiccho”ti.",
  "kv20.1:9.6": "Attheva suttantoti?",
  "kv20.1:9.7": "Āmantā.",
  "kv20.1:9.8": "Tena hi na vattabbaṁ—",
  "kv20.1:9.9": "“dhammasaññī saṅghabhedako ānantariko”ti.",
  "kv20.1:10.1": "Na vattabbaṁ—",
  "kv20.1:10.2": "“dhammasaññī saṅghabhedako ānantariko”ti?",
  "kv20.1:10.3": "Āmantā.",
  "kv20.1:10.4": "Nanu vuttaṁ bhagavatā—",
  "kv20.1:11.1": "“Āpāyiko nerayiko,",
  "kv20.1:11.2": "Kappaṭṭho saṅghabhedako;",
  "kv20.1:11.3": "Vaggarato adhammaṭṭho,",
  "kv20.1:11.4": "Yogakkhemā padhaṁsati;",
  "kv20.1:11.5": "Saṅghaṁ samaggaṁ bhetvāna,",
  "kv20.1:11.6": "Kappaṁ nirayamhi paccatī”ti.",
  "kv20.1:12.1": "Attheva suttantoti?",
  "kv20.1:12.2": "Āmantā.",
  "kv20.1:12.3": "Tena hi saṅghabhedako ānantarikoti.",
  "kv20.1:12.4": "Asañciccakathā niṭṭhitā."
}