{
  "kv13.9:0.1": "Kathāvatthu",
  "kv13.9:0.2": "Mahāpaṇṇāsaka",
  "kv13.9:0.3": "Terasamavagga",
  "kv13.9:0.4": "Dhammataṇhāabyākatātikathā",
  "kv13.9:1.1": "Dhammataṇhā abyākatāti?",
  "kv13.9:1.2": "Āmantā.",
  "kv13.9:1.3": "Vipākābyākatā kiriyābyākatā rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe…",
  "kv13.9:1.4": "phoṭṭhabbāyatananti?",
  "kv13.9:1.5": "Na hevaṁ vattabbe …pe….",
  "kv13.9:2.1": "Dhammataṇhā abyākatāti?",
  "kv13.9:2.2": "Āmantā.",
  "kv13.9:2.3": "Rūpataṇhā abyākatāti?",
  "kv13.9:2.4": "Na hevaṁ vattabbe …pe…",
  "kv13.9:2.5": "dhammataṇhā abyākatāti?",
  "kv13.9:2.6": "Āmantā.",
  "kv13.9:2.7": "Saddataṇhā …pe…",
  "kv13.9:2.8": "gandhataṇhā …pe…",
  "kv13.9:2.9": "rasataṇhā …pe…",
  "kv13.9:2.10": "phoṭṭhabbataṇhā abyākatāti?",
  "kv13.9:2.11": "Na hevaṁ vattabbe …pe….",
  "kv13.9:3.1": "Rūpataṇhā akusalāti?",
  "kv13.9:3.2": "Āmantā.",
  "kv13.9:3.3": "Dhammataṇhā akusalāti?",
  "kv13.9:3.4": "Na hevaṁ vattabbe …pe…",
  "kv13.9:3.5": "saddataṇhā …pe…",
  "kv13.9:3.6": "phoṭṭhabbataṇhā akusalāti?",
  "kv13.9:3.7": "Āmantā.",
  "kv13.9:3.8": "Dhammataṇhā akusalāti?",
  "kv13.9:3.9": "Na hevaṁ vattabbe …pe….",
  "kv13.9:4.1": "Dhammataṇhā abyākatāti?",
  "kv13.9:4.2": "Āmantā.",
  "kv13.9:4.3": "Nanu taṇhā akusalā vuttā bhagavatāti?",
  "kv13.9:4.4": "Āmantā.",
  "kv13.9:4.5": "Hañci taṇhā akusalā vuttā bhagavatā, no ca vata re vattabbe—",
  "kv13.9:4.6": "“dhammataṇhā abyākatā”ti.",
  "kv13.9:5.1": "Dhammataṇhā abyākatāti?",
  "kv13.9:5.2": "Āmantā.",
  "kv13.9:5.3": "Nanu lobho akusalo vutto bhagavatā, dhammataṇhā lobhoti?",
  "kv13.9:5.4": "Āmantā.",
  "kv13.9:5.5": "Hañci lobho akusalo vutto bhagavatā, dhammataṇhā lobho, no ca vata re vattabbe—",
  "kv13.9:5.6": "“dhammataṇhā abyākatā”ti.",
  "kv13.9:6.1": "Dhammataṇhā lobho abyākatoti?",
  "kv13.9:6.2": "Āmantā.",
  "kv13.9:6.3": "Rūpataṇhā lobho abyākatoti?",
  "kv13.9:6.4": "Na hevaṁ vattabbe …pe…",
  "kv13.9:6.5": "dhammataṇhā lobho abyākatoti?",
  "kv13.9:6.6": "Āmantā.",
  "kv13.9:6.7": "Saddataṇhā …pe…",
  "kv13.9:6.8": "phoṭṭhabbataṇhā lobho abyākatoti?",
  "kv13.9:6.9": "Na hevaṁ vattabbe …pe….",
  "kv13.9:7.1": "Rūpataṇhā lobho akusaloti?",
  "kv13.9:7.2": "Āmantā.",
  "kv13.9:7.3": "Dhammataṇhā lobho akusaloti?",
  "kv13.9:7.4": "Na hevaṁ vattabbe …pe…",
  "kv13.9:7.5": "saddataṇhā …pe…",
  "kv13.9:7.6": "phoṭṭhabbataṇhā lobho akusaloti?",
  "kv13.9:7.7": "Āmantā.",
  "kv13.9:7.8": "Dhammataṇhā lobho akusaloti?",
  "kv13.9:7.9": "Na hevaṁ vattabbe …pe….",
  "kv13.9:8.1": "Dhammataṇhā abyākatāti?",
  "kv13.9:8.2": "Āmantā.",
  "kv13.9:8.3": "Nanu vuttaṁ bhagavatā—",
  "kv13.9:8.4": "“yāyaṁ taṇhā ponobbhavikā nandīrāgasahagatā tatratatrābhinandinī, seyyathidaṁ—",
  "kv13.9:8.5": "kāmataṇhā, bhavataṇhā, vibhavataṇhā”ti.",
  "kv13.9:8.6": "Attheva suttantoti?",
  "kv13.9:8.7": "Āmantā.",
  "kv13.9:8.8": "Tena hi na vattabbaṁ—",
  "kv13.9:8.9": "“dhammataṇhā abyākatā”ti.",
  "kv13.9:9.1": "Na vattabbaṁ—",
  "kv13.9:9.2": "“dhammataṇhā abyākatā”ti?",
  "kv13.9:9.3": "Āmantā.",
  "kv13.9:9.4": "Nanu sā dhammataṇhāti?",
  "kv13.9:9.5": "Āmantā.",
  "kv13.9:9.6": "Hañci sā dhammataṇhā, tena vata re vattabbe—",
  "kv13.9:9.7": "“dhammataṇhā abyākatā”ti.",
  "kv13.9:9.8": "Dhammataṇhā abyākatātikathā niṭṭhitā."
}