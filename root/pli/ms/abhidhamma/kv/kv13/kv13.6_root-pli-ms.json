{
  "kv13.6:0.1": "Kathāvatthu",
  "kv13.6:0.2": "Mahāpaṇṇāsaka",
  "kv13.6:0.3": "Terasamavagga",
  "kv13.6:0.4": "Sammukhībhūtakathā",
  "kv13.6:1.1": "Sammukhībhūto saṁyojanaṁ jahatīti?",
  "kv13.6:1.2": "Āmantā.",
  "kv13.6:1.3": "Ratto rāgaṁ jahati, duṭṭho dosaṁ jahati, mūḷho mohaṁ jahati, kiliṭṭho kilese jahatīti?",
  "kv13.6:1.4": "Na hevaṁ vattabbe …pe…",
  "kv13.6:1.5": "rāgena rāgaṁ jahati, dosena dosaṁ jahati, mohena mohaṁ jahati, kilesehi kilese jahatīti?",
  "kv13.6:1.6": "Na hevaṁ vattabbe …pe…",
  "kv13.6:2.1": "Rāgo cittasampayutto, maggo cittasampayuttoti?",
  "kv13.6:2.2": "Āmantā.",
  "kv13.6:2.3": "Dvinnaṁ phassānaṁ …pe…",
  "kv13.6:2.4": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv13.6:2.5": "Na hevaṁ vattabbe …pe…",
  "kv13.6:2.6": "rāgo akusalo, maggo kusaloti?",
  "kv13.6:2.7": "Āmantā.",
  "kv13.6:2.8": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv13.6:2.9": "Na hevaṁ vattabbe …pe….",
  "kv13.6:3.1": "Kusalākusalā …pe…",
  "kv13.6:3.2": "sammukhībhāvaṁ āgacchantīti?",
  "kv13.6:3.3": "Āmantā.",
  "kv13.6:3.4": "Nanu vuttaṁ bhagavatā—",
  "kv13.6:3.5": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv13.6:3.6": "Katamāni cattāri?",
  "kv13.6:3.7": "Nabhañca, bhikkhave, pathavī ca—",
  "kv13.6:3.8": "idaṁ paṭhamaṁ suvidūravidūraṁ …pe… tasmā sataṁ dhammo asabbhi ārakā”ti.",
  "kv13.6:3.9": "Attheva suttantoti?",
  "kv13.6:3.10": "Āmantā.",
  "kv13.6:3.11": "Tena hi na vattabbaṁ—",
  "kv13.6:3.12": "“kusalākusalā …pe…",
  "kv13.6:3.13": "sammukhībhāvaṁ āgacchantī”ti.",
  "kv13.6:4.1": "Sammukhībhūto saṁyojanaṁ jahatīti?",
  "kv13.6:4.2": "Āmantā.",
  "kv13.6:4.3": "Nanu vuttaṁ bhagavatā—",
  "kv13.6:4.4": "“so evaṁ samāhite citte …pe…",
  "kv13.6:4.5": "āsavānaṁ khayañāṇāya cittaṁ abhininnāmetī”ti.",
  "kv13.6:4.6": "Attheva suttantoti?",
  "kv13.6:4.7": "Āmantā.",
  "kv13.6:4.8": "Tena hi na vattabbaṁ—",
  "kv13.6:4.9": "“sammukhībhūto saṁyojanaṁ jahatī”ti.",
  "kv13.6:5.1": "Na vattabbaṁ—",
  "kv13.6:5.2": "“sammukhībhūto saṁyojanaṁ jahatī”ti?",
  "kv13.6:5.3": "Āmantā.",
  "kv13.6:5.4": "Nanu vuttaṁ bhagavatā—",
  "kv13.6:5.5": "“tassa evaṁ jānato evaṁ passato kāmāsavāpi cittaṁ vimuccati …pe…",
  "kv13.6:5.6": "avijjāsavāpi cittaṁ vimuccatī”ti.",
  "kv13.6:5.7": "Attheva suttantoti?",
  "kv13.6:5.8": "Āmantā.",
  "kv13.6:5.9": "Tena hi sammukhībhūto saṁyojanaṁ jahatīti.",
  "kv13.6:5.10": "Sammukhībhūtakathā niṭṭhitā."
}