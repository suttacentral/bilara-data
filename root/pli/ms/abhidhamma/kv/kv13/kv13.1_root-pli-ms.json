{
  "kv13.1:0.1": "Kathāvatthu",
  "kv13.1:0.2": "Anusayapaṇṇāsaka",
  "kv13.1:0.3": "Terasamavagga",
  "kv13.1:0.4": "Kappaṭṭhakathā",
  "kv13.1:1.1": "Kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:1.2": "Āmantā.",
  "kv13.1:1.3": "Kappo ca saṇṭhāti buddho ca loke uppajjatīti?",
  "kv13.1:1.4": "Na hevaṁ vattabbe …pe….",
  "kv13.1:1.5": "Kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:1.6": "Āmantā.",
  "kv13.1:1.7": "Kappo ca saṇṭhāti saṅgho ca bhijjatīti?",
  "kv13.1:1.8": "Na hevaṁ vattabbe …pe…",
  "kv13.1:1.9": "kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:1.10": "Āmantā.",
  "kv13.1:1.11": "Kappo ca saṇṭhāti kappaṭṭho ca kappaṭṭhiyaṁ kammaṁ karotīti?",
  "kv13.1:1.12": "Na hevaṁ vattabbe …pe…",
  "kv13.1:1.13": "kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:1.14": "Āmantā.",
  "kv13.1:1.15": "Kappo ca saṇṭhāti kappaṭṭho ca puggalo kālaṁ karotīti?",
  "kv13.1:1.16": "Na hevaṁ vattabbe …pe….",
  "kv13.1:2.1": "Kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:2.2": "Āmantā.",
  "kv13.1:2.3": "Atītaṁ kappaṁ tiṭṭheyya, anāgataṁ kappaṁ tiṭṭheyyāti?",
  "kv13.1:2.4": "Na hevaṁ vattabbe …pe…",
  "kv13.1:2.5": "kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:2.6": "Āmantā.",
  "kv13.1:2.7": "Dve kappe tiṭṭheyya … tayo kappe tiṭṭheyya … cattāro kappe tiṭṭheyyāti?",
  "kv13.1:2.8": "Na hevaṁ vattabbe …pe….",
  "kv13.1:3.1": "Kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:3.2": "Āmantā.",
  "kv13.1:3.3": "Kappaṭṭho kappe ḍayhante kattha gacchatīti?",
  "kv13.1:3.4": "Aññaṁ lokadhātuṁ gacchatīti.",
  "kv13.1:3.5": "Mato gacchati, vehāsaṁ gacchatīti?",
  "kv13.1:3.6": "Mato gacchatīti.",
  "kv13.1:3.7": "Kappaṭṭhiyaṁ kammaṁ aparāpariyavepakkanti?",
  "kv13.1:3.8": "Na hevaṁ vattabbe …pe…",
  "kv13.1:3.9": "vehāsaṁ gacchatīti?",
  "kv13.1:3.10": "Āmantā.",
  "kv13.1:3.11": "Kappaṭṭho iddhimāti?",
  "kv13.1:3.12": "Na hevaṁ vattabbe …pe…",
  "kv13.1:3.13": "kappaṭṭho iddhimāti?",
  "kv13.1:3.14": "Āmantā.",
  "kv13.1:3.15": "Kappaṭṭhena chandiddhipādo bhāvito vīriyiddhipādo bhāvito cittiddhipādo bhāvito vīmaṁsiddhipādo bhāvitoti?",
  "kv13.1:3.16": "Na hevaṁ vattabbe …pe….",
  "kv13.1:4.1": "Na vattabbaṁ—",
  "kv13.1:4.2": "“kappaṭṭho kappaṁ tiṭṭheyyā”ti?",
  "kv13.1:4.3": "Āmantā.",
  "kv13.1:4.4": "Nanu vuttaṁ bhagavatā—",
  "kv13.1:5.1": "“Āpāyiko nerayiko,",
  "kv13.1:5.2": "Kappaṭṭho saṅghabhedako;",
  "kv13.1:5.3": "Vaggarato adhammaṭṭho,",
  "kv13.1:5.4": "Yogakkhemā padhaṁsati;",
  "kv13.1:5.5": "Saṅghaṁ samaggaṁ bhetvāna,",
  "kv13.1:5.6": "Kappaṁ nirayamhi paccatī”ti.",
  "kv13.1:6.1": "Attheva suttantoti?",
  "kv13.1:6.2": "Āmantā.",
  "kv13.1:6.3": "Tena hi kappaṭṭho kappaṁ tiṭṭheyyāti.",
  "kv13.1:6.4": "Kappaṭṭhakathā niṭṭhitā."
}