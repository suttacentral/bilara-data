{
  "kv21.2:0.1": "Kathāvatthu",
  "kv21.2:0.2": "Khuddakaaḍḍhapaṇṇāsaka",
  "kv21.2:0.3": "Ekavīsatimavagga",
  "kv21.2:0.4": "Avivittakathā",
  "kv21.2:1.1": "Puthujjano tedhātukehi dhammehi avivittoti?",
  "kv21.2:1.2": "Āmantā.",
  "kv21.2:1.3": "Puthujjano tedhātukehi phassehi …pe…",
  "kv21.2:1.4": "tedhātukāhi vedanāhi …",
  "kv21.2:1.5": "saññāhi …",
  "kv21.2:1.6": "cetanāhi …",
  "kv21.2:1.7": "cittehi …",
  "kv21.2:1.8": "saddhāhi …",
  "kv21.2:1.9": "vīriyehi …",
  "kv21.2:1.10": "satīhi …",
  "kv21.2:1.11": "samādhīhi …pe…",
  "kv21.2:1.12": "tedhātukāhi paññāhi avivittoti?",
  "kv21.2:1.13": "Na hevaṁ vattabbe …pe….",
  "kv21.2:2.1": "Puthujjano tedhātukehi kammehi avivittoti?",
  "kv21.2:2.2": "Āmantā.",
  "kv21.2:2.3": "Yasmiṁ khaṇe puthujjano cīvaraṁ deti, tasmiṁ khaṇe paṭhamaṁ jhānaṁ upasampajja viharati …pe…",
  "kv21.2:2.4": "ākāsānañcāyatanaṁ upasampajja viharatīti?",
  "kv21.2:2.5": "Na hevaṁ vattabbe …pe…",
  "kv21.2:2.6": "yasmiṁ khaṇe puthujjano piṇḍapātaṁ deti …pe…",
  "kv21.2:2.7": "senāsanaṁ deti …pe…",
  "kv21.2:2.8": "gilānapaccayabhesajjaparikkhāraṁ deti, tasmiṁ khaṇe catutthaṁ jhānaṁ upasampajja viharati …",
  "kv21.2:2.9": "nevasaññānāsaññāyatanaṁ upasampajja viharatīti?",
  "kv21.2:2.10": "Na hevaṁ vattabbe …pe….",
  "kv21.2:3.1": "Na vattabbaṁ—",
  "kv21.2:3.2": "“puthujjano tedhātukehi kammehi avivitto”ti?",
  "kv21.2:3.3": "Āmantā.",
  "kv21.2:3.4": "Puthujjanassa rūpadhātuarūpadhātūpagaṁ kammaṁ pariññātanti?",
  "kv21.2:3.5": "Na hevaṁ vattabbe.",
  "kv21.2:3.6": "Tena hi puthujjano tedhātukehi kammehi avivittoti …pe….",
  "kv21.2:3.7": "Avivittakathā niṭṭhitā."
}