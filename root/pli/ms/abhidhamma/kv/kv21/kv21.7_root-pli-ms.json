{
  "kv21.7:0.1": "Kathāvatthu",
  "kv21.7:0.2": "Mahāpaṇṇāsaka",
  "kv21.7:0.3": "Ekavīsatimavagga",
  "kv21.7:0.4": "Dhammakathā",
  "kv21.7:1.1": "Sabbe dhammā niyatāti?",
  "kv21.7:1.2": "Āmantā.",
  "kv21.7:1.3": "Micchattaniyatāti?",
  "kv21.7:1.4": "Na hevaṁ vattabbe …pe…",
  "kv21.7:1.5": "sammattaniyatāti?",
  "kv21.7:1.6": "Na hevaṁ vattabbe …pe…",
  "kv21.7:1.7": "natthi aniyato rāsīti?",
  "kv21.7:1.8": "Na hevaṁ vattabbe …pe…",
  "kv21.7:1.9": "nanu atthi aniyato rāsīti?",
  "kv21.7:1.10": "Āmantā.",
  "kv21.7:1.11": "Hañci atthi aniyato rāsi, no ca vata re vattabbe—",
  "kv21.7:1.12": "“sabbe dhammā niyatā”ti.",
  "kv21.7:2.1": "Sabbe dhammā niyatāti?",
  "kv21.7:2.2": "Āmantā.",
  "kv21.7:2.3": "Nanu tayo rāsī vuttā bhagavatā—",
  "kv21.7:2.4": "“micchattaniyato rāsi, sammattaniyato rāsi, aniyato rāsī”ti?",
  "kv21.7:2.5": "Āmantā.",
  "kv21.7:2.6": "Hañci tayo rāsī vuttā bhagavatā—",
  "kv21.7:2.7": "“micchattaniyato rāsi, sammattaniyato rāsi, aniyato rāsi”, no ca vata re vattabbe—",
  "kv21.7:2.8": "“sabbe dhammā niyatā”ti.",
  "kv21.7:3.1": "Rūpaṁ rūpaṭṭhena niyatanti?",
  "kv21.7:3.2": "Āmantā.",
  "kv21.7:3.3": "Micchattaniyatanti?",
  "kv21.7:3.4": "Na hevaṁ vattabbe …pe…",
  "kv21.7:3.5": "sammattaniyatanti?",
  "kv21.7:3.6": "Na hevaṁ vattabbe …pe…",
  "kv21.7:3.7": "vedanā …pe…",
  "kv21.7:3.8": "saññā …pe…",
  "kv21.7:3.9": "saṅkhārā …pe…",
  "kv21.7:3.10": "viññāṇaṁ viññāṇaṭṭhena niyatanti?",
  "kv21.7:3.11": "Āmantā.",
  "kv21.7:3.12": "Micchattaniyatanti?",
  "kv21.7:3.13": "Na hevaṁ vattabbe …pe…",
  "kv21.7:3.14": "sammattaniyatanti?",
  "kv21.7:3.15": "Na hevaṁ vattabbe …pe….",
  "kv21.7:4.1": "Na vattabbaṁ—",
  "kv21.7:4.2": "rūpaṁ rūpaṭṭhena niyataṁ …pe…",
  "kv21.7:4.3": "vedanā …pe…",
  "kv21.7:4.4": "saññā …pe…",
  "kv21.7:4.5": "saṅkhārā …pe…",
  "kv21.7:4.6": "viññāṇaṁ viññāṇaṭṭhena niyatanti?",
  "kv21.7:4.7": "Āmantā.",
  "kv21.7:4.8": "Rūpaṁ vedanā hoti …pe…",
  "kv21.7:4.9": "saññā hoti …",
  "kv21.7:4.10": "saṅkhārā honti …",
  "kv21.7:4.11": "viññāṇaṁ hoti …",
  "kv21.7:4.12": "vedanā …pe…",
  "kv21.7:4.13": "saññā …pe…",
  "kv21.7:4.14": "saṅkhārā …pe…",
  "kv21.7:4.15": "viññāṇaṁ rūpaṁ hoti …pe…",
  "kv21.7:4.16": "vedanā hoti …",
  "kv21.7:4.17": "saññā hoti …",
  "kv21.7:4.18": "saṅkhārā hontīti?",
  "kv21.7:4.19": "Na hevaṁ vattabbe.",
  "kv21.7:4.20": "Tena hi rūpaṁ rūpaṭṭhena niyataṁ, vedanā …pe…",
  "kv21.7:4.21": "saññā …pe…",
  "kv21.7:4.22": "saṅkhārā …pe…",
  "kv21.7:4.23": "viññāṇaṁ viññāṇaṭṭhena niyatanti.",
  "kv21.7:4.24": "Dhammakathā niṭṭhitā."
}