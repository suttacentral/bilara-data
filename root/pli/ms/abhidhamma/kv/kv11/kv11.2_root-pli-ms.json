{
  "kv11.2:0.1": "Kathāvatthu",
  "kv11.2:0.2": "Anusayapaṇṇāsaka",
  "kv11.2:0.3": "Ekādasamavagga",
  "kv11.2:0.4": "Ñāṇakathā",
  "kv11.2:1.1": "Aññāṇe vigate ñāṇavippayutte citte vattamāne na vattabbaṁ—",
  "kv11.2:1.2": "“ñāṇī”ti?",
  "kv11.2:1.3": "Āmantā.",
  "kv11.2:1.4": "Rāge vigate na vattabbaṁ—",
  "kv11.2:1.5": "“vītarāgo”ti?",
  "kv11.2:1.6": "Na hevaṁ vattabbe …pe…",
  "kv11.2:1.7": "aññāṇe vigate ñāṇavippayutte citte vattamāne na vattabbaṁ—",
  "kv11.2:1.8": "“ñāṇī”ti?",
  "kv11.2:1.9": "Āmantā.",
  "kv11.2:1.10": "Dose vigate …",
  "kv11.2:1.11": "mohe vigate …",
  "kv11.2:1.12": "kilese vigate na vattabbaṁ—",
  "kv11.2:1.13": "“nikkileso”ti?",
  "kv11.2:1.14": "Na hevaṁ vattabbe …pe….",
  "kv11.2:2.1": "Rāge vigate vattabbaṁ—",
  "kv11.2:2.2": "“vītarāgo”ti?",
  "kv11.2:2.3": "Āmantā.",
  "kv11.2:2.4": "Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṁ—",
  "kv11.2:2.5": "“ñāṇī”ti?",
  "kv11.2:2.6": "Na hevaṁ vattabbe …pe…",
  "kv11.2:2.7": "dose vigate …",
  "kv11.2:2.8": "mohe vigate …",
  "kv11.2:2.9": "kilese vigate vattabbaṁ—",
  "kv11.2:2.10": "“nikkileso”ti?",
  "kv11.2:2.11": "Āmantā.",
  "kv11.2:2.12": "Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṁ—",
  "kv11.2:2.13": "“ñāṇī”ti?",
  "kv11.2:2.14": "Na hevaṁ vattabbe …pe….",
  "kv11.2:3.1": "Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṁ—",
  "kv11.2:3.2": "“ñāṇī”ti?",
  "kv11.2:3.3": "Āmantā.",
  "kv11.2:3.4": "Atītena ñāṇena ñāṇī niruddhena vigatena paṭipassaddhena ñāṇena ñāṇīti?",
  "kv11.2:3.5": "Na hevaṁ vattabbe …pe….",
  "kv11.2:3.6": "Ñāṇakathā niṭṭhitā."
}