{
  "kv14.5:0.1": "Kathāvatthu",
  "kv14.5:0.2": "Mahāpaṇṇāsaka",
  "kv14.5:0.3": "Cuddasamavagga",
  "kv14.5:0.4": "Aññoanusayotikathā",
  "kv14.5:1.1": "Añño kāmarāgānusayo aññaṁ kāmarāgapariyuṭṭhānanti?",
  "kv14.5:1.2": "Āmantā.",
  "kv14.5:1.3": "Añño kāmarāgo aññaṁ kāmarāgapariyuṭṭhānanti?",
  "kv14.5:1.4": "Na hevaṁ vattabbe …pe…",
  "kv14.5:1.5": "sveva kāmarāgo taṁ kāmarāgapariyuṭṭhānanti?",
  "kv14.5:1.6": "Āmantā.",
  "kv14.5:1.7": "Sveva kāmarāgānusayo taṁ kāmarāgapariyuṭṭhānanti?",
  "kv14.5:1.8": "Na hevaṁ vattabbe …pe….",
  "kv14.5:2.1": "Añño paṭighānusayo aññaṁ paṭighapariyuṭṭhānanti?",
  "kv14.5:2.2": "Āmantā.",
  "kv14.5:2.3": "Aññaṁ paṭighaṁ aññaṁ paṭighapariyuṭṭhānanti?",
  "kv14.5:2.4": "Na hevaṁ vattabbe …pe…",
  "kv14.5:2.5": "taññeva paṭighaṁ taṁ paṭighapariyuṭṭhānanti?",
  "kv14.5:2.6": "Āmantā.",
  "kv14.5:2.7": "Sveva paṭighānusayo taṁ paṭighapariyuṭṭhānanti?",
  "kv14.5:2.8": "Na hevaṁ vattabbe …pe….",
  "kv14.5:3.1": "Añño mānānusayo aññaṁ mānapariyuṭṭhānanti?",
  "kv14.5:3.2": "Āmantā.",
  "kv14.5:3.3": "Añño māno aññaṁ mānapariyuṭṭhānanti?",
  "kv14.5:3.4": "Na hevaṁ vattabbe …pe…",
  "kv14.5:3.5": "sveva māno taṁ mānapariyuṭṭhānanti?",
  "kv14.5:3.6": "Āmantā.",
  "kv14.5:3.7": "Sveva mānānusayo taṁ mānapariyuṭṭhānanti?",
  "kv14.5:3.8": "Na hevaṁ vattabbe …pe….",
  "kv14.5:4.1": "Añño diṭṭhānusayo aññaṁ diṭṭhipariyuṭṭhānanti?",
  "kv14.5:4.2": "Āmantā.",
  "kv14.5:4.3": "Aññā diṭṭhi aññaṁ diṭṭhipariyuṭṭhānanti?",
  "kv14.5:4.4": "Na hevaṁ vattabbe …pe…",
  "kv14.5:4.5": "sāva diṭṭhi taṁ diṭṭhipariyuṭṭhānanti?",
  "kv14.5:4.6": "Āmantā.",
  "kv14.5:4.7": "Sveva diṭṭhānusayo taṁ diṭṭhipariyuṭṭhānanti?",
  "kv14.5:4.8": "Na hevaṁ vattabbe …pe….",
  "kv14.5:5.1": "Añño vicikicchānusayo aññaṁ vicikicchāpariyuṭṭhānanti?",
  "kv14.5:5.2": "Āmantā.",
  "kv14.5:5.3": "Aññā vicikicchā aññaṁ vicikicchāpariyuṭṭhānanti?",
  "kv14.5:5.4": "Na hevaṁ vattabbe …pe…",
  "kv14.5:5.5": "sāva vicikicchā taṁ vicikicchāpariyuṭṭhānanti?",
  "kv14.5:5.6": "Āmantā.",
  "kv14.5:5.7": "Sveva vicikicchānusayo taṁ vicikicchāpariyuṭṭhānanti?",
  "kv14.5:5.8": "Na hevaṁ vattabbe …pe….",
  "kv14.5:6.1": "Añño bhavarāgānusayo aññaṁ bhavarāgapariyuṭṭhānanti?",
  "kv14.5:6.2": "Āmantā.",
  "kv14.5:6.3": "Añño bhavarāgo aññaṁ bhavarāgapariyuṭṭhānanti?",
  "kv14.5:6.4": "Na hevaṁ vattabbe …pe…",
  "kv14.5:6.5": "sveva bhavarāgo taṁ bhavarāgapariyuṭṭhānanti?",
  "kv14.5:6.6": "Āmantā.",
  "kv14.5:6.7": "Sveva bhavarāgānusayo taṁ bhavarāgapariyuṭṭhānanti?",
  "kv14.5:6.8": "Na hevaṁ vattabbe …pe….",
  "kv14.5:7.1": "Añño avijjānusayo aññaṁ avijjāpariyuṭṭhānanti?",
  "kv14.5:7.2": "Āmantā.",
  "kv14.5:7.3": "Aññā avijjā aññaṁ avijjāpariyuṭṭhānanti?",
  "kv14.5:7.4": "Na hevaṁ vattabbe …pe…",
  "kv14.5:7.5": "sāva avijjā taṁ avijjāpariyuṭṭhānanti?",
  "kv14.5:7.6": "Āmantā.",
  "kv14.5:7.7": "Sveva avijjānusayo taṁ avijjāpariyuṭṭhānanti?",
  "kv14.5:7.8": "Na hevaṁ vattabbe …pe….",
  "kv14.5:8.1": "Na vattabbaṁ—",
  "kv14.5:8.2": "“añño anusayo aññaṁ pariyuṭṭhānan”ti?",
  "kv14.5:8.3": "Āmantā.",
  "kv14.5:8.4": "Puthujjano kusalābyākate citte vattamāne “sānusayo”ti vattabboti?",
  "kv14.5:8.5": "Āmantā.",
  "kv14.5:8.6": "“Pariyuṭṭhito”ti vattabboti?",
  "kv14.5:8.7": "Na hevaṁ vattabbe.",
  "kv14.5:8.8": "Tena hi añño anusayo aññaṁ pariyuṭṭhānanti.",
  "kv14.5:8.9": "Puthujjano kusalābyākate citte vattamāne “sarāgo”ti vattabboti?",
  "kv14.5:8.10": "Āmantā.",
  "kv14.5:8.11": "“Pariyuṭṭhito”ti vattabboti?",
  "kv14.5:8.12": "Na hevaṁ vattabbe.",
  "kv14.5:8.13": "Tena hi añño rāgo aññaṁ pariyuṭṭhānanti.",
  "kv14.5:8.14": "Añño anusayotikathā niṭṭhitā."
}