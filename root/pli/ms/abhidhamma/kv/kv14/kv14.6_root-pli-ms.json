{
  "kv14.6:0.1": "Kathāvatthu",
  "kv14.6:0.2": "Mahāpaṇṇāsaka",
  "kv14.6:0.3": "Cuddasamavagga",
  "kv14.6:0.4": "Pariyuṭṭhānaṁcittavippayuttantikathā",
  "kv14.6:1.1": "Pariyuṭṭhānaṁ cittavippayuttanti?",
  "kv14.6:1.2": "Āmantā.",
  "kv14.6:1.3": "Rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe…",
  "kv14.6:1.4": "phoṭṭhabbāyatananti?",
  "kv14.6:1.5": "Na hevaṁ vattabbe …pe…",
  "kv14.6:1.6": "pariyuṭṭhānaṁ cittavippayuttanti?",
  "kv14.6:1.7": "Āmantā.",
  "kv14.6:1.8": "Natthi sarāgaṁ cittaṁ sadosaṁ cittaṁ samohaṁ cittaṁ …pe…",
  "kv14.6:1.9": "akusalaṁ cittaṁ saṅkiliṭṭhaṁ cittanti?",
  "kv14.6:1.10": "Na hevaṁ vattabbe …pe…",
  "kv14.6:1.11": "nanu atthi sarāgaṁ cittaṁ sadosaṁ cittaṁ samohaṁ cittaṁ …pe…",
  "kv14.6:1.12": "akusalaṁ cittaṁ saṅkiliṭṭhaṁ cittanti?",
  "kv14.6:1.13": "Āmantā.",
  "kv14.6:1.14": "Hañci atthi sarāgaṁ cittaṁ sadosaṁ cittaṁ samohaṁ cittaṁ …pe…",
  "kv14.6:1.15": "akusalaṁ cittaṁ saṅkiliṭṭhaṁ cittaṁ, no ca vata re vattabbe—",
  "kv14.6:1.16": "“pariyuṭṭhānaṁ cittavippayuttan”ti.",
  "kv14.6:1.17": "Pariyuṭṭhānaṁ cittavippayuttantikathā niṭṭhitā."
}