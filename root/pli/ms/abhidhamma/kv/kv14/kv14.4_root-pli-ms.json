{
  "kv14.4:0.1": "Kathāvatthu",
  "kv14.4:0.2": "Mahāpaṇṇāsaka",
  "kv14.4:0.3": "Cuddasamavagga",
  "kv14.4:0.4": "Ariyarūpakathā",
  "kv14.4:1.1": "Ariyarūpaṁ mahābhūtānaṁ upādāyāti?",
  "kv14.4:1.2": "Āmantā.",
  "kv14.4:1.3": "Ariyarūpaṁ kusalanti?",
  "kv14.4:1.4": "Āmantā.",
  "kv14.4:1.5": "Mahābhūtā kusalāti?",
  "kv14.4:1.6": "Na hevaṁ vattabbe …pe…",
  "kv14.4:1.7": "mahābhūtā abyākatāti?",
  "kv14.4:1.8": "Āmantā.",
  "kv14.4:1.9": "Ariyarūpaṁ abyākatanti?",
  "kv14.4:1.10": "Na hevaṁ vattabbe …pe…",
  "kv14.4:1.11": "ariyarūpaṁ mahābhūtānaṁ upādāyāti?",
  "kv14.4:1.12": "Āmantā.",
  "kv14.4:1.13": "Ariyarūpaṁ anāsavaṁ asaṁyojaniyaṁ aganthaniyaṁ anoghaniyaṁ ayoganiyaṁ anīvaraṇiyaṁ aparāmaṭṭhaṁ anupādāniyaṁ asaṅkilesiyanti?",
  "kv14.4:1.14": "Āmantā.",
  "kv14.4:1.15": "Mahābhūtā anāsavā …pe… asaṅkilesiyāti?",
  "kv14.4:1.16": "Na hevaṁ vattabbe …pe…",
  "kv14.4:1.17": "mahābhūtā sāsavā saṁyojaniyā …pe… saṅkilesiyāti?",
  "kv14.4:1.18": "Āmantā.",
  "kv14.4:1.19": "Ariyarūpaṁ sāsavaṁ saṁyojaniyaṁ …pe… saṅkilesiyanti?",
  "kv14.4:1.20": "Na hevaṁ vattabbe …pe….",
  "kv14.4:2.1": "Na vattabbaṁ—",
  "kv14.4:2.2": "“ariyarūpaṁ mahābhūtānaṁ upādāyā”ti?",
  "kv14.4:2.3": "Āmantā.",
  "kv14.4:2.4": "Nanu vuttaṁ bhagavatā—",
  "kv14.4:2.5": "“yaṁ kiñci, bhikkhave, rūpaṁ cattāri mahābhūtāni catunnañca mahābhūtānaṁ upādāyarūpan”ti.",
  "kv14.4:2.6": "Attheva suttantoti?",
  "kv14.4:2.7": "Āmantā.",
  "kv14.4:2.8": "Tena hi ariyarūpaṁ mahābhūtānaṁ upādāyāti.",
  "kv14.4:2.9": "Ariyarūpakathā niṭṭhitā."
}