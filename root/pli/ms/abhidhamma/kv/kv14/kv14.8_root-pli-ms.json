{
  "kv14.8:0.1": "Kathāvatthu",
  "kv14.8:0.2": "Mahāpaṇṇāsaka",
  "kv14.8:0.3": "Cuddasamavagga",
  "kv14.8:0.4": "Abyākatakathā",
  "kv14.8:1.1": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:1.2": "Āmantā.",
  "kv14.8:1.3": "Vipākābyākataṁ kiriyābyākataṁ rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe…",
  "kv14.8:1.4": "phoṭṭhabbāyatananti?",
  "kv14.8:1.5": "Na hevaṁ vattabbe …pe…",
  "kv14.8:1.6": "diṭṭhigataṁ abyākatanti?",
  "kv14.8:1.7": "Āmantā.",
  "kv14.8:1.8": "Diṭṭhigatasampayutto phasso abyākatoti?",
  "kv14.8:1.9": "Na hevaṁ vattabbe …pe…",
  "kv14.8:1.10": "diṭṭhigataṁ abyākatanti?",
  "kv14.8:1.11": "Āmantā.",
  "kv14.8:1.12": "Diṭṭhigatasampayuttā vedanā …pe…",
  "kv14.8:1.13": "saññā …pe…",
  "kv14.8:1.14": "cetanā …pe…",
  "kv14.8:1.15": "cittaṁ abyākatanti?",
  "kv14.8:1.16": "Na hevaṁ vattabbe …pe….",
  "kv14.8:2.1": "Diṭṭhigatasampayutto phasso akusaloti?",
  "kv14.8:2.2": "Āmantā.",
  "kv14.8:2.3": "Diṭṭhigataṁ akusalanti?",
  "kv14.8:2.4": "Na hevaṁ vattabbe …pe…",
  "kv14.8:2.5": "diṭṭhigatasampayuttā vedanā saññā cetanā cittaṁ akusalanti?",
  "kv14.8:2.6": "Āmantā.",
  "kv14.8:2.7": "Diṭṭhigataṁ akusalanti?",
  "kv14.8:2.8": "Na hevaṁ vattabbe …pe….",
  "kv14.8:3.1": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:3.2": "Āmantā.",
  "kv14.8:3.3": "Aphalaṁ avipākanti?",
  "kv14.8:3.4": "Na hevaṁ vattabbe …pe…",
  "kv14.8:3.5": "nanu saphalaṁ savipākanti?",
  "kv14.8:3.6": "Āmantā.",
  "kv14.8:3.7": "Hañci saphalaṁ savipākaṁ, no ca vata re vattabbe—",
  "kv14.8:3.8": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:4.1": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:4.2": "Āmantā.",
  "kv14.8:4.3": "Nanu micchādiṭṭhiparamāni vajjāni vuttāni bhagavatāti?",
  "kv14.8:4.4": "Āmantā.",
  "kv14.8:4.5": "Hañci micchādiṭṭhiparamāni vajjāni vuttāni bhagavatā, no ca vata re vattabbe—",
  "kv14.8:4.6": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:5.1": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:5.2": "Āmantā.",
  "kv14.8:5.3": "Nanu vuttaṁ bhagavatā—",
  "kv14.8:5.4": "“micchādiṭṭhi kho, vaccha, akusalā, sammādiṭṭhi kusalā”ti.",
  "kv14.8:5.5": "Attheva suttantoti?",
  "kv14.8:5.6": "Āmantā.",
  "kv14.8:5.7": "Tena hi na vattabbaṁ—",
  "kv14.8:5.8": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:6.1": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:6.2": "Āmantā.",
  "kv14.8:6.3": "Nanu vuttaṁ bhagavatā—",
  "kv14.8:6.4": "“micchādiṭṭhissa kho ahaṁ, puṇṇa, dvinnaṁ gatīnaṁ aññataraṁ gatiṁ vadāmi—",
  "kv14.8:6.5": "nirayaṁ vā tiracchānayoniṁ vā”ti.",
  "kv14.8:6.6": "Attheva suttantoti?",
  "kv14.8:6.7": "Āmantā.",
  "kv14.8:6.8": "Tena hi na vattabbaṁ—",
  "kv14.8:6.9": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:7.1": "Na vattabbaṁ—",
  "kv14.8:7.2": "“diṭṭhigataṁ abyākatan”ti?",
  "kv14.8:7.3": "Āmantā.",
  "kv14.8:7.4": "Nanu vuttaṁ bhagavatā—",
  "kv14.8:7.5": "“‘sassato loko’ti kho, vaccha, abyākatametaṁ,",
  "kv14.8:7.6": "‘asassato loko’ti kho, vaccha, abyākatametaṁ,",
  "kv14.8:7.7": "‘antavā loko’ti kho, vaccha, abyākatametaṁ,",
  "kv14.8:7.8": "‘anantavā loko’ti kho, vaccha …pe…",
  "kv14.8:7.9": "‘taṁ jīvaṁ taṁ sarīran’ti kho, vaccha …pe…",
  "kv14.8:7.10": "‘aññaṁ jīvaṁ aññaṁ sarīran’ti kho, vaccha …pe…",
  "kv14.8:7.11": "‘hoti tathāgato paraṁ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.12": "‘na hoti tathāgato paraṁ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.13": "‘hoti ca na ca hoti tathāgato paraṁ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.14": "‘neva hoti na na hoti tathāgato paraṁ maraṇā’ti kho, vaccha, abyākatametan”ti.",
  "kv14.8:7.15": "Attheva suttantoti?",
  "kv14.8:7.16": "Āmantā.",
  "kv14.8:7.17": "Tena hi diṭṭhigataṁ abyākatanti.",
  "kv14.8:8.1": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:8.2": "Āmantā.",
  "kv14.8:8.3": "Nanu vuttaṁ bhagavatā—",
  "kv14.8:8.4": "“micchādiṭṭhikassa, bhikkhave, purisapuggalassa yañceva kāyakammaṁ yathādiṭṭhisamattaṁ samādinnaṁ, yañca vacīkammaṁ …pe… yañca manokammaṁ, yā ca cetanā, yā ca patthanā, yo ca paṇidhi, ye ca saṅkhārā, sabbe te dhammā aniṭṭhāya akantāya amanāpāya ahitāya dukkhāya saṁvattantī”ti.",
  "kv14.8:8.5": "Attheva suttantoti?",
  "kv14.8:8.6": "Āmantā.",
  "kv14.8:8.7": "Tena hi na vattabbaṁ—",
  "kv14.8:8.8": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:8.9": "Abyākatakathā niṭṭhitā."
}