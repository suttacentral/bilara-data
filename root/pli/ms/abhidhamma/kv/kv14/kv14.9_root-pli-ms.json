{
  "kv14.9:0.1": "Kathāvatthu",
  "kv14.9:0.2": "Mahāpaṇṇāsaka",
  "kv14.9:0.3": "Cuddasamavagga",
  "kv14.9:0.4": "Apariyāpannakathā",
  "kv14.9:1.1": "Diṭṭhigataṁ apariyāpannanti?",
  "kv14.9:1.2": "Āmantā.",
  "kv14.9:1.3": "Maggo phalaṁ nibbānaṁ, sotāpattimaggo sotāpattiphalaṁ, sakadāgāmimaggo sakadāgāmiphalaṁ, anāgāmimaggo anāgāmiphalaṁ, arahattamaggo arahattaphalaṁ, satipaṭṭhānaṁ sammappadhānaṁ iddhipādo indriyaṁ balaṁ bojjhaṅgoti?",
  "kv14.9:1.4": "Na hevaṁ vattabbe …pe….",
  "kv14.9:2.1": "Na vattabbaṁ—",
  "kv14.9:2.2": "“diṭṭhigataṁ apariyāpannan”ti?",
  "kv14.9:2.3": "Āmantā.",
  "kv14.9:2.4": "Puthujjano “kāmesu vītarāgo”ti vattabboti?",
  "kv14.9:2.5": "Āmantā.",
  "kv14.9:2.6": "“Vigatadiṭṭhiyo”ti vattabboti?",
  "kv14.9:2.7": "Na hevaṁ vattabbe.",
  "kv14.9:2.8": "Tena hi diṭṭhigataṁ apariyāpannanti.",
  "kv14.9:2.9": "Apariyāpannakathā niṭṭhitā.",
  "kv14.9:2.10": "Cuddasamo vaggo.",
  "kv14.9:2.11": "Tassuddānaṁ",
  "kv14.9:3.1": "Akusalamūlaṁ paṭisandahati kusalamūlaṁ,",
  "kv14.9:3.2": "kusalamūlaṁ paṭisandahati akusalamūlaṁ,",
  "kv14.9:3.3": "saḷāyatanaṁ chaviññāṇakāyā,",
  "kv14.9:3.4": "ariyarūpaṁ mahābhūtānaṁ upādāya,",
  "kv14.9:3.5": "sveva anusayo taṁ pariyuṭṭhānaṁ,",
  "kv14.9:3.6": "pariyuṭṭhānaṁ cittavippayuttaṁ,",
  "kv14.9:3.7": "yathādhātu taññeva anuseti,",
  "kv14.9:3.8": "diṭṭhigataṁ abyākataṁ,",
  "kv14.9:3.9": "diṭṭhigataṁ apariyāpannanti."
}