{
  "kv1.10:0.1": "Kathāvatthu",
  "kv1.10:0.2": "Mahāpaṇṇāsaka",
  "kv1.10:0.3": "Paṭhamavagga",
  "kv1.10:0.4": "Hevatthikathā",
  "kv1.10:1.1": "Atītaṁ atthīti?",
  "kv1.10:1.2": "Hevatthi, heva natthīti.",
  "kv1.10:1.3": "Sevatthi, seva natthīti?",
  "kv1.10:1.4": "Na hevaṁ vattabbe …pe…",
  "kv1.10:1.5": "sevatthi, seva natthīti?",
  "kv1.10:1.6": "Āmantā.",
  "kv1.10:1.7": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā, natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
  "kv1.10:1.8": "Na hevaṁ vattabbe …pe….",
  "kv1.10:2.1": "Anāgataṁ atthīti?",
  "kv1.10:2.2": "Hevatthi, heva natthīti.",
  "kv1.10:2.3": "Sevatthi, seva natthīti?",
  "kv1.10:2.4": "Na hevaṁ vattabbe …pe…",
  "kv1.10:2.5": "sevatthi, seva natthīti?",
  "kv1.10:2.6": "Āmantā.",
  "kv1.10:2.7": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā, natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
  "kv1.10:2.8": "Na hevaṁ vattabbe …pe….",
  "kv1.10:3.1": "Paccuppannaṁ atthīti?",
  "kv1.10:3.2": "Hevatthi, heva natthīti.",
  "kv1.10:3.3": "Sevatthi, seva natthīti?",
  "kv1.10:3.4": "Na hevaṁ vattabbe …pe…",
  "kv1.10:3.5": "sevatthi, seva natthīti?",
  "kv1.10:3.6": "Āmantā.",
  "kv1.10:3.7": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:3.8": "same samabhāge tajjāteti?",
  "kv1.10:3.9": "Na hevaṁ vattabbe …pe….",
  "kv1.10:4.1": "Atītaṁ hevatthi, heva natthīti?",
  "kv1.10:4.2": "Āmantā.",
  "kv1.10:4.3": "Kintatthi, kinti natthīti?",
  "kv1.10:4.4": "Atītaṁ atītanti hevatthi, atītaṁ anāgatanti heva natthi, atītaṁ paccuppannanti heva natthīti.",
  "kv1.10:4.5": "Sevatthi, seva natthīti?",
  "kv1.10:4.6": "Na hevaṁ vattabbe …pe…",
  "kv1.10:4.7": "sevatthi, seva natthīti?",
  "kv1.10:4.8": "Āmantā.",
  "kv1.10:4.9": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā, natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
  "kv1.10:4.10": "Na hevaṁ vattabbe …pe….",
  "kv1.10:5.1": "Anāgataṁ hevatthi, heva natthīti?",
  "kv1.10:5.2": "Āmantā.",
  "kv1.10:5.3": "Kintatthi, kinti natthīti?",
  "kv1.10:5.4": "Anāgataṁ anāgatanti hevatthi, anāgataṁ atītanti heva natthi, anāgataṁ paccuppannanti heva natthīti.",
  "kv1.10:5.5": "Sevatthi, seva natthīti?",
  "kv1.10:5.6": "Na hevaṁ vattabbe …pe…",
  "kv1.10:5.7": "sevatthi, seva natthīti?",
  "kv1.10:5.8": "Āmantā.",
  "kv1.10:5.9": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho …pe…",
  "kv1.10:5.10": "same samabhāge tajjāteti?",
  "kv1.10:5.11": "Na hevaṁ vattabbe …pe….",
  "kv1.10:6.1": "Paccuppannaṁ hevatthi, heva natthīti?",
  "kv1.10:6.2": "Āmantā.",
  "kv1.10:6.3": "Kintatthi, kinti natthīti?",
  "kv1.10:6.4": "Paccuppannaṁ paccuppannanti hevatthi, paccuppannaṁ atītanti heva natthi, paccuppannaṁ anāgatanti heva natthīti.",
  "kv1.10:6.5": "Sevatthi, seva natthīti?",
  "kv1.10:6.6": "Na hevaṁ vattabbe …pe…",
  "kv1.10:6.7": "sevatthi, seva natthīti?",
  "kv1.10:6.8": "Āmantā.",
  "kv1.10:6.9": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:6.10": "same samabhāge tajjāteti?",
  "kv1.10:6.11": "Na hevaṁ vattabbe …pe….",
  "kv1.10:7.1": "Na vattabbaṁ—",
  "kv1.10:7.2": "“atītaṁ hevatthi, heva natthi;",
  "kv1.10:7.3": "anāgataṁ hevatthi, heva natthi;",
  "kv1.10:7.4": "paccuppannaṁ hevatthi, heva natthī”ti?",
  "kv1.10:7.5": "Āmantā.",
  "kv1.10:7.6": "Atītaṁ anāgatanti hevatthi, atītaṁ paccuppannanti hevatthi, anāgataṁ atītanti hevatthi, anāgataṁ paccuppannanti hevatthi, paccuppannaṁ atītanti hevatthi, paccuppannaṁ anāgatanti hevatthīti?",
  "kv1.10:7.7": "Na hevaṁ vattabbe.",
  "kv1.10:7.8": "…pe….",
  "kv1.10:7.9": "Tena hi atītaṁ hevatthi heva natthi, anāgataṁ hevatthi heva natthi, paccuppannaṁ hevatthi, heva natthīti.",
  "kv1.10:8.1": "Rūpaṁ atthīti?",
  "kv1.10:8.2": "Hevatthi, heva natthīti.",
  "kv1.10:8.3": "Sevatthi, seva natthīti?",
  "kv1.10:8.4": "Na hevaṁ vattabbe …pe…",
  "kv1.10:8.5": "sevatthi, seva natthīti?",
  "kv1.10:8.6": "Āmantā.",
  "kv1.10:8.7": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā, natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
  "kv1.10:8.8": "Na hevaṁ vattabbe …pe….",
  "kv1.10:9.1": "Vedanā …",
  "kv1.10:9.2": "saññā …",
  "kv1.10:9.3": "saṅkhārā …",
  "kv1.10:9.4": "viññāṇaṁ atthīti?",
  "kv1.10:9.5": "Hevatthi, heva natthīti.",
  "kv1.10:9.6": "Sevatthi, seva natthīti?",
  "kv1.10:9.7": "Na hevaṁ vattabbe …pe…",
  "kv1.10:9.8": "sevatthi, seva natthīti?",
  "kv1.10:9.9": "Āmantā.",
  "kv1.10:9.10": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:9.11": "same samabhāge tajjāteti?",
  "kv1.10:9.12": "Na hevaṁ vattabbe …pe….",
  "kv1.10:10.1": "Rūpaṁ hevatthi, heva natthīti?",
  "kv1.10:10.2": "Āmantā.",
  "kv1.10:10.3": "Kintatthi, kinti natthīti?",
  "kv1.10:10.4": "Rūpaṁ rūpanti hevatthi, rūpaṁ vedanāti heva natthi …pe…",
  "kv1.10:10.5": "rūpaṁ saññāti heva natthi …pe…",
  "kv1.10:10.6": "rūpaṁ saṅkhārāti heva natthi …pe…",
  "kv1.10:10.7": "rūpaṁ viññāṇanti heva natthīti.",
  "kv1.10:10.8": "Sevatthi, seva natthīti?",
  "kv1.10:10.9": "Na hevaṁ vattabbe …pe…",
  "kv1.10:10.10": "sevatthi, seva natthīti?",
  "kv1.10:10.11": "Āmantā.",
  "kv1.10:10.12": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:10.13": "same samabhāge tajjāteti?",
  "kv1.10:10.14": "Na hevaṁ vattabbe …pe….",
  "kv1.10:11.1": "Vedanā …",
  "kv1.10:11.2": "saññā …",
  "kv1.10:11.3": "saṅkhārā …",
  "kv1.10:11.4": "viññāṇaṁ hevatthi, heva natthīti?",
  "kv1.10:11.5": "Āmantā.",
  "kv1.10:11.6": "Kintatthi, kinti natthīti?",
  "kv1.10:11.7": "Viññāṇaṁ viññāṇanti hevatthi.",
  "kv1.10:11.8": "Viññāṇaṁ rūpanti heva natthi …pe…",
  "kv1.10:11.9": "viññāṇaṁ vedanāti heva natthi …pe…",
  "kv1.10:11.10": "viññāṇaṁ saññāti heva natthi …pe…",
  "kv1.10:11.11": "viññāṇaṁ saṅkhārāti heva natthīti.",
  "kv1.10:11.12": "Sevatthi, seva natthīti?",
  "kv1.10:11.13": "Na hevaṁ vattabbe …pe…",
  "kv1.10:11.14": "sevatthi, seva natthīti?",
  "kv1.10:11.15": "Āmantā.",
  "kv1.10:11.16": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:11.17": "same samabhāge tajjāteti?",
  "kv1.10:11.18": "Na hevaṁ vattabbe …pe….",
  "kv1.10:12.1": "Na vattabbaṁ—",
  "kv1.10:12.2": "“rūpaṁ hevatthi, heva natthīti;",
  "kv1.10:12.3": "vedanā …",
  "kv1.10:12.4": "saññā …",
  "kv1.10:12.5": "saṅkhārā …",
  "kv1.10:12.6": "viññāṇaṁ hevatthi, heva natthīti?",
  "kv1.10:12.7": "Āmantā.",
  "kv1.10:12.8": "Rūpaṁ vedanāti hevatthi …pe…",
  "kv1.10:12.9": "rūpaṁ saññāti hevatthi …pe…",
  "kv1.10:12.10": "rūpaṁ saṅkhārāti hevatthi …pe…",
  "kv1.10:12.11": "rūpaṁ viññāṇanti hevatthi …",
  "kv1.10:12.12": "vedanā …",
  "kv1.10:12.13": "saññā …",
  "kv1.10:12.14": "saṅkhārā …",
  "kv1.10:12.15": "viññāṇaṁ rūpanti hevatthi …",
  "kv1.10:12.16": "viññāṇaṁ vedanāti hevatthi …",
  "kv1.10:12.17": "viññāṇaṁ saññāti hevatthi …",
  "kv1.10:12.18": "viññāṇaṁ saṅkhārāti hevatthīti?",
  "kv1.10:12.19": "Na hevaṁ vattabbe …pe…",
  "kv1.10:12.20": "tena hi rūpaṁ hevatthi, heva natthi;",
  "kv1.10:12.21": "vedanā …",
  "kv1.10:12.22": "saññā …",
  "kv1.10:12.23": "saṅkhārā …",
  "kv1.10:12.24": "viññāṇaṁ hevatthi, heva natthīti.",
  "kv1.10:12.25": "Hevatthikathā niṭṭhitā.",
  "kv1.10:12.26": "Tassuddānaṁ",
  "kv1.10:13.1": "Upalabbho parihāni,",
  "kv1.10:13.2": "Brahmacariyavāso odhiso;",
  "kv1.10:13.3": "Pariññā kāmarāgappahānaṁ,",
  "kv1.10:13.4": "Sabbatthivādo āyatanaṁ;",
  "kv1.10:13.5": "Atītānāgato subhaṅgo,",
  "kv1.10:13.6": "Sabbe dhammā satipaṭṭhānā;",
  "kv1.10:13.7": "Hevatthi heva natthīti.",
  "kv1.10:13.8": "Mahāvaggo."
}