{
  "kv16.2:0.1": "Kathāvatthu",
  "kv16.2:0.2": "Niggahapaṇṇāsaka",
  "kv16.2:0.3": "Soḷasamavagga",
  "kv16.2:0.4": "Paggahakathā",
  "kv16.2:1.1": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:1.2": "Āmantā.",
  "kv16.2:1.3": "Paro parassa cittaṁ “mā rajji”, “mā dussi”, “mā muyhi”, “mā kilissī”ti paggaṇhātīti?",
  "kv16.2:1.4": "Na hevaṁ vattabbe …pe…",
  "kv16.2:1.5": "paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:1.6": "Āmantā.",
  "kv16.2:1.7": "Paro parassa alobhaṁ kusalamūlaṁ janeti …",
  "kv16.2:1.8": "adosaṁ kusalamūlaṁ janeti …",
  "kv16.2:1.9": "amohaṁ kusalamūlaṁ janeti …",
  "kv16.2:1.10": "saddhaṁ janeti …",
  "kv16.2:1.11": "vīriyaṁ janeti …",
  "kv16.2:1.12": "satiṁ janeti …",
  "kv16.2:1.13": "samādhiṁ janeti …",
  "kv16.2:1.14": "paññaṁ janetīti?",
  "kv16.2:1.15": "Na hevaṁ vattabbe …pe…",
  "kv16.2:1.16": "paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:1.17": "Āmantā.",
  "kv16.2:1.18": "Paro parassa uppanno phasso “mā nirujjhī”ti paggaṇhātīti?",
  "kv16.2:1.19": "Na hevaṁ vattabbe …pe…",
  "kv16.2:1.20": "paro parassa uppannā vedanā …pe…",
  "kv16.2:1.21": "uppannā paññā “mā nirujjhī”ti paggaṇhātīti?",
  "kv16.2:1.22": "Na hevaṁ vattabbe …pe….",
  "kv16.2:2.1": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:2.2": "Āmantā.",
  "kv16.2:2.3": "Paro parassa atthāya rāgaṁ pajahati …",
  "kv16.2:2.4": "dosaṁ pajahati …",
  "kv16.2:2.5": "mohaṁ pajahati …pe…",
  "kv16.2:2.6": "anottappaṁ pajahatīti?",
  "kv16.2:2.7": "Na hevaṁ vattabbe …pe….",
  "kv16.2:3.1": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:3.2": "Āmantā.",
  "kv16.2:3.3": "Paro parassa atthāya maggaṁ bhāveti …",
  "kv16.2:3.4": "satipaṭṭhānaṁ bhāveti …pe…",
  "kv16.2:3.5": "bojjhaṅgaṁ bhāvetīti?",
  "kv16.2:3.6": "Na hevaṁ vattabbe …pe….",
  "kv16.2:4.1": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:4.2": "Āmantā.",
  "kv16.2:4.3": "Paro parassa atthāya dukkhaṁ parijānāti …pe…",
  "kv16.2:4.4": "maggaṁ bhāvetīti?",
  "kv16.2:4.5": "Na hevaṁ vattabbe …pe….",
  "kv16.2:5.1": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:5.2": "Āmantā.",
  "kv16.2:5.3": "Añño aññassa kārako, paraṅkataṁ sukhaṁ dukkhaṁ añño karoti añño paṭisaṁvedetīti?",
  "kv16.2:5.4": "Na hevaṁ vattabbe …pe….",
  "kv16.2:6.1": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:6.2": "Āmantā.",
  "kv16.2:6.3": "Nanu vuttaṁ bhagavatā—",
  "kv16.2:7.1": "“Attanāva kataṁ pāpaṁ …pe…",
  "kv16.2:7.2": "Nāñño aññaṁ visodhaye”ti.",
  "kv16.2:8.1": "Attheva suttantoti?",
  "kv16.2:8.2": "Āmantā.",
  "kv16.2:8.3": "Tena hi na vattabbaṁ—",
  "kv16.2:8.4": "“paro parassa cittaṁ paggaṇhātī”ti.",
  "kv16.2:9.1": "Na vattabbaṁ—",
  "kv16.2:9.2": "“paro parassa cittaṁ paggaṇhātī”ti?",
  "kv16.2:9.3": "Āmantā.",
  "kv16.2:9.4": "Nanu atthi balappattā, atthi vasībhūtāti?",
  "kv16.2:9.5": "Āmantā.",
  "kv16.2:9.6": "Hañci atthi balappattā, atthi vasībhūtā, tena vata re vattabbe—",
  "kv16.2:9.7": "“paro parassa cittaṁ paggaṇhātī”ti.",
  "kv16.2:9.8": "Paggahakathā niṭṭhitā."
}