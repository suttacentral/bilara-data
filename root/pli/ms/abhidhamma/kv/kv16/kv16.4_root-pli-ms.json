{
  "kv16.4:0.1": "Kathāvatthu",
  "kv16.4:0.2": "Mahāpaṇṇāsaka",
  "kv16.4:0.3": "Soḷasamavagga",
  "kv16.4:0.4": "Adhigayhamanasikārakathā",
  "kv16.4:1.1": "Adhigayha manasi karotīti?",
  "kv16.4:1.2": "Āmantā.",
  "kv16.4:1.3": "Tena cittena taṁ cittaṁ pajānātīti?",
  "kv16.4:1.4": "Na hevaṁ vattabbe …pe…",
  "kv16.4:1.5": "tena cittena taṁ cittaṁ pajānātīti?",
  "kv16.4:1.6": "Āmantā.",
  "kv16.4:1.7": "Tena cittena taṁ cittaṁ “cittan”ti pajānātīti?",
  "kv16.4:1.8": "Na hevaṁ vattabbe …pe…",
  "kv16.4:1.9": "tena cittena taṁ cittaṁ “cittan”ti pajānātīti?",
  "kv16.4:1.10": "Āmantā.",
  "kv16.4:1.11": "Taṁ cittaṁ tassa cittassa ārammaṇanti?",
  "kv16.4:1.12": "Na hevaṁ vattabbe …pe….",
  "kv16.4:2.1": "Taṁ cittaṁ tassa cittassa ārammaṇanti?",
  "kv16.4:2.2": "Āmantā.",
  "kv16.4:2.3": "Tena phassena taṁ phassaṁ phusati, tāya vedanāya …pe…",
  "kv16.4:2.4": "tāya saññāya …",
  "kv16.4:2.5": "tāya cetanāya …",
  "kv16.4:2.6": "tena cittena …",
  "kv16.4:2.7": "tena vitakkena …",
  "kv16.4:2.8": "tena vicārena …",
  "kv16.4:2.9": "tāya pītiyā …",
  "kv16.4:2.10": "tāya satiyā …",
  "kv16.4:2.11": "tāya paññāya taṁ paññaṁ pajānātīti?",
  "kv16.4:2.12": "Na hevaṁ vattabbe …pe….",
  "kv16.4:3.1": "Atītaṁ “atītan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:3.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:3.3": "atītaṁ “atītan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:3.4": "Āmantā.",
  "kv16.4:3.5": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:3.6": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:3.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:4.1": "Atītaṁ “atītan”ti manasikaronto, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:4.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:4.3": "atītaṁ “atītan”ti manasikaronto, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:4.4": "Āmantā.",
  "kv16.4:4.5": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:4.6": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:4.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:5.1": "Atītaṁ “atītan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karoti, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:5.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:5.3": "atītaṁ “atītan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karoti, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:5.4": "Āmantā.",
  "kv16.4:5.5": "Tiṇṇaṁ phassānaṁ …pe…",
  "kv16.4:5.6": "tiṇṇaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:5.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:6.1": "Anāgataṁ “anāgatan”ti manasikaronto, atītaṁ “atītan”ti manasi karotīti?",
  "kv16.4:6.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:6.3": "anāgataṁ “anāgatan”ti manasikaronto, atītaṁ “atītan”ti manasi karotīti?",
  "kv16.4:6.4": "Āmantā.",
  "kv16.4:6.5": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:6.6": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:6.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:7.1": "Anāgataṁ “anāgatan”ti manasikaronto, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:7.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:7.3": "anāgataṁ “anāgatan”ti manasikaronto, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:7.4": "Āmantā.",
  "kv16.4:7.5": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:7.6": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:7.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:8.1": "Anāgataṁ “anāgatan”ti manasikaronto, atītaṁ “atītan”ti manasi karoti, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:8.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:8.3": "anāgataṁ “anāgatan”ti manasikaronto, atītaṁ “atītan”ti manasi karoti, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:8.4": "Āmantā.",
  "kv16.4:8.5": "Tiṇṇaṁ phassānaṁ …pe…",
  "kv16.4:8.6": "tiṇṇaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:8.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:9.1": "Paccuppannaṁ “paccuppannan”ti manasikaronto, atītaṁ “atītan”ti manasi karotīti?",
  "kv16.4:9.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:9.3": "paccuppannaṁ “paccuppannan”ti manasikaronto, atītaṁ “atītan”ti manasi karotīti?",
  "kv16.4:9.4": "Āmantā.",
  "kv16.4:9.5": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:9.6": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:9.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:10.1": "Paccuppannaṁ “paccuppannan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:10.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:10.3": "paccuppannaṁ “paccuppannan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:10.4": "Āmantā.",
  "kv16.4:10.5": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:10.6": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:10.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:11.1": "Paccuppannaṁ “paccuppannan”ti manasikaronto, atītaṁ “atītan”ti manasi karoti, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:11.2": "Na hevaṁ vattabbe …pe…",
  "kv16.4:11.3": "paccuppannaṁ “paccuppannan”ti manasikaronto, atītaṁ “atītan”ti manasi karoti, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:11.4": "Āmantā.",
  "kv16.4:11.5": "Tiṇṇaṁ phassānaṁ …pe…",
  "kv16.4:11.6": "tiṇṇaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:11.7": "Na hevaṁ vattabbe …pe….",
  "kv16.4:12.1": "Na vattabbaṁ—",
  "kv16.4:12.2": "“adhigayha manasi karotī”ti?",
  "kv16.4:12.3": "Āmantā.",
  "kv16.4:12.4": "Nanu vuttaṁ bhagavatā—",
  "kv16.4:13.1": "“Sabbe saṅkhārā aniccāti,",
  "kv16.4:13.2": "yadā paññāya passati;",
  "kv16.4:13.3": "Atha nibbindati dukkhe,",
  "kv16.4:13.4": "esa maggo visuddhiyā.",
  "kv16.4:14.1": "Sabbe saṅkhārā dukkhāti,",
  "kv16.4:14.2": "yadā paññāya passati;",
  "kv16.4:14.3": "Atha nibbindati dukkhe,",
  "kv16.4:14.4": "esa maggo visuddhiyā.",
  "kv16.4:15.1": "Sabbe dhammā anattāti,",
  "kv16.4:15.2": "yadā paññāya passati;",
  "kv16.4:15.3": "Atha nibbindati dukkhe,",
  "kv16.4:15.4": "esa maggo visuddhiyā”ti.",
  "kv16.4:16.1": "Attheva suttantoti?",
  "kv16.4:16.2": "Āmantā.",
  "kv16.4:16.3": "Tena hi adhigayha manasi karotīti.",
  "kv16.4:16.4": "Adhigayhamanasikārakathā niṭṭhitā."
}