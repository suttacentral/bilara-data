{
  "kv23.4:0.1": "Kathāvatthu",
  "kv23.4:0.2": "Mahāpaṇṇāsaka",
  "kv23.4:0.3": "Tevīsatimavagga",
  "kv23.4:0.4": "Patirūpakathā",
  "kv23.4:1.1": "Atthi na rāgo rāgapatirūpakoti?",
  "kv23.4:1.2": "Āmantā.",
  "kv23.4:1.3": "Atthi na phasso phassapatirūpako,",
  "kv23.4:1.4": "atthi na vedanā vedanāpatirūpikā,",
  "kv23.4:1.5": "atthi na saññā saññāpatirūpikā,",
  "kv23.4:1.6": "atthi na cetanā cetanāpatirūpikā,",
  "kv23.4:1.7": "atthi na cittaṁ cittapatirūpakaṁ,",
  "kv23.4:1.8": "atthi na saddhā saddhāpatirūpikā,",
  "kv23.4:1.9": "atthi na vīriyaṁ vīriyapatirūpakaṁ,",
  "kv23.4:1.10": "atthi na sati satipatirūpikā,",
  "kv23.4:1.11": "atthi na samādhi samādhipatirūpako,",
  "kv23.4:1.12": "atthi na paññā paññāpatirūpikāti?",
  "kv23.4:1.13": "Na hevaṁ vattabbe …pe….",
  "kv23.4:2.1": "Atthi na doso dosapatirūpako,",
  "kv23.4:2.2": "atthi na moho mohapatirūpako,",
  "kv23.4:2.3": "atthi na kileso kilesapatirūpakoti?",
  "kv23.4:2.4": "Āmantā.",
  "kv23.4:2.5": "Atthi na phasso phassapatirūpako …pe…",
  "kv23.4:2.6": "atthi na paññā paññāpatirūpikāti?",
  "kv23.4:2.7": "Na hevaṁ vattabbe …pe….",
  "kv23.4:2.8": "Patirūpakathā niṭṭhitā."
}