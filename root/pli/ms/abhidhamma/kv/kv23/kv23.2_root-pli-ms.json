{
  "kv23.2:0.1": "Kathāvatthu",
  "kv23.2:0.2": "Mahāpaṇṇāsaka",
  "kv23.2:0.3": "Tevīsatimavagga",
  "kv23.2:0.4": "Arahantavaṇṇakathā",
  "kv23.2:1.1": "Arahantānaṁ vaṇṇena amanussā methunaṁ dhammaṁ paṭisevantīti?",
  "kv23.2:1.2": "Āmantā.",
  "kv23.2:1.3": "Arahantānaṁ vaṇṇena amanussā pāṇaṁ hananti …pe…",
  "kv23.2:1.4": "adinnaṁ ādiyanti,",
  "kv23.2:1.5": "musā bhaṇanti,",
  "kv23.2:1.6": "pisuṇaṁ bhaṇanti,",
  "kv23.2:1.7": "pharusaṁ bhaṇanti,",
  "kv23.2:1.8": "samphaṁ palapanti,",
  "kv23.2:1.9": "sandhiṁ chindanti,",
  "kv23.2:1.10": "nillopaṁ haranti,",
  "kv23.2:1.11": "ekāgārikaṁ karonti,",
  "kv23.2:1.12": "paripanthe tiṭṭhanti,",
  "kv23.2:1.13": "paradāraṁ gacchanti,",
  "kv23.2:1.14": "gāmaghātakaṁ karonti …pe…",
  "kv23.2:1.15": "nigamaghātakaṁ karontīti?",
  "kv23.2:1.16": "Na hevaṁ vattabbe …pe….",
  "kv23.2:1.17": "Arahantavaṇṇakathā niṭṭhitā."
}