{
  "kv10.5:0.1": "Kathāvatthu",
  "kv10.5:0.2": "Mahāpaṇṇāsaka",
  "kv10.5:0.3": "Dasamavagga",
  "kv10.5:0.4": "Sābhogātikathā",
  "kv10.5:1.1": "Pañcaviññāṇā sābhogāti?",
  "kv10.5:1.2": "Āmantā.",
  "kv10.5:1.3": "Nanu pañcaviññāṇā uppannavatthukā uppannārammaṇāti?",
  "kv10.5:1.4": "Āmantā.",
  "kv10.5:1.5": "Hañci pañcaviññāṇā uppannavatthukā uppannārammaṇā, no ca vata re vattabbe—",
  "kv10.5:1.6": "“pañcaviññāṇā sābhogā”ti.",
  "kv10.5:1.7": "Nanu pañcaviññāṇā purejātavatthukā purejātārammaṇā ajjhattikavatthukā bāhirārammaṇā asambhinnavatthukā asambhinnārammaṇā nānāvatthukā nānārammaṇā na aññamaññassa gocaravisayaṁ paccanubhonti, na asamannāhārā uppajjanti, na amanasikārā uppajjanti, na abbokiṇṇā uppajjanti, na apubbaṁ acarimaṁ uppajjanti, nanu pañcaviññāṇā na aññamaññassa samanantarā uppajjantīti?",
  "kv10.5:1.8": "Āmantā.",
  "kv10.5:1.9": "Hañci pañcaviññāṇā na aññamaññassa samanantarā uppajjanti, no ca vata re vattabbe—",
  "kv10.5:1.10": "“pañcaviññāṇā sābhogā”ti.",
  "kv10.5:2.1": "Cakkhuviññāṇaṁ sābhoganti?",
  "kv10.5:2.2": "Āmantā.",
  "kv10.5:2.3": "Cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.5:2.4": "Na hevaṁ vattabbe …pe…",
  "kv10.5:2.5": "cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.5:2.6": "Āmantā.",
  "kv10.5:2.7": "Cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇanti?",
  "kv10.5:2.8": "Na hevaṁ vattabbe …pe…",
  "kv10.5:2.9": "cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇanti?",
  "kv10.5:2.10": "Āmantā.",
  "kv10.5:2.11": "“Cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇan”ti—",
  "kv10.5:2.12": "attheva suttantoti?",
  "kv10.5:2.13": "Natthi …pe…",
  "kv10.5:2.14": "“cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv10.5:2.15": "attheva suttantoti?",
  "kv10.5:2.16": "Āmantā.",
  "kv10.5:2.17": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv10.5:2.18": "attheva suttanto, no ca vata re vattabbe—",
  "kv10.5:2.19": "“cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇan”ti.",
  "kv10.5:3.1": "Cakkhuviññāṇaṁ sābhoganti?",
  "kv10.5:3.2": "Āmantā.",
  "kv10.5:3.3": "Cakkhuviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.5:3.4": "Na hevaṁ vattabbe …pe…",
  "kv10.5:3.5": "cakkhuviññāṇaṁ sābhoganti?",
  "kv10.5:3.6": "Āmantā.",
  "kv10.5:3.7": "Cakkhuviññāṇaṁ phassaṁ ārabbha …pe…",
  "kv10.5:3.8": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.5:3.9": "Na hevaṁ vattabbe …pe…",
  "kv10.5:3.10": "manoviññāṇaṁ sābhogaṁ, manoviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.5:3.11": "Āmantā.",
  "kv10.5:3.12": "Cakkhuviññāṇaṁ sābhogaṁ, cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.5:3.13": "Na hevaṁ vattabbe …pe…",
  "kv10.5:3.14": "manoviññāṇaṁ sābhogaṁ, manoviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.5:3.15": "Āmantā.",
  "kv10.5:3.16": "Cakkhuviññāṇaṁ sābhogaṁ, cakkhuviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.5:3.17": "Na hevaṁ vattabbe …pe…",
  "kv10.5:3.18": "manoviññāṇaṁ sābhogaṁ, manoviññāṇaṁ phassaṁ ārabbha …pe…",
  "kv10.5:3.19": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.5:3.20": "Āmantā.",
  "kv10.5:3.21": "Cakkhuviññāṇaṁ sābhogaṁ, cakkhuviññāṇaṁ phassaṁ ārabbha …pe…",
  "kv10.5:3.22": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.5:3.23": "Na hevaṁ vattabbe …pe….",
  "kv10.5:4.1": "Na vattabbaṁ—",
  "kv10.5:4.2": "“pañcaviññāṇā sābhogā”ti?",
  "kv10.5:4.3": "Āmantā.",
  "kv10.5:4.4": "Nanu vuttaṁ bhagavatā—",
  "kv10.5:4.5": "“idha, bhikkhave, bhikkhu cakkhunā rūpaṁ disvā nimittaggāhī hoti …pe…",
  "kv10.5:4.6": "na nimittaggāhī hoti …pe…",
  "kv10.5:4.7": "kāyena phoṭṭhabbaṁ phusitvā nimittaggāhī hoti …pe…",
  "kv10.5:4.8": "na nimittaggāhī hotī”ti.",
  "kv10.5:4.9": "Attheva suttantoti?",
  "kv10.5:4.10": "Āmantā.",
  "kv10.5:4.11": "Tena hi pañcaviññāṇā sābhogāti …pe….",
  "kv10.5:4.12": "Sābhogātikathā niṭṭhitā."
}