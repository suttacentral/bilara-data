{
  "thig6.5:1.1": "Therīgāthā",
  "thig6.5:2.1": "Chakkanipāta",
  "thig6.5:3.1": "5. Anopamātherīgāthā",
  "thig6.5:4.1": "“Ucce kule ahaṃ jātā,",
  "thig6.5:4.2": "bahuvitte mahaddhane;",
  "thig6.5:4.3": "Vaṇṇarūpena sampannā,",
  "thig6.5:4.4": "dhītā majjhassa atrajā.",
  "thig6.5:5.1": "Patthitā rājaputtehi,",
  "thig6.5:5.2": "seṭṭhiputtehi gijjhitā;",
  "thig6.5:5.3": "Pitu me pesayī dūtaṃ,",
  "thig6.5:5.4": "detha mayhaṃ anopamaṃ.",
  "thig6.5:6.1": "Yattakaṃ tulitā esā,",
  "thig6.5:6.2": "tuyhaṃ dhītā anopamā;",
  "thig6.5:6.3": "Tato aṭṭhaguṇaṃ dassaṃ,",
  "thig6.5:6.4": "hiraññaṃ ratanāni ca.",
  "thig6.5:7.1": "Sāhaṃ disvāna sambuddhaṃ,",
  "thig6.5:7.2": "lokajeṭṭhaṃ anuttaraṃ;",
  "thig6.5:7.3": "Tassa pādāni vanditvā,",
  "thig6.5:7.4": "ekamantaṃ upāvisiṃ.",
  "thig6.5:8.1": "So me dhammamadesesi,",
  "thig6.5:8.2": "anukampāya gotamo;",
  "thig6.5:8.3": "Nisinnā āsane tasmiṃ,",
  "thig6.5:8.4": "phusayiṃ tatiyaṃ phalaṃ.",
  "thig6.5:9.1": "Tato kesāni chetvāna,",
  "thig6.5:9.2": "pabbajiṃ anagāriyaṃ;",
  "thig6.5:9.3": "Ajja me sattamī ratti,",
  "thig6.5:9.4": "yato taṇhā visositā”ti.",
  "thig6.5:10.1": "…",
  "thig6.5:10.2": "Anopamā therī …."
}