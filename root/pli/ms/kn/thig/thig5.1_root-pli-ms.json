{
  "thig5.1:1.1": "Therīgāthā",
  "thig5.1:2.1": "Pañcakanipāta",
  "thig5.1:3.1": "1. Aññataratherīgāthā",
  "thig5.1:4.1": "“Paṇṇavīsativassāni,",
  "thig5.1:4.2": "yato pabbajitā ahaṃ;",
  "thig5.1:4.3": "Nāccharāsaṅghātamattampi,",
  "thig5.1:4.4": "cittassūpasamajjhagaṃ.",
  "thig5.1:5.1": "Aladdhā cetaso santiṃ,",
  "thig5.1:5.2": "kāmarāgenavassutā;",
  "thig5.1:5.3": "Bāhā paggayha kandantī,",
  "thig5.1:5.4": "vihāraṃ pāvisiṃ ahaṃ.",
  "thig5.1:6.1": "Sā bhikkhuniṃ upāgacchiṃ,",
  "thig5.1:6.2": "yā me saddhāyikā ahu;",
  "thig5.1:6.3": "Sā me dhammamadesesi,",
  "thig5.1:6.4": "khandhāyatanadhātuyo.",
  "thig5.1:7.1": "Tassā dhammaṃ suṇitvāna,",
  "thig5.1:7.2": "ekamante upāvisiṃ;",
  "thig5.1:7.3": "Pubbenivāsaṃ jānāmi,",
  "thig5.1:7.4": "dibbacakkhu visodhitaṃ.",
  "thig5.1:8.1": "Cetopariccañāṇañca,",
  "thig5.1:8.2": "sotadhātu visodhitā;",
  "thig5.1:8.3": "Iddhīpi me sacchikatā,",
  "thig5.1:8.4": "patto me āsavakkhayo;",
  "thig5.1:8.5": "Chaḷabhiññā sacchikatā,",
  "thig5.1:8.6": "kataṃ buddhassa sāsanan”ti.",
  "thig5.1:9.1": "…",
  "thig5.1:9.2": "Aññatarā therī …."
}