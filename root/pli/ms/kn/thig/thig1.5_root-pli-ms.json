{
  "thig1.5:1.1": "Therīgāthā",
  "thig1.5:2.1": "Ekakanipāta",
  "thig1.5:3.1": "5. Aññatarātissātherīgāthā",
  "thig1.5:4.1": "“Tisse yuñjassu dhammehi,",
  "thig1.5:4.2": "khaṇo taṃ mā upaccagā;",
  "thig1.5:4.3": "Khaṇātītā hi socanti,",
  "thig1.5:4.4": "nirayamhi samappitā”ti.",
  "thig1.5:5.1": "…",
  "thig1.5:5.2": "Aññatarā tissā therī …."
}