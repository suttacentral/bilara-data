{
  "thig1.6:1.1": "Therīgāthā",
  "thig1.6:2.1": "Ekakanipāta",
  "thig1.6:3.1": "6. Dhīrātherīgāthā",
  "thig1.6:4.1": "“Dhīre nirodhaṃ phusehi,",
  "thig1.6:4.2": "Saññāvūpasamaṃ sukhaṃ;",
  "thig1.6:4.3": "Ārādhayāhi nibbānaṃ,",
  "thig1.6:4.4": "Yogakkhemamanuttaran”ti.",
  "thig1.6:5.1": "…",
  "thig1.6:5.2": "Dhīrā therī …."
}