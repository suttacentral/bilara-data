{
  "thig1.10:1.1": "Therīgāthā",
  "thig1.10:2.1": "Ekakanipāta",
  "thig1.10:3.1": "10. Upasamātherīgāthā",
  "thig1.10:4.1": "“Upasame tare oghaṃ,",
  "thig1.10:4.2": "maccudheyyaṃ suduttaraṃ;",
  "thig1.10:4.3": "Dhārehi antimaṃ dehaṃ,",
  "thig1.10:4.4": "jetvā māraṃ savāhinin”ti.",
  "thig1.10:5.1": "…",
  "thig1.10:5.2": "Upasamā therī …."
}