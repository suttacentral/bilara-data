{
  "thig2.10:1.1": "Therīgāthā",
  "thig2.10:2.1": "Dukanipāta",
  "thig2.10:3.1": "10. Sāmātherīgāthā",
  "thig2.10:4.1": "“Catukkhattuṃ pañcakkhattuṃ,",
  "thig2.10:4.2": "vihārā upanikkhamiṃ;",
  "thig2.10:4.3": "Aladdhā cetaso santiṃ,",
  "thig2.10:4.4": "citte avasavattinī;",
  "thig2.10:4.5": "Tassā me aṭṭhamī ratti,",
  "thig2.10:4.6": "yato taṇhā samūhatā.",
  "thig2.10:5.1": "Bahūhi dukkhadhammehi,",
  "thig2.10:5.2": "appamādaratāya me;",
  "thig2.10:5.3": "Taṇhakkhayo anuppatto,",
  "thig2.10:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thig2.10:6.1": "…",
  "thig2.10:6.2": "Sāmā therī ….",
  "thig2.10:7.1": "Dukanipāto niṭṭhito."
}