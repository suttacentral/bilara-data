{
  "thig2.9:1.1": "Therīgāthā",
  "thig2.9:2.1": "Dukanipāta",
  "thig2.9:3.1": "9. Abhayātherīgāthā",
  "thig2.9:4.1": "“Abhaye bhiduro kāyo,",
  "thig2.9:4.2": "yattha satā puthujjanā;",
  "thig2.9:4.3": "Nikkhipissāmimaṃ dehaṃ,",
  "thig2.9:4.4": "sampajānā satīmatī.",
  "thig2.9:5.1": "Bahūhi dukkhadhammehi,",
  "thig2.9:5.2": "appamādaratāya me;",
  "thig2.9:5.3": "Taṇhakkhayo anuppatto,",
  "thig2.9:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thig2.9:6.1": "…",
  "thig2.9:6.2": "Abhayā therī …."
}