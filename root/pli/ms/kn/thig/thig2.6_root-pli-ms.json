{
  "thig2.6:1.1": "Therīgāthā",
  "thig2.6:2.1": "Dukanipāta",
  "thig2.6:3.1": "6. Mettikātherīgāthā",
  "thig2.6:4.1": "“Kiñcāpi khomhi dukkhitā,",
  "thig2.6:4.2": "dubbalā gatayobbanā;",
  "thig2.6:4.3": "Daṇḍamolubbha gacchāmi,",
  "thig2.6:4.4": "pabbataṃ abhirūhiya.",
  "thig2.6:5.1": "Nikkhipitvāna saṅghāṭiṃ,",
  "thig2.6:5.2": "pattakañca nikujjiya;",
  "thig2.6:5.3": "Nisinnā camhi selamhi,",
  "thig2.6:5.4": "atha cittaṃ vimucci me;",
  "thig2.6:5.5": "Tisso vijjā anuppattā,",
  "thig2.6:5.6": "kataṃ buddhassa sāsanan”ti.",
  "thig2.6:6.1": "…",
  "thig2.6:6.2": "Mettikā therī …."
}