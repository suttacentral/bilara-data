{
  "thig1.4:1.1": "Therīgāthā",
  "thig1.4:2.1": "Ekakanipāta",
  "thig1.4:3.1": "4. Tissātherīgāthā",
  "thig1.4:4.1": "“Tisse sikkhassu sikkhāya,",
  "thig1.4:4.2": "mā taṃ yogā upaccaguṃ;",
  "thig1.4:4.3": "Sabbayogavisaṃyuttā,",
  "thig1.4:4.4": "cara loke anāsavā”ti.",
  "thig1.4:5.1": "…",
  "thig1.4:5.2": "Tissā therī …."
}