{
  "thig3.3:1.1": "Therīgāthā",
  "thig3.3:2.1": "Tikanipāta",
  "thig3.3:3.1": "3. Aparāuttamātherīgāthā",
  "thig3.3:4.1": "“Ye ime satta bojjhaṅgā,",
  "thig3.3:4.2": "maggā nibbānapattiyā;",
  "thig3.3:4.3": "Bhāvitā te mayā sabbe,",
  "thig3.3:4.4": "yathā buddhena desitā.",
  "thig3.3:5.1": "Suññatassānimittassa,",
  "thig3.3:5.2": "lābhinīhaṃ yadicchakaṃ;",
  "thig3.3:5.3": "Orasā dhītā buddhassa,",
  "thig3.3:5.4": "nibbānābhiratā sadā.",
  "thig3.3:6.1": "Sabbe kāmā samucchinnā,",
  "thig3.3:6.2": "ye dibbā ye ca mānusā;",
  "thig3.3:6.3": "Vikkhīṇo jātisaṃsāro,",
  "thig3.3:6.4": "natthi dāni punabbhavo”ti.",
  "thig3.3:7.1": "…",
  "thig3.3:7.2": "Aparā uttamā therī …."
}