{
  "thig1.8:1.1": "Therīgāthā",
  "thig1.8:2.1": "Ekakanipāta",
  "thig1.8:3.1": "8. Mittātherīgāthā",
  "thig1.8:4.1": "“Saddhāya pabbajitvāna,",
  "thig1.8:4.2": "mitte mittaratā bhava;",
  "thig1.8:4.3": "Bhāvehi kusale dhamme,",
  "thig1.8:4.4": "yogakkhemassa pattiyā”ti.",
  "thig1.8:5.1": "…",
  "thig1.8:5.2": "Mittā therī …."
}