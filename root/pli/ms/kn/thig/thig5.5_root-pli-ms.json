{
  "thig5.5:1.1": "Therīgāthā",
  "thig5.5:2.1": "Pañcakanipāta",
  "thig5.5:3.1": "5. Nanduttarātherīgāthā",
  "thig5.5:4.1": "“Aggiṃ candañca sūriyañca,",
  "thig5.5:4.2": "devatā ca namassihaṃ;",
  "thig5.5:4.3": "Nadītitthāni gantvāna,",
  "thig5.5:4.4": "udakaṃ oruhāmihaṃ.",
  "thig5.5:5.1": "Bahūvatasamādānā,",
  "thig5.5:5.2": "aḍḍhaṃ sīsassa olikhiṃ;",
  "thig5.5:5.3": "Chamāya seyyaṃ kappemi,",
  "thig5.5:5.4": "rattiṃ bhattaṃ na bhuñjahaṃ.",
  "thig5.5:6.1": "Vibhūsāmaṇḍanaratā,",
  "thig5.5:6.2": "nhāpanucchādanehi ca;",
  "thig5.5:6.3": "Upakāsiṃ imaṃ kāyaṃ,",
  "thig5.5:6.4": "kāmarāgena aṭṭitā.",
  "thig5.5:7.1": "Tato saddhaṃ labhitvāna,",
  "thig5.5:7.2": "pabbajiṃ anagāriyaṃ;",
  "thig5.5:7.3": "Disvā kāyaṃ yathābhūtaṃ,",
  "thig5.5:7.4": "kāmarāgo samūhato.",
  "thig5.5:8.1": "Sabbe bhavā samucchinnā,",
  "thig5.5:8.2": "icchā ca patthanāpi ca;",
  "thig5.5:8.3": "Sabbayogavisaṃyuttā,",
  "thig5.5:8.4": "santiṃ pāpuṇi cetaso”ti.",
  "thig5.5:9.1": "…",
  "thig5.5:9.2": "Nanduttarā therī …."
}