{
  "thig1.14:1.1": "Therīgāthā",
  "thig1.14:2.1": "Ekakanipāta",
  "thig1.14:3.1": "14. Sumanātherīgāthā",
  "thig1.14:4.1": "“Dhātuyo dukkhato disvā,",
  "thig1.14:4.2": "mā jātiṃ punarāgami;",
  "thig1.14:4.3": "Bhave chandaṃ virājetvā,",
  "thig1.14:4.4": "upasantā carissasī”ti.",
  "thig1.14:5.1": "…",
  "thig1.14:5.2": "Sumanā therī …."
}