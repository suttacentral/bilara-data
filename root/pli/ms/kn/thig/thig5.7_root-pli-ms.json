{
  "thig5.7:1.1": "Therīgāthā",
  "thig5.7:2.1": "Pañcakanipāta",
  "thig5.7:3.1": "7. Sakulātherīgāthā",
  "thig5.7:4.1": "“Agārasmiṃ vasantīhaṃ,",
  "thig5.7:4.2": "dhammaṃ sutvāna bhikkhuno;",
  "thig5.7:4.3": "Addasaṃ virajaṃ dhammaṃ,",
  "thig5.7:4.4": "nibbānaṃ padamaccutaṃ.",
  "thig5.7:5.1": "Sāhaṃ puttaṃ dhītarañca,",
  "thig5.7:5.2": "dhanadhaññañca chaḍḍiya;",
  "thig5.7:5.3": "Kese chedāpayitvāna,",
  "thig5.7:5.4": "pabbajiṃ anagāriyaṃ.",
  "thig5.7:6.1": "Sikkhamānā ahaṃ santī,",
  "thig5.7:6.2": "bhāventī maggamañjasaṃ;",
  "thig5.7:6.3": "Pahāsiṃ rāgadosañca,",
  "thig5.7:6.4": "tadekaṭṭhe ca āsave.",
  "thig5.7:7.1": "Bhikkhunī upasampajja,",
  "thig5.7:7.2": "pubbajātimanussariṃ;",
  "thig5.7:7.3": "Dibbacakkhu visodhitaṃ,",
  "thig5.7:7.4": "vimalaṃ sādhubhāvitaṃ.",
  "thig5.7:8.1": "Saṅkhāre parato disvā,",
  "thig5.7:8.2": "hetujāte palokite;",
  "thig5.7:8.3": "Pahāsiṃ āsave sabbe,",
  "thig5.7:8.4": "sītibhūtāmhi nibbutā”ti.",
  "thig5.7:9.1": "…",
  "thig5.7:9.2": "Sakulā therī …."
}