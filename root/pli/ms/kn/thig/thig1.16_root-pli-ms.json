{
  "thig1.16:1.1": "Therīgāthā",
  "thig1.16:2.1": "Ekakanipāta",
  "thig1.16:3.1": "16 Vuḍḍhapabbajitasumanātherīgāthā",
  "thig1.16:4.1": "“Sukhaṃ tvaṃ vuḍḍhike sehi,",
  "thig1.16:4.2": "katvā coḷena pārutā;",
  "thig1.16:4.3": "Upasanto hi te rāgo,",
  "thig1.16:4.4": "sītibhūtāsi nibbutā”ti.",
  "thig1.16:5.1": "…",
  "thig1.16:5.2": "Sumanā vuḍḍhapabbajitā therī …."
}