{
  "thig6.7:1.1": "Therīgāthā",
  "thig6.7:2.1": "Chakkanipāta",
  "thig6.7:3.1": "7. Guttātherīgāthā",
  "thig6.7:4.1": "“Gutte yadatthaṃ pabbajjā,",
  "thig6.7:4.2": "hitvā puttaṃ vasuṃ piyaṃ;",
  "thig6.7:4.3": "Tameva anubrūhehi,",
  "thig6.7:4.4": "mā cittassa vasaṃ gami.",
  "thig6.7:5.1": "Cittena vañcitā sattā,",
  "thig6.7:5.2": "mārassa visaye ratā;",
  "thig6.7:5.3": "Anekajātisaṃsāraṃ,",
  "thig6.7:5.4": "sandhāvanti aviddasū.",
  "thig6.7:6.1": "Kāmacchandañca byāpādaṃ,",
  "thig6.7:6.2": "sakkāyadiṭṭhimeva ca;",
  "thig6.7:6.3": "Sīlabbataparāmāsaṃ,",
  "thig6.7:6.4": "vicikicchañca pañcamaṃ.",
  "thig6.7:7.1": "Saṃyojanāni etāni,",
  "thig6.7:7.2": "pajahitvāna bhikkhunī;",
  "thig6.7:7.3": "Orambhāgamanīyāni,",
  "thig6.7:7.4": "nayidaṃ punarehisi.",
  "thig6.7:8.1": "Rāgaṃ mānaṃ avijjañca,",
  "thig6.7:8.2": "uddhaccañca vivajjiya;",
  "thig6.7:8.3": "Saṃyojanāni chetvāna,",
  "thig6.7:8.4": "dukkhassantaṃ karissasi.",
  "thig6.7:9.1": "Khepetvā jātisaṃsāraṃ,",
  "thig6.7:9.2": "pariññāya punabbhavaṃ;",
  "thig6.7:9.3": "Diṭṭheva dhamme nicchātā,",
  "thig6.7:9.4": "upasantā carissatī”ti.",
  "thig6.7:10.1": "…",
  "thig6.7:10.2": "Guttā therī …."
}