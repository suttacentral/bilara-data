{
  "thig5.8:1.1": "Therīgāthā",
  "thig5.8:2.1": "Pañcakanipāta",
  "thig5.8:3.1": "8. Soṇātherīgāthā",
  "thig5.8:4.1": "“Dasa putte vijāyitvā,",
  "thig5.8:4.2": "asmiṃ rūpasamussaye;",
  "thig5.8:4.3": "Tatohaṃ dubbalā jiṇṇā,",
  "thig5.8:4.4": "bhikkhuniṃ upasaṅkamiṃ.",
  "thig5.8:5.1": "Sā me dhammamadesesi,",
  "thig5.8:5.2": "khandhāyatanadhātuyo;",
  "thig5.8:5.3": "Tassā dhammaṃ suṇitvāna,",
  "thig5.8:5.4": "kese chetvāna pabbajiṃ.",
  "thig5.8:6.1": "Tassā me sikkhamānāya,",
  "thig5.8:6.2": "dibbacakkhu visodhitaṃ;",
  "thig5.8:6.3": "Pubbenivāsaṃ jānāmi,",
  "thig5.8:6.4": "yattha me vusitaṃ pure.",
  "thig5.8:7.1": "Animittañca bhāvemi,",
  "thig5.8:7.2": "ekaggā susamāhitā;",
  "thig5.8:7.3": "Anantarāvimokkhāsiṃ,",
  "thig5.8:7.4": "anupādāya nibbutā.",
  "thig5.8:8.1": "Pañcakkhandhā pariññātā,",
  "thig5.8:8.2": "tiṭṭhanti chinnamūlakā;",
  "thig5.8:8.3": "Dhi tavatthu jare jamme,",
  "thig5.8:8.4": "natthi dāni punabbhavo”ti.",
  "thig5.8:9.1": "…",
  "thig5.8:9.2": "Soṇā therī …."
}