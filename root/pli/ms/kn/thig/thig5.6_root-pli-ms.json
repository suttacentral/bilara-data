{
  "thig5.6:1.1": "Therīgāthā",
  "thig5.6:2.1": "Pañcakanipāta",
  "thig5.6:3.1": "6 Mittākāḷītherīgāthā",
  "thig5.6:4.1": "“Saddhāya pabbajitvāna,",
  "thig5.6:4.2": "agārasmānagāriyaṃ;",
  "thig5.6:4.3": "Vicariṃhaṃ tena tena,",
  "thig5.6:4.4": "lābhasakkāraussukā.",
  "thig5.6:5.1": "Riñcitvā paramaṃ atthaṃ,",
  "thig5.6:5.2": "hīnamatthaṃ asevihaṃ;",
  "thig5.6:5.3": "Kilesānaṃ vasaṃ gantvā,",
  "thig5.6:5.4": "sāmaññatthaṃ na bujjhihaṃ.",
  "thig5.6:6.1": "Tassā me ahu saṃvego,",
  "thig5.6:6.2": "nisinnāya vihārake;",
  "thig5.6:6.3": "Ummaggapaṭipannāmhi,",
  "thig5.6:6.4": "taṇhāya vasamāgatā.",
  "thig5.6:7.1": "Appakaṃ jīvitaṃ mayhaṃ,",
  "thig5.6:7.2": "jarā byādhi ca maddati;",
  "thig5.6:7.3": "Purāyaṃ bhijjati kāyo,",
  "thig5.6:7.4": "na me kālo pamajjituṃ.",
  "thig5.6:8.1": "Yathābhūtamavekkhantī,",
  "thig5.6:8.2": "khandhānaṃ udayabbayaṃ;",
  "thig5.6:8.3": "Vimuttacittā uṭṭhāsiṃ,",
  "thig5.6:8.4": "kataṃ buddhassa sāsanan”ti.",
  "thig5.6:9.1": "…",
  "thig5.6:9.2": "Mittā kāḷī therī …."
}