{
  "thig1.17:1.1": "Therīgāthā",
  "thig1.17:2.1": "Ekakanipāta",
  "thig1.17:3.1": "17. Dhammātherīgāthā",
  "thig1.17:4.1": "“Piṇḍapātaṃ caritvāna,",
  "thig1.17:4.2": "daṇḍamolubbha dubbalā;",
  "thig1.17:4.3": "Vedhamānehi gattehi,",
  "thig1.17:4.4": "tattheva nipatiṃ chamā;",
  "thig1.17:4.5": "Disvā ādīnavaṃ kāye,",
  "thig1.17:4.6": "atha cittaṃ vimucci me”ti.",
  "thig1.17:5.1": "…",
  "thig1.17:5.2": "Dhammā therī …."
}