{
  "thig3.8:1.1": "Therīgāthā",
  "thig3.8:2.1": "Tikanipāta",
  "thig3.8:3.1": "8. Somātherīgāthā",
  "thig3.8:4.1": "“Yaṃ taṃ isīhi pattabbaṃ,",
  "thig3.8:4.2": "ṭhānaṃ durabhisambhavaṃ;",
  "thig3.8:4.3": "Na taṃ dvaṅgulapaññāya,",
  "thig3.8:4.4": "sakkā pappotumitthiyā”.",
  "thig3.8:5.1": "“Itthibhāvo no kiṃ kayirā,",
  "thig3.8:5.2": "cittamhi susamāhite;",
  "thig3.8:5.3": "Ñāṇamhi vattamānamhi,",
  "thig3.8:5.4": "sammā dhammaṃ vipassato.",
  "thig3.8:6.1": "Sabbattha vihatā nandī,",
  "thig3.8:6.2": "Tamokhandho padālito;",
  "thig3.8:6.3": "Evaṃ jānāhi pāpima,",
  "thig3.8:6.4": "Nihato tvamasi antakā”ti.",
  "thig3.8:7.1": "…",
  "thig3.8:7.2": "Somā therī ….",
  "thig3.8:8.1": "Tikanipāto niṭṭhito."
}