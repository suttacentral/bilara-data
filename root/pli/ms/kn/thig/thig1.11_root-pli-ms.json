{
  "thig1.11:1.1": "Therīgāthā",
  "thig1.11:2.1": "Ekakanipāta",
  "thig1.11:3.1": "11. Muttātherīgāthā",
  "thig1.11:4.1": "“Sumuttā sādhumuttāmhi,",
  "thig1.11:4.2": "tīhi khujjehi muttiyā;",
  "thig1.11:4.3": "Udukkhalena musalena,",
  "thig1.11:4.4": "patinā khujjakena ca;",
  "thig1.11:4.5": "Muttāmhi jātimaraṇā,",
  "thig1.11:4.6": "bhavanetti samūhatā”ti.",
  "thig1.11:5.1": "…",
  "thig1.11:5.2": "Muttā therī …."
}