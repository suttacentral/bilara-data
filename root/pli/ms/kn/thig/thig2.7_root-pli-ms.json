{
  "thig2.7:1.1": "Therīgāthā",
  "thig2.7:2.1": "Dukanipāta",
  "thig2.7:3.1": "7. Mittātherīgāthā",
  "thig2.7:4.1": "“Cātuddasiṃ pañcadasiṃ,",
  "thig2.7:4.2": "yā ca pakkhassa aṭṭhamī;",
  "thig2.7:4.3": "Pāṭihāriyapakkhañca,",
  "thig2.7:4.4": "aṭṭhaṅgasusamāgataṃ.",
  "thig2.7:5.1": "Uposathaṃ upāgacchiṃ,",
  "thig2.7:5.2": "devakāyābhinandinī;",
  "thig2.7:5.3": "Sājja ekena bhattena,",
  "thig2.7:5.4": "muṇḍā saṅghāṭipārutā;",
  "thig2.7:5.5": "Devakāyaṃ na patthehaṃ,",
  "thig2.7:5.6": "vineyya hadaye daran”ti.",
  "thig2.7:6.1": "…",
  "thig2.7:6.2": "Mittā therī …."
}