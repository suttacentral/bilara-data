{
  "thig1.7:1.1": "Therīgāthā",
  "thig1.7:2.1": "Ekakanipāta",
  "thig1.7:3.1": "7. Vīrātherīgāthā",
  "thig1.7:4.1": "“Vīrā vīrehi dhammehi,",
  "thig1.7:4.2": "bhikkhunī bhāvitindriyā;",
  "thig1.7:4.3": "Dhāreti antimaṃ dehaṃ,",
  "thig1.7:4.4": "jetvā māraṃ savāhinin”ti.",
  "thig1.7:5.1": "…",
  "thig1.7:5.2": "Vīrā therī …."
}