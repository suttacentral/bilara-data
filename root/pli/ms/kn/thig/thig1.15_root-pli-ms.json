{
  "thig1.15:1.1": "Therīgāthā",
  "thig1.15:2.1": "Ekakanipāta",
  "thig1.15:3.1": "15. Uttarātherīgāthā",
  "thig1.15:4.1": "“Kāyena saṃvutā āsiṃ,",
  "thig1.15:4.2": "vācāya uda cetasā;",
  "thig1.15:4.3": "Samūlaṃ taṇhamabbuyha,",
  "thig1.15:4.4": "sītibhūtāmhi nibbutā”ti.",
  "thig1.15:5.1": "…",
  "thig1.15:5.2": "Uttarā therī …."
}