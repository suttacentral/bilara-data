{
  "thig13.2:1.1": "Therīgāthā",
  "thig13.2:2.1": "Vīsatinipāta",
  "thig13.2:3.1": "2 Rohinītherīgāthā",
  "thig13.2:4.1": "“‘Samaṇā’ti bhoti supi,",
  "thig13.2:4.2": "‘samaṇā’ti pabujjhasi;",
  "thig13.2:4.3": "Samaṇāneva kittesi,",
  "thig13.2:4.4": "samaṇī nūna bhavissasi.",
  "thig13.2:5.1": "Vipulaṃ annañca pānañca,",
  "thig13.2:5.2": "samaṇānaṃ paveccasi;",
  "thig13.2:5.3": "Rohinī dāni pucchāmi,",
  "thig13.2:5.4": "kena te samaṇā piyā.",
  "thig13.2:6.1": "Akammakāmā alasā,",
  "thig13.2:6.2": "paradattūpajīvino;",
  "thig13.2:6.3": "Āsaṃsukā sādukāmā,",
  "thig13.2:6.4": "kena te samaṇā piyā”.",
  "thig13.2:7.1": "“Cirassaṃ vata maṃ tāta,",
  "thig13.2:7.2": "samaṇānaṃ paripucchasi;",
  "thig13.2:7.3": "Tesaṃ te kittayissāmi,",
  "thig13.2:7.4": "paññāsīlaparakkamaṃ.",
  "thig13.2:8.1": "Kammakāmā analasā,",
  "thig13.2:8.2": "kammaseṭṭhassa kārakā;",
  "thig13.2:8.3": "Rāgaṃ dosaṃ pajahanti,",
  "thig13.2:8.4": "tena me samaṇā piyā.",
  "thig13.2:9.1": "Tīṇi pāpassa mūlāni,",
  "thig13.2:9.2": "dhunanti sucikārino;",
  "thig13.2:9.3": "Sabbaṃ pāpaṃ pahīnesaṃ,",
  "thig13.2:9.4": "tena me samaṇā piyā.",
  "thig13.2:10.1": "Kāyakammaṃ suci nesaṃ,",
  "thig13.2:10.2": "vacīkammañca tādisaṃ;",
  "thig13.2:10.3": "Manokammaṃ suci nesaṃ,",
  "thig13.2:10.4": "tena me samaṇā piyā.",
  "thig13.2:11.1": "Vimalā saṅkhamuttāva,",
  "thig13.2:11.2": "suddhā santarabāhirā;",
  "thig13.2:11.3": "Puṇṇā sukkāna dhammānaṃ,",
  "thig13.2:11.4": "tena me samaṇā piyā.",
  "thig13.2:12.1": "Bahussutā dhammadharā,",
  "thig13.2:12.2": "ariyā dhammajīvino;",
  "thig13.2:12.3": "Atthaṃ dhammañca desenti,",
  "thig13.2:12.4": "tena me samaṇā piyā.",
  "thig13.2:13.1": "Bahussutā dhammadharā,",
  "thig13.2:13.2": "ariyā dhammajīvino;",
  "thig13.2:13.3": "Ekaggacittā satimanto,",
  "thig13.2:13.4": "tena me samaṇā piyā.",
  "thig13.2:14.1": "Dūraṅgamā satimanto,",
  "thig13.2:14.2": "mantabhāṇī anuddhatā;",
  "thig13.2:14.3": "Dukkhassantaṃ pajānanti,",
  "thig13.2:14.4": "tena me samaṇā piyā.",
  "thig13.2:15.1": "Yasmā gāmā pakkamanti,",
  "thig13.2:15.2": "na vilokenti kiñcanaṃ;",
  "thig13.2:15.3": "Anapekkhāva gacchanti,",
  "thig13.2:15.4": "tena me samaṇā piyā.",
  "thig13.2:16.1": "Na te saṃ koṭṭhe openti,",
  "thig13.2:16.2": "na kumbhiṃ na khaḷopiyaṃ;",
  "thig13.2:16.3": "Pariniṭṭhitamesānā,",
  "thig13.2:16.4": "tena me samaṇā piyā.",
  "thig13.2:17.1": "Na te hiraññaṃ gaṇhanti,",
  "thig13.2:17.2": "na suvaṇṇaṃ na rūpiyaṃ;",
  "thig13.2:17.3": "Paccuppannena yāpenti,",
  "thig13.2:17.4": "tena me samaṇā piyā.",
  "thig13.2:18.1": "Nānākulā pabbajitā,",
  "thig13.2:18.2": "nānājanapadehi ca;",
  "thig13.2:18.3": "Aññamaññaṃ piyāyanti,",
  "thig13.2:18.4": "tena me samaṇā piyā”.",
  "thig13.2:19.1": "“Atthāya vata no bhoti,",
  "thig13.2:19.2": "kule jātāsi rohinī;",
  "thig13.2:19.3": "Saddhā buddhe ca dhamme ca,",
  "thig13.2:19.4": "saṃghe ca tibbagāravā.",
  "thig13.2:20.1": "Tuvaṃ hetaṃ pajānāsi,",
  "thig13.2:20.2": "puññakkhettaṃ anuttaraṃ;",
  "thig13.2:20.3": "Amhampi ete samaṇā,",
  "thig13.2:20.4": "paṭigaṇhanti dakkhiṇaṃ.",
  "thig13.2:21.1": "Patiṭṭhito hettha yañño,",
  "thig13.2:21.2": "vipulo no bhavissati”;",
  "thig13.2:21.3": "“Sace bhāyasi dukkhassa,",
  "thig13.2:21.4": "sace te dukkhamappiyaṃ.",
  "thig13.2:22.1": "Upehi saraṇaṃ buddhaṃ,",
  "thig13.2:22.2": "dhammaṃ saṃghañca tādinaṃ;",
  "thig13.2:22.3": "Samādiyāhi sīlāni,",
  "thig13.2:22.4": "taṃ te atthāya hehiti”.",
  "thig13.2:23.1": "“Upemi saraṇaṃ buddhaṃ,",
  "thig13.2:23.2": "dhammaṃ saṃghañca tādinaṃ;",
  "thig13.2:23.3": "Samādiyāmi sīlāni,",
  "thig13.2:23.4": "taṃ me atthāya hehiti.",
  "thig13.2:24.1": "Brahmabandhu pure āsiṃ,",
  "thig13.2:24.2": "so idānimhi brāhmaṇo;",
  "thig13.2:24.3": "Tevijjo sottiyo camhi,",
  "thig13.2:24.4": "vedagū camhi nhātako”.",
  "thig13.2:25.1": "…",
  "thig13.2:25.2": "Rohinī therī …."
}