{
  "thig13.1:1.1": "Therīgāthā",
  "thig13.1:2.1": "Vīsatinipāta",
  "thig13.1:3.1": "1. Ambapālītherīgāthā",
  "thig13.1:4.1": "“Kāḷakā bhamaravaṇṇasādisā,",
  "thig13.1:4.2": "Vellitaggā mama muddhajā ahuṃ;",
  "thig13.1:4.3": "Te jarāya sāṇavākasādisā,",
  "thig13.1:4.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:5.1": "Vāsitova surabhī karaṇḍako,",
  "thig13.1:5.2": "Pupphapūra mama uttamaṅgajo;",
  "thig13.1:5.3": "Taṃ jarāyatha salomagandhikaṃ,",
  "thig13.1:5.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:6.1": "Kānanaṃva sahitaṃ suropitaṃ,",
  "thig13.1:6.2": "Kocchasūcivicitaggasobhitaṃ;",
  "thig13.1:6.3": "Taṃ jarāya viralaṃ tahiṃ tahiṃ,",
  "thig13.1:6.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:7.1": "Kaṇhakhandhakasuvaṇṇamaṇḍitaṃ,",
  "thig13.1:7.2": "Sobhate suveṇīhilaṅkataṃ;",
  "thig13.1:7.3": "Taṃ jarāya khalitaṃ siraṃ kataṃ,",
  "thig13.1:7.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:8.1": "Cittakārasukatāva lekhikā,",
  "thig13.1:8.2": "Sobhare su bhamukā pure mama;",
  "thig13.1:8.3": "Tā jarāya valibhippalambitā,",
  "thig13.1:8.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:9.1": "Bhassarā surucirā yathā maṇī,",
  "thig13.1:9.2": "Nettahesumabhinīlamāyatā;",
  "thig13.1:9.3": "Te jarāyabhihatā na sobhare,",
  "thig13.1:9.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:10.1": "Saṇhatuṅgasadisī ca nāsikā,",
  "thig13.1:10.2": "Sobhate su abhiyobbanaṃ pati;",
  "thig13.1:10.3": "Sā jarāya upakūlitā viya,",
  "thig13.1:10.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:11.1": "Kaṅkaṇaṃva sukataṃ suniṭṭhitaṃ,",
  "thig13.1:11.2": "Sobhare su mama kaṇṇapāḷiyo;",
  "thig13.1:11.3": "Tā jarāya valibhippalambitā,",
  "thig13.1:11.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:12.1": "Pattalīmakulavaṇṇasādisā,",
  "thig13.1:12.2": "Sobhare su dantā pure mama;",
  "thig13.1:12.3": "Te jarāya khaṇḍitā cāsitā,",
  "thig13.1:12.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:13.1": "Kānanamhi vanasaṇḍacārinī,",
  "thig13.1:13.2": "Kokilāva madhuraṃ nikūjihaṃ;",
  "thig13.1:13.3": "Taṃ jarāya khalitaṃ tahiṃ tahiṃ,",
  "thig13.1:13.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:14.1": "Saṇhakamburiva suppamajjitā,",
  "thig13.1:14.2": "Sobhate su gīvā pure mama;",
  "thig13.1:14.3": "Sā jarāya bhaggā vināmitā,",
  "thig13.1:14.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:15.1": "Vaṭṭapalighasadisopamā ubho,",
  "thig13.1:15.2": "Sobhare su bāhā pure mama;",
  "thig13.1:15.3": "Tā jarāya yatha pāṭalibbalitā,",
  "thig13.1:15.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:16.1": "Saṇhamuddikasuvaṇṇamaṇḍitā,",
  "thig13.1:16.2": "Sobhare su hatthā pure mama;",
  "thig13.1:16.3": "Te jarāya yathā mūlamūlikā,",
  "thig13.1:16.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:17.1": "Pīnavaṭṭasahituggatā ubho,",
  "thig13.1:17.2": "Sobhare su thanakā pure mama;",
  "thig13.1:17.3": "Thevikīva lambanti nodakā,",
  "thig13.1:17.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:18.1": "Kañcanassa phalakaṃva sammaṭṭhaṃ,",
  "thig13.1:18.2": "Sobhate su kāyo pure mama;",
  "thig13.1:18.3": "So valīhi sukhumāhi otato,",
  "thig13.1:18.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:19.1": "Nāgabhogasadisopamā ubho,",
  "thig13.1:19.2": "Sobhare su ūrū pure mama;",
  "thig13.1:19.3": "Te jarāya yathā veḷunāḷiyo,",
  "thig13.1:19.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:20.1": "Saṇhanūpurasuvaṇṇamaṇḍitā,",
  "thig13.1:20.2": "Sobhare su jaṅghā pure mama;",
  "thig13.1:20.3": "Tā jarāya tiladaṇḍakāriva,",
  "thig13.1:20.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:21.1": "Tūlapuṇṇasadisopamā ubho,",
  "thig13.1:21.2": "Sobhare su pādā pure mama;",
  "thig13.1:21.3": "Te jarāya phuṭitā valīmatā,",
  "thig13.1:21.4": "Saccavādivacanaṃ anaññathā.",
  "thig13.1:22.1": "Ediso ahu ayaṃ samussayo,",
  "thig13.1:22.2": "Jajjaro bahudukkhānamālayo;",
  "thig13.1:22.3": "Sopalepapatito jarāgharo,",
  "thig13.1:22.4": "Saccavādivacanaṃ anaññathā”.",
  "thig13.1:23.1": "…",
  "thig13.1:23.2": "Ambapālī therī …."
}