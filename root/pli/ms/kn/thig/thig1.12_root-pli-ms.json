{
  "thig1.12:1.1": "Therīgāthā",
  "thig1.12:2.1": "Ekakanipāta",
  "thig1.12:3.1": "12. Dhammadinnātherīgāthā",
  "thig1.12:4.1": "“Chandajātā avasāyī,",
  "thig1.12:4.2": "Manasā ca phuṭā siyā;",
  "thig1.12:4.3": "Kāmesu appaṭibaddhacittā,",
  "thig1.12:4.4": "Uddhaṃsotāti vuccatī”ti.",
  "thig1.12:5.1": "…",
  "thig1.12:5.2": "Dhammadinnā therī …."
}