{
  "thig2.5:1.1": "Therīgāthā",
  "thig2.5:2.1": "Dukanipāta",
  "thig2.5:3.1": "5. Cittātherīgāthā",
  "thig2.5:4.1": "“Kiñcāpi khomhi kisikā,",
  "thig2.5:4.2": "gilānā bāḷhadubbalā;",
  "thig2.5:4.3": "Daṇḍamolubbha gacchāmi,",
  "thig2.5:4.4": "pabbataṃ abhirūhiya.",
  "thig2.5:5.1": "Saṅghāṭiṃ nikkhipitvāna,",
  "thig2.5:5.2": "pattakañca nikujjiya;",
  "thig2.5:5.3": "Sele khambhesimattānaṃ,",
  "thig2.5:5.4": "tamokhandhaṃ padāliyā”ti.",
  "thig2.5:6.1": "…",
  "thig2.5:6.2": "Cittā therī …."
}