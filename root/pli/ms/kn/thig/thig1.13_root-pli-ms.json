{
  "thig1.13:1.1": "Therīgāthā",
  "thig1.13:2.1": "Ekakanipāta",
  "thig1.13:3.1": "13. Visākhātherīgāthā",
  "thig1.13:4.1": "“Karotha buddhasāsanaṃ,",
  "thig1.13:4.2": "yaṃ katvā nānutappati;",
  "thig1.13:4.3": "Khippaṃ pādāni dhovitvā,",
  "thig1.13:4.4": "ekamante nisīdathā”ti.",
  "thig1.13:5.1": "…",
  "thig1.13:5.2": "Visākhā therī …."
}