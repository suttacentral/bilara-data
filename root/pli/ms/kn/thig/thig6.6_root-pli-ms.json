{
  "thig6.6:1.1": "Therīgāthā",
  "thig6.6:2.1": "Chakkanipāta",
  "thig6.6:3.1": "6 Mahāpajāpatigotamītherīgāthā",
  "thig6.6:4.1": "“Buddha vīra namo tyatthu,",
  "thig6.6:4.2": "sabbasattānamuttama;",
  "thig6.6:4.3": "Yo maṃ dukkhā pamocesi,",
  "thig6.6:4.4": "aññañca bahukaṃ janaṃ.",
  "thig6.6:5.1": "Sabbadukkhaṃ pariññātaṃ,",
  "thig6.6:5.2": "Hetutaṇhā visositā;",
  "thig6.6:5.3": "Bhāvito aṭṭhaṅgiko maggo,",
  "thig6.6:5.4": "Nirodho phusito mayā.",
  "thig6.6:6.1": "Mātā putto pitā bhātā,",
  "thig6.6:6.2": "ayyakā ca pure ahuṃ;",
  "thig6.6:6.3": "Yathābhuccamajānantī,",
  "thig6.6:6.4": "saṃsariṃhaṃ anibbisaṃ.",
  "thig6.6:7.1": "Diṭṭho hi me so bhagavā,",
  "thig6.6:7.2": "antimoyaṃ samussayo;",
  "thig6.6:7.3": "Vikkhīṇo jātisaṃsāro,",
  "thig6.6:7.4": "natthi dāni punabbhavo.",
  "thig6.6:8.1": "Āraddhavīriye pahitatte,",
  "thig6.6:8.2": "Niccaṃ daḷhaparakkame;",
  "thig6.6:8.3": "Samagge sāvake passe,",
  "thig6.6:8.4": "Esā buddhāna vandanā.",
  "thig6.6:9.1": "Bahūnaṃ vata atthāya,",
  "thig6.6:9.2": "Māyā janayi gotamaṃ;",
  "thig6.6:9.3": "Byādhimaraṇatunnānaṃ,",
  "thig6.6:9.4": "Dukkhakkhandhaṃ byapānudī”ti.",
  "thig6.6:10.1": "…",
  "thig6.6:10.2": "Mahāpajāpatigotamī therī …."
}