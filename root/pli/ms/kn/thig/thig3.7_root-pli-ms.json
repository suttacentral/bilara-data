{
  "thig3.7:1.1": "Therīgāthā",
  "thig3.7:2.1": "Tikanipāta",
  "thig3.7:3.1": "7. Selātherīgāthā",
  "thig3.7:4.1": "“Natthi nissaraṇaṃ loke,",
  "thig3.7:4.2": "kiṃ vivekena kāhasi;",
  "thig3.7:4.3": "Bhuñjāhi kāmaratiyo,",
  "thig3.7:4.4": "māhu pacchānutāpinī”.",
  "thig3.7:5.1": "“Sattisūlūpamā kāmā,",
  "thig3.7:5.2": "khandhāsaṃ adhikuṭṭanā;",
  "thig3.7:5.3": "Yaṃ tvaṃ ‘kāmaratiṃ’ brūsi,",
  "thig3.7:5.4": "‘aratī’ dāni sā mama.",
  "thig3.7:6.1": "Sabbattha vihatā nandī,",
  "thig3.7:6.2": "Tamokhandho padālito;",
  "thig3.7:6.3": "Evaṃ jānāhi pāpima,",
  "thig3.7:6.4": "Nihato tvamasi antakā”ti.",
  "thig3.7:7.1": "…",
  "thig3.7:7.2": "Selā therī …."
}