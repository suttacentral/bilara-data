{
  "thig3.5:1.1": "Therīgāthā",
  "thig3.5:2.1": "Tikanipāta",
  "thig3.5:3.1": "5. Ubbiritherīgāthā",
  "thig3.5:4.1": "“Amma jīvāti vanamhi kandasi,",
  "thig3.5:4.2": "Attānaṃ adhigaccha ubbiri;",
  "thig3.5:4.3": "Cullāsītisahassāni,",
  "thig3.5:4.4": "Sabbā jīvasanāmikā;",
  "thig3.5:4.5": "Etamhāḷāhane daḍḍhā,",
  "thig3.5:4.6": "Tāsaṃ kamanusocasi”.",
  "thig3.5:5.1": "“Abbahī vata me sallaṃ,",
  "thig3.5:5.2": "duddasaṃ hadayassitaṃ;",
  "thig3.5:5.3": "Yaṃ me sokaparetāya,",
  "thig3.5:5.4": "dhītusokaṃ byapānudi.",
  "thig3.5:6.1": "Sājja abbūḷhasallāhaṃ,",
  "thig3.5:6.2": "Nicchātā parinibbutā;",
  "thig3.5:6.3": "Buddhaṃ dhammañca saṃghañca,",
  "thig3.5:6.4": "Upemi saraṇaṃ munin”ti.",
  "thig3.5:7.1": "…",
  "thig3.5:7.2": "Ubbirī therī …."
}