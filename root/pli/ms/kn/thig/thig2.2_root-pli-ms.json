{
  "thig2.2:1.1": "Therīgāthā",
  "thig2.2:2.1": "Dukanipāta",
  "thig2.2:3.1": "2. Jentātherīgāthā",
  "thig2.2:4.1": "“Ye ime satta bojjhaṅgā,",
  "thig2.2:4.2": "maggā nibbānapattiyā;",
  "thig2.2:4.3": "Bhāvitā te mayā sabbe,",
  "thig2.2:4.4": "yathā buddhena desitā.",
  "thig2.2:5.1": "Diṭṭho hi me so bhagavā,",
  "thig2.2:5.2": "antimoyaṃ samussayo;",
  "thig2.2:5.3": "Vikkhīṇo jātisaṃsāro,",
  "thig2.2:5.4": "natthi dāni punabbhavo”ti.",
  "thig2.2:6.1": "Itthaṃ sudaṃ jentā therī gāthāyo abhāsitthāti."
}