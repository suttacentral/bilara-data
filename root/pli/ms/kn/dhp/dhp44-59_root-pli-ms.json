{
  "dhp44:^0.1": "Khuddakanikāya",
  "dhp44:^0.2": "Dhammapada",
  "dhp44:^0.3": "Pupphavagga",
  "dhp44:0": "Pathavikathāpasutapañcasatabhikkhuvatthu",
  "dhp44:1": "Ko imaṃ pathaviṃ vicessati,",
  "dhp44:2": "Yamalokañca imaṃ sadevakaṃ;",
  "dhp44:3": "Ko dhammapadaṃ sudesitaṃ,",
  "dhp44:4": "Kusalo pupphamiva pacessati.",
  "dhp45:1": "Sekho pathaviṃ vicessati,",
  "dhp45:2": "Yamalokañca imaṃ sadevakaṃ;",
  "dhp45:3": "Sekho dhammapadaṃ sudesitaṃ,",
  "dhp45:4": "Kusalo pupphamiva pacessati.",
  "dhp46:0": "Marīcikammaṭṭhānikabhikkhuvatthu",
  "dhp46:1": "Pheṇūpamaṃ kāyamimaṃ viditvā,",
  "dhp46:2": "Marīcidhammaṃ abhisambudhāno;",
  "dhp46:3": "Chetvāna mārassa papupphakāni,",
  "dhp46:4": "Adassanaṃ maccurājassa gacche.",
  "dhp47:0": "Viṭaṭūbhavatthu",
  "dhp47:1": "Pupphāni heva pacinantaṃ,",
  "dhp47:2": "byāsattamanasaṃ naraṃ;",
  "dhp47:3": "Suttaṃ gāmaṃ mahoghova,",
  "dhp47:4": "maccu ādāya gacchati.",
  "dhp48:0": "Patipūjikakumārivatthu",
  "dhp48:1": "Pupphāni heva pacinantaṃ,",
  "dhp48:2": "byāsattamanasaṃ naraṃ;",
  "dhp48:3": "Atittaññeva kāmesu,",
  "dhp48:4": "antako kurute vasaṃ.",
  "dhp49:0": "Macchariyakosiyaseṭṭhivatthu",
  "dhp49:1": "Yathāpi bhamaro pupphaṃ,",
  "dhp49:2": "vaṇṇagandhamaheṭhayaṃ;",
  "dhp49:3": "Paleti rasamādāya,",
  "dhp49:4": "evaṃ gāme munī care.",
  "dhp50:0": "Pāveyyaājīvakavatthu",
  "dhp50:1": "Na paresaṃ vilomāni,",
  "dhp50:2": "na paresaṃ katākataṃ;",
  "dhp50:3": "Attanova avekkheyya,",
  "dhp50:4": "katāni akatāni ca.",
  "dhp51:0": "Chattapāṇiupāsakavatthu",
  "dhp51:1": "Yathāpi ruciraṃ pupphaṃ,",
  "dhp51:2": "vaṇṇavantaṃ agandhakaṃ;",
  "dhp51:3": "Evaṃ subhāsitā vācā,",
  "dhp51:4": "aphalā hoti akubbato.",
  "dhp52:1": "Yathāpi ruciraṃ pupphaṃ,",
  "dhp52:2": "Vaṇṇavantaṃ sagandhakaṃ;",
  "dhp52:3": "Evaṃ subhāsitā vācā,",
  "dhp52:4": "Saphalā hoti kubbato.",
  "dhp53:0": "Visākhāvatthu",
  "dhp53:1": "Yathāpi puppharāsimhā,",
  "dhp53:2": "kayirā mālāguṇe bahū;",
  "dhp53:3": "Evaṃ jātena maccena,",
  "dhp53:4": "kattabbaṃ kusalaṃ bahuṃ.",
  "dhp54:0": "Ānandattherapañhavatthu",
  "dhp54:1": "Na pupphagandho paṭivātameti,",
  "dhp54:2": "Na candanaṃ tagaramallikā vā;",
  "dhp54:3": "Satañca gandho paṭivātameti,",
  "dhp54:4": "Sabbā disā sappuriso pavāyati.",
  "dhp55:1": "Candanaṃ tagaraṃ vāpi,",
  "dhp55:2": "uppalaṃ atha vassikī;",
  "dhp55:3": "Etesaṃ gandhajātānaṃ,",
  "dhp55:4": "sīlagandho anuttaro.",
  "dhp56:0": "Mahākassapattherapiṇḍapātadinnavatthu",
  "dhp56:1": "Appamatto ayaṃ gandho,",
  "dhp56:2": "yāyaṃ tagaracandanī;",
  "dhp56:3": "Yo ca sīlavataṃ gandho,",
  "dhp56:4": "vāti devesu uttamo.",
  "dhp57:0": "Godhikattheraparinibbānavatthu",
  "dhp57:1": "Tesaṃ sampannasīlānaṃ,",
  "dhp57:2": "appamādavihārinaṃ;",
  "dhp57:3": "Sammadaññāvimuttānaṃ,",
  "dhp57:4": "māro maggaṃ na vindati.",
  "dhp58:0": "Garahadinnavatthu",
  "dhp58:1": "Yathā saṅkāradhānasmiṃ,",
  "dhp58:2": "ujjhitasmiṃ mahāpathe;",
  "dhp58:3": "Padumaṃ tattha jāyetha,",
  "dhp58:4": "sucigandhaṃ manoramaṃ.",
  "dhp59:1": "Evaṃ saṅkārabhūtesu,",
  "dhp59:2": "andhabhūte puthujjane;",
  "dhp59:3": "Atirocati paññāya,",
  "dhp59:4": "sammāsambuddhasāvako.",
  "dhp59:5": "Pupphavaggo catuttho."
}