{
  "dhp146:^0.1": "Khuddakanikāya",
  "dhp146:^0.2": "Dhammapada",
  "dhp146:^0.3": "Jarāvagga",
  "dhp146:0": "Visākhāyasahāyikānaṃvatthu",
  "dhp146:1": "Ko nu hāso kimānando,",
  "dhp146:2": "niccaṃ pajjalite sati;",
  "dhp146:3": "Andhakārena onaddhā,",
  "dhp146:4": "padīpaṃ na gavesatha.",
  "dhp147:0": "Sirimāvatthu",
  "dhp147:1": "Passa cittakataṃ bimbaṃ,",
  "dhp147:2": "arukāyaṃ samussitaṃ;",
  "dhp147:3": "Āturaṃ bahusaṅkappaṃ,",
  "dhp147:4": "yassa natthi dhuvaṃ ṭhiti.",
  "dhp148:0": "Uttarātherīvatthu",
  "dhp148:1": "Parijiṇṇamidaṃ rūpaṃ,",
  "dhp148:2": "roganīḷaṃ pabhaṅguraṃ;",
  "dhp148:3": "Bhijjati pūtisandeho,",
  "dhp148:4": "maraṇantañhi jīvitaṃ.",
  "dhp149:0": "Sambahulaadhimānikabhikkhuvatthu",
  "dhp149:1": "Yānimāni apatthāni,",
  "dhp149:2": "alābūneva sārade;",
  "dhp149:3": "Kāpotakāni aṭṭhīni,",
  "dhp149:4": "tāni disvāna kā rati.",
  "dhp150:0": "Janapadakalyāṇīrūpanandātherīvatthu",
  "dhp150:1": "Aṭṭhīnaṃ nagaraṃ kataṃ,",
  "dhp150:2": "maṃsalohitalepanaṃ;",
  "dhp150:3": "Yattha jarā ca maccu ca,",
  "dhp150:4": "māno makkho ca ohito.",
  "dhp151:0": "Mallikādevīvatthu",
  "dhp151:1": "Jīranti ve rājarathā sucittā,",
  "dhp151:2": "Atho sarīrampi jaraṃ upeti;",
  "dhp151:3": "Satañca dhammo na jaraṃ upeti,",
  "dhp151:4": "Santo have sabbhi pavedayanti.",
  "dhp152:0": "Lāḷudāyītheravatthu",
  "dhp152:1": "Appassutāyaṃ puriso,",
  "dhp152:2": "balībaddhova jīrati;",
  "dhp152:3": "Maṃsāni tassa vaḍḍhanti,",
  "dhp152:4": "paññā tassa na vaḍḍhati.",
  "dhp153:0": "Udānavatthu",
  "dhp153:1": "Anekajātisaṃsāraṃ,",
  "dhp153:2": "sandhāvissaṃ anibbisaṃ;",
  "dhp153:3": "Gahakāraṃ gavesanto,",
  "dhp153:4": "dukkhā jāti punappunaṃ.",
  "dhp154:1": "Gahakāraka diṭṭhosi,",
  "dhp154:2": "puna gehaṃ na kāhasi;",
  "dhp154:3": "Sabbā te phāsukā bhaggā,",
  "dhp154:4": "gahakūṭaṃ visaṅkhataṃ;",
  "dhp154:5": "Visaṅkhāragataṃ cittaṃ,",
  "dhp154:6": "taṇhānaṃ khayamajjhagā.",
  "dhp155:0": "Mahādhanaseṭṭhiputtavatthu",
  "dhp155:1": "Acaritvā brahmacariyaṃ,",
  "dhp155:2": "aladdhā yobbane dhanaṃ;",
  "dhp155:3": "Jiṇṇakoñcāva jhāyanti,",
  "dhp155:4": "khīṇamaccheva pallale.",
  "dhp156:1": "Acaritvā brahmacariyaṃ,",
  "dhp156:2": "aladdhā yobbane dhanaṃ;",
  "dhp156:3": "Senti cāpātikhīṇāva,",
  "dhp156:4": "purāṇāni anutthunaṃ.",
  "dhp156:5": "Jarāvaggo ekādasamo."
}