{
  "dhp290:^0.1": "Khuddakanikāya",
  "dhp290:^0.2": "Dhammapada",
  "dhp290:^0.3": "Pakiṇṇakavagga",
  "dhp290:0": "Attanopubbakammavatthu",
  "dhp290:1": "Mattāsukhapariccāgā,",
  "dhp290:2": "passe ce vipulaṃ sukhaṃ;",
  "dhp290:3": "Caje mattāsukhaṃ dhīro,",
  "dhp290:4": "sampassaṃ vipulaṃ sukhaṃ.",
  "dhp291:0": "Kukkuṭaṇḍakhādikāvatthu",
  "dhp291:1": "Paradukkhūpadhānena,",
  "dhp291:2": "attano sukhamicchati;",
  "dhp291:3": "Verasaṃsaggasaṃsaṭṭho,",
  "dhp291:4": "verā so na parimuccati.",
  "dhp292:0": "Bhaddiyānaṃbhikkhūnaṃvatthu",
  "dhp292:1": "Yañhi kiccaṃ apaviddhaṃ,",
  "dhp292:2": "akiccaṃ pana karīyati;",
  "dhp292:3": "Unnaḷānaṃ pamattānaṃ,",
  "dhp292:4": "tesaṃ vaḍḍhanti āsavā.",
  "dhp293:1": "Yesañca susamāraddhā,",
  "dhp293:2": "niccaṃ kāyagatā sati;",
  "dhp293:3": "Akiccaṃ te na sevanti,",
  "dhp293:4": "kicce sātaccakārino;",
  "dhp293:5": "Satānaṃ sampajānānaṃ,",
  "dhp293:6": "atthaṃ gacchanti āsavā.",
  "dhp294:0": "Lakuṇḍakabhaddiyavatthu",
  "dhp294:1": "Mātaraṃ pitaraṃ hantvā,",
  "dhp294:2": "rājāno dve ca khattiye;",
  "dhp294:3": "Raṭṭhaṃ sānucaraṃ hantvā,",
  "dhp294:4": "anīgho yāti brāhmaṇo.",
  "dhp295:1": "Mātaraṃ pitaraṃ hantvā,",
  "dhp295:2": "rājāno dve ca sotthiye;",
  "dhp295:3": "Veyagghapañcamaṃ hantvā,",
  "dhp295:4": "anīgho yāti brāhmaṇo.",
  "dhp296:0": "Dārusākaṭikaputtavatthu",
  "dhp296:1": "Suppabuddhaṃ pabujjhanti,",
  "dhp296:2": "sadā gotamasāvakā;",
  "dhp296:3": "Yesaṃ divā ca ratto ca,",
  "dhp296:4": "niccaṃ buddhagatā sati.",
  "dhp297:1": "Suppabuddhaṃ pabujjhanti,",
  "dhp297:2": "sadā gotamasāvakā;",
  "dhp297:3": "Yesaṃ divā ca ratto ca,",
  "dhp297:4": "niccaṃ dhammagatā sati.",
  "dhp298:1": "Suppabuddhaṃ pabujjhanti,",
  "dhp298:2": "sadā gotamasāvakā;",
  "dhp298:3": "Yesaṃ divā ca ratto ca,",
  "dhp298:4": "niccaṃ saṅghagatā sati.",
  "dhp299:1": "Suppabuddhaṃ pabujjhanti,",
  "dhp299:2": "sadā gotamasāvakā;",
  "dhp299:3": "Yesaṃ divā ca ratto ca,",
  "dhp299:4": "niccaṃ kāyagatā sati.",
  "dhp300:1": "Suppabuddhaṃ pabujjhanti,",
  "dhp300:2": "sadā gotamasāvakā;",
  "dhp300:3": "Yesaṃ divā ca ratto ca,",
  "dhp300:4": "ahiṃsāya rato mano.",
  "dhp301:1": "Suppabuddhaṃ pabujjhanti,",
  "dhp301:2": "sadā gotamasāvakā;",
  "dhp301:3": "Yesaṃ divā ca ratto ca,",
  "dhp301:4": "bhāvanāya rato mano.",
  "dhp302:0": "Vajjiputtakabhikkhuvatthu",
  "dhp302:1": "Duppabbajjaṃ durabhiramaṃ,",
  "dhp302:2": "Durāvāsā gharā dukhā;",
  "dhp302:3": "Dukkhosamānasaṃvāso,",
  "dhp302:4": "Dukkhānupatitaddhagū;",
  "dhp302:5": "Tasmā na caddhagū siyā,",
  "dhp302:6": "Na ca dukkhānupatito siyā.",
  "dhp303:0": "Cittagahapativatthu",
  "dhp303:1": "Saddho sīlena sampanno,",
  "dhp303:2": "yasobhogasamappito;",
  "dhp303:3": "Yaṃ yaṃ padesaṃ bhajati,",
  "dhp303:4": "tattha tattheva pūjito.",
  "dhp304:0": "Cūḷasubhaddāvatthu",
  "dhp304:1": "Dūre santo pakāsenti,",
  "dhp304:2": "himavantova pabbato;",
  "dhp304:3": "Asantettha na dissanti,",
  "dhp304:4": "rattiṃ khittā yathā sarā.",
  "dhp305:0": "Ekavihārittheravatthu",
  "dhp305:1": "Ekāsanaṃ ekaseyyaṃ,",
  "dhp305:2": "eko caramatandito;",
  "dhp305:3": "Eko damayamattānaṃ,",
  "dhp305:4": "vanante ramito siyā.",
  "dhp305:5": "Pakiṇṇakavaggo ekavīsatimo."
}