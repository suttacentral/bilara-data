{
  "dhp100:^0.1": "Khuddakanikāya",
  "dhp100:^0.2": "Dhammapada",
  "dhp100:^0.3": "Sahassavagga",
  "dhp100:0": "Tambadāṭhikacoraghātakavatthu",
  "dhp100:1": "Sahassamapi ce vācā,",
  "dhp100:2": "anatthapadasaṃhitā;",
  "dhp100:3": "Ekaṃ atthapadaṃ seyyo,",
  "dhp100:4": "yaṃ sutvā upasammati.",
  "dhp101:0": "Bāhiyadārucīriyattheravatthu",
  "dhp101:1": "Sahassamapi ce gāthā,",
  "dhp101:2": "anatthapadasaṃhitā;",
  "dhp101:3": "Ekaṃ gāthāpadaṃ seyyo,",
  "dhp101:4": "yaṃ sutvā upasammati.",
  "dhp102:0": "Kuṇḍalakesitherīvatthu",
  "dhp102:1": "Yo ca gāthā sataṃ bhāse,",
  "dhp102:2": "anatthapadasaṃhitā;",
  "dhp102:3": "Ekaṃ dhammapadaṃ seyyo,",
  "dhp102:4": "yaṃ sutvā upasammati.",
  "dhp103:1": "Yo sahassaṃ sahassena,",
  "dhp103:2": "saṅgāme mānuse jine;",
  "dhp103:3": "Ekañca jeyyamattānaṃ,",
  "dhp103:4": "sa ve saṅgāmajuttamo.",
  "dhp104:0": "Anatthapucchakabrāhmaṇavatthu",
  "dhp104:1": "Attā have jitaṃ seyyo,",
  "dhp104:2": "yā cāyaṃ itarā pajā;",
  "dhp104:3": "Attadantassa posassa,",
  "dhp104:4": "niccaṃ saññatacārino.",
  "dhp105:1": "Neva devo na gandhabbo,",
  "dhp105:2": "na māro saha brahmunā;",
  "dhp105:3": "Jitaṃ apajitaṃ kayirā,",
  "dhp105:4": "tathārūpassa jantuno.",
  "dhp106:0": "Sāriputtattherassa mātulabrāhmaṇavatthu",
  "dhp106:1": "Māse māse sahassena,",
  "dhp106:2": "yo yajetha sataṃ samaṃ;",
  "dhp106:3": "Ekañca bhāvitattānaṃ,",
  "dhp106:4": "muhuttamapi pūjaye;",
  "dhp106:5": "Sāyeva pūjanā seyyo,",
  "dhp106:6": "yañce vassasataṃ hutaṃ.",
  "dhp107:0": "Sāriputtattherassa bhāgineyyavatthu",
  "dhp107:1": "Yo ca vassasataṃ jantu,",
  "dhp107:2": "aggiṃ paricare vane;",
  "dhp107:3": "Ekañca bhāvitattānaṃ,",
  "dhp107:4": "muhuttamapi pūjaye;",
  "dhp107:5": "Sāyeva pūjanā seyyo,",
  "dhp107:6": "yañce vassasataṃ hutaṃ.",
  "dhp108:0": "Sāriputtatherassa sahāyakabrāhmaṇavatthu",
  "dhp108:1": "Yaṃ kiñci yiṭṭhaṃ va hutaṃ va loke,",
  "dhp108:2": "Saṃvaccharaṃ yajetha puññapekkho;",
  "dhp108:3": "Sabbampi taṃ na catubhāgameti,",
  "dhp108:4": "Abhivādanā ujjugatesu seyyo.",
  "dhp109:0": "Āyuvaḍḍhanakumāravatthu",
  "dhp109:1": "Abhivādanasīlissa,",
  "dhp109:2": "niccaṃ vuḍḍhāpacāyino;",
  "dhp109:3": "Cattāro dhammā vaḍḍhanti,",
  "dhp109:4": "āyu vaṇṇo sukhaṃ balaṃ.",
  "dhp110:0": "Saṅkiccasāmaṇeravatthu",
  "dhp110:1": "Yo ca vassasataṃ jīve,",
  "dhp110:2": "dussīlo asamāhito;",
  "dhp110:3": "Ekāhaṃ jīvitaṃ seyyo,",
  "dhp110:4": "sīlavantassa jhāyino.",
  "dhp111:0": "Khāṇukoṇḍaññattheravatthu",
  "dhp111:1": "Yo ca vassasataṃ jīve,",
  "dhp111:2": "duppañño asamāhito;",
  "dhp111:3": "Ekāhaṃ jīvitaṃ seyyo,",
  "dhp111:4": "paññavantassa jhāyino.",
  "dhp112:0": "Sabbadāsattheravatthu",
  "dhp112:1": "Yo ca vassasataṃ jīve,",
  "dhp112:2": "kusīto hīnavīriyo;",
  "dhp112:3": "Ekāhaṃ jīvitaṃ seyyo,",
  "dhp112:4": "vīriyamārabhato daḷhaṃ.",
  "dhp113:0": "Paṭācārātherīvatthu",
  "dhp113:1": "Yo ca vassasataṃ jīve,",
  "dhp113:2": "apassaṃ udayabbayaṃ;",
  "dhp113:3": "Ekāhaṃ jīvitaṃ seyyo,",
  "dhp113:4": "passato udayabbayaṃ.",
  "dhp114:0": "Kisāgotamīvatthu",
  "dhp114:1": "Yo ca vassasataṃ jīve,",
  "dhp114:2": "apassaṃ amataṃ padaṃ;",
  "dhp114:3": "Ekāhaṃ jīvitaṃ seyyo,",
  "dhp114:4": "passato amataṃ padaṃ.",
  "dhp115:0": "Bahuputtikattherīvatthu",
  "dhp115:1": "Yo ca vassasataṃ jīve,",
  "dhp115:2": "apassaṃ dhammamuttamaṃ;",
  "dhp115:3": "Ekāhaṃ jīvitaṃ seyyo,",
  "dhp115:4": "passato dhammamuttamaṃ.",
  "dhp115:5": "Sahassavaggo aṭṭhamo."
}