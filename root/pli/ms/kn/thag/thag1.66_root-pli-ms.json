{
  "thag1.66:1.1": "Theragāthā",
  "thag1.66:2.1": "Ekakanipāta",
  "thag1.66:3.1": "Sattamavagga",
  "thag1.66:4.1": "1.66. Meghiyattheragāthā",
  "thag1.66:5.1": "“Anusāsi mahāvīro,",
  "thag1.66:5.2": "sabbadhammāna pāragū;",
  "thag1.66:5.3": "Tassāhaṃ dhammaṃ sutvāna,",
  "thag1.66:5.4": "vihāsiṃ santike sato;",
  "thag1.66:5.5": "Tisso vijjā anuppattā,",
  "thag1.66:5.6": "kataṃ buddhassa sāsanan”ti.",
  "thag1.66:6.1": "…",
  "thag1.66:6.2": "Meghiyo thero …."
}