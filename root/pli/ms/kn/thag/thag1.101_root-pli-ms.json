{
  "thag1.101:1.1": "Theragāthā",
  "thag1.101:2.1": "Ekakanipāta",
  "thag1.101:3.1": "Ekādasamavagga",
  "thag1.101:4.1": "1.101. Belaṭṭhānikattheragāthā",
  "thag1.101:5.1": "“Hitvā gihittaṃ anavositatto,",
  "thag1.101:5.2": "Mukhanaṅgalī odariko kusīto;",
  "thag1.101:5.3": "Mahāvarāhova nivāpapuṭṭho,",
  "thag1.101:5.4": "Punappunaṃ gabbhamupeti mando”ti.",
  "thag1.101:6.1": "…",
  "thag1.101:6.2": "Belaṭṭhāniko thero …."
}