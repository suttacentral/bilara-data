{
  "thag1.97:1.1": "Theragāthā",
  "thag1.97:2.1": "Ekakanipāta",
  "thag1.97:3.1": "Dasamavagga",
  "thag1.97:4.1": "1.97. Tissattheragāthā",
  "thag1.97:5.1": "“Hitvā satapalaṃ kaṃsaṃ,",
  "thag1.97:5.2": "Sovaṇṇaṃ satarājikaṃ;",
  "thag1.97:5.3": "Aggahiṃ mattikāpattaṃ,",
  "thag1.97:5.4": "Idaṃ dutiyābhisecanan”ti.",
  "thag1.97:6.1": "…",
  "thag1.97:6.2": "Tisso thero …."
}