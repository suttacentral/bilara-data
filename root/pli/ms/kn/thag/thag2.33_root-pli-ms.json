{
  "thag2.33:1.1": "Theragāthā",
  "thag2.33:2.1": "Dukanipāta",
  "thag2.33:3.1": "Catutthavagga",
  "thag2.33:4.1": "2.33 Upavāṇattheragāthā",
  "thag2.33:5.1": "“Arahaṃ sugato loke,",
  "thag2.33:5.2": "vātehābādhiko muni;",
  "thag2.33:5.3": "Sace uṇhodakaṃ atthi,",
  "thag2.33:5.4": "munino dehi brāhmaṇa.",
  "thag2.33:6.1": "Pūjito pūjaneyyānaṃ,",
  "thag2.33:6.2": "sakkareyyāna sakkato;",
  "thag2.33:6.3": "Apacitopaceyyānaṃ,",
  "thag2.33:6.4": "tassa icchāmi hātave”ti.",
  "thag2.33:7.1": "…",
  "thag2.33:7.2": "Upavāṇo thero …."
}