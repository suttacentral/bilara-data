{
  "thag1.82:1.1": "Theragāthā",
  "thag1.82:2.1": "Ekakanipāta",
  "thag1.82:3.1": "Navamavagga",
  "thag1.82:4.1": "1.82. Kassapattheragāthā",
  "thag1.82:5.1": "“Yena yena subhikkhāni,",
  "thag1.82:5.2": "sivāni abhayāni ca;",
  "thag1.82:5.3": "Tena puttaka gacchassu,",
  "thag1.82:5.4": "mā sokāpahato bhavā”ti.",
  "thag1.82:6.1": "…",
  "thag1.82:6.2": "Kassapo thero …."
}