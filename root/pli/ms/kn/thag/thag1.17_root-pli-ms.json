{
  "thag1.17:1.1": "Theragāthā",
  "thag1.17:2.1": "Ekakanipāta",
  "thag1.17:3.1": "Dutiyavagga",
  "thag1.17:4.1": "1.17. Dāsakattheragāthā",
  "thag1.17:5.1": "“Middhī yadā hoti mahagghaso ca,",
  "thag1.17:5.2": "Niddāyitā samparivattasāyī;",
  "thag1.17:5.3": "Mahāvarāhova nivāpapuṭṭho,",
  "thag1.17:5.4": "Punappunaṃ gabbhamupeti mando”ti.",
  "thag1.17:6.1": "…",
  "thag1.17:6.2": "Dāsako thero …."
}