{
  "thag1.29:1.1": "Theragāthā",
  "thag1.29:2.1": "Ekakanipāta",
  "thag1.29:3.1": "Tatiyavagga",
  "thag1.29:4.1": "1.29. Hāritattheragāthā",
  "thag1.29:5.1": "“Samunnamayamattānaṃ,",
  "thag1.29:5.2": "usukārova tejanaṃ;",
  "thag1.29:5.3": "Cittaṃ ujuṃ karitvāna,",
  "thag1.29:5.4": "avijjaṃ bhinda hāritā”ti.",
  "thag1.29:6.1": "…",
  "thag1.29:6.2": "Hārito thero …."
}