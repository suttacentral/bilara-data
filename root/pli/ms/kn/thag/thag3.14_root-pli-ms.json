{
  "thag3.14:1.1": "Theragāthā",
  "thag3.14:2.1": "Tikanipāta",
  "thag3.14:3.1": "Paṭhamavagga",
  "thag3.14:4.1": "3.14. Gotamattheragāthā",
  "thag3.14:5.1": "“Saṃsaraṃ hi nirayaṃ agacchissaṃ,",
  "thag3.14:5.2": "Petalokamagamaṃ punappunaṃ;",
  "thag3.14:5.3": "Dukkhamamhipi tiracchānayoniyaṃ,",
  "thag3.14:5.4": "Nekadhā hi vusitaṃ ciraṃ mayā.",
  "thag3.14:6.1": "Mānusopi ca bhavobhirādhito,",
  "thag3.14:6.2": "Saggakāyamagamaṃ sakiṃ sakiṃ;",
  "thag3.14:6.3": "Rūpadhātusu arūpadhātusu,",
  "thag3.14:6.4": "Nevasaññisu asaññisuṭṭhitaṃ.",
  "thag3.14:7.1": "Sambhavā suviditā asārakā,",
  "thag3.14:7.2": "Saṅkhatā pacalitā saderitā;",
  "thag3.14:7.3": "Taṃ viditvā mahamattasambhavaṃ,",
  "thag3.14:7.4": "Santimeva satimā samajjhagan”ti.",
  "thag3.14:8.1": "…",
  "thag3.14:8.2": "Gotamo thero …."
}