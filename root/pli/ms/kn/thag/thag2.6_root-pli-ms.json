{
  "thag2.6:1.1": "Theragāthā",
  "thag2.6:2.1": "Dukanipāta",
  "thag2.6:3.1": "Paṭhamavagga",
  "thag2.6:4.1": "2.6 Meḷajinattheragāthā",
  "thag2.6:5.1": "“Yadāhaṃ dhammamassosiṃ,",
  "thag2.6:5.2": "bhāsamānassa satthuno;",
  "thag2.6:5.3": "Na kaṅkhamabhijānāmi,",
  "thag2.6:5.4": "sabbaññūaparājite.",
  "thag2.6:6.1": "Satthavāhe mahāvīre,",
  "thag2.6:6.2": "sārathīnaṃ varuttame;",
  "thag2.6:6.3": "Magge paṭipadāyaṃ vā,",
  "thag2.6:6.4": "kaṅkhā mayhaṃ na vijjatī”ti.",
  "thag2.6:7.1": "…",
  "thag2.6:7.2": "Meḷajino thero …."
}