{
  "thag2.21:1.1": "Theragāthā",
  "thag2.21:2.1": "Dukanipāta",
  "thag2.21:3.1": "Tatiyavagga",
  "thag2.21:4.1": "2.21. Uttarattheragāthā",
  "thag2.21:5.1": "“Khandhā mayā pariññātā,",
  "thag2.21:5.2": "taṇhā me susamūhatā;",
  "thag2.21:5.3": "Bhāvitā mama bojjhaṅgā,",
  "thag2.21:5.4": "patto me āsavakkhayo.",
  "thag2.21:6.1": "Sohaṃ khandhe pariññāya,",
  "thag2.21:6.2": "abbahitvāna jāliniṃ;",
  "thag2.21:6.3": "Bhāvayitvāna bojjhaṅge,",
  "thag2.21:6.4": "nibbāyissaṃ anāsavo”ti.",
  "thag2.21:7.1": "…",
  "thag2.21:7.2": "Uttaro thero …."
}