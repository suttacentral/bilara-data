{
  "thag2.17:1.1": "Theragāthā",
  "thag2.17:2.1": "Dukanipāta",
  "thag2.17:3.1": "Dutiyavagga",
  "thag2.17:4.1": "2.17. Tissattheragāthā",
  "thag2.17:5.1": "“Bahū sapatte labhati,",
  "thag2.17:5.2": "muṇḍo saṅghāṭipāruto;",
  "thag2.17:5.3": "Lābhī annassa pānassa,",
  "thag2.17:5.4": "vatthassa sayanassa ca.",
  "thag2.17:6.1": "Etamādīnavaṃ ñatvā,",
  "thag2.17:6.2": "sakkāresu mahabbhayaṃ;",
  "thag2.17:6.3": "Appalābho anavassuto,",
  "thag2.17:6.4": "sato bhikkhu paribbaje”ti.",
  "thag2.17:7.1": "…",
  "thag2.17:7.2": "Tisso thero …."
}