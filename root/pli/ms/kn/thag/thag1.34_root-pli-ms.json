{
  "thag1.34:1.1": "Theragāthā",
  "thag1.34:2.1": "Ekakanipāta",
  "thag1.34:3.1": "Catutthavagga",
  "thag1.34:4.1": "1.34. Posiyattheragāthā",
  "thag1.34:5.1": "“Anāsannavarā etā,",
  "thag1.34:5.2": "niccameva vijānatā;",
  "thag1.34:5.3": "Gāmā araññamāgamma,",
  "thag1.34:5.4": "tato gehaṃ upāvisi;",
  "thag1.34:5.5": "Tato uṭṭhāya pakkāmi,",
  "thag1.34:5.6": "anāmantetvā posiyo”ti.",
  "thag1.34:6.1": "…",
  "thag1.34:6.2": "Posiyo thero …."
}