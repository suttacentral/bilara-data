{
  "thag1.21:1.1": "Theragāthā",
  "thag1.21:2.1": "Ekakanipāta",
  "thag1.21:3.1": "Tatiyavagga",
  "thag1.21:4.1": "1.21. Nigrodhattheragāthā",
  "thag1.21:5.1": "“Nāhaṃ bhayassa bhāyāmi,",
  "thag1.21:5.2": "Satthā no amatassa kovido;",
  "thag1.21:5.3": "Yattha bhayaṃ nāvatiṭṭhati,",
  "thag1.21:5.4": "Tena maggena vajanti bhikkhavo”ti.",
  "thag1.21:6.1": "…",
  "thag1.21:6.2": "Nigrodho thero …."
}