{
  "thag1.49:1.1": "Theragāthā",
  "thag1.49:2.1": "Ekakanipāta",
  "thag1.49:3.1": "Pañcamavagga",
  "thag1.49:4.1": "1.49. Rāmaṇeyyakattheragāthā",
  "thag1.49:5.1": "“Cihacihābhinadite,",
  "thag1.49:5.2": "sippikābhirutehi ca;",
  "thag1.49:5.3": "Na me taṃ phandati cittaṃ,",
  "thag1.49:5.4": "ekattanirataṃ hi me”ti.",
  "thag1.49:6.1": "…",
  "thag1.49:6.2": "Rāmaṇeyyako thero …."
}