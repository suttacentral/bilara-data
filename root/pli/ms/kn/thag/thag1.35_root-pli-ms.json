{
  "thag1.35:1.1": "Theragāthā",
  "thag1.35:2.1": "Ekakanipāta",
  "thag1.35:3.1": "Catutthavagga",
  "thag1.35:4.1": "1.35. Sāmaññakānittheragāthā",
  "thag1.35:5.1": "“Sukhaṃ sukhattho labhate tadācaraṃ,",
  "thag1.35:5.2": "Kittiñca pappoti yasassa vaḍḍhati;",
  "thag1.35:5.3": "Yo ariyamaṭṭhaṅgikamañjasaṃ ujuṃ,",
  "thag1.35:5.4": "Bhāveti maggaṃ amatassa pattiyā”ti.",
  "thag1.35:6.1": "…",
  "thag1.35:6.2": "Sāmaññakānitthero …."
}