{
  "thag2.18:1.1": "Theragāthā",
  "thag2.18:2.1": "Dukanipāta",
  "thag2.18:3.1": "Dutiyavagga",
  "thag2.18:4.1": "2.18 Kimilattheragāthā",
  "thag2.18:5.1": "“Pācīnavaṃsadāyamhi,",
  "thag2.18:5.2": "sakyaputtā sahāyakā;",
  "thag2.18:5.3": "Pahāyānappake bhoge,",
  "thag2.18:5.4": "uñchāpattāgate ratā.",
  "thag2.18:6.1": "Āraddhavīriyā pahitattā,",
  "thag2.18:6.2": "Niccaṃ daḷhaparakkamā;",
  "thag2.18:6.3": "Ramanti dhammaratiyā,",
  "thag2.18:6.4": "Hitvāna lokiyaṃ ratin”ti.",
  "thag2.18:7.1": "…",
  "thag2.18:7.2": "Kimilo thero …."
}