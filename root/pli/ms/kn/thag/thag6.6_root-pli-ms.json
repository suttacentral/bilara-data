{
  "thag6.6:1.1": "Theragāthā",
  "thag6.6:2.1": "Chakkanipāta",
  "thag6.6:3.1": "Paṭhamavagga",
  "thag6.6:4.1": "6.6. Sappadāsattheragāthā",
  "thag6.6:5.1": "“Paṇṇavīsativassāni,",
  "thag6.6:5.2": "yato pabbajito ahaṃ;",
  "thag6.6:5.3": "Accharāsaṅghātamattampi,",
  "thag6.6:5.4": "cetosantimanajjhagaṃ.",
  "thag6.6:6.1": "Aladdhā cittassekaggaṃ,",
  "thag6.6:6.2": "kāmarāgena aṭṭito;",
  "thag6.6:6.3": "Bāhā paggayha kandanto,",
  "thag6.6:6.4": "vihārā upanikkhamiṃ.",
  "thag6.6:7.1": "Satthaṃ vā āharissāmi,",
  "thag6.6:7.2": "Ko attho jīvitena me;",
  "thag6.6:7.3": "Kathaṃ hi sikkhaṃ paccakkhaṃ,",
  "thag6.6:7.4": "Kālaṃ kubbetha mādiso.",
  "thag6.6:8.1": "Tadāhaṃ khuramādāya,",
  "thag6.6:8.2": "mañcakamhi upāvisiṃ;",
  "thag6.6:8.3": "Parinīto khuro āsi,",
  "thag6.6:8.4": "dhamaniṃ chettumattano.",
  "thag6.6:9.1": "Tato me manasīkāro,",
  "thag6.6:9.2": "yoniso udapajjatha;",
  "thag6.6:9.3": "Ādīnavo pāturahu,",
  "thag6.6:9.4": "nibbidā samatiṭṭhatha.",
  "thag6.6:10.1": "Tato cittaṃ vimucci me,",
  "thag6.6:10.2": "passa dhammasudhammataṃ;",
  "thag6.6:10.3": "Tisso vijjā anuppattā,",
  "thag6.6:10.4": "kataṃ buddhassa sāsanan”ti.",
  "thag6.6:11.1": "…",
  "thag6.6:11.2": "Sappadāso thero …."
}