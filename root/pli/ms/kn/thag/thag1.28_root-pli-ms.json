{
  "thag1.28:1.1": "Theragāthā",
  "thag1.28:2.1": "Ekakanipāta",
  "thag1.28:3.1": "Tatiyavagga",
  "thag1.28:4.1": "1.28 Jambugāmikaputtattheragāthā",
  "thag1.28:5.1": "“Kacci no vatthapasuto,",
  "thag1.28:5.2": "Kacci no bhūsanārato;",
  "thag1.28:5.3": "Kacci sīlamayaṃ gandhaṃ,",
  "thag1.28:5.4": "Kiṃ tvaṃ vāyasi netarā pajā”ti.",
  "thag1.28:6.1": "…",
  "thag1.28:6.2": "Jambugāmikaputto thero …."
}