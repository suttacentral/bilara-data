{
  "thag3.9:1.1": "Theragāthā",
  "thag3.9:2.1": "Tikanipāta",
  "thag3.9:3.1": "Paṭhamavagga",
  "thag3.9:4.1": "3.9. Yasojattheragāthā",
  "thag3.9:5.1": "“Kālapabbaṅgasaṅkāso,",
  "thag3.9:5.2": "kiso dhamanisanthato;",
  "thag3.9:5.3": "Mattaññū annapānamhi,",
  "thag3.9:5.4": "adīnamānaso naro”.",
  "thag3.9:6.1": "“Phuṭṭho ḍaṃsehi makasehi,",
  "thag3.9:6.2": "araññasmiṃ brahāvane;",
  "thag3.9:6.3": "Nāgo saṅgāmasīseva,",
  "thag3.9:6.4": "sato tatrādhivāsaye.",
  "thag3.9:7.1": "Yathā brahmā tathā eko,",
  "thag3.9:7.2": "yathā devo tathā duve;",
  "thag3.9:7.3": "Yathā gāmo tathā tayo,",
  "thag3.9:7.4": "kolāhalaṃ tatuttarin”ti.",
  "thag3.9:8.1": "…",
  "thag3.9:8.2": "Yasojo thero …."
}