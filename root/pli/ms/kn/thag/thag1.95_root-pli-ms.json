{
  "thag1.95:1.1": "Theragāthā",
  "thag1.95:2.1": "Ekakanipāta",
  "thag1.95:3.1": "Dasamavagga",
  "thag1.95:4.1": "1.95. Cakkhupālattheragāthā",
  "thag1.95:5.1": "“Andhohaṃ hatanettosmi,",
  "thag1.95:5.2": "kantāraddhānapakkhando;",
  "thag1.95:5.3": "Sayamānopi gacchissaṃ,",
  "thag1.95:5.4": "na sahāyena pāpenā”ti.",
  "thag1.95:6.1": "…",
  "thag1.95:6.2": "Cakkhupālo thero …."
}