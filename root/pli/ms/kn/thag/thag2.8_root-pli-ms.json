{
  "thag2.8:1.1": "Theragāthā",
  "thag2.8:2.1": "Dukanipāta",
  "thag2.8:3.1": "Paṭhamavagga",
  "thag2.8:4.1": "2.8. Surādhattheragāthā",
  "thag2.8:5.1": "“Khīṇā hi mayhaṃ jāti,",
  "thag2.8:5.2": "vusitaṃ jinasāsanaṃ;",
  "thag2.8:5.3": "Pahīno jālasaṅkhāto,",
  "thag2.8:5.4": "bhavanetti samūhatā.",
  "thag2.8:6.1": "Yassatthāya pabbajito,",
  "thag2.8:6.2": "agārasmānagāriyaṃ;",
  "thag2.8:6.3": "So me attho anuppatto,",
  "thag2.8:6.4": "sabbasaṃyojanakkhayo”ti.",
  "thag2.8:7.1": "…",
  "thag2.8:7.2": "Surādho thero …."
}