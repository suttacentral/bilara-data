{
  "thag2.29:1.1": "Theragāthā",
  "thag2.29:2.1": "Dukanipāta",
  "thag2.29:3.1": "Tatiyavagga",
  "thag2.29:4.1": "2.29. Bhāradvājattheragāthā",
  "thag2.29:5.1": "“Nadanti evaṃ sappaññā,",
  "thag2.29:5.2": "sīhāva girigabbhare;",
  "thag2.29:5.3": "Vīrā vijitasaṅgāmā,",
  "thag2.29:5.4": "jetvā māraṃ savāhiniṃ.",
  "thag2.29:6.1": "Satthā ca pariciṇṇo me,",
  "thag2.29:6.2": "dhammo saṃgho ca pūjito;",
  "thag2.29:6.3": "Ahañca vitto sumano,",
  "thag2.29:6.4": "puttaṃ disvā anāsavan”ti.",
  "thag2.29:7.1": "…",
  "thag2.29:7.2": "Bhāradvājo thero …."
}