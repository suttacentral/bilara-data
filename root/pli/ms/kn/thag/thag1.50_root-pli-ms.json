{
  "thag1.50:1.1": "Theragāthā",
  "thag1.50:2.1": "Ekakanipāta",
  "thag1.50:3.1": "Pañcamavagga",
  "thag1.50:4.1": "1.50. Vimalattheragāthā",
  "thag1.50:5.1": "“Dharaṇī ca siñcati vāti,",
  "thag1.50:5.2": "Māluto vijjutā carati nabhe;",
  "thag1.50:5.3": "Upasamanti vitakkā,",
  "thag1.50:5.4": "Cittaṃ susamāhitaṃ mamā”ti.",
  "thag1.50:6.1": "…",
  "thag1.50:6.2": "Vimalo thero ….",
  "thag1.50:6.3": "Vaggo pañcamo.",
  "thag1.50:7.1": "Tassuddānaṃ",
  "thag1.50:8.1": "Sirīvaḍḍho revato thero,",
  "thag1.50:8.2": "sumaṅgalo sānusavhayo;",
  "thag1.50:8.3": "Ramaṇīyavihārī ca,",
  "thag1.50:8.4": "samiddhiujjayasañjayā;",
  "thag1.50:8.5": "Rāmaṇeyyo ca so thero,",
  "thag1.50:8.6": "vimalo ca raṇañjahoti."
}