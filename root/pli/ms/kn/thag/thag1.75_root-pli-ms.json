{
  "thag1.75:1.1": "Theragāthā",
  "thag1.75:2.1": "Ekakanipāta",
  "thag1.75:3.1": "Aṭṭhamavagga",
  "thag1.75:4.1": "1.75. Susāradattheragāthā",
  "thag1.75:5.1": "“Sādhu suvihitāna dassanaṃ,",
  "thag1.75:5.2": "Kaṅkhā chijjati buddhi vaḍḍhati;",
  "thag1.75:5.3": "Bālampi karonti paṇḍitaṃ,",
  "thag1.75:5.4": "Tasmā sādhu sataṃ samāgamo”ti.",
  "thag1.75:6.1": "…",
  "thag1.75:6.2": "Susārado thero …."
}