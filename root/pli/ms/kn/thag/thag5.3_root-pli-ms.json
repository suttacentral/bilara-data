{
  "thag5.3:1.1": "Theragāthā",
  "thag5.3:2.1": "Pañcakanipāta",
  "thag5.3:3.1": "Paṭhamavagga",
  "thag5.3:4.1": "5.3. Girimānandattheragāthā",
  "thag5.3:5.1": "“Vassati devo yathā sugītaṃ,",
  "thag5.3:5.2": "Channā me kuṭikā sukhā nivātā;",
  "thag5.3:5.3": "Tassaṃ viharāmi vūpasanto,",
  "thag5.3:5.4": "Atha ce patthayasī pavassa deva.",
  "thag5.3:6.1": "Vassati devo yathā sugītaṃ,",
  "thag5.3:6.2": "Channā me kuṭikā sukhā nivātā;",
  "thag5.3:6.3": "Tassaṃ viharāmi santacitto,",
  "thag5.3:6.4": "Atha ce patthayasī pavassa deva.",
  "thag5.3:7.1": "Vassati devo,",
  "thag5.3:7.2": "…",
  "thag5.3:7.3": "pe…",
  "thag5.3:7.4": "tassaṃ viharāmi vītarāgo,",
  "thag5.3:7.5": "…",
  "thag5.3:7.6": "pe…",
  "thag5.3:8.1": "Vassati devo,",
  "thag5.3:8.2": "…",
  "thag5.3:8.3": "pe…",
  "thag5.3:8.4": "tassaṃ viharāmi vītadoso,",
  "thag5.3:8.5": "…",
  "thag5.3:8.6": "pe…",
  "thag5.3:9.1": "Vassati devo,",
  "thag5.3:9.2": "…",
  "thag5.3:9.3": "pe…",
  "thag5.3:9.4": "tassaṃ viharāmi vītamoho,",
  "thag5.3:9.5": "Atha ce patthayasī pavassa devā”ti.",
  "thag5.3:10.1": "…",
  "thag5.3:10.2": "Girimānando thero …."
}