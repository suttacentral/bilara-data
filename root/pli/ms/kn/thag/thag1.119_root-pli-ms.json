{
  "thag1.119:1.1": "Theragāthā",
  "thag1.119:2.1": "Ekakanipāta",
  "thag1.119:3.1": "Dvādasamavagga",
  "thag1.119:4.1": "1.119. Vajjiputtattheragāthā",
  "thag1.119:5.1": "“Rukkhamūlagahanaṃ pasakkiya,",
  "thag1.119:5.2": "Nibbānaṃ hadayasmiṃ opiya;",
  "thag1.119:5.3": "Jhāya gotama mā ca pamādo,",
  "thag1.119:5.4": "Kiṃ te biḷibiḷikā karissatī”ti.",
  "thag1.119:6.1": "…",
  "thag1.119:6.2": "Vajjiputto thero …."
}