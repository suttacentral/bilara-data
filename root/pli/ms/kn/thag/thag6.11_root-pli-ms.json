{
  "thag6.11:1.1": "Theragāthā",
  "thag6.11:2.1": "Chakkanipāta",
  "thag6.11:3.1": "Paṭhamavagga",
  "thag6.11:4.1": "6.11. Nhātakamunittheragāthā",
  "thag6.11:5.1": "“Vātarogābhinīto tvaṃ,",
  "thag6.11:5.2": "viharaṃ kānane vane;",
  "thag6.11:5.3": "Paviddhagocare lūkhe,",
  "thag6.11:5.4": "kathaṃ bhikkhu karissasi”.",
  "thag6.11:6.1": "“Pītisukhena vipulena,",
  "thag6.11:6.2": "pharitvāna samussayaṃ;",
  "thag6.11:6.3": "Lūkhampi abhisambhonto,",
  "thag6.11:6.4": "viharissāmi kānane.",
  "thag6.11:7.1": "Bhāvento satta bojjhaṅge,",
  "thag6.11:7.2": "indriyāni balāni ca;",
  "thag6.11:7.3": "Jhānasokhummasampanno,",
  "thag6.11:7.4": "viharissaṃ anāsavo.",
  "thag6.11:8.1": "Vippamuttaṃ kilesehi,",
  "thag6.11:8.2": "suddhacittaṃ anāvilaṃ;",
  "thag6.11:8.3": "Abhiṇhaṃ paccavekkhanto,",
  "thag6.11:8.4": "viharissaṃ anāsavo.",
  "thag6.11:9.1": "Ajjhattañca bahiddhā ca,",
  "thag6.11:9.2": "ye me vijjiṃsu āsavā;",
  "thag6.11:9.3": "Sabbe asesā ucchinnā,",
  "thag6.11:9.4": "na ca uppajjare puna.",
  "thag6.11:10.1": "Pañcakkhandhā pariññātā,",
  "thag6.11:10.2": "tiṭṭhanti chinnamūlakā;",
  "thag6.11:10.3": "Dukkhakkhayo anuppatto,",
  "thag6.11:10.4": "natthi dāni punabbhavo”ti.",
  "thag6.11:11.1": "…",
  "thag6.11:11.2": "Nhātakamunitthero …."
}