{
  "thag3.6:1.1": "Theragāthā",
  "thag3.6:2.1": "Tikanipāta",
  "thag3.6:3.1": "Paṭhamavagga",
  "thag3.6:4.1": "3.6. Khujjasobhitattheragāthā",
  "thag3.6:5.1": "“Ye cittakathī bahussutā,",
  "thag3.6:5.2": "Samaṇā pāṭaliputtavāsino;",
  "thag3.6:5.3": "Tesaññataroyamāyuvā,",
  "thag3.6:5.4": "Dvāre tiṭṭhati khujjasobhito”.",
  "thag3.6:6.1": "“Ye cittakathī bahussutā,",
  "thag3.6:6.2": "Samaṇā pāṭaliputtavāsino;",
  "thag3.6:6.3": "Tesaññataroyamāyuvā,",
  "thag3.6:6.4": "Dvāre tiṭṭhati māluterito”.",
  "thag3.6:7.1": "“Suyuddhena suyiṭṭhena,",
  "thag3.6:7.2": "saṅgāmavijayena ca;",
  "thag3.6:7.3": "Brahmacariyānuciṇṇena,",
  "thag3.6:7.4": "evāyaṃ sukhamedhatī”ti.",
  "thag3.6:8.1": "…",
  "thag3.6:8.2": "Khujjasobhito thero …."
}