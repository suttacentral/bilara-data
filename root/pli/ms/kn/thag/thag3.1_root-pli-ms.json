{
  "thag3.1:1.1": "Theragāthā",
  "thag3.1:2.1": "Tikanipāta",
  "thag3.1:3.1": "Paṭhamavagga",
  "thag3.1:4.1": "3.1 Aṅgaṇikabhāradvājattheragāthā",
  "thag3.1:5.1": "“Ayoni suddhimanvesaṃ,",
  "thag3.1:5.2": "aggiṃ paricariṃ vane;",
  "thag3.1:5.3": "Suddhimaggaṃ ajānanto,",
  "thag3.1:5.4": "akāsiṃ amaraṃ tapaṃ.",
  "thag3.1:6.1": "Taṃ sukhena sukhaṃ laddhaṃ,",
  "thag3.1:6.2": "passa dhammasudhammataṃ;",
  "thag3.1:6.3": "Tisso vijjā anuppattā,",
  "thag3.1:6.4": "kataṃ buddhassa sāsanaṃ.",
  "thag3.1:7.1": "Brahmabandhu pure āsiṃ,",
  "thag3.1:7.2": "idāni khomhi brāhmaṇo;",
  "thag3.1:7.3": "Tevijjo nhātako camhi,",
  "thag3.1:7.4": "sottiyo camhi vedagū”ti.",
  "thag3.1:8.1": "…",
  "thag3.1:8.2": "Aṅgaṇikabhāradvājo thero …."
}