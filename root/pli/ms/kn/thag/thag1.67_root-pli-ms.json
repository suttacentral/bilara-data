{
  "thag1.67:1.1": "Theragāthā",
  "thag1.67:2.1": "Ekakanipāta",
  "thag1.67:3.1": "Sattamavagga",
  "thag1.67:4.1": "1.67. Ekadhammasavanīyattheragāthā",
  "thag1.67:5.1": "“Kilesā jhāpitā mayhaṃ,",
  "thag1.67:5.2": "bhavā sabbe samūhatā;",
  "thag1.67:5.3": "Vikkhīṇo jātisaṃsāro,",
  "thag1.67:5.4": "natthi dāni punabbhavo”ti.",
  "thag1.67:6.1": "…",
  "thag1.67:6.2": "Ekadhammasavanīyo thero …."
}