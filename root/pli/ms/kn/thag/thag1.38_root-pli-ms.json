{
  "thag1.38:1.1": "Theragāthā",
  "thag1.38:2.1": "Ekakanipāta",
  "thag1.38:3.1": "Catutthavagga",
  "thag1.38:4.1": "1.38. Gavampatittheragāthā",
  "thag1.38:5.1": "“Yo iddhiyā sarabhuṃ aṭṭhapesi,",
  "thag1.38:5.2": "So gavampati asito anejo;",
  "thag1.38:5.3": "Taṃ sabbasaṅgātigataṃ mahāmuniṃ,",
  "thag1.38:5.4": "Devā namassanti bhavassa pāragun”ti.",
  "thag1.38:6.1": "…",
  "thag1.38:6.2": "Gavampatitthero …."
}