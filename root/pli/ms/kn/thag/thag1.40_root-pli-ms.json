{
  "thag1.40:1.1": "Theragāthā",
  "thag1.40:2.1": "Ekakanipāta",
  "thag1.40:3.1": "Catutthavagga",
  "thag1.40:4.1": "1.40. Vaḍḍhamānattheragāthā",
  "thag1.40:5.1": "“Sattiyā viya omaṭṭho,",
  "thag1.40:5.2": "ḍayhamānova matthake;",
  "thag1.40:5.3": "Bhavarāgappahānāya,",
  "thag1.40:5.4": "sato bhikkhu paribbaje”ti.",
  "thag1.40:6.1": "…",
  "thag1.40:6.2": "Vaḍḍhamāno thero ….",
  "thag1.40:6.3": "Vaggo catuttho.",
  "thag1.40:7.1": "Tassuddānaṃ",
  "thag1.40:8.1": "Gahvaratīriyo suppiyo,",
  "thag1.40:8.2": "sopāko ceva posiyo;",
  "thag1.40:8.3": "Sāmaññakāni kumāputto,",
  "thag1.40:8.4": "kumāputtasahāyako;",
  "thag1.40:8.5": "Gavampati tissatthero,",
  "thag1.40:8.6": "vaḍḍhamāno mahāyasoti."
}