{
  "thag1.106:1.1": "Theragāthā",
  "thag1.106:2.1": "Ekakanipāta",
  "thag1.106:3.1": "Ekādasamavagga",
  "thag1.106:4.1": "1.106. Suhemantattheragāthā",
  "thag1.106:5.1": "“Sataliṅgassa atthassa,",
  "thag1.106:5.2": "satalakkhaṇadhārino;",
  "thag1.106:5.3": "Ekaṅgadassī dummedho,",
  "thag1.106:5.4": "satadassī ca paṇḍito”ti.",
  "thag1.106:6.1": "…",
  "thag1.106:6.2": "Suhemanto thero …."
}