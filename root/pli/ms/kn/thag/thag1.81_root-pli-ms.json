{
  "thag1.81:1.1": "Theragāthā",
  "thag1.81:2.1": "Ekakanipāta",
  "thag1.81:3.1": "Navamavagga",
  "thag1.81:4.1": "1.81. Samitiguttattheragāthā",
  "thag1.81:5.1": "“Yaṃ mayā pakataṃ pāpaṃ,",
  "thag1.81:5.2": "pubbe aññāsu jātisu;",
  "thag1.81:5.3": "Idheva taṃ vedanīyaṃ,",
  "thag1.81:5.4": "vatthu aññaṃ na vijjatī”ti.",
  "thag1.81:6.1": "…",
  "thag1.81:6.2": "Samitigutto thero …."
}