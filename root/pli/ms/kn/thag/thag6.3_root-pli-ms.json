{
  "thag6.3:1.1": "Theragāthā",
  "thag6.3:2.1": "Chakkanipāta",
  "thag6.3:3.1": "Paṭhamavagga",
  "thag6.3:4.1": "6.3. Mahānāgattheragāthā",
  "thag6.3:5.1": "“Yassa sabrahmacārīsu,",
  "thag6.3:5.2": "gāravo nūpalabbhati;",
  "thag6.3:5.3": "Parihāyati saddhammā,",
  "thag6.3:5.4": "maccho appodake yathā.",
  "thag6.3:6.1": "Yassa sabrahmacārīsu,",
  "thag6.3:6.2": "gāravo nūpalabbhati;",
  "thag6.3:6.3": "Na virūhati saddhamme,",
  "thag6.3:6.4": "khette bījaṃva pūtikaṃ.",
  "thag6.3:7.1": "Yassa sabrahmacārīsu,",
  "thag6.3:7.2": "gāravo nūpalabbhati;",
  "thag6.3:7.3": "Ārakā hoti nibbānā,",
  "thag6.3:7.4": "dhammarājassa sāsane.",
  "thag6.3:8.1": "Yassa sabrahmacārīsu,",
  "thag6.3:8.2": "gāravo upalabbhati;",
  "thag6.3:8.3": "Na vihāyati saddhammā,",
  "thag6.3:8.4": "maccho bavhodake yathā.",
  "thag6.3:9.1": "Yassa sabrahmacārīsu,",
  "thag6.3:9.2": "gāravo upalabbhati;",
  "thag6.3:9.3": "So virūhati saddhamme,",
  "thag6.3:9.4": "khette bījaṃva bhaddakaṃ.",
  "thag6.3:10.1": "Yassa sabrahmacārīsu,",
  "thag6.3:10.2": "gāravo upalabbhati;",
  "thag6.3:10.3": "Santike hoti nibbānaṃ,",
  "thag6.3:10.4": "dhammarājassa sāsane”ti.",
  "thag6.3:11.1": "…",
  "thag6.3:11.2": "Mahānāgo thero …."
}