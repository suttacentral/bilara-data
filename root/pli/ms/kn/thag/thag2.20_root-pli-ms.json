{
  "thag2.20:1.1": "Theragāthā",
  "thag2.20:2.1": "Dukanipāta",
  "thag2.20:3.1": "Dutiyavagga",
  "thag2.20:4.1": "2.20. Sirimattheragāthā",
  "thag2.20:5.1": "“Pare ca naṃ pasaṃsanti,",
  "thag2.20:5.2": "attā ce asamāhito;",
  "thag2.20:5.3": "Moghaṃ pare pasaṃsanti,",
  "thag2.20:5.4": "attā hi asamāhito.",
  "thag2.20:6.1": "Pare ca naṃ garahanti,",
  "thag2.20:6.2": "attā ce susamāhito;",
  "thag2.20:6.3": "Moghaṃ pare garahanti,",
  "thag2.20:6.4": "attā hi susamāhito”ti.",
  "thag2.20:7.1": "…",
  "thag2.20:7.2": "Sirimā thero ….",
  "thag2.20:7.3": "Vaggo dutiyo.",
  "thag2.20:8.1": "Tassuddānaṃ",
  "thag2.20:9.1": "Cundo ca jotidāso ca,",
  "thag2.20:9.2": "thero heraññakāni ca;",
  "thag2.20:9.3": "Somamitto sabbamitto,",
  "thag2.20:9.4": "kāḷo tisso ca kimilo;",
  "thag2.20:9.5": "Nando ca sirimā ceva,",
  "thag2.20:9.6": "dasa therā mahiddhikāti."
}