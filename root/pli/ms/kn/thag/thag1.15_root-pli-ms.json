{
  "thag1.15:1.1": "Theragāthā",
  "thag1.15:2.1": "Ekakanipāta",
  "thag1.15:3.1": "Dutiyavagga",
  "thag1.15:4.1": "1.15. Kuṇḍadhānattheragāthā",
  "thag1.15:5.1": "“Pañca chinde pañca jahe,",
  "thag1.15:5.2": "pañca cuttari bhāvaye;",
  "thag1.15:5.3": "Pañcasaṅgātigo bhikkhu,",
  "thag1.15:5.4": "oghatiṇṇoti vuccatī”ti.",
  "thag1.15:6.1": "…",
  "thag1.15:6.2": "Kuṇḍadhāno thero …."
}