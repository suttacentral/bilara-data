{
  "thag1.96:1.1": "Theragāthā",
  "thag1.96:2.1": "Ekakanipāta",
  "thag1.96:3.1": "Dasamavagga",
  "thag1.96:4.1": "1.96. Khaṇḍasumanattheragāthā",
  "thag1.96:5.1": "“Ekapupphaṃ cajitvāna,",
  "thag1.96:5.2": "asīti vassakoṭiyo;",
  "thag1.96:5.3": "Saggesu paricāretvā,",
  "thag1.96:5.4": "sesakenamhi nibbuto”ti.",
  "thag1.96:6.1": "…",
  "thag1.96:6.2": "Khaṇḍasumano thero …."
}