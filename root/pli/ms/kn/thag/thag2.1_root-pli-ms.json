{
  "thag2.1:1.1": "Theragāthā",
  "thag2.1:2.1": "Dukanipāta",
  "thag2.1:3.1": "Paṭhamavagga",
  "thag2.1:4.1": "2.1. Uttarattheragāthā",
  "thag2.1:5.1": "“Natthi koci bhavo nicco,",
  "thag2.1:5.2": "saṅkhārā vāpi sassatā;",
  "thag2.1:5.3": "Uppajjanti ca te khandhā,",
  "thag2.1:5.4": "cavanti aparāparaṃ.",
  "thag2.1:6.1": "Etamādīnavaṃ ñatvā,",
  "thag2.1:6.2": "bhavenamhi anatthiko;",
  "thag2.1:6.3": "Nissaṭo sabbakāmehi,",
  "thag2.1:6.4": "patto me āsavakkhayo”ti.",
  "thag2.1:7.1": "Itthaṃ sudaṃ āyasmā uttaro thero gāthāyo abhāsitthāti."
}