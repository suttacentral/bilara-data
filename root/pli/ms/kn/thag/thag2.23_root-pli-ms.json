{
  "thag2.23:1.1": "Theragāthā",
  "thag2.23:2.1": "Dukanipāta",
  "thag2.23:3.1": "Tatiyavagga",
  "thag2.23:4.1": "2.23. Sobhitattheragāthā",
  "thag2.23:5.1": "“Satimā paññavā bhikkhu,",
  "thag2.23:5.2": "āraddhabalavīriyo;",
  "thag2.23:5.3": "Pañca kappasatānāhaṃ,",
  "thag2.23:5.4": "ekarattiṃ anussariṃ.",
  "thag2.23:6.1": "Cattāro satipaṭṭhāne,",
  "thag2.23:6.2": "satta aṭṭha ca bhāvayaṃ;",
  "thag2.23:6.3": "Pañca kappasatānāhaṃ,",
  "thag2.23:6.4": "ekarattiṃ anussarin”ti.",
  "thag2.23:7.1": "…",
  "thag2.23:7.2": "Sobhito thero …."
}