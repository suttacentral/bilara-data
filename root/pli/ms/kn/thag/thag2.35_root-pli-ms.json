{
  "thag2.35:1.1": "Theragāthā",
  "thag2.35:2.1": "Dukanipāta",
  "thag2.35:3.1": "Catutthavagga",
  "thag2.35:4.1": "2.35. Sambulakaccānattheragāthā",
  "thag2.35:5.1": "“Devo ca vassati, devo ca gaḷagaḷāyati,",
  "thag2.35:5.2": "Ekako cāhaṃ bherave bile viharāmi;",
  "thag2.35:5.3": "Tassa mayhaṃ ekakassa bherave bile viharato,",
  "thag2.35:5.4": "Natthi bhayaṃ vā chambhitattaṃ vā lomahaṃso vā.",
  "thag2.35:6.1": "Dhammatā mamesā yassa me,",
  "thag2.35:6.2": "Ekakassa bherave bile;",
  "thag2.35:6.3": "Viharato natthi bhayaṃ vā,",
  "thag2.35:6.4": "Chambhitattaṃ vā lomahaṃso vā”ti.",
  "thag2.35:7.1": "…",
  "thag2.35:7.2": "Sambulakaccāno thero …."
}