{
  "thag2.32:1.1": "Theragāthā",
  "thag2.32:2.1": "Dukanipāta",
  "thag2.32:3.1": "Catutthavagga",
  "thag2.32:4.1": "2.32 Sivakattheragāthā",
  "thag2.32:5.1": "“Aniccāni gahakāni,",
  "thag2.32:5.2": "tattha tattha punappunaṃ;",
  "thag2.32:5.3": "Gahakāraṃ gavesanto,",
  "thag2.32:5.4": "dukkhā jāti punappunaṃ.",
  "thag2.32:6.1": "Gahakāraka diṭṭhosi,",
  "thag2.32:6.2": "puna gehaṃ na kāhasi;",
  "thag2.32:6.3": "Sabbā te phāsukā bhaggā,",
  "thag2.32:6.4": "thūṇikā ca vidālitā;",
  "thag2.32:6.5": "Vimariyādikataṃ cittaṃ,",
  "thag2.32:6.6": "idheva vidhamissatī”ti.",
  "thag2.32:7.1": "…",
  "thag2.32:7.2": "Sivako thero …."
}