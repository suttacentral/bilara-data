{
  "thag2.45:1.1": "Theragāthā",
  "thag2.45:2.1": "Dukanipāta",
  "thag2.45:3.1": "Pañcamavagga",
  "thag2.45:4.1": "2.45. Visākhapañcālaputtattheragāthā",
  "thag2.45:5.1": "“Na ukkhipe no ca parikkhipe pare,",
  "thag2.45:5.2": "Na okkhipe pāragataṃ na eraye;",
  "thag2.45:5.3": "Na cattavaṇṇaṃ parisāsu byāhare,",
  "thag2.45:5.4": "Anuddhato sammitabhāṇi subbato.",
  "thag2.45:6.1": "Susukhumanipuṇatthadassinā,",
  "thag2.45:6.2": "Matikusalena nivātavuttinā;",
  "thag2.45:6.3": "Saṃsevitavuddhasīlinā,",
  "thag2.45:6.4": "Nibbānaṃ na hi tena dullabhan”ti.",
  "thag2.45:7.1": "…",
  "thag2.45:7.2": "Visākho pañcālaputto thero …."
}