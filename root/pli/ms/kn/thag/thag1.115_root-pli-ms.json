{
  "thag1.115:1.1": "Theragāthā",
  "thag1.115:2.1": "Ekakanipāta",
  "thag1.115:3.1": "Dvādasamavagga",
  "thag1.115:4.1": "1.115. Mahānāmattheragāthā",
  "thag1.115:5.1": "“Esāvahiyyase pabbatena,",
  "thag1.115:5.2": "Bahukuṭajasallakikena;",
  "thag1.115:5.3": "Nesādakena girinā,",
  "thag1.115:5.4": "Yasassinā paricchadenā”ti.",
  "thag1.115:6.1": "…",
  "thag1.115:6.2": "Mahānāmo thero …."
}