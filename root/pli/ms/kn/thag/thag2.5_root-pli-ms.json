{
  "thag2.5:1.1": "Theragāthā",
  "thag2.5:2.1": "Dukanipāta",
  "thag2.5:3.1": "Paṭhamavagga",
  "thag2.5:4.1": "2.5. Ajinattheragāthā",
  "thag2.5:5.1": "“Api ce hoti tevijjo,",
  "thag2.5:5.2": "maccuhāyī anāsavo;",
  "thag2.5:5.3": "Appaññātoti naṃ bālā,",
  "thag2.5:5.4": "avajānanti ajānatā.",
  "thag2.5:6.1": "Yo ca kho annapānassa,",
  "thag2.5:6.2": "lābhī hotidha puggalo;",
  "thag2.5:6.3": "Pāpadhammopi ce hoti,",
  "thag2.5:6.4": "so nesaṃ hoti sakkato”ti.",
  "thag2.5:7.1": "…",
  "thag2.5:7.2": "Ajino thero …."
}