{
  "thag1.24:1.1": "Theragāthā",
  "thag1.24:2.1": "Ekakanipāta",
  "thag1.24:3.1": "Tatiyavagga",
  "thag1.24:4.1": "1.24. Sugandhattheragāthā",
  "thag1.24:5.1": "“Anuvassiko pabbajito,",
  "thag1.24:5.2": "passa dhammasudhammataṃ;",
  "thag1.24:5.3": "Tisso vijjā anuppattā,",
  "thag1.24:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.24:6.1": "…",
  "thag1.24:6.2": "Sugandho thero …."
}