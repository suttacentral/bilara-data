{
  "thag1.60:1.1": "Theragāthā",
  "thag1.60:2.1": "Ekakanipāta",
  "thag1.60:3.1": "Chaṭṭhavagga",
  "thag1.60:4.1": "1.60. Sīvalittheragāthā",
  "thag1.60:5.1": "“Te me ijjhiṃsu saṅkappā,",
  "thag1.60:5.2": "yadattho pāvisiṃ kuṭiṃ;",
  "thag1.60:5.3": "Vijjāvimuttiṃ paccesaṃ,",
  "thag1.60:5.4": "mānānusayamujjahan”ti.",
  "thag1.60:6.1": "…",
  "thag1.60:6.2": "Sīvalitthero ….",
  "thag1.60:6.3": "Vaggo chaṭṭho.",
  "thag1.60:7.1": "Tassuddānaṃ",
  "thag1.60:8.1": "Godhiko ca subāhu ca,",
  "thag1.60:8.2": "valliyo uttiyo isi;",
  "thag1.60:8.3": "Añjanavaniyo thero,",
  "thag1.60:8.4": "duve kuṭivihārino;",
  "thag1.60:8.5": "Ramaṇīyakuṭiko ca,",
  "thag1.60:8.6": "kosalavhayasīvalīti."
}