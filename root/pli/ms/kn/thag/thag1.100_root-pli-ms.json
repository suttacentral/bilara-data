{
  "thag1.100:1.1": "Theragāthā",
  "thag1.100:2.1": "Ekakanipāta",
  "thag1.100:3.1": "Dasamavagga",
  "thag1.100:4.1": "1.100. (Dutiya) Devasabhattheragāthā",
  "thag1.100:5.1": "“Sammappadhānasampanno,",
  "thag1.100:5.2": "Satipaṭṭhānagocaro;",
  "thag1.100:5.3": "Vimuttikusumasañchanno,",
  "thag1.100:5.4": "Parinibbissatyanāsavo”ti.",
  "thag1.100:6.1": "…",
  "thag1.100:6.2": "Devasabho thero ….",
  "thag1.100:6.3": "Vaggo dasamo.",
  "thag1.100:7.1": "Tassuddānaṃ",
  "thag1.100:8.1": "Paripuṇṇako ca vijayo,",
  "thag1.100:8.2": "erako mettajī muni;",
  "thag1.100:8.3": "Cakkhupālo khaṇḍasumano,",
  "thag1.100:8.4": "tisso ca abhayo tathā;",
  "thag1.100:8.5": "Uttiyo ca mahāpañño,",
  "thag1.100:8.6": "thero devasabhopi cāti."
}