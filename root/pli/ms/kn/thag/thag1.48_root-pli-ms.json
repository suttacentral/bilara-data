{
  "thag1.48:1.1": "Theragāthā",
  "thag1.48:2.1": "Ekakanipāta",
  "thag1.48:3.1": "Pañcamavagga",
  "thag1.48:4.1": "1.48. Sañjayattheragāthā",
  "thag1.48:5.1": "“Yato ahaṃ pabbajito,",
  "thag1.48:5.2": "Agārasmānagāriyaṃ;",
  "thag1.48:5.3": "Nābhijānāmi saṅkappaṃ,",
  "thag1.48:5.4": "Anariyaṃ dosasaṃhitan”ti.",
  "thag1.48:6.1": "…",
  "thag1.48:6.2": "Sañjayo thero …."
}