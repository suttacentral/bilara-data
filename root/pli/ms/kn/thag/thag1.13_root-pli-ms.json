{
  "thag1.13:1.1": "Theragāthā",
  "thag1.13:2.1": "Ekakanipāta",
  "thag1.13:3.1": "Dutiyavagga",
  "thag1.13:4.1": "1.13. Vanavacchattheragāthā",
  "thag1.13:5.1": "“Nīlabbhavaṇṇā rucirā,",
  "thag1.13:5.2": "sītavārī sucindharā;",
  "thag1.13:5.3": "Indagopakasañchannā,",
  "thag1.13:5.4": "te selā ramayanti man”ti.",
  "thag1.13:6.1": "…",
  "thag1.13:6.2": "Vanavaccho thero …."
}