{
  "thag2.39:1.1": "Theragāthā",
  "thag2.39:2.1": "Dukanipāta",
  "thag2.39:3.1": "Catutthavagga",
  "thag2.39:4.1": "2.39. Usabhattheragāthā",
  "thag2.39:5.1": "“Ambapallavasaṅkāsaṃ,",
  "thag2.39:5.2": "aṃse katvāna cīvaraṃ;",
  "thag2.39:5.3": "Nisinno hatthigīvāyaṃ,",
  "thag2.39:5.4": "gāmaṃ piṇḍāya pāvisiṃ.",
  "thag2.39:6.1": "Hatthikkhandhato oruyha,",
  "thag2.39:6.2": "saṃvegaṃ alabhiṃ tadā;",
  "thag2.39:6.3": "Sohaṃ ditto tadā santo,",
  "thag2.39:6.4": "patto me āsavakkhayo”ti.",
  "thag2.39:7.1": "…",
  "thag2.39:7.2": "Usabho thero …."
}