{
  "thag1.113:1.1": "Theragāthā",
  "thag1.113:2.1": "Ekakanipāta",
  "thag1.113:3.1": "Dvādasamavagga",
  "thag1.113:4.1": "1.113. Vanavacchattheragāthā",
  "thag1.113:5.1": "“Acchodikā puthusilā,",
  "thag1.113:5.2": "gonaṅgulamigāyutā;",
  "thag1.113:5.3": "Ambusevālasañchannā,",
  "thag1.113:5.4": "te selā ramayanti man”ti.",
  "thag1.113:6.1": "…",
  "thag1.113:6.2": "Vanavaccho thero …."
}