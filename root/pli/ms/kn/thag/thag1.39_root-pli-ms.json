{
  "thag1.39:1.1": "Theragāthā",
  "thag1.39:2.1": "Ekakanipāta",
  "thag1.39:3.1": "Catutthavagga",
  "thag1.39:4.1": "1.39. Tissattheragāthā",
  "thag1.39:5.1": "“Sattiyā viya omaṭṭho,",
  "thag1.39:5.2": "ḍayhamānova matthake;",
  "thag1.39:5.3": "Kāmarāgappahānāya,",
  "thag1.39:5.4": "sato bhikkhu paribbaje”ti.",
  "thag1.39:6.1": "…",
  "thag1.39:6.2": "Tisso thero …."
}