{
  "thag1.14:1.1": "Theragāthā",
  "thag1.14:2.1": "Ekakanipāta",
  "thag1.14:3.1": "Dutiyavagga",
  "thag1.14:4.1": "1.14. Sivakasāmaṇeragāthā",
  "thag1.14:5.1": "“Upajjhāyo maṃ avaca,",
  "thag1.14:5.2": "ito gacchāma sīvaka;",
  "thag1.14:5.3": "Gāme me vasati kāyo,",
  "thag1.14:5.4": "araññaṃ me gato mano;",
  "thag1.14:5.5": "Semānakopi gacchāmi,",
  "thag1.14:5.6": "natthi saṅgo vijānatan”ti.",
  "thag1.14:6.1": "…",
  "thag1.14:6.2": "Vanavacchassa therassa (siviko) sāmaṇero …."
}