{
  "thag5.2:1.1": "Theragāthā",
  "thag5.2:2.1": "Pañcakanipāta",
  "thag5.2:3.1": "Paṭhamavagga",
  "thag5.2:4.1": "5.2. Subhūtattheragāthā",
  "thag5.2:5.1": "“Ayoge yuñjamattānaṃ,",
  "thag5.2:5.2": "puriso kiccamicchako;",
  "thag5.2:5.3": "Carañce nādhigaccheyya,",
  "thag5.2:5.4": "‘taṃ me dubbhagalakkhaṇaṃ’.",
  "thag5.2:6.1": "Abbūḷhaṃ aghagataṃ vijitaṃ,",
  "thag5.2:6.2": "Ekañce ossajeyya kalīva siyā;",
  "thag5.2:6.3": "Sabbānipi ce ossajeyya andhova siyā,",
  "thag5.2:6.4": "Samavisamassa adassanato.",
  "thag5.2:7.1": "Yañhi kayirā tañhi vade,",
  "thag5.2:7.2": "yaṃ na kayirā na taṃ vade;",
  "thag5.2:7.3": "Akarontaṃ bhāsamānaṃ,",
  "thag5.2:7.4": "parijānanti paṇḍitā.",
  "thag5.2:8.1": "Yathāpi ruciraṃ pupphaṃ,",
  "thag5.2:8.2": "vaṇṇavantaṃ agandhakaṃ;",
  "thag5.2:8.3": "Evaṃ subhāsitā vācā,",
  "thag5.2:8.4": "aphalā hoti akubbato.",
  "thag5.2:9.1": "Yathāpi ruciraṃ pupphaṃ,",
  "thag5.2:9.2": "Vaṇṇavantaṃ sugandhakaṃ;",
  "thag5.2:9.3": "Evaṃ subhāsitā vācā,",
  "thag5.2:9.4": "Saphalā hoti kubbato”ti.",
  "thag5.2:10.1": "…",
  "thag5.2:10.2": "Subhūto thero …."
}