{
  "thag1.36:1.1": "Theragāthā",
  "thag1.36:2.1": "Ekakanipāta",
  "thag1.36:3.1": "Catutthavagga",
  "thag1.36:4.1": "1.36. Kumāputtattheragāthā",
  "thag1.36:5.1": "“Sādhu sutaṃ sādhu caritakaṃ,",
  "thag1.36:5.2": "Sādhu sadā aniketavihāro;",
  "thag1.36:5.3": "Atthapucchanaṃ padakkhiṇakammaṃ,",
  "thag1.36:5.4": "Etaṃ sāmaññamakiñcanassā”ti.",
  "thag1.36:6.1": "…",
  "thag1.36:6.2": "Kumāputto thero …."
}