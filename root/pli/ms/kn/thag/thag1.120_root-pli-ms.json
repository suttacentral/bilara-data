{
  "thag1.120:1.1": "Theragāthā",
  "thag1.120:2.1": "Ekakanipāta",
  "thag1.120:3.1": "Dvādasamavagga",
  "thag1.120:4.1": "1.120. Isidattattheragāthā",
  "thag1.120:5.1": "“Pañcakkhandhā pariññātā,",
  "thag1.120:5.2": "tiṭṭhanti chinnamūlakā;",
  "thag1.120:5.3": "Dukkhakkhayo anuppatto,",
  "thag1.120:5.4": "patto me āsavakkhayo”ti.",
  "thag1.120:6.1": "…",
  "thag1.120:6.2": "Isidatto thero ….",
  "thag1.120:6.3": "Vaggo dvādasamo.",
  "thag1.120:7.1": "Tassuddānaṃ",
  "thag1.120:8.1": "Jento ca vacchagotto ca,",
  "thag1.120:8.2": "vaccho ca vanasavhayo;",
  "thag1.120:8.3": "Adhimutto mahānāmo,",
  "thag1.120:8.4": "pārāpariyo yasopi ca;",
  "thag1.120:8.5": "Kimilo vajjiputto ca,",
  "thag1.120:8.6": "isidatto mahāyasoti.",
  "thag1.120:9.1": "Ekakanipāto niṭṭhito.",
  "thag1.120:10.1": "Tatruddānaṃ",
  "thag1.120:11.1": "Vīsuttarasataṃ therā,",
  "thag1.120:11.2": "katakiccā anāsavā;",
  "thag1.120:11.3": "Ekakeva nipātamhi,",
  "thag1.120:11.4": "susaṅgītā mahesibhīti."
}