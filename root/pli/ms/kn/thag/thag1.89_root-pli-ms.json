{
  "thag1.89:1.1": "Theragāthā",
  "thag1.89:2.1": "Ekakanipāta",
  "thag1.89:3.1": "Navamavagga",
  "thag1.89:4.1": "1.89. (Paṭhama) Devasabhattheragāthā",
  "thag1.89:5.1": "“Uttiṇṇā paṅkapalipā,",
  "thag1.89:5.2": "pātālā parivajjitā;",
  "thag1.89:5.3": "Mutto oghā ca ganthā ca,",
  "thag1.89:5.4": "sabbe mānā visaṃhatā”ti.",
  "thag1.89:6.1": "…",
  "thag1.89:6.2": "Devasabho thero …."
}