{
  "thag1.86:1.1": "Theragāthā",
  "thag1.86:2.1": "Ekakanipāta",
  "thag1.86:3.1": "Navamavagga",
  "thag1.86:4.1": "1.86. Nāgitattheragāthā",
  "thag1.86:5.1": "“Ito bahiddhā puthu aññavādinaṃ,",
  "thag1.86:5.2": "Maggo na nibbānagamo yathā ayaṃ;",
  "thag1.86:5.3": "Itissu saṃghaṃ bhagavānusāsati,",
  "thag1.86:5.4": "Satthā sayaṃ pāṇitaleva dassayan”ti.",
  "thag1.86:6.1": "…",
  "thag1.86:6.2": "Nāgito thero …."
}