{
  "thag6.8:1.1": "Theragāthā",
  "thag6.8:2.1": "Chakkanipāta",
  "thag6.8:3.1": "Paṭhamavagga",
  "thag6.8:4.1": "6.8. Migajālattheragāthā",
  "thag6.8:5.1": "“Sudesito cakkhumatā,",
  "thag6.8:5.2": "buddhenādiccabandhunā;",
  "thag6.8:5.3": "Sabbasaṃyojanātīto,",
  "thag6.8:5.4": "sabbavaṭṭavināsano.",
  "thag6.8:6.1": "Niyyāniko uttaraṇo,",
  "thag6.8:6.2": "taṇhāmūlavisosano;",
  "thag6.8:6.3": "Visamūlaṃ āghātanaṃ,",
  "thag6.8:6.4": "chetvā pāpeti nibbutiṃ.",
  "thag6.8:7.1": "Aññāṇamūlabhedāya,",
  "thag6.8:7.2": "kammayantavighāṭano;",
  "thag6.8:7.3": "Viññāṇānaṃ pariggahe,",
  "thag6.8:7.4": "ñāṇavajiranipātano.",
  "thag6.8:8.1": "Vedanānaṃ viññāpano,",
  "thag6.8:8.2": "upādānappamocano;",
  "thag6.8:8.3": "Bhavaṃ aṅgārakāsuṃva,",
  "thag6.8:8.4": "ñāṇena anupassano.",
  "thag6.8:9.1": "Mahāraso sugambhīro,",
  "thag6.8:9.2": "jarāmaccunivāraṇo;",
  "thag6.8:9.3": "Ariyo aṭṭhaṅgiko maggo,",
  "thag6.8:9.4": "dukkhūpasamano sivo.",
  "thag6.8:10.1": "Kammaṃ kammanti ñatvāna,",
  "thag6.8:10.2": "vipākañca vipākato;",
  "thag6.8:10.3": "Paṭiccuppannadhammānaṃ,",
  "thag6.8:10.4": "yathāvālokadassano;",
  "thag6.8:10.5": "Mahākhemaṅgamo santo,",
  "thag6.8:10.6": "pariyosānabhaddako”ti.",
  "thag6.8:11.1": "…",
  "thag6.8:11.2": "Migajālo thero …."
}