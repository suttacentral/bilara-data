{
  "thag1.41:1.1": "Theragāthā",
  "thag1.41:2.1": "Ekakanipāta",
  "thag1.41:3.1": "Pañcamavagga",
  "thag1.41:4.1": "1.41. Sirivaḍḍhattheragāthā",
  "thag1.41:5.1": "“Vivaramanupatanti vijjutā,",
  "thag1.41:5.2": "Vebhārassa ca paṇḍavassa ca;",
  "thag1.41:5.3": "Nagavivaragato ca jhāyati,",
  "thag1.41:5.4": "Putto appaṭimassa tādino”ti.",
  "thag1.41:6.1": "…",
  "thag1.41:6.2": "Sirivaḍḍho thero …."
}