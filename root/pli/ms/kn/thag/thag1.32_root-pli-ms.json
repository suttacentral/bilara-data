{
  "thag1.32:1.1": "Theragāthā",
  "thag1.32:2.1": "Ekakanipāta",
  "thag1.32:3.1": "Catutthavagga",
  "thag1.32:4.1": "1.32. Suppiyattheragāthā",
  "thag1.32:5.1": "“Ajaraṃ jīramānena,",
  "thag1.32:5.2": "tappamānena nibbutiṃ;",
  "thag1.32:5.3": "Nimiyaṃ paramaṃ santiṃ,",
  "thag1.32:5.4": "yogakkhemaṃ anuttaran”ti.",
  "thag1.32:6.1": "…",
  "thag1.32:6.2": "Suppiyo thero …."
}