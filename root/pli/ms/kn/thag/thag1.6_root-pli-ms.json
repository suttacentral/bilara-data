{
  "thag1.6:1.1": "Theragāthā",
  "thag1.6:2.1": "Ekakanipāta",
  "thag1.6:3.1": "Paṭhamavagga",
  "thag1.6:4.1": "1.6. Sītavaniyattheragāthā",
  "thag1.6:5.1": "“Yo sītavanaṃ upagā bhikkhu,",
  "thag1.6:5.2": "Eko santusito samāhitatto;",
  "thag1.6:5.3": "Vijitāvī apetalomahaṃso,",
  "thag1.6:5.4": "Rakkhaṃ kāyagatāsatiṃ dhitimā”ti.",
  "thag1.6:6.1": "Itthaṃ sudaṃ āyasmā sītavaniyo thero gāthaṃ abhāsitthāti."
}