{
  "thag1.69:1.1": "Theragāthā",
  "thag1.69:2.1": "Ekakanipāta",
  "thag1.69:3.1": "Sattamavagga",
  "thag1.69:4.1": "1.69. Channattheragāthā",
  "thag1.69:5.1": "“Sutvāna dhammaṃ mahato mahārasaṃ,",
  "thag1.69:5.2": "Sabbaññutaññāṇavarena desitaṃ;",
  "thag1.69:5.3": "Maggaṃ papajjiṃ amatassa pattiyā,",
  "thag1.69:5.4": "So yogakkhemassa pathassa kovido”ti.",
  "thag1.69:6.1": "…",
  "thag1.69:6.2": "Channo thero …."
}