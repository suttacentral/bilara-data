{
  "thag2.49:1.1": "Theragāthā",
  "thag2.49:2.1": "Dukanipāta",
  "thag2.49:3.1": "Pañcamavagga",
  "thag2.49:4.1": "2.49. Sandhitattheragāthā",
  "thag2.49:5.1": "“Assatthe haritobhāse,",
  "thag2.49:5.2": "saṃvirūḷhamhi pādape;",
  "thag2.49:5.3": "Ekaṃ buddhagataṃ saññaṃ,",
  "thag2.49:5.4": "alabhitthaṃ patissato.",
  "thag2.49:6.1": "Ekatiṃse ito kappe,",
  "thag2.49:6.2": "yaṃ saññamalabhiṃ tadā;",
  "thag2.49:6.3": "Tassā saññāya vāhasā,",
  "thag2.49:6.4": "patto me āsavakkhayo”ti.",
  "thag2.49:7.1": "…",
  "thag2.49:7.2": "Sandhito thero ….",
  "thag2.49:7.3": "Vaggo pañcamo.",
  "thag2.49:8.1": "Tassuddānaṃ",
  "thag2.49:9.1": "Kumārakassapo thero,",
  "thag2.49:9.2": "dhammapālo ca brahmāli;",
  "thag2.49:9.3": "Mogharājā visākho ca,",
  "thag2.49:9.4": "cūḷako ca anūpamo;",
  "thag2.49:9.5": "Vajjito sandhito thero,",
  "thag2.49:9.6": "kilesarajavāhanoti.",
  "thag2.49:10.1": "Dukanipāto niṭṭhito.",
  "thag2.49:11.1": "Tatruddānaṃ",
  "thag2.49:12.1": "Gāthādukanipātamhi,",
  "thag2.49:12.2": "navuti ceva aṭṭha ca;",
  "thag2.49:12.3": "Therā ekūnapaññāsaṃ,",
  "thag2.49:12.4": "bhāsitā nayakovidāti."
}