{
  "thag4.9:1.1": "Theragāthā",
  "thag4.9:2.1": "Catukkanipāta",
  "thag4.9:3.1": "Paṭhamavagga",
  "thag4.9:4.1": "4.9. Candanattheragāthā",
  "thag4.9:5.1": "“Jātarūpena sañchannā,",
  "thag4.9:5.2": "dāsīgaṇapurakkhatā;",
  "thag4.9:5.3": "Aṅkena puttamādāya,",
  "thag4.9:5.4": "bhariyā maṃ upāgami.",
  "thag4.9:6.1": "Tañca disvāna āyantiṃ,",
  "thag4.9:6.2": "sakaputtassa mātaraṃ;",
  "thag4.9:6.3": "Alaṅkataṃ suvasanaṃ,",
  "thag4.9:6.4": "maccupāsaṃva oḍḍitaṃ.",
  "thag4.9:7.1": "Tato me manasīkāro,",
  "thag4.9:7.2": "yoniso udapajjatha;",
  "thag4.9:7.3": "Ādīnavo pāturahu,",
  "thag4.9:7.4": "nibbidā samatiṭṭhatha.",
  "thag4.9:8.1": "Tato cittaṃ vimucci me,",
  "thag4.9:8.2": "passa dhammasudhammataṃ;",
  "thag4.9:8.3": "Tisso vijjā anuppattā,",
  "thag4.9:8.4": "kataṃ buddhassa sāsanan”ti.",
  "thag4.9:9.1": "…",
  "thag4.9:9.2": "Candano thero …."
}