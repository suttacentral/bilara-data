{
  "thag1.55:1.1": "Theragāthā",
  "thag1.55:2.1": "Ekakanipāta",
  "thag1.55:3.1": "Chaṭṭhavagga",
  "thag1.55:4.1": "1.55. Añjanavaniyattheragāthā",
  "thag1.55:5.1": "“Āsandiṃ kuṭikaṃ katvā,",
  "thag1.55:5.2": "ogayha añjanaṃ vanaṃ;",
  "thag1.55:5.3": "Tisso vijjā anuppattā,",
  "thag1.55:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.55:6.1": "…",
  "thag1.55:6.2": "Añjanavaniyo thero …."
}