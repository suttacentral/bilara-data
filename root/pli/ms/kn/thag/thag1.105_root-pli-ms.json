{
  "thag1.105:1.1": "Theragāthā",
  "thag1.105:2.1": "Ekakanipāta",
  "thag1.105:3.1": "Ekādasamavagga",
  "thag1.105:4.1": "1.105. Malitavambhattheragāthā",
  "thag1.105:5.1": "“Ukkaṇṭhitopi na vase,",
  "thag1.105:5.2": "ramamānopi pakkame;",
  "thag1.105:5.3": "Na tvevānatthasaṃhitaṃ,",
  "thag1.105:5.4": "vase vāsaṃ vicakkhaṇo”ti.",
  "thag1.105:6.1": "…",
  "thag1.105:6.2": "Malitavambho thero …."
}