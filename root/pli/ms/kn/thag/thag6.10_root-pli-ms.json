{
  "thag6.10:1.1": "Theragāthā",
  "thag6.10:2.1": "Chakkanipāta",
  "thag6.10:3.1": "Paṭhamavagga",
  "thag6.10:4.1": "6.10. Sumanattheragāthā",
  "thag6.10:5.1": "“Yadā navo pabbajito,",
  "thag6.10:5.2": "jātiyā sattavassiko;",
  "thag6.10:5.3": "Iddhiyā abhibhotvāna,",
  "thag6.10:5.4": "pannagindaṃ mahiddhikaṃ.",
  "thag6.10:6.1": "Upajjhāyassa udakaṃ,",
  "thag6.10:6.2": "anotattā mahāsarā;",
  "thag6.10:6.3": "Āharāmi tato disvā,",
  "thag6.10:6.4": "maṃ satthā etadabravi”.",
  "thag6.10:7.1": "“Sāriputta imaṃ passa,",
  "thag6.10:7.2": "āgacchantaṃ kumārakaṃ;",
  "thag6.10:7.3": "Udakakumbhamādāya,",
  "thag6.10:7.4": "ajjhattaṃ susamāhitaṃ.",
  "thag6.10:8.1": "Pāsādikena vattena,",
  "thag6.10:8.2": "kalyāṇairiyāpatho;",
  "thag6.10:8.3": "Sāmaṇeronuruddhassa,",
  "thag6.10:8.4": "iddhiyā ca visārado.",
  "thag6.10:9.1": "Ājānīyena ājañño,",
  "thag6.10:9.2": "sādhunā sādhukārito;",
  "thag6.10:9.3": "Vinīto anuruddhena,",
  "thag6.10:9.4": "katakiccena sikkhito.",
  "thag6.10:10.1": "So patvā paramaṃ santiṃ,",
  "thag6.10:10.2": "sacchikatvā akuppataṃ;",
  "thag6.10:10.3": "Sāmaṇero sa sumano,",
  "thag6.10:10.4": "mā maṃ jaññāti icchatī”ti.",
  "thag6.10:11.1": "…",
  "thag6.10:11.2": "Sumano thero …."
}