{
  "thag1.72:1.1": "Theragāthā",
  "thag1.72:2.1": "Ekakanipāta",
  "thag1.72:3.1": "Aṭṭhamavagga",
  "thag1.72:4.1": "1.72. Ātumattheragāthā",
  "thag1.72:5.1": "“Yathā kaḷīro susu vaḍḍhitaggo,",
  "thag1.72:5.2": "Dunnikkhamo hoti pasākhajāto;",
  "thag1.72:5.3": "Evaṃ ahaṃ bhariyāyānitāya,",
  "thag1.72:5.4": "Anumaññaṃ maṃ pabbajitomhi dānī”ti.",
  "thag1.72:6.1": "…",
  "thag1.72:6.2": "Ātumo thero …."
}