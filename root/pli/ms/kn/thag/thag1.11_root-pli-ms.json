{
  "thag1.11:1.1": "Theragāthā",
  "thag1.11:2.1": "Ekakanipāta",
  "thag1.11:3.1": "Dutiyavagga",
  "thag1.11:4.1": "1.11. Cūḷavacchattheragāthā",
  "thag1.11:5.1": "“Pāmojjabahulo bhikkhu,",
  "thag1.11:5.2": "dhamme buddhappavedite;",
  "thag1.11:5.3": "Adhigacche padaṃ santaṃ,",
  "thag1.11:5.4": "saṅkhārūpasamaṃ sukhan”ti.",
  "thag1.11:6.1": "…",
  "thag1.11:6.2": "Cūḷavaccho thero …."
}