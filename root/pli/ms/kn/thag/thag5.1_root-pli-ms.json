{
  "thag5.1:1.1": "Theragāthā",
  "thag5.1:2.1": "Pañcakanipāta",
  "thag5.1:3.1": "Paṭhamavagga",
  "thag5.1:4.1": "5.1. Rājadattattheragāthā",
  "thag5.1:5.1": "“Bhikkhu sivathikaṃ gantvā,",
  "thag5.1:5.2": "Addasa itthimujjhitaṃ;",
  "thag5.1:5.3": "Apaviddhaṃ susānasmiṃ,",
  "thag5.1:5.4": "Khajjantiṃ kimihī phuṭaṃ.",
  "thag5.1:6.1": "Yañhi eke jigucchanti,",
  "thag5.1:6.2": "mataṃ disvāna pāpakaṃ;",
  "thag5.1:6.3": "Kāmarāgo pāturahu,",
  "thag5.1:6.4": "andhova savatī ahuṃ.",
  "thag5.1:7.1": "Oraṃ odanapākamhā,",
  "thag5.1:7.2": "tamhā ṭhānā apakkamiṃ;",
  "thag5.1:7.3": "Satimā sampajānohaṃ,",
  "thag5.1:7.4": "ekamantaṃ upāvisiṃ.",
  "thag5.1:8.1": "Tato me manasīkāro,",
  "thag5.1:8.2": "yoniso udapajjatha;",
  "thag5.1:8.3": "Ādīnavo pāturahu,",
  "thag5.1:8.4": "nibbidā samatiṭṭhatha.",
  "thag5.1:9.1": "Tato cittaṃ vimucci me,",
  "thag5.1:9.2": "passa dhammasudhammataṃ;",
  "thag5.1:9.3": "Tisso vijjā anuppattā,",
  "thag5.1:9.4": "kataṃ buddhassa sāsanan”ti.",
  "thag5.1:10.1": "…",
  "thag5.1:10.2": "Rājadatto thero …."
}