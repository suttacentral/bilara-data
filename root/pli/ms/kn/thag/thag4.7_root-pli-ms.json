{
  "thag4.7:1.1": "Theragāthā",
  "thag4.7:2.1": "Catukkanipāta",
  "thag4.7:3.1": "Paṭhamavagga",
  "thag4.7:4.1": "4.7. Sambhūtattheragāthā",
  "thag4.7:5.1": "“Yo dandhakāle tarati,",
  "thag4.7:5.2": "taraṇīye ca dandhaye;",
  "thag4.7:5.3": "Ayoni saṃvidhānena,",
  "thag4.7:5.4": "bālo dukkhaṃ nigacchati.",
  "thag4.7:6.1": "Tassatthā parihāyanti,",
  "thag4.7:6.2": "kāḷapakkheva candimā;",
  "thag4.7:6.3": "Āyasakyañca pappoti,",
  "thag4.7:6.4": "mittehi ca virujjhati.",
  "thag4.7:7.1": "Yo dandhakāle dandheti,",
  "thag4.7:7.2": "taraṇīye ca tāraye;",
  "thag4.7:7.3": "Yoniso saṃvidhānena,",
  "thag4.7:7.4": "sukhaṃ pappoti paṇḍito.",
  "thag4.7:8.1": "Tassatthā paripūrenti,",
  "thag4.7:8.2": "sukkapakkheva candimā;",
  "thag4.7:8.3": "Yaso kittiñca pappoti,",
  "thag4.7:8.4": "mittehi na virujjhatī”ti.",
  "thag4.7:9.1": "…",
  "thag4.7:9.2": "Sambhūto thero …."
}