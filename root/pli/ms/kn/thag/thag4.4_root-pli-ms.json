{
  "thag4.4:1.1": "Theragāthā",
  "thag4.4:2.1": "Catukkanipāta",
  "thag4.4:3.1": "Paṭhamavagga",
  "thag4.4:4.1": "4.4. Nandakattheragāthā",
  "thag4.4:5.1": "“Dhiratthu pūre duggandhe,",
  "thag4.4:5.2": "mārapakkhe avassute;",
  "thag4.4:5.3": "Navasotāni te kāye,",
  "thag4.4:5.4": "yāni sandanti sabbadā.",
  "thag4.4:6.1": "Mā purāṇaṃ amaññittho,",
  "thag4.4:6.2": "māsādesi tathāgate;",
  "thag4.4:6.3": "Saggepi te na rajjanti,",
  "thag4.4:6.4": "kimaṅgaṃ pana mānuse.",
  "thag4.4:7.1": "Ye ca kho bālā dummedhā,",
  "thag4.4:7.2": "dummantī mohapārutā;",
  "thag4.4:7.3": "Tādisā tattha rajjanti,",
  "thag4.4:7.4": "mārakhittamhi bandhane.",
  "thag4.4:8.1": "Yesaṃ rāgo ca doso ca,",
  "thag4.4:8.2": "avijjā ca virājitā;",
  "thag4.4:8.3": "Tādī tattha na rajjanti,",
  "thag4.4:8.4": "chinnasuttā abandhanā”ti.",
  "thag4.4:9.1": "…",
  "thag4.4:9.2": "Nandako thero …."
}