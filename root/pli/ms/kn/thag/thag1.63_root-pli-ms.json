{
  "thag1.63:1.1": "Theragāthā",
  "thag1.63:2.1": "Ekakanipāta",
  "thag1.63:3.1": "Sattamavagga",
  "thag1.63:4.1": "1.63. Pakkhattheragāthā",
  "thag1.63:5.1": "“Cutā patanti patitā,",
  "thag1.63:5.2": "giddhā ca punarāgatā;",
  "thag1.63:5.3": "Kataṃ kiccaṃ rataṃ rammaṃ,",
  "thag1.63:5.4": "sukhenanvāgataṃ sukhan”ti.",
  "thag1.63:6.1": "…",
  "thag1.63:6.2": "Pakkho thero …."
}