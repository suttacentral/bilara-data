{
  "thag2.46:1.1": "Theragāthā",
  "thag2.46:2.1": "Dukanipāta",
  "thag2.46:3.1": "Pañcamavagga",
  "thag2.46:4.1": "2.46. Cūḷakattheragāthā",
  "thag2.46:5.1": "“Nadanti morā susikhā supekhuṇā,",
  "thag2.46:5.2": "Sunīlagīvā sumukhā sugajjino;",
  "thag2.46:5.3": "Susaddalā cāpi mahāmahī ayaṃ,",
  "thag2.46:5.4": "Subyāpitambu suvalāhakaṃ nabhaṃ.",
  "thag2.46:6.1": "Sukallarūpo sumanassa jhāyataṃ,",
  "thag2.46:6.2": "Sunikkamo sādhu subuddhasāsane;",
  "thag2.46:6.3": "Susukkasukkaṃ nipuṇaṃ sududdasaṃ,",
  "thag2.46:6.4": "Phusāhi taṃ uttamamaccutaṃ padan”ti.",
  "thag2.46:7.1": "…",
  "thag2.46:7.2": "Cūḷako thero …."
}