{
  "thag1.88:1.1": "Theragāthā",
  "thag1.88:2.1": "Ekakanipāta",
  "thag1.88:3.1": "Navamavagga",
  "thag1.88:4.1": "1.88. Ajjunattheragāthā",
  "thag1.88:5.1": "“Asakkhiṃ vata attānaṃ,",
  "thag1.88:5.2": "uddhātuṃ udakā thalaṃ;",
  "thag1.88:5.3": "Vuyhamāno mahogheva,",
  "thag1.88:5.4": "saccāni paṭivijjhahan”ti.",
  "thag1.88:6.1": "…",
  "thag1.88:6.2": "Ajjuno thero …."
}