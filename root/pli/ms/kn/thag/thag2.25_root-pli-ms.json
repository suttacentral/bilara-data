{
  "thag2.25:1.1": "Theragāthā",
  "thag2.25:2.1": "Dukanipāta",
  "thag2.25:3.1": "Tatiyavagga",
  "thag2.25:4.1": "2.25. Vītasokattheragāthā",
  "thag2.25:5.1": "“Kese me olikhissanti,",
  "thag2.25:5.2": "kappako upasaṅkami;",
  "thag2.25:5.3": "Tato ādāsamādāya,",
  "thag2.25:5.4": "sarīraṃ paccavekkhisaṃ.",
  "thag2.25:6.1": "Tuccho kāyo adissittha,",
  "thag2.25:6.2": "andhakāro tamo byagā;",
  "thag2.25:6.3": "Sabbe coḷā samucchinnā,",
  "thag2.25:6.4": "natthi dāni punabbhavo”ti.",
  "thag2.25:7.1": "…",
  "thag2.25:7.2": "Vītasoko thero …."
}