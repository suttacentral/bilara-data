{
  "thag1.47:1.1": "Theragāthā",
  "thag1.47:2.1": "Ekakanipāta",
  "thag1.47:3.1": "Pañcamavagga",
  "thag1.47:4.1": "1.47. Ujjayattheragāthā",
  "thag1.47:5.1": "“Namo te buddha vīratthu,",
  "thag1.47:5.2": "vippamuttosi sabbadhi;",
  "thag1.47:5.3": "Tuyhāpadāne viharaṃ,",
  "thag1.47:5.4": "viharāmi anāsavo”ti.",
  "thag1.47:6.1": "…",
  "thag1.47:6.2": "Ujjayo thero …."
}