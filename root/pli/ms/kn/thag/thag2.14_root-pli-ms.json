{
  "thag2.14:1.1": "Theragāthā",
  "thag2.14:2.1": "Dukanipāta",
  "thag2.14:3.1": "Dutiyavagga",
  "thag2.14:4.1": "2.14 Somamittattheragāthā",
  "thag2.14:5.1": "“Parittaṃ dārumāruyha,",
  "thag2.14:5.2": "yathā sīde mahaṇṇave;",
  "thag2.14:5.3": "Evaṃ kusītamāgamma,",
  "thag2.14:5.4": "sādhujīvīpi sīdati;",
  "thag2.14:5.5": "Tasmā taṃ parivajjeyya,",
  "thag2.14:5.6": "kusītaṃ hīnavīriyaṃ.",
  "thag2.14:6.1": "Pavivittehi ariyehi,",
  "thag2.14:6.2": "pahitattehi jhāyibhi;",
  "thag2.14:6.3": "Niccaṃ āraddhavīriyehi,",
  "thag2.14:6.4": "paṇḍitehi sahāvase”ti.",
  "thag2.14:7.1": "…",
  "thag2.14:7.2": "Somamitto thero …."
}