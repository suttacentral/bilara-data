{
  "thag1.64:1.1": "Theragāthā",
  "thag1.64:2.1": "Ekakanipāta",
  "thag1.64:3.1": "Sattamavagga",
  "thag1.64:4.1": "1.64. Vimalakoṇḍaññattheragāthā",
  "thag1.64:5.1": "“Dumavhayāya uppanno,",
  "thag1.64:5.2": "jāto paṇḍaraketunā;",
  "thag1.64:5.3": "Ketuhā ketunāyeva,",
  "thag1.64:5.4": "mahāketuṃ padhaṃsayī”ti.",
  "thag1.64:6.1": "…",
  "thag1.64:6.2": "Vimalakoṇḍañño thero …."
}