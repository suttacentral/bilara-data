{
  "thag1.80:1.1": "Theragāthā",
  "thag1.80:2.1": "Ekakanipāta",
  "thag1.80:3.1": "Aṭṭhamavagga",
  "thag1.80:4.1": "1.80. Uggattheragāthā",
  "thag1.80:5.1": "“Yaṃ mayā pakataṃ kammaṃ,",
  "thag1.80:5.2": "appaṃ vā yadi vā bahuṃ;",
  "thag1.80:5.3": "Sabbametaṃ parikkhīṇaṃ,",
  "thag1.80:5.4": "natthi dāni punabbhavo”ti.",
  "thag1.80:6.1": "…",
  "thag1.80:6.2": "Uggo thero ….",
  "thag1.80:6.3": "Vaggo aṭṭhamo.",
  "thag1.80:7.1": "Tassuddānaṃ",
  "thag1.80:8.1": "Vacchapālo ca yo thero,",
  "thag1.80:8.2": "ātumo māṇavo isi;",
  "thag1.80:8.3": "Suyāmano susārado,",
  "thag1.80:8.4": "thero yo ca piyañjaho;",
  "thag1.80:8.5": "Ārohaputto meṇḍasiro,",
  "thag1.80:8.6": "rakkhito uggasavhayoti."
}