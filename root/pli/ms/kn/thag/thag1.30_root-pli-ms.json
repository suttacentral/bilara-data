{
  "thag1.30:1.1": "Theragāthā",
  "thag1.30:2.1": "Ekakanipāta",
  "thag1.30:3.1": "Tatiyavagga",
  "thag1.30:4.1": "1.30. Uttiyattheragāthā",
  "thag1.30:5.1": "“Ābādhe me samuppanne,",
  "thag1.30:5.2": "sati me udapajjatha;",
  "thag1.30:5.3": "Ābādho me samuppanno,",
  "thag1.30:5.4": "kālo me nappamajjitun”ti.",
  "thag1.30:6.1": "…",
  "thag1.30:6.2": "Uttiyo thero ….",
  "thag1.30:6.3": "Vaggo tatiyo.",
  "thag1.30:7.1": "Tassuddānaṃ",
  "thag1.30:8.1": "Nigrodho cittako thero,",
  "thag1.30:8.2": "gosālathero sugandho;",
  "thag1.30:8.3": "Nandiyo abhayo thero,",
  "thag1.30:8.4": "thero lomasakaṅgiyo;",
  "thag1.30:8.5": "Jambugāmikaputto ca,",
  "thag1.30:8.6": "hārito uttiyo isīti."
}