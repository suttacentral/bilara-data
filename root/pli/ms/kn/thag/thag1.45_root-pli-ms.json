{
  "thag1.45:1.1": "Theragāthā",
  "thag1.45:2.1": "Ekakanipāta",
  "thag1.45:3.1": "Pañcamavagga",
  "thag1.45:4.1": "1.45. Ramaṇīyavihārittheragāthā",
  "thag1.45:5.1": "“Yathāpi bhaddo ājañño,",
  "thag1.45:5.2": "khalitvā patitiṭṭhati;",
  "thag1.45:5.3": "Evaṃ dassanasampannaṃ,",
  "thag1.45:5.4": "sammāsambuddhasāvakan”ti.",
  "thag1.45:6.1": "…",
  "thag1.45:6.2": "Ramaṇīyavihāritthero …."
}