{
  "thag1.65:1.1": "Theragāthā",
  "thag1.65:2.1": "Ekakanipāta",
  "thag1.65:3.1": "Sattamavagga",
  "thag1.65:4.1": "1.65 Ukkhepakatavacchattheragāthā",
  "thag1.65:5.1": "“Ukkhepakatavacchassa,",
  "thag1.65:5.2": "Saṅkalitaṃ bahūhi vassehi;",
  "thag1.65:5.3": "Taṃ bhāsati gahaṭṭhānaṃ,",
  "thag1.65:5.4": "Sunisinno uḷārapāmojjo”ti.",
  "thag1.65:6.1": "…",
  "thag1.65:6.2": "Ukkhepakatavaccho thero …."
}