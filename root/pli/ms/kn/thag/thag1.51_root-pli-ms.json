{
  "thag1.51:1.1": "Theragāthā",
  "thag1.51:2.1": "Ekakanipāta",
  "thag1.51:3.1": "Chaṭṭhavagga",
  "thag1.51:4.1": "1.51. Godhikattheragāthā",
  "thag1.51:5.1": "“Vassati devo yathā sugītaṃ,",
  "thag1.51:5.2": "Channā me kuṭikā sukhā nivātā;",
  "thag1.51:5.3": "Cittaṃ susamāhitañca mayhaṃ,",
  "thag1.51:5.4": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.51:6.1": "…",
  "thag1.51:6.2": "Godhiko thero …."
}