{
  "thag1.12:1.1": "Theragāthā",
  "thag1.12:2.1": "Ekakanipāta",
  "thag1.12:3.1": "Dutiyavagga",
  "thag1.12:4.1": "1.12. Mahāvacchattheragāthā",
  "thag1.12:5.1": "“Paññābalī sīlavatūpapanno,",
  "thag1.12:5.2": "Samāhito jhānarato satīmā;",
  "thag1.12:5.3": "Yadatthiyaṃ bhojanaṃ bhuñjamāno,",
  "thag1.12:5.4": "Kaṅkhetha kālaṃ idha vītarāgo”ti.",
  "thag1.12:6.1": "…",
  "thag1.12:6.2": "Mahāvaccho thero …."
}