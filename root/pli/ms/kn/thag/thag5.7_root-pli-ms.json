{
  "thag5.7:1.1": "Theragāthā",
  "thag5.7:2.1": "Pañcakanipāta",
  "thag5.7:3.1": "Paṭhamavagga",
  "thag5.7:4.1": "5.7. Gayākassapattheragāthā",
  "thag5.7:5.1": "“Pāto majjhanhikaṃ sāyaṃ,",
  "thag5.7:5.2": "tikkhattuṃ divasassahaṃ;",
  "thag5.7:5.3": "Otariṃ udakaṃ sohaṃ,",
  "thag5.7:5.4": "gayāya gayaphagguyā.",
  "thag5.7:6.1": "‘Yaṃ mayā pakataṃ pāpaṃ,",
  "thag5.7:6.2": "pubbe aññāsu jātisu;",
  "thag5.7:6.3": "Taṃ dānīdha pavāhemi’,",
  "thag5.7:6.4": "evaṃdiṭṭhi pure ahuṃ.",
  "thag5.7:7.1": "Sutvā subhāsitaṃ vācaṃ,",
  "thag5.7:7.2": "dhammatthasahitaṃ padaṃ;",
  "thag5.7:7.3": "Tathaṃ yāthāvakaṃ atthaṃ,",
  "thag5.7:7.4": "yoniso paccavekkhisaṃ.",
  "thag5.7:8.1": "Ninhātasabbapāpomhi,",
  "thag5.7:8.2": "nimmalo payato suci;",
  "thag5.7:8.3": "Suddho suddhassa dāyādo,",
  "thag5.7:8.4": "putto buddhassa oraso.",
  "thag5.7:9.1": "Ogayhaṭṭhaṅgikaṃ sotaṃ,",
  "thag5.7:9.2": "sabbapāpaṃ pavāhayiṃ;",
  "thag5.7:9.3": "Tisso vijjā ajjhagamiṃ,",
  "thag5.7:9.4": "kataṃ buddhassa sāsanan”ti.",
  "thag5.7:10.1": "…",
  "thag5.7:10.2": "Gayākassapo thero …."
}