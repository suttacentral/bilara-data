{
  "thag1.52:1.1": "Theragāthā",
  "thag1.52:2.1": "Ekakanipāta",
  "thag1.52:3.1": "Chaṭṭhavagga",
  "thag1.52:4.1": "1.52. Subāhuttheragāthā",
  "thag1.52:5.1": "“Vassati devo yathā sugītaṃ,",
  "thag1.52:5.2": "Channā me kuṭikā sukhā nivātā;",
  "thag1.52:5.3": "Cittaṃ susamāhitañca kāye,",
  "thag1.52:5.4": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.52:6.1": "…",
  "thag1.52:6.2": "Subāhutthero …."
}