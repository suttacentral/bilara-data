{
  "thag4.5:1.1": "Theragāthā",
  "thag4.5:2.1": "Catukkanipāta",
  "thag4.5:3.1": "Paṭhamavagga",
  "thag4.5:4.1": "4.5. Jambukattheragāthā",
  "thag4.5:5.1": "“Pañcapaññāsavassāni,",
  "thag4.5:5.2": "rajojallamadhārayiṃ;",
  "thag4.5:5.3": "Bhuñjanto māsikaṃ bhattaṃ,",
  "thag4.5:5.4": "kesamassuṃ alocayiṃ.",
  "thag4.5:6.1": "Ekapādena aṭṭhāsiṃ,",
  "thag4.5:6.2": "āsanaṃ parivajjayiṃ;",
  "thag4.5:6.3": "Sukkhagūthāni ca khādiṃ,",
  "thag4.5:6.4": "uddesañca na sādiyiṃ.",
  "thag4.5:7.1": "Etādisaṃ karitvāna,",
  "thag4.5:7.2": "bahuṃ duggatigāminaṃ;",
  "thag4.5:7.3": "Vuyhamāno mahoghena,",
  "thag4.5:7.4": "buddhaṃ saraṇamāgamaṃ.",
  "thag4.5:8.1": "Saraṇagamanaṃ passa,",
  "thag4.5:8.2": "passa dhammasudhammataṃ;",
  "thag4.5:8.3": "Tisso vijjā anuppattā,",
  "thag4.5:8.4": "kataṃ buddhassa sāsanan”ti.",
  "thag4.5:9.1": "…",
  "thag4.5:9.2": "Jambuko thero …."
}