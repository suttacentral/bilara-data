{
  "thag1.25:1.1": "Theragāthā",
  "thag1.25:2.1": "Ekakanipāta",
  "thag1.25:3.1": "Tatiyavagga",
  "thag1.25:4.1": "1.25. Nandiyattheragāthā",
  "thag1.25:5.1": "“Obhāsajātaṃ phalagaṃ,",
  "thag1.25:5.2": "cittaṃ yassa abhiṇhaso;",
  "thag1.25:5.3": "Tādisaṃ bhikkhumāsajja,",
  "thag1.25:5.4": "kaṇha dukkhaṃ nigacchasī”ti.",
  "thag1.25:6.1": "…",
  "thag1.25:6.2": "Nandiyo thero …."
}