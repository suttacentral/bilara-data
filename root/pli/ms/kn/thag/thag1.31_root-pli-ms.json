{
  "thag1.31:1.1": "Theragāthā",
  "thag1.31:2.1": "Ekakanipāta",
  "thag1.31:3.1": "Catutthavagga",
  "thag1.31:4.1": "1.31 Gahvaratīriyattheragāthā",
  "thag1.31:5.1": "“Phuṭṭho ḍaṃsehi makasehi,",
  "thag1.31:5.2": "araññasmiṃ brahāvane;",
  "thag1.31:5.3": "Nāgo saṃgāmasīseva,",
  "thag1.31:5.4": "sato tatrādhivāsaye”ti.",
  "thag1.31:6.1": "…",
  "thag1.31:6.2": "Gahvaratīriyo thero …."
}