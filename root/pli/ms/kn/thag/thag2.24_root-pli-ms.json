{
  "thag2.24:1.1": "Theragāthā",
  "thag2.24:2.1": "Dukanipāta",
  "thag2.24:3.1": "Tatiyavagga",
  "thag2.24:4.1": "2.24. Valliyattheragāthā",
  "thag2.24:5.1": "“Yaṃ kiccaṃ daḷhavīriyena,",
  "thag2.24:5.2": "yaṃ kiccaṃ boddhumicchatā;",
  "thag2.24:5.3": "Karissaṃ nāvarajjhissaṃ,",
  "thag2.24:5.4": "passa vīriyaṃ parakkamaṃ.",
  "thag2.24:6.1": "Tvañca me maggamakkhāhi,",
  "thag2.24:6.2": "añjasaṃ amatogadhaṃ;",
  "thag2.24:6.3": "Ahaṃ monena monissaṃ,",
  "thag2.24:6.4": "gaṅgāsotova sāgaran”ti.",
  "thag2.24:7.1": "…",
  "thag2.24:7.2": "Valliyo thero …."
}