{
  "thag1.16:1.1": "Theragāthā",
  "thag1.16:2.1": "Ekakanipāta",
  "thag1.16:3.1": "Dutiyavagga",
  "thag1.16:4.1": "1.16 Belaṭṭhasīsattheragāthā",
  "thag1.16:5.1": "“Yathāpi bhaddo ājañño,",
  "thag1.16:5.2": "naṅgalāvattanī sikhī;",
  "thag1.16:5.3": "Gacchati appakasirena,",
  "thag1.16:5.4": "evaṃ rattindivā mama;",
  "thag1.16:5.5": "Gacchanti appakasirena,",
  "thag1.16:5.6": "sukhe laddhe nirāmise”ti.",
  "thag1.16:6.1": "…",
  "thag1.16:6.2": "Belaṭṭhasīso thero …."
}