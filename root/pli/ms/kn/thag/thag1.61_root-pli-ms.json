{
  "thag1.61:1.1": "Theragāthā",
  "thag1.61:2.1": "Ekakanipāta",
  "thag1.61:3.1": "Sattamavagga",
  "thag1.61:4.1": "1.61. Vappattheragāthā",
  "thag1.61:5.1": "“Passati passo passantaṃ,",
  "thag1.61:5.2": "apassantañca passati;",
  "thag1.61:5.3": "Apassanto apassantaṃ,",
  "thag1.61:5.4": "passantañca na passatī”ti.",
  "thag1.61:6.1": "…",
  "thag1.61:6.2": "Vappo thero …."
}