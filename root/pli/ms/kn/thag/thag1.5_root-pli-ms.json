{
  "thag1.5:1.1": "Theragāthā",
  "thag1.5:2.1": "Ekakanipāta",
  "thag1.5:3.1": "Paṭhamavagga",
  "thag1.5:4.1": "1.5. Dabbattheragāthā",
  "thag1.5:5.1": "“Yo duddamiyo damena danto,",
  "thag1.5:5.2": "Dabbo santusito vitiṇṇakaṅkho;",
  "thag1.5:5.3": "Vijitāvī apetabheravo hi,",
  "thag1.5:5.4": "Dabbo so parinibbuto ṭhitatto”ti.",
  "thag1.5:6.1": "Itthaṃ sudaṃ āyasmā dabbo thero gāthaṃ abhāsitthāti."
}