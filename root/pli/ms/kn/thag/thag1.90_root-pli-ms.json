{
  "thag1.90:1.1": "Theragāthā",
  "thag1.90:2.1": "Ekakanipāta",
  "thag1.90:3.1": "Navamavagga",
  "thag1.90:4.1": "1.90. Sāmidattattheragāthā",
  "thag1.90:5.1": "“Pañcakkhandhā pariññātā,",
  "thag1.90:5.2": "tiṭṭhanti chinnamūlakā;",
  "thag1.90:5.3": "Vikkhīṇo jātisaṃsāro,",
  "thag1.90:5.4": "natthi dāni punabbhavo”ti.",
  "thag1.90:6.1": "…",
  "thag1.90:6.2": "Sāmidatto thero ….",
  "thag1.90:6.3": "Vaggo navamo.",
  "thag1.90:7.1": "Tassuddānaṃ",
  "thag1.90:8.1": "Thero samitigutto ca,",
  "thag1.90:8.2": "kassapo sīhasavhayo;",
  "thag1.90:8.3": "Nīto sunāgo nāgito,",
  "thag1.90:8.4": "paviṭṭho ajjuno isi;",
  "thag1.90:8.5": "Devasabho ca yo thero,",
  "thag1.90:8.6": "sāmidatto mahabbaloti."
}