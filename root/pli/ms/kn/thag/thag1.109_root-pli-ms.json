{
  "thag1.109:1.1": "Theragāthā",
  "thag1.109:2.1": "Ekakanipāta",
  "thag1.109:3.1": "Ekādasamavagga",
  "thag1.109:4.1": "1.109. Saṃgharakkhitattheragāthā",
  "thag1.109:5.1": "“Na nūnāyaṃ paramahitānukampino,",
  "thag1.109:5.2": "Rahogato anuvigaṇeti sāsanaṃ;",
  "thag1.109:5.3": "Tathāhayaṃ viharati pākatindriyo,",
  "thag1.109:5.4": "Migī yathā taruṇajātikā vane”ti.",
  "thag1.109:6.1": "…",
  "thag1.109:6.2": "Saṃgharakkhito thero …."
}