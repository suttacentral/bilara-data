{
  "thag1.19:1.1": "Theragāthā",
  "thag1.19:2.1": "Ekakanipāta",
  "thag1.19:3.1": "Dutiyavagga",
  "thag1.19:4.1": "1.19 Kulattheragāthā",
  "thag1.19:5.1": "“Udakañhi nayanti nettikā,",
  "thag1.19:5.2": "Usukārā namayanti tejanaṃ;",
  "thag1.19:5.3": "Dāruṃ namayanti tacchakā,",
  "thag1.19:5.4": "Attānaṃ damayanti subbatā”ti.",
  "thag1.19:6.1": "…",
  "thag1.19:6.2": "Kulo thero …."
}