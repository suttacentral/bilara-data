{
  "thag6.4:1.1": "Theragāthā",
  "thag6.4:2.1": "Chakkanipāta",
  "thag6.4:3.1": "Paṭhamavagga",
  "thag6.4:4.1": "6.4. Kullattheragāthā",
  "thag6.4:5.1": "“Kullo sivathikaṃ gantvā,",
  "thag6.4:5.2": "addasa itthimujjhitaṃ;",
  "thag6.4:5.3": "Apaviddhaṃ susānasmiṃ,",
  "thag6.4:5.4": "khajjantiṃ kimihī phuṭaṃ.",
  "thag6.4:6.1": "Āturaṃ asuciṃ pūtiṃ,",
  "thag6.4:6.2": "passa kulla samussayaṃ;",
  "thag6.4:6.3": "Uggharantaṃ paggharantaṃ,",
  "thag6.4:6.4": "bālānaṃ abhinanditaṃ.",
  "thag6.4:7.1": "Dhammādāsaṃ gahetvāna,",
  "thag6.4:7.2": "ñāṇadassanapattiyā;",
  "thag6.4:7.3": "Paccavekkhiṃ imaṃ kāyaṃ,",
  "thag6.4:7.4": "tucchaṃ santarabāhiraṃ.",
  "thag6.4:8.1": "Yathā idaṃ tathā etaṃ,",
  "thag6.4:8.2": "yathā etaṃ tathā idaṃ;",
  "thag6.4:8.3": "Yathā adho tathā uddhaṃ,",
  "thag6.4:8.4": "yathā uddhaṃ tathā adho.",
  "thag6.4:9.1": "Yathā divā tathā rattiṃ,",
  "thag6.4:9.2": "yathā rattiṃ tathā divā;",
  "thag6.4:9.3": "Yathā pure tathā pacchā,",
  "thag6.4:9.4": "yathā pacchā tathā pure.",
  "thag6.4:10.1": "Pañcaṅgikena turiyena,",
  "thag6.4:10.2": "Na ratī hoti tādisī;",
  "thag6.4:10.3": "Yathā ekaggacittassa,",
  "thag6.4:10.4": "Sammā dhammaṃ vipassato”ti.",
  "thag6.4:11.1": "…",
  "thag6.4:11.2": "Kullo thero …."
}