{
  "thag1.44:1.1": "Theragāthā",
  "thag1.44:2.1": "Ekakanipāta",
  "thag1.44:3.1": "Pañcamavagga",
  "thag1.44:4.1": "1.44. Sānuttheragāthā",
  "thag1.44:5.1": "“Mataṃ vā amma rodanti,",
  "thag1.44:5.2": "Yo vā jīvaṃ na dissati;",
  "thag1.44:5.3": "Jīvantaṃ maṃ amma passantī,",
  "thag1.44:5.4": "Kasmā maṃ amma rodasī”ti.",
  "thag1.44:6.1": "…",
  "thag1.44:6.2": "Sānutthero …."
}