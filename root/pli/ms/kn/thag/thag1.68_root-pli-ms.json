{
  "thag1.68:1.1": "Theragāthā",
  "thag1.68:2.1": "Ekakanipāta",
  "thag1.68:3.1": "Sattamavagga",
  "thag1.68:4.1": "1.68. Ekudāniyattheragāthā",
  "thag1.68:5.1": "“Adhicetaso appamajjato,",
  "thag1.68:5.2": "Munino monapathesu sikkhato;",
  "thag1.68:5.3": "Sokā na bhavanti tādino,",
  "thag1.68:5.4": "Upasantassa sadā satīmato”ti.",
  "thag1.68:6.1": "…",
  "thag1.68:6.2": "Ekudāniyo thero …."
}