{
  "thag4.6:1.1": "Theragāthā",
  "thag4.6:2.1": "Catukkanipāta",
  "thag4.6:3.1": "Paṭhamavagga",
  "thag4.6:4.1": "4.6. Senakattheragāthā",
  "thag4.6:5.1": "“Svāgataṃ vata me āsi,",
  "thag4.6:5.2": "gayāyaṃ gayaphagguyā;",
  "thag4.6:5.3": "Yaṃ addasāsiṃ sambuddhaṃ,",
  "thag4.6:5.4": "desentaṃ dhammamuttamaṃ.",
  "thag4.6:6.1": "Mahappabhaṃ gaṇācariyaṃ,",
  "thag4.6:6.2": "aggapattaṃ vināyakaṃ;",
  "thag4.6:6.3": "Sadevakassa lokassa,",
  "thag4.6:6.4": "jinaṃ atuladassanaṃ.",
  "thag4.6:7.1": "Mahānāgaṃ mahāvīraṃ,",
  "thag4.6:7.2": "mahājutimanāsavaṃ;",
  "thag4.6:7.3": "Sabbāsavaparikkhīṇaṃ,",
  "thag4.6:7.4": "satthāramakutobhayaṃ.",
  "thag4.6:8.1": "Cirasaṃkiliṭṭhaṃ vata maṃ,",
  "thag4.6:8.2": "diṭṭhisandānabandhitaṃ;",
  "thag4.6:8.3": "Vimocayi so bhagavā,",
  "thag4.6:8.4": "sabbaganthehi senakan”ti.",
  "thag4.6:9.1": "…",
  "thag4.6:9.2": "Senako thero …."
}