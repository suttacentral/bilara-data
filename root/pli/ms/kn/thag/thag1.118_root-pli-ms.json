{
  "thag1.118:1.1": "Theragāthā",
  "thag1.118:2.1": "Ekakanipāta",
  "thag1.118:3.1": "Dvādasamavagga",
  "thag1.118:4.1": "1.118 Kimilattheragāthā",
  "thag1.118:5.1": "“Abhisattova nipatati,",
  "thag1.118:5.2": "Vayo rūpaṃ aññamiva tatheva santaṃ;",
  "thag1.118:5.3": "Tasseva sato avippavasato,",
  "thag1.118:5.4": "Aññasseva sarāmi attānan”ti.",
  "thag1.118:6.1": "…",
  "thag1.118:6.2": "Kimilo thero …."
}