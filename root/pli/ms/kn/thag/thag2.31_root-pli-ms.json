{
  "thag2.31:1.1": "Theragāthā",
  "thag2.31:2.1": "Dukanipāta",
  "thag2.31:3.1": "Catutthavagga",
  "thag2.31:4.1": "2.31. Migasirattheragāthā",
  "thag2.31:5.1": "“Yato ahaṃ pabbajito,",
  "thag2.31:5.2": "sammāsambuddhasāsane;",
  "thag2.31:5.3": "Vimuccamāno uggacchiṃ,",
  "thag2.31:5.4": "kāmadhātuṃ upaccagaṃ.",
  "thag2.31:6.1": "Brahmuno pekkhamānassa,",
  "thag2.31:6.2": "tato cittaṃ vimucci me;",
  "thag2.31:6.3": "Akuppā me vimuttīti,",
  "thag2.31:6.4": "sabbasaṃyojanakkhayā”ti.",
  "thag2.31:7.1": "…",
  "thag2.31:7.2": "Migasiro thero …."
}