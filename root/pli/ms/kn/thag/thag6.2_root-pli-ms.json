{
  "thag6.2:1.1": "Theragāthā",
  "thag6.2:2.1": "Chakkanipāta",
  "thag6.2:3.1": "Paṭhamavagga",
  "thag6.2:4.1": "6.2. Tekicchakārittheragāthā",
  "thag6.2:5.1": "“Atihitā vīhi,",
  "thag6.2:5.2": "khalagatā sālī;",
  "thag6.2:5.3": "Na ca labhe piṇḍaṃ,",
  "thag6.2:5.4": "kathamahaṃ kassaṃ”.",
  "thag6.2:6.1": "“Buddhamappameyyaṃ anussara pasanno,",
  "thag6.2:6.2": "Pītiyā phuṭasarīro hohisi satatamudaggo.",
  "thag6.2:7.1": "Dhammamappameyyaṃ anussara pasanno,",
  "thag6.2:7.2": "Pītiyā phuṭasarīro hohisi satatamudaggo.",
  "thag6.2:8.1": "Saṃghamappameyyaṃ anussara pasanno,",
  "thag6.2:8.2": "Pītiyā phuṭasarīro hohisi satatamudaggo”.",
  "thag6.2:9.1": "“Abbhokāse viharasi,",
  "thag6.2:9.2": "Sītā hemantikā imā ratyo;",
  "thag6.2:9.3": "Mā sītena pareto vihaññittho,",
  "thag6.2:9.4": "Pavisa tvaṃ vihāraṃ phusitaggaḷaṃ”.",
  "thag6.2:10.1": "“Phusissaṃ catasso appamaññāyo,",
  "thag6.2:10.2": "Tāhi ca sukhito viharissaṃ;",
  "thag6.2:10.3": "Nāhaṃ sītena vihaññissaṃ,",
  "thag6.2:10.4": "Aniñjito viharanto”ti.",
  "thag6.2:11.1": "…",
  "thag6.2:11.2": "Tekicchakārī thero …."
}