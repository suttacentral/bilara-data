{
  "thag2.30:1.1": "Theragāthā",
  "thag2.30:2.1": "Dukanipāta",
  "thag2.30:3.1": "Tatiyavagga",
  "thag2.30:4.1": "2.30. Kaṇhadinnattheragāthā",
  "thag2.30:5.1": "“Upāsitā sappurisā,",
  "thag2.30:5.2": "sutā dhammā abhiṇhaso;",
  "thag2.30:5.3": "Sutvāna paṭipajjissaṃ,",
  "thag2.30:5.4": "añjasaṃ amatogadhaṃ.",
  "thag2.30:6.1": "Bhavarāgahatassa me sato,",
  "thag2.30:6.2": "Bhavarāgo puna me na vijjati;",
  "thag2.30:6.3": "Na cāhu na ca me bhavissati,",
  "thag2.30:6.4": "Na ca me etarahi vijjatī”ti.",
  "thag2.30:7.1": "…",
  "thag2.30:7.2": "Kaṇhadinno thero ….",
  "thag2.30:7.3": "Vaggo tatiyo.",
  "thag2.30:8.1": "Tassuddānaṃ",
  "thag2.30:9.1": "Uttaro bhaddajitthero,",
  "thag2.30:9.2": "sobhito valliyo isi;",
  "thag2.30:9.3": "Vītasoko ca yo thero,",
  "thag2.30:9.4": "puṇṇamāso ca nandako;",
  "thag2.30:9.5": "Bharato bhāradvājo ca,",
  "thag2.30:9.6": "kaṇhadinno mahāmunīti."
}