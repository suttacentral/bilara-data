{
  "thag2.41:1.1": "Theragāthā",
  "thag2.41:2.1": "Dukanipāta",
  "thag2.41:3.1": "Pañcamavagga",
  "thag2.41:4.1": "2.41. Kumārakassapattheragāthā",
  "thag2.41:5.1": "“Aho buddhā aho dhammā,",
  "thag2.41:5.2": "aho no satthu sampadā;",
  "thag2.41:5.3": "Yattha etādisaṃ dhammaṃ,",
  "thag2.41:5.4": "sāvako sacchikāhiti.",
  "thag2.41:6.1": "Asaṅkheyyesu kappesu,",
  "thag2.41:6.2": "sakkāyādhigatā ahū;",
  "thag2.41:6.3": "Tesamayaṃ pacchimako,",
  "thag2.41:6.4": "carimoyaṃ samussayo;",
  "thag2.41:6.5": "Jātimaraṇasaṃsāro,",
  "thag2.41:6.6": "natthi dāni punabbhavo”ti.",
  "thag2.41:7.1": "…",
  "thag2.41:7.2": "Kumārakassapo thero …."
}