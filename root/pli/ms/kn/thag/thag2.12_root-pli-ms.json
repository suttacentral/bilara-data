{
  "thag2.12:1.1": "Theragāthā",
  "thag2.12:2.1": "Dukanipāta",
  "thag2.12:3.1": "Dutiyavagga",
  "thag2.12:4.1": "2.12. Jotidāsattheragāthā",
  "thag2.12:5.1": "“Ye kho te veṭhamissena,",
  "thag2.12:5.2": "nānattena ca kammunā;",
  "thag2.12:5.3": "Manusse uparundhanti,",
  "thag2.12:5.4": "pharusūpakkamā janā;",
  "thag2.12:5.5": "Tepi tattheva kīranti,",
  "thag2.12:5.6": "na hi kammaṃ panassati.",
  "thag2.12:6.1": "Yaṃ karoti naro kammaṃ,",
  "thag2.12:6.2": "Kalyāṇaṃ yadi pāpakaṃ;",
  "thag2.12:6.3": "Tassa tasseva dāyādo,",
  "thag2.12:6.4": "Yaṃ yaṃ kammaṃ pakubbatī”ti.",
  "thag2.12:7.1": "…",
  "thag2.12:7.2": "Jotidāso thero …."
}