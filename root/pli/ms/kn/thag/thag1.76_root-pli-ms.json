{
  "thag1.76:1.1": "Theragāthā",
  "thag1.76:2.1": "Ekakanipāta",
  "thag1.76:3.1": "Aṭṭhamavagga",
  "thag1.76:4.1": "1.76. Piyañjahattheragāthā",
  "thag1.76:5.1": "“Uppatantesu nipate,",
  "thag1.76:5.2": "nipatantesu uppate;",
  "thag1.76:5.3": "Vase avasamānesu,",
  "thag1.76:5.4": "ramamānesu no rame”ti.",
  "thag1.76:6.1": "…",
  "thag1.76:6.2": "Piyañjaho thero …."
}