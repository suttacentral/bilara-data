{
  "thag1.84:1.1": "Theragāthā",
  "thag1.84:2.1": "Ekakanipāta",
  "thag1.84:3.1": "Navamavagga",
  "thag1.84:4.1": "1.84. Nītattheragāthā",
  "thag1.84:5.1": "“Sabbarattiṃ supitvāna,",
  "thag1.84:5.2": "divā saṅgaṇike rato;",
  "thag1.84:5.3": "Kudāssu nāma dummedho,",
  "thag1.84:5.4": "dukkhassantaṃ karissatī”ti.",
  "thag1.84:6.1": "…",
  "thag1.84:6.2": "Nīto thero …."
}