{
  "thag1.102:1.1": "Theragāthā",
  "thag1.102:2.1": "Ekakanipāta",
  "thag1.102:3.1": "Ekādasamavagga",
  "thag1.102:4.1": "1.102. Setucchattheragāthā",
  "thag1.102:5.1": "“Mānena vañcitāse,",
  "thag1.102:5.2": "Saṅkhāresu saṅkilissamānāse;",
  "thag1.102:5.3": "Lābhālābhena mathitā,",
  "thag1.102:5.4": "Samādhiṃ nādhigacchantī”ti.",
  "thag1.102:6.1": "…",
  "thag1.102:6.2": "Setuccho thero …."
}