{
  "thag2.9:1.1": "Theragāthā",
  "thag2.9:2.1": "Dukanipāta",
  "thag2.9:3.1": "Paṭhamavagga",
  "thag2.9:4.1": "2.9. Gotamattheragāthā",
  "thag2.9:5.1": "“Sukhaṃ supanti munayo,",
  "thag2.9:5.2": "ye itthīsu na bajjhare;",
  "thag2.9:5.3": "Sadā ve rakkhitabbāsu,",
  "thag2.9:5.4": "yāsu saccaṃ sudullabhaṃ.",
  "thag2.9:6.1": "Vadhaṃ carimha te kāma,",
  "thag2.9:6.2": "anaṇā dāni te mayaṃ;",
  "thag2.9:6.3": "Gacchāma dāni nibbānaṃ,",
  "thag2.9:6.4": "yattha gantvā na socatī”ti.",
  "thag2.9:7.1": "…",
  "thag2.9:7.2": "Gotamo thero …."
}