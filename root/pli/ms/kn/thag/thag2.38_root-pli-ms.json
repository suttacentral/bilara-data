{
  "thag2.38:1.1": "Theragāthā",
  "thag2.38:2.1": "Dukanipāta",
  "thag2.38:3.1": "Catutthavagga",
  "thag2.38:4.1": "2.38. Nisabhattheragāthā",
  "thag2.38:5.1": "“Pañca kāmaguṇe hitvā,",
  "thag2.38:5.2": "Piyarūpe manorame;",
  "thag2.38:5.3": "Saddhāya gharā nikkhamma,",
  "thag2.38:5.4": "Dukkhassantakaro bhave.",
  "thag2.38:6.1": "Nābhinandāmi maraṇaṃ,",
  "thag2.38:6.2": "nābhinandāmi jīvitaṃ;",
  "thag2.38:6.3": "Kālañca paṭikaṅkhāmi,",
  "thag2.38:6.4": "sampajāno patissato”ti.",
  "thag2.38:7.1": "…",
  "thag2.38:7.2": "Nisabho thero …."
}