{
  "thag6.14:1.1": "Theragāthā",
  "thag6.14:2.1": "Chakkanipāta",
  "thag6.14:3.1": "Paṭhamavagga",
  "thag6.14:4.1": "6.14. Sabbakāmittheragāthā",
  "thag6.14:5.1": "“Dvipādakoyaṃ asuci,",
  "thag6.14:5.2": "duggandho parihīrati;",
  "thag6.14:5.3": "Nānākuṇapaparipūro,",
  "thag6.14:5.4": "vissavanto tato tato.",
  "thag6.14:6.1": "Migaṃ nilīnaṃ kūṭena,",
  "thag6.14:6.2": "baḷiseneva ambujaṃ;",
  "thag6.14:6.3": "Vānaraṃ viya lepena,",
  "thag6.14:6.4": "bādhayanti puthujjanaṃ.",
  "thag6.14:7.1": "Rūpā saddā rasā gandhā,",
  "thag6.14:7.2": "phoṭṭhabbā ca manoramā;",
  "thag6.14:7.3": "Pañca kāmaguṇā ete,",
  "thag6.14:7.4": "itthirūpasmi dissare.",
  "thag6.14:8.1": "Ye etā upasevanti,",
  "thag6.14:8.2": "rattacittā puthujjanā;",
  "thag6.14:8.3": "Vaḍḍhenti kaṭasiṃ ghoraṃ,",
  "thag6.14:8.4": "ācinanti punabbhavaṃ.",
  "thag6.14:9.1": "Yo cetā parivajjeti,",
  "thag6.14:9.2": "sappasseva padā siro;",
  "thag6.14:9.3": "Somaṃ visattikaṃ loke,",
  "thag6.14:9.4": "sato samativattati.",
  "thag6.14:10.1": "Kāmesvādīnavaṃ disvā,",
  "thag6.14:10.2": "nekkhammaṃ daṭṭhu khemato;",
  "thag6.14:10.3": "Nissaṭo sabbakāmehi,",
  "thag6.14:10.4": "patto me āsavakkhayo”ti.",
  "thag6.14:11.1": "…",
  "thag6.14:11.2": "Sabbakāmitthero ….",
  "thag6.14:12.1": "Chakkanipāto niṭṭhito.",
  "thag6.14:13.1": "Tatruddānaṃ",
  "thag6.14:14.1": "Uruveḷakassapo ca,",
  "thag6.14:14.2": "thero tekicchakāri ca;",
  "thag6.14:14.3": "Mahānāgo ca kullo ca,",
  "thag6.14:14.4": "mālukyo sappadāsako;",
  "thag6.14:14.5": "Kātiyāno migajālo,",
  "thag6.14:14.6": "jento sumanasavhayo;",
  "thag6.14:14.7": "Nhātamuni brahmadatto,",
  "thag6.14:14.8": "sirimaṇḍo sabbakāmī ca;",
  "thag6.14:14.9": "Gāthāyo caturāsīti,",
  "thag6.14:14.10": "therā cettha catuddasāti."
}