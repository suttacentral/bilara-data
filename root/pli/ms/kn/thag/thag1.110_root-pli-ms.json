{
  "thag1.110:1.1": "Theragāthā",
  "thag1.110:2.1": "Ekakanipāta",
  "thag1.110:3.1": "Ekādasamavagga",
  "thag1.110:4.1": "1.110. Usabhattheragāthā",
  "thag1.110:5.1": "“Nagā nagaggesu susaṃvirūḷhā,",
  "thag1.110:5.2": "Udaggameghena navena sittā;",
  "thag1.110:5.3": "Vivekakāmassa araññasaññino,",
  "thag1.110:5.4": "Janeti bhiyyo usabhassa kalyatan”ti.",
  "thag1.110:6.1": "…",
  "thag1.110:6.2": "Usabho thero ….",
  "thag1.110:6.3": "Vaggo ekādasamo.",
  "thag1.110:7.1": "Tassuddānaṃ",
  "thag1.110:8.1": "Belaṭṭhāniko setuccho,",
  "thag1.110:8.2": "Bandhuro khitako isi;",
  "thag1.110:8.3": "Malitavambho suhemanto,",
  "thag1.110:8.4": "Dhammasavo dhammasavapitā;",
  "thag1.110:8.5": "Saṃgharakkhitatthero ca,",
  "thag1.110:8.6": "Usabho ca mahāmunīti."
}