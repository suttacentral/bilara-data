{
  "thag1.1:1.1": "Theragāthā",
  "thag1.1:2.1": "Ekakanipāta",
  "thag1.1:3.1": "Paṭhamavagga",
  "thag1.1:4.1": "1.1. Subhūtittheragāthā",
  "thag1.1:5.1": "Nidānagāthā",
  "thag1.1:6.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "thag1.1:7.1": "Sīhānaṃva nadantānaṃ,",
  "thag1.1:7.2": "dāṭhīnaṃ girigabbhare;",
  "thag1.1:7.3": "Suṇātha bhāvitattānaṃ,",
  "thag1.1:7.4": "gāthā atthūpanāyikā.",
  "thag1.1:8.1": "Yathānāmā yathāgottā,",
  "thag1.1:8.2": "yathādhammavihārino;",
  "thag1.1:8.3": "Yathādhimuttā sappaññā,",
  "thag1.1:8.4": "vihariṃsu atanditā.",
  "thag1.1:9.1": "Tattha tattha vipassitvā,",
  "thag1.1:9.2": "phusitvā accutaṃ padaṃ;",
  "thag1.1:9.3": "Katantaṃ paccavekkhantā,",
  "thag1.1:9.4": "imamatthamabhāsisuṃ.",
  "thag1.1:10.1": "Subhūtittheragāthā",
  "thag1.1:11.1": "“Channā me kuṭikā sukhā nivātā,",
  "thag1.1:11.2": "Vassa deva yathāsukhaṃ;",
  "thag1.1:11.3": "Cittaṃ me susamāhitaṃ vimuttaṃ,",
  "thag1.1:11.4": "Ātāpī viharāmi vassa devā”ti.",
  "thag1.1:12.1": "Itthaṃ sudaṃ āyasmā subhūtitthero gāthaṃ abhāsitthāti."
}