{
  "thag15.1:1.1": "Theragāthā",
  "thag15.1:2.1": "Soḷasakanipāta",
  "thag15.1:3.1": "Paṭhamavagga",
  "thag15.1:4.1": "15.1 Aññāsikoṇḍaññattheragāthā",
  "thag15.1:5.1": "“Esa bhiyyo pasīdāmi,",
  "thag15.1:5.2": "sutvā dhammaṃ mahārasaṃ;",
  "thag15.1:5.3": "Virāgo desito dhammo,",
  "thag15.1:5.4": "anupādāya sabbaso”.",
  "thag15.1:6.1": "“Bahūni loke citrāni,",
  "thag15.1:6.2": "asmiṃ pathavimaṇḍale;",
  "thag15.1:6.3": "Mathenti maññe saṅkappaṃ,",
  "thag15.1:6.4": "subhaṃ rāgūpasaṃhitaṃ.",
  "thag15.1:7.1": "Rajamuhatañca vātena,",
  "thag15.1:7.2": "yathā meghopasammaye;",
  "thag15.1:7.3": "Evaṃ sammanti saṅkappā,",
  "thag15.1:7.4": "yadā paññāya passati.",
  "thag15.1:8.1": "Sabbe saṅkhārā aniccāti,",
  "thag15.1:8.2": "yadā paññāya passati;",
  "thag15.1:8.3": "Atha nibbindati dukkhe,",
  "thag15.1:8.4": "esa maggo visuddhiyā.",
  "thag15.1:9.1": "Sabbe saṅkhārā dukkhāti,",
  "thag15.1:9.2": "yadā paññāya passati;",
  "thag15.1:9.3": "Atha nibbindati dukkhe,",
  "thag15.1:9.4": "esa maggo visuddhiyā.",
  "thag15.1:10.1": "Sabbe dhammā anattāti,",
  "thag15.1:10.2": "yadā paññāya passati;",
  "thag15.1:10.3": "Atha nibbindati dukkhe,",
  "thag15.1:10.4": "esa maggo visuddhiyā.",
  "thag15.1:11.1": "Buddhānubuddho yo thero,",
  "thag15.1:11.2": "koṇḍañño tibbanikkamo;",
  "thag15.1:11.3": "Pahīnajātimaraṇo,",
  "thag15.1:11.4": "brahmacariyassa kevalī.",
  "thag15.1:12.1": "Oghapāso daḷhakhilo,",
  "thag15.1:12.2": "Pabbato duppadālayo;",
  "thag15.1:12.3": "Chetvā khilañca pāsañca,",
  "thag15.1:12.4": "Selaṃ bhetvāna dubbhidaṃ;",
  "thag15.1:12.5": "Tiṇṇo pāraṅgato jhāyī,",
  "thag15.1:12.6": "Mutto so mārabandhanā.",
  "thag15.1:13.1": "Uddhato capalo bhikkhu,",
  "thag15.1:13.2": "mitte āgamma pāpake;",
  "thag15.1:13.3": "Saṃsīdati mahoghasmiṃ,",
  "thag15.1:13.4": "ūmiyā paṭikujjito.",
  "thag15.1:14.1": "Anuddhato acapalo,",
  "thag15.1:14.2": "nipako saṃvutindriyo;",
  "thag15.1:14.3": "Kalyāṇamitto medhāvī,",
  "thag15.1:14.4": "dukkhassantakaro siyā.",
  "thag15.1:15.1": "Kālapabbaṅgasaṅkāso,",
  "thag15.1:15.2": "kiso dhamanisanthato;",
  "thag15.1:15.3": "Mattaññū annapānasmiṃ,",
  "thag15.1:15.4": "adīnamanaso naro.",
  "thag15.1:16.1": "Phuṭṭho ḍaṃsehi makasehi,",
  "thag15.1:16.2": "araññasmiṃ brahāvane;",
  "thag15.1:16.3": "Nāgo saṅgāmasīseva,",
  "thag15.1:16.4": "sato tatrādhivāsaye.",
  "thag15.1:17.1": "Nābhinandāmi maraṇaṃ,",
  "thag15.1:17.2": "…",
  "thag15.1:17.3": "pe…",
  "thag15.1:17.4": "nibbisaṃ bhatako yathā.",
  "thag15.1:18.1": "Nābhinandāmi maraṇaṃ,",
  "thag15.1:18.2": "…",
  "thag15.1:18.3": "pe…",
  "thag15.1:18.4": "sampajāno patissato.",
  "thag15.1:19.1": "Pariciṇṇo mayā satthā,",
  "thag15.1:19.2": "…",
  "thag15.1:19.3": "pe…",
  "thag15.1:19.4": "bhavanetti samūhatā.",
  "thag15.1:20.1": "Yassa catthāya pabbajito,",
  "thag15.1:20.2": "agārasmānagāriyaṃ;",
  "thag15.1:20.3": "So me attho anuppatto,",
  "thag15.1:20.4": "kiṃ me saddhivihārinā”ti.",
  "thag15.1:21.1": "…",
  "thag15.1:21.2": "Aññāsikoṇḍañño thero …."
}