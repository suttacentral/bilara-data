{
  "thag1.112:1.1": "Theragāthā",
  "thag1.112:2.1": "Ekakanipāta",
  "thag1.112:3.1": "Dvādasamavagga",
  "thag1.112:4.1": "1.112. Vacchagottattheragāthā",
  "thag1.112:5.1": "“Tevijjohaṃ mahājhāyī,",
  "thag1.112:5.2": "cetosamathakovido;",
  "thag1.112:5.3": "Sadattho me anuppatto,",
  "thag1.112:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.112:6.1": "…",
  "thag1.112:6.2": "Vacchagotto thero …."
}