{
  "thag1.62:1.1": "Theragāthā",
  "thag1.62:2.1": "Ekakanipāta",
  "thag1.62:3.1": "Sattamavagga",
  "thag1.62:4.1": "1.62. Vajjiputtattheragāthā",
  "thag1.62:5.1": "“Ekakā mayaṃ araññe viharāma,",
  "thag1.62:5.2": "Apaviddhaṃva vanasmiṃ dārukaṃ;",
  "thag1.62:5.3": "Tassa me bahukā pihayanti,",
  "thag1.62:5.4": "Nerayikā viya saggagāminan”ti.",
  "thag1.62:6.1": "…",
  "thag1.62:6.2": "Vajjiputto thero …."
}