{
  "thag1.74:1.1": "Theragāthā",
  "thag1.74:2.1": "Ekakanipāta",
  "thag1.74:3.1": "Aṭṭhamavagga",
  "thag1.74:4.1": "1.74. Suyāmanattheragāthā",
  "thag1.74:5.1": "“Kāmacchando ca byāpādo,",
  "thag1.74:5.2": "Thinamiddhañca bhikkhuno;",
  "thag1.74:5.3": "Uddhaccaṃ vicikicchā ca,",
  "thag1.74:5.4": "Sabbasova na vijjatī”ti.",
  "thag1.74:6.1": "…",
  "thag1.74:6.2": "Suyāmano thero …."
}