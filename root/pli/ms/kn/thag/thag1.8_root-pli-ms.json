{
  "thag1.8:1.1": "Theragāthā",
  "thag1.8:2.1": "Ekakanipāta",
  "thag1.8:3.1": "Paṭhamavagga",
  "thag1.8:4.1": "1.8. Vīrattheragāthā",
  "thag1.8:5.1": "“Yo duddamiyo damena danto,",
  "thag1.8:5.2": "Vīro santusito vitiṇṇakaṅkho;",
  "thag1.8:5.3": "Vijitāvī apetalomahaṃso,",
  "thag1.8:5.4": "Vīro so parinibbuto ṭhitatto”ti.",
  "thag1.8:6.1": "Itthaṃ sudaṃ āyasmā vīro thero gāthaṃ abhāsitthāti."
}