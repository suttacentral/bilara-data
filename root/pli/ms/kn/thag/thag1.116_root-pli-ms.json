{
  "thag1.116:1.1": "Theragāthā",
  "thag1.116:2.1": "Ekakanipāta",
  "thag1.116:3.1": "Dvādasamavagga",
  "thag1.116:4.1": "1.116 Pārāpariyattheragāthā",
  "thag1.116:5.1": "“Chaphassāyatane hitvā,",
  "thag1.116:5.2": "guttadvāro susaṃvuto;",
  "thag1.116:5.3": "Aghamūlaṃ vamitvāna,",
  "thag1.116:5.4": "patto me āsavakkhayo”ti.",
  "thag1.116:6.1": "…",
  "thag1.116:6.2": "Pārāpariyo thero …."
}