{
  "thag2.15:1.1": "Theragāthā",
  "thag2.15:2.1": "Dukanipāta",
  "thag2.15:3.1": "Dutiyavagga",
  "thag2.15:4.1": "2.15. Sabbamittattheragāthā",
  "thag2.15:5.1": "“Jano janamhi sambaddho,",
  "thag2.15:5.2": "janamevassito jano;",
  "thag2.15:5.3": "Jano janena heṭhīyati,",
  "thag2.15:5.4": "heṭheti ca jano janaṃ.",
  "thag2.15:6.1": "Ko hi tassa janenattho,",
  "thag2.15:6.2": "janena janitena vā;",
  "thag2.15:6.3": "Janaṃ ohāya gacchaṃ taṃ,",
  "thag2.15:6.4": "heṭhayitvā bahuṃ janan”ti.",
  "thag2.15:7.1": "…",
  "thag2.15:7.2": "Sabbamitto thero …."
}