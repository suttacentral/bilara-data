{
  "thag1.23:1.1": "Theragāthā",
  "thag1.23:2.1": "Ekakanipāta",
  "thag1.23:3.1": "Tatiyavagga",
  "thag1.23:4.1": "1.23. Gosālattheragāthā",
  "thag1.23:5.1": "“Ahaṃ kho veḷugumbasmiṃ,",
  "thag1.23:5.2": "bhutvāna madhupāyasaṃ;",
  "thag1.23:5.3": "Padakkhiṇaṃ sammasanto,",
  "thag1.23:5.4": "khandhānaṃ udayabbayaṃ;",
  "thag1.23:5.5": "Sānuṃ paṭigamissāmi,",
  "thag1.23:5.6": "vivekamanubrūhayan”ti.",
  "thag1.23:6.1": "…",
  "thag1.23:6.2": "Gosālo thero …."
}