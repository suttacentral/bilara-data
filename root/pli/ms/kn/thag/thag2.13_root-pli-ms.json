{
  "thag2.13:1.1": "Theragāthā",
  "thag2.13:2.1": "Dukanipāta",
  "thag2.13:3.1": "Dutiyavagga",
  "thag2.13:4.1": "2.13. Heraññakānittheragāthā",
  "thag2.13:5.1": "“Accayanti ahorattā,",
  "thag2.13:5.2": "jīvitaṃ uparujjhati;",
  "thag2.13:5.3": "Āyu khīyati maccānaṃ,",
  "thag2.13:5.4": "kunnadīnaṃva odakaṃ.",
  "thag2.13:6.1": "Atha pāpāni kammāni,",
  "thag2.13:6.2": "karaṃ bālo na bujjhati;",
  "thag2.13:6.3": "Pacchāssa kaṭukaṃ hoti,",
  "thag2.13:6.4": "vipāko hissa pāpako”ti.",
  "thag2.13:7.1": "…",
  "thag2.13:7.2": "Heraññakānitthero …."
}