{
  "thag1.93:1.1": "Theragāthā",
  "thag1.93:2.1": "Ekakanipāta",
  "thag1.93:3.1": "Dasamavagga",
  "thag1.93:4.1": "1.93. Erakattheragāthā",
  "thag1.93:5.1": "“Dukkhā kāmā eraka,",
  "thag1.93:5.2": "Na sukhā kāmā eraka;",
  "thag1.93:5.3": "Yo kāme kāmayati,",
  "thag1.93:5.4": "Dukkhaṃ so kāmayati eraka;",
  "thag1.93:5.5": "Yo kāme na kāmayati,",
  "thag1.93:5.6": "Dukkhaṃ so na kāmayati erakā”ti.",
  "thag1.93:6.1": "…",
  "thag1.93:6.2": "Erako thero …."
}