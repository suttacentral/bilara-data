{
  "thag1.91:1.1": "Theragāthā",
  "thag1.91:2.1": "Ekakanipāta",
  "thag1.91:3.1": "Dasamavagga",
  "thag1.91:4.1": "1.91. Paripuṇṇakattheragāthā",
  "thag1.91:5.1": "“Na tathā mataṃ satarasaṃ,",
  "thag1.91:5.2": "Sudhannaṃ yaṃ mayajja paribhuttaṃ;",
  "thag1.91:5.3": "Aparimitadassinā gotamena,",
  "thag1.91:5.4": "Buddhena desito dhammo”ti.",
  "thag1.91:6.1": "…",
  "thag1.91:6.2": "Paripuṇṇako thero …."
}