{
  "thag1.108:1.1": "Theragāthā",
  "thag1.108:2.1": "Ekakanipāta",
  "thag1.108:3.1": "Ekādasamavagga",
  "thag1.108:4.1": "1.108. Dhammasavapituttheragāthā",
  "thag1.108:5.1": "“Sa vīsavassasatiko,",
  "thag1.108:5.2": "pabbajiṃ anagāriyaṃ;",
  "thag1.108:5.3": "Tisso vijjā anuppattā,",
  "thag1.108:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.108:6.1": "…",
  "thag1.108:6.2": "Dhammasavapitu thero …."
}