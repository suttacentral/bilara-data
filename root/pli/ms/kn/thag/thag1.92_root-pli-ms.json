{
  "thag1.92:1.1": "Theragāthā",
  "thag1.92:2.1": "Ekakanipāta",
  "thag1.92:3.1": "Dasamavagga",
  "thag1.92:4.1": "1.92. Vijayattheragāthā",
  "thag1.92:5.1": "“Yassāsavā parikkhīṇā,",
  "thag1.92:5.2": "āhāre ca anissito;",
  "thag1.92:5.3": "Suññato animitto ca,",
  "thag1.92:5.4": "vimokkho yassa gocaro;",
  "thag1.92:5.5": "Ākāseva sakuntānaṃ,",
  "thag1.92:5.6": "padaṃ tassa durannayan”ti.",
  "thag1.92:6.1": "…",
  "thag1.92:6.2": "Vijayo thero …."
}