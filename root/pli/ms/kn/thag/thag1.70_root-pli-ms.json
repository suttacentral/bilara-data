{
  "thag1.70:1.1": "Theragāthā",
  "thag1.70:2.1": "Ekakanipāta",
  "thag1.70:3.1": "Sattamavagga",
  "thag1.70:4.1": "1.70. Puṇṇattheragāthā",
  "thag1.70:5.1": "“Sīlameva idha aggaṃ,",
  "thag1.70:5.2": "paññavā pana uttamo;",
  "thag1.70:5.3": "Manussesu ca devesu,",
  "thag1.70:5.4": "sīlapaññāṇato jayan”ti.",
  "thag1.70:6.1": "…",
  "thag1.70:6.2": "Puṇṇo thero ….",
  "thag1.70:6.3": "Vaggo sattamo.",
  "thag1.70:7.1": "Tassuddānaṃ",
  "thag1.70:8.1": "Vappo ca vajjiputto ca,",
  "thag1.70:8.2": "pakkho vimalakoṇḍañño;",
  "thag1.70:8.3": "Ukkhepakatavaccho ca,",
  "thag1.70:8.4": "meghiyo ekadhammiko;",
  "thag1.70:8.5": "Ekudāniyachannā ca,",
  "thag1.70:8.6": "puṇṇatthero mahabbaloti."
}