{
  "thag1.37:1.1": "Theragāthā",
  "thag1.37:2.1": "Ekakanipāta",
  "thag1.37:3.1": "Catutthavagga",
  "thag1.37:4.1": "1.37. Kumāputtasahāyakattheragāthā",
  "thag1.37:5.1": "“Nānājanapadaṃ yanti,",
  "thag1.37:5.2": "Vicarantā asaññatā;",
  "thag1.37:5.3": "Samādhiñca virādhenti,",
  "thag1.37:5.4": "Kiṃsu raṭṭhacariyā karissati;",
  "thag1.37:5.5": "Tasmā vineyya sārambhaṃ,",
  "thag1.37:5.6": "Jhāyeyya apurakkhato”ti.",
  "thag1.37:6.1": "…",
  "thag1.37:6.2": "Kumāputtattherassa sahāyako thero …."
}