{
  "thag1.79:1.1": "Theragāthā",
  "thag1.79:2.1": "Ekakanipāta",
  "thag1.79:3.1": "Aṭṭhamavagga",
  "thag1.79:4.1": "1.79. Rakkhitattheragāthā",
  "thag1.79:5.1": "“Sabbo rāgo pahīno me,",
  "thag1.79:5.2": "sabbo doso samūhato;",
  "thag1.79:5.3": "Sabbo me vigato moho,",
  "thag1.79:5.4": "sītibhūtosmi nibbuto”ti.",
  "thag1.79:6.1": "…",
  "thag1.79:6.2": "Rakkhito thero …."
}