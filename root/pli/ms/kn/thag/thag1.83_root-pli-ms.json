{
  "thag1.83:1.1": "Theragāthā",
  "thag1.83:2.1": "Ekakanipāta",
  "thag1.83:3.1": "Navamavagga",
  "thag1.83:4.1": "1.83. Sīhattheragāthā",
  "thag1.83:5.1": "“Sīhappamatto vihara,",
  "thag1.83:5.2": "rattindivamatandito;",
  "thag1.83:5.3": "Bhāvehi kusalaṃ dhammaṃ,",
  "thag1.83:5.4": "jaha sīghaṃ samussayan”ti.",
  "thag1.83:6.1": "…",
  "thag1.83:6.2": "Sīho thero …."
}