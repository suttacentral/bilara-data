{
  "thag16.5:1.1": "Theragāthā",
  "thag16.5:2.1": "Vīsatinipāta",
  "thag16.5:3.1": "Paṭhamavagga",
  "thag16.5:4.1": "16.5 Mālukyaputtattheragāthā",
  "thag16.5:5.1": "“Rūpaṃ disvā sati muṭṭhā,",
  "thag16.5:5.2": "Piyaṃ nimittaṃ manasi karoto;",
  "thag16.5:5.3": "Sārattacitto vedeti,",
  "thag16.5:5.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:6.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:6.2": "anekā rūpasambhavā;",
  "thag16.5:6.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:6.4": "cittamassūpahaññati;",
  "thag16.5:6.5": "Evamācinato dukkhaṃ,",
  "thag16.5:6.6": "ārā nibbāna vuccati.",
  "thag16.5:7.1": "Saddaṃ sutvā sati muṭṭhā,",
  "thag16.5:7.2": "Piyaṃ nimittaṃ manasi karoto;",
  "thag16.5:7.3": "Sārattacitto vedeti,",
  "thag16.5:7.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:8.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:8.2": "anekā saddasambhavā;",
  "thag16.5:8.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:8.4": "cittamassūpahaññati;",
  "thag16.5:8.5": "Evamācinato dukkhaṃ,",
  "thag16.5:8.6": "ārā nibbāna vuccati.",
  "thag16.5:9.1": "Gandhaṃ ghatvā sati muṭṭhā,",
  "thag16.5:9.2": "Piyaṃ nimittaṃ manasi karoto;",
  "thag16.5:9.3": "Sārattacitto vedeti,",
  "thag16.5:9.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:10.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:10.2": "anekā gandhasambhavā;",
  "thag16.5:10.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:10.4": "cittamassūpahaññati;",
  "thag16.5:10.5": "Evamācinato dukkhaṃ,",
  "thag16.5:10.6": "ārā nibbāna vuccati.",
  "thag16.5:11.1": "Rasaṃ bhotvā sati muṭṭhā,",
  "thag16.5:11.2": "Piyaṃ nimittaṃ manasi karoto;",
  "thag16.5:11.3": "Sārattacitto vedeti,",
  "thag16.5:11.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:12.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:12.2": "anekā rasasambhavā;",
  "thag16.5:12.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:12.4": "cittamassūpahaññati;",
  "thag16.5:12.5": "Evamācinato dukkhaṃ,",
  "thag16.5:12.6": "ārā nibbāna vuccati.",
  "thag16.5:13.1": "Phassaṃ phussa sati muṭṭhā,",
  "thag16.5:13.2": "Piyaṃ nimittaṃ manasi karoto;",
  "thag16.5:13.3": "Sārattacitto vedeti,",
  "thag16.5:13.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:14.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:14.2": "anekā phassasambhavā;",
  "thag16.5:14.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:14.4": "cittamassūpahaññati;",
  "thag16.5:14.5": "Evamācinato dukkhaṃ,",
  "thag16.5:14.6": "ārā nibbāna vuccati.",
  "thag16.5:15.1": "Dhammaṃ ñatvā sati muṭṭhā,",
  "thag16.5:15.2": "Piyaṃ nimittaṃ manasi karoto;",
  "thag16.5:15.3": "Sārattacitto vedeti,",
  "thag16.5:15.4": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:16.1": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:16.2": "anekā dhammasambhavā;",
  "thag16.5:16.3": "Abhijjhā ca vihesā ca,",
  "thag16.5:16.4": "cittamassūpahaññati;",
  "thag16.5:16.5": "Evamācinato dukkhaṃ,",
  "thag16.5:16.6": "ārā nibbāna vuccati.",
  "thag16.5:17.1": "Na so rajjati rūpesu,",
  "thag16.5:17.2": "rūpaṃ disvā patissato;",
  "thag16.5:17.3": "Virattacitto vedeti,",
  "thag16.5:17.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:18.1": "Yathāssa passato rūpaṃ,",
  "thag16.5:18.2": "sevato cāpi vedanaṃ;",
  "thag16.5:18.3": "Khīyati nopacīyati,",
  "thag16.5:18.4": "evaṃ so caratī sato;",
  "thag16.5:18.5": "Evaṃ apacinato dukkhaṃ,",
  "thag16.5:18.6": "santike nibbāna vuccati.",
  "thag16.5:19.1": "Na so rajjati saddesu,",
  "thag16.5:19.2": "saddaṃ sutvā patissato;",
  "thag16.5:19.3": "Virattacitto vedeti,",
  "thag16.5:19.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:20.1": "Yathāssa suṇato saddaṃ,",
  "thag16.5:20.2": "sevato cāpi vedanaṃ;",
  "thag16.5:20.3": "Khīyati nopacīyati,",
  "thag16.5:20.4": "evaṃ so caratī sato;",
  "thag16.5:20.5": "Evaṃ apacinato dukkhaṃ,",
  "thag16.5:20.6": "santike nibbāna vuccati.",
  "thag16.5:21.1": "Na so rajjati gandhesu,",
  "thag16.5:21.2": "gandhaṃ ghatvā patissato;",
  "thag16.5:21.3": "Virattacitto vedeti,",
  "thag16.5:21.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:22.1": "Yathāssa ghāyato gandhaṃ,",
  "thag16.5:22.2": "sevato cāpi vedanaṃ;",
  "thag16.5:22.3": "Khīyati nopacīyati,",
  "thag16.5:22.4": "evaṃ so caratī sato;",
  "thag16.5:22.5": "Evaṃ apacinato dukkhaṃ,",
  "thag16.5:22.6": "santike nibbāna vuccati.",
  "thag16.5:23.1": "Na so rajjati rasesu,",
  "thag16.5:23.2": "rasaṃ bhotvā patissato;",
  "thag16.5:23.3": "Virattacitto vedeti,",
  "thag16.5:23.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:24.1": "Yathāssa sāyarato rasaṃ,",
  "thag16.5:24.2": "sevato cāpi vedanaṃ;",
  "thag16.5:24.3": "Khīyati nopacīyati,",
  "thag16.5:24.4": "evaṃ so caratī sato;",
  "thag16.5:24.5": "Evaṃ apacinato dukkhaṃ,",
  "thag16.5:24.6": "santike nibbāna vuccati.",
  "thag16.5:25.1": "Na so rajjati phassesu,",
  "thag16.5:25.2": "phassaṃ phussa patissato;",
  "thag16.5:25.3": "Virattacitto vedeti,",
  "thag16.5:25.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:26.1": "Yathāssa phusato phassaṃ,",
  "thag16.5:26.2": "sevato cāpi vedanaṃ;",
  "thag16.5:26.3": "Khīyati nopacīyati,",
  "thag16.5:26.4": "evaṃ so caratī sato;",
  "thag16.5:26.5": "Evaṃ apacinato dukkhaṃ,",
  "thag16.5:26.6": "santike nibbāna vuccati.",
  "thag16.5:27.1": "Na so rajjati dhammesu,",
  "thag16.5:27.2": "dhammaṃ ñatvā patissato;",
  "thag16.5:27.3": "Virattacitto vedeti,",
  "thag16.5:27.4": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:28.1": "Yathāssa vijānato dhammaṃ,",
  "thag16.5:28.2": "sevato cāpi vedanaṃ;",
  "thag16.5:28.3": "Khīyati nopacīyati,",
  "thag16.5:28.4": "evaṃ so caratī sato;",
  "thag16.5:28.5": "Evaṃ apacinato dukkhaṃ,",
  "thag16.5:28.6": "santike nibbāna vuccati”.",
  "thag16.5:29.1": "…",
  "thag16.5:29.2": "Mālukyaputto thero …."
}