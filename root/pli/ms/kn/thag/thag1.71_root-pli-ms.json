{
  "thag1.71:1.1": "Theragāthā",
  "thag1.71:2.1": "Ekakanipāta",
  "thag1.71:3.1": "Aṭṭhamavagga",
  "thag1.71:4.1": "1.71. Vacchapālattheragāthā",
  "thag1.71:5.1": "“Susukhumanipuṇatthadassinā,",
  "thag1.71:5.2": "Matikusalena nivātavuttinā;",
  "thag1.71:5.3": "Saṃsevitavuddhasīlinā,",
  "thag1.71:5.4": "Nibbānaṃ na hi tena dullabhan”ti.",
  "thag1.71:6.1": "…",
  "thag1.71:6.2": "Vacchapālo thero …."
}