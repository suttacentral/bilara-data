{
  "thag2.34:1.1": "Theragāthā",
  "thag2.34:2.1": "Dukanipāta",
  "thag2.34:3.1": "Catutthavagga",
  "thag2.34:4.1": "2.34. Isidinnattheragāthā",
  "thag2.34:5.1": "“Diṭṭhā mayā dhammadharā upāsakā,",
  "thag2.34:5.2": "Kāmā aniccā iti bhāsamānā;",
  "thag2.34:5.3": "Sārattarattā maṇikuṇḍalesu,",
  "thag2.34:5.4": "Puttesu dāresu ca te apekkhā.",
  "thag2.34:6.1": "Addhā na jānanti yatodha dhammaṃ,",
  "thag2.34:6.2": "Kāmā aniccā iti cāpi āhu;",
  "thag2.34:6.3": "Rāgañca tesaṃ na balatthi chettuṃ,",
  "thag2.34:6.4": "Tasmā sitā puttadāraṃ dhanañcā”ti.",
  "thag2.34:7.1": "…",
  "thag2.34:7.2": "Isidinno thero …."
}