{
  "thag2.10:1.1": "Theragāthā",
  "thag2.10:2.1": "Dukanipāta",
  "thag2.10:3.1": "Paṭhamavagga",
  "thag2.10:4.1": "2.10. Vasabhattheragāthā",
  "thag2.10:5.1": "“Pubbe hanati attānaṃ,",
  "thag2.10:5.2": "pacchā hanati so pare;",
  "thag2.10:5.3": "Suhataṃ hanti attānaṃ,",
  "thag2.10:5.4": "vītaṃseneva pakkhimā.",
  "thag2.10:6.1": "Na brāhmaṇo bahivaṇṇo,",
  "thag2.10:6.2": "anto vaṇṇo hi brāhmaṇo;",
  "thag2.10:6.3": "Yasmiṃ pāpāni kammāni,",
  "thag2.10:6.4": "sa ve kaṇho sujampatī”ti.",
  "thag2.10:7.1": "…",
  "thag2.10:7.2": "Vasabho thero ….",
  "thag2.10:7.3": "Vaggo paṭhamo.",
  "thag2.10:8.1": "Tassuddānaṃ",
  "thag2.10:9.1": "Uttaro ceva piṇḍolo,",
  "thag2.10:9.2": "valliyo tīriyo isi;",
  "thag2.10:9.3": "Ajino ca meḷajino,",
  "thag2.10:9.4": "rādho surādho gotamo;",
  "thag2.10:9.5": "Vasabhena ime honti,",
  "thag2.10:9.6": "dasa therā mahiddhikāti."
}