{
  "thag5.10:1.1": "Theragāthā",
  "thag5.10:2.1": "Pañcakanipāta",
  "thag5.10:3.1": "Paṭhamavagga",
  "thag5.10:4.1": "5.10. Yasadattattheragāthā",
  "thag5.10:5.1": "“Upārambhacitto dummedho,",
  "thag5.10:5.2": "suṇāti jinasāsanaṃ;",
  "thag5.10:5.3": "Ārakā hoti saddhammā,",
  "thag5.10:5.4": "nabhaso pathavī yathā.",
  "thag5.10:6.1": "Upārambhacitto dummedho,",
  "thag5.10:6.2": "suṇāti jinasāsanaṃ;",
  "thag5.10:6.3": "Parihāyati saddhammā,",
  "thag5.10:6.4": "kāḷapakkheva candimā.",
  "thag5.10:7.1": "Upārambhacitto dummedho,",
  "thag5.10:7.2": "suṇāti jinasāsanaṃ;",
  "thag5.10:7.3": "Parisussati saddhamme,",
  "thag5.10:7.4": "maccho appodake yathā.",
  "thag5.10:8.1": "Upārambhacitto dummedho,",
  "thag5.10:8.2": "suṇāti jinasāsanaṃ;",
  "thag5.10:8.3": "Na virūhati saddhamme,",
  "thag5.10:8.4": "khette bījaṃva pūtikaṃ.",
  "thag5.10:9.1": "Yo ca tuṭṭhena cittena,",
  "thag5.10:9.2": "suṇāti jinasāsanaṃ;",
  "thag5.10:9.3": "Khepetvā āsave sabbe,",
  "thag5.10:9.4": "sacchikatvā akuppataṃ;",
  "thag5.10:9.5": "Pappuyya paramaṃ santiṃ,",
  "thag5.10:9.6": "parinibbātināsavo”ti.",
  "thag5.10:10.1": "…",
  "thag5.10:10.2": "Yasadatto thero …."
}