{
  "thag1.46:1.1": "Theragāthā",
  "thag1.46:2.1": "Ekakanipāta",
  "thag1.46:3.1": "Pañcamavagga",
  "thag1.46:4.1": "1.46. Samiddhittheragāthā",
  "thag1.46:5.1": "“Saddhāyāhaṃ pabbajito,",
  "thag1.46:5.2": "Agārasmānagāriyaṃ;",
  "thag1.46:5.3": "Sati paññā ca me vuḍḍhā,",
  "thag1.46:5.4": "Cittañca susamāhitaṃ;",
  "thag1.46:5.5": "Kāmaṃ karassu rūpāni,",
  "thag1.46:5.6": "Neva maṃ byādhayissasī”ti.",
  "thag1.46:6.1": "…",
  "thag1.46:6.2": "Samiddhitthero …."
}