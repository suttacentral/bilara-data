{
  "thag1.94:1.1": "Theragāthā",
  "thag1.94:2.1": "Ekakanipāta",
  "thag1.94:3.1": "Dasamavagga",
  "thag1.94:4.1": "1.94. Mettajittheragāthā",
  "thag1.94:5.1": "“Namo hi tassa bhagavato,",
  "thag1.94:5.2": "sakyaputtassa sirīmato;",
  "thag1.94:5.3": "Tenāyaṃ aggappattena,",
  "thag1.94:5.4": "aggadhammo sudesito”ti.",
  "thag1.94:6.1": "…",
  "thag1.94:6.2": "Mettaji thero …."
}