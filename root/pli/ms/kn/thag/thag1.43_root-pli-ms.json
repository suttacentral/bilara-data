{
  "thag1.43:1.1": "Theragāthā",
  "thag1.43:2.1": "Ekakanipāta",
  "thag1.43:3.1": "Pañcamavagga",
  "thag1.43:4.1": "1.43. Sumaṅgalattheragāthā",
  "thag1.43:5.1": "“Sumuttiko sumuttiko,",
  "thag1.43:5.2": "Sāhu sumuttikomhi tīhi khujjakehi;",
  "thag1.43:5.3": "Asitāsu mayā naṅgalāsu mayā,",
  "thag1.43:5.4": "Khuddakuddālāsu mayā.",
  "thag1.43:5.5": "Yadipi idhameva idhameva,",
  "thag1.43:5.6": "Atha vāpi alameva alameva;",
  "thag1.43:5.7": "Jhāya sumaṅgala jhāya sumaṅgala,",
  "thag1.43:5.8": "Appamatto vihara sumaṅgalā”ti.",
  "thag1.43:6.1": "…",
  "thag1.43:6.2": "Sumaṅgalo thero …."
}