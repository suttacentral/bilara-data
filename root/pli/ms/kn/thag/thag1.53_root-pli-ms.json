{
  "thag1.53:1.1": "Theragāthā",
  "thag1.53:2.1": "Ekakanipāta",
  "thag1.53:3.1": "Chaṭṭhavagga",
  "thag1.53:4.1": "1.53. Valliyattheragāthā",
  "thag1.53:5.1": "“Vassati devo yathā sugītaṃ,",
  "thag1.53:5.2": "Channā me kuṭikā sukhā nivātā;",
  "thag1.53:5.3": "Tassaṃ viharāmi appamatto,",
  "thag1.53:5.4": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.53:6.1": "…",
  "thag1.53:6.2": "Valliyo thero …."
}