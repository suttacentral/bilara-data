{
  "thag2.11:1.1": "Theragāthā",
  "thag2.11:2.1": "Dukanipāta",
  "thag2.11:3.1": "Dutiyavagga",
  "thag2.11:4.1": "2.11. Mahācundattheragāthā",
  "thag2.11:5.1": "“Sussūsā sutavaddhanī,",
  "thag2.11:5.2": "sutaṃ paññāya vaddhanaṃ;",
  "thag2.11:5.3": "Paññāya atthaṃ jānāti,",
  "thag2.11:5.4": "ñāto attho sukhāvaho.",
  "thag2.11:6.1": "Sevetha pantāni senāsanāni,",
  "thag2.11:6.2": "Careyya saṃyojanavippamokkhaṃ;",
  "thag2.11:6.3": "Sace ratiṃ nādhigaccheyya tattha,",
  "thag2.11:6.4": "Saṃghe vase rakkhitatto satimā”ti.",
  "thag2.11:7.1": "…",
  "thag2.11:7.2": "Mahācundo thero …."
}