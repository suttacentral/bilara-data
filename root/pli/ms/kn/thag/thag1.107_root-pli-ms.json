{
  "thag1.107:1.1": "Theragāthā",
  "thag1.107:2.1": "Ekakanipāta",
  "thag1.107:3.1": "Ekādasamavagga",
  "thag1.107:4.1": "1.107. Dhammasavattheragāthā",
  "thag1.107:5.1": "“Pabbajiṃ tulayitvāna,",
  "thag1.107:5.2": "agārasmānagāriyaṃ;",
  "thag1.107:5.3": "Tisso vijjā anuppattā,",
  "thag1.107:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.107:6.1": "…",
  "thag1.107:6.2": "Dhammasavo thero …."
}