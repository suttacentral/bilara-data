{
  "thag1.85:1.1": "Theragāthā",
  "thag1.85:2.1": "Ekakanipāta",
  "thag1.85:3.1": "Navamavagga",
  "thag1.85:4.1": "1.85. Sunāgattheragāthā",
  "thag1.85:5.1": "“Cittanimittassa kovido,",
  "thag1.85:5.2": "Pavivekarasaṃ vijāniya;",
  "thag1.85:5.3": "Jhāyaṃ nipako patissato,",
  "thag1.85:5.4": "Adhigaccheyya sukhaṃ nirāmisan”ti.",
  "thag1.85:6.1": "…",
  "thag1.85:6.2": "Sunāgo thero …."
}