{
  "thag1.56:1.1": "Theragāthā",
  "thag1.56:2.1": "Ekakanipāta",
  "thag1.56:3.1": "Chaṭṭhavagga",
  "thag1.56:4.1": "1.56. Kuṭivihārittheragāthā",
  "thag1.56:5.1": "“Ko kuṭikāyaṃ bhikkhu kuṭikāyaṃ,",
  "thag1.56:5.2": "Vītarāgo susamāhitacitto;",
  "thag1.56:5.3": "Evaṃ jānāhi āvuso,",
  "thag1.56:5.4": "Amoghā te kuṭikā katā”ti.",
  "thag1.56:6.1": "…",
  "thag1.56:6.2": "Kuṭivihāritthero …."
}