{
  "thag1.78:1.1": "Theragāthā",
  "thag1.78:2.1": "Ekakanipāta",
  "thag1.78:3.1": "Aṭṭhamavagga",
  "thag1.78:4.1": "1.78. Meṇḍasirattheragāthā",
  "thag1.78:5.1": "“Anekajātisaṃsāraṃ,",
  "thag1.78:5.2": "sandhāvissaṃ anibbisaṃ;",
  "thag1.78:5.3": "Tassa me dukkhajātassa,",
  "thag1.78:5.4": "dukkhakkhandho aparaddho”ti.",
  "thag1.78:6.1": "…",
  "thag1.78:6.2": "Meṇḍasiro thero …."
}