{
  "thag10.5:1.1": "Theragāthā",
  "thag10.5:2.1": "Dasakanipāta",
  "thag10.5:3.1": "Paṭhamavagga",
  "thag10.5:4.1": "10.5. Kappattheragāthā",
  "thag10.5:5.1": "“Nānākulamalasampuṇṇo,",
  "thag10.5:5.2": "mahāukkārasambhavo;",
  "thag10.5:5.3": "Candanikaṃva paripakkaṃ,",
  "thag10.5:5.4": "mahāgaṇḍo mahāvaṇo.",
  "thag10.5:6.1": "Pubbaruhirasampuṇṇo,",
  "thag10.5:6.2": "gūthakūpena gāḷhito;",
  "thag10.5:6.3": "Āpopaggharaṇo kāyo,",
  "thag10.5:6.4": "sadā sandati pūtikaṃ.",
  "thag10.5:7.1": "Saṭṭhikaṇḍarasambandho,",
  "thag10.5:7.2": "maṃsalepanalepito;",
  "thag10.5:7.3": "Cammakañcukasannaddho,",
  "thag10.5:7.4": "pūtikāyo niratthako.",
  "thag10.5:8.1": "Aṭṭhisaṅghātaghaṭito,",
  "thag10.5:8.2": "nhārusuttanibandhano;",
  "thag10.5:8.3": "Nekesaṃ saṅgatībhāvā,",
  "thag10.5:8.4": "kappeti iriyāpathaṃ.",
  "thag10.5:9.1": "Dhuvappayāto maraṇāya,",
  "thag10.5:9.2": "maccurājassa santike;",
  "thag10.5:9.3": "Idheva chaḍḍayitvāna,",
  "thag10.5:9.4": "yenakāmaṅgamo naro.",
  "thag10.5:10.1": "Avijjāya nivuto kāyo,",
  "thag10.5:10.2": "catuganthena ganthito;",
  "thag10.5:10.3": "Oghasaṃsīdano kāyo,",
  "thag10.5:10.4": "anusayajālamotthato.",
  "thag10.5:11.1": "Pañcanīvaraṇe yutto,",
  "thag10.5:11.2": "vitakkena samappito;",
  "thag10.5:11.3": "Taṇhāmūlenānugato,",
  "thag10.5:11.4": "mohacchādanachādito.",
  "thag10.5:12.1": "Evāyaṃ vattate kāyo,",
  "thag10.5:12.2": "kammayantena yantito;",
  "thag10.5:12.3": "Sampatti ca vipatyantā,",
  "thag10.5:12.4": "nānābhāvo vipajjati.",
  "thag10.5:13.1": "Yemaṃ kāyaṃ mamāyanti,",
  "thag10.5:13.2": "andhabālā puthujjanā;",
  "thag10.5:13.3": "Vaḍḍhenti kaṭasiṃ ghoraṃ,",
  "thag10.5:13.4": "ādiyanti punabbhavaṃ.",
  "thag10.5:14.1": "Yemaṃ kāyaṃ vivajjenti,",
  "thag10.5:14.2": "Gūthalittaṃva pannagaṃ;",
  "thag10.5:14.3": "Bhavamūlaṃ vamitvāna,",
  "thag10.5:14.4": "Parinibbissantināsavā”ti.",
  "thag10.5:15.1": "…",
  "thag10.5:15.2": "Kappo thero …."
}