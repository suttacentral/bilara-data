{
  "thag6.9:1.1": "Theragāthā",
  "thag6.9:2.1": "Chakkanipāta",
  "thag6.9:3.1": "Paṭhamavagga",
  "thag6.9:4.1": "6.9 Purohitaputtajentattheragāthā",
  "thag6.9:5.1": "“Jātimadena mattohaṃ,",
  "thag6.9:5.2": "bhogaissariyena ca;",
  "thag6.9:5.3": "Saṇṭhānavaṇṇarūpena,",
  "thag6.9:5.4": "madamatto acārihaṃ.",
  "thag6.9:6.1": "Nāttano samakaṃ kañci,",
  "thag6.9:6.2": "atirekaṃ ca maññisaṃ;",
  "thag6.9:6.3": "Atimānahato bālo,",
  "thag6.9:6.4": "patthaddho ussitaddhajo.",
  "thag6.9:7.1": "Mātaraṃ pitarañcāpi,",
  "thag6.9:7.2": "aññepi garusammate;",
  "thag6.9:7.3": "Na kañci abhivādesiṃ,",
  "thag6.9:7.4": "mānatthaddho anādaro.",
  "thag6.9:8.1": "Disvā vināyakaṃ aggaṃ,",
  "thag6.9:8.2": "sārathīnaṃ varuttamaṃ;",
  "thag6.9:8.3": "Tapantamiva ādiccaṃ,",
  "thag6.9:8.4": "bhikkhusaṃghapurakkhataṃ.",
  "thag6.9:9.1": "Mānaṃ madañca chaḍḍetvā,",
  "thag6.9:9.2": "vippasannena cetasā;",
  "thag6.9:9.3": "Sirasā abhivādesiṃ,",
  "thag6.9:9.4": "sabbasattānamuttamaṃ.",
  "thag6.9:10.1": "Atimāno ca omāno,",
  "thag6.9:10.2": "pahīnā susamūhatā;",
  "thag6.9:10.3": "Asmimāno samucchinno,",
  "thag6.9:10.4": "sabbe mānavidhā hatā”ti.",
  "thag6.9:11.1": "…",
  "thag6.9:11.2": "Jento purohitaputto thero …."
}