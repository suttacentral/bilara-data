{
  "thag1.22:1.1": "Theragāthā",
  "thag1.22:2.1": "Ekakanipāta",
  "thag1.22:3.1": "Tatiyavagga",
  "thag1.22:4.1": "1.22. Cittakattheragāthā",
  "thag1.22:5.1": "“Nīlā sugīvā sikhino,",
  "thag1.22:5.2": "Morā kārambhiyaṃ abhinadanti;",
  "thag1.22:5.3": "Te sītavātakīḷitā,",
  "thag1.22:5.4": "Suttaṃ jhāyaṃ nibodhentī”ti.",
  "thag1.22:6.1": "…",
  "thag1.22:6.2": "Cittako thero …."
}