{
  "thag1.57:1.1": "Theragāthā",
  "thag1.57:2.1": "Ekakanipāta",
  "thag1.57:3.1": "Chaṭṭhavagga",
  "thag1.57:4.1": "1.57. Dutiyakuṭivihārittheragāthā",
  "thag1.57:5.1": "“Ayamāhu purāṇiyā kuṭi,",
  "thag1.57:5.2": "Aññaṃ patthayase navaṃ kuṭiṃ;",
  "thag1.57:5.3": "Āsaṃ kuṭiyā virājaya,",
  "thag1.57:5.4": "Dukkhā bhikkhu puna navā kuṭī”ti.",
  "thag1.57:6.1": "…",
  "thag1.57:6.2": "Dutiyakuṭivihāritthero …."
}