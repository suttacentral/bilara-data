{
  "thag1.103:1.1": "Theragāthā",
  "thag1.103:2.1": "Ekakanipāta",
  "thag1.103:3.1": "Ekādasamavagga",
  "thag1.103:4.1": "1.103. Bandhurattheragāthā",
  "thag1.103:5.1": "“Nāhaṃ etena atthiko,",
  "thag1.103:5.2": "Sukhito dhammarasena tappito;",
  "thag1.103:5.3": "Pitvā rasaggamuttamaṃ,",
  "thag1.103:5.4": "Na ca kāhāmi visena santhavan”ti.",
  "thag1.103:6.1": "…",
  "thag1.103:6.2": "Bandhuro thero …."
}