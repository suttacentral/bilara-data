{
  "thag5.11:1.1": "Theragāthā",
  "thag5.11:2.1": "Pañcakanipāta",
  "thag5.11:3.1": "Paṭhamavagga",
  "thag5.11:4.1": "5.11. Soṇakuṭikaṇṇattheragāthā",
  "thag5.11:5.1": "“Upasampadā ca me laddhā,",
  "thag5.11:5.2": "vimutto camhi anāsavo;",
  "thag5.11:5.3": "So ca me bhagavā diṭṭho,",
  "thag5.11:5.4": "vihāre ca sahāvasiṃ.",
  "thag5.11:6.1": "Bahudeva rattiṃ bhagavā,",
  "thag5.11:6.2": "abbhokāsetināmayi;",
  "thag5.11:6.3": "Vihārakusalo satthā,",
  "thag5.11:6.4": "vihāraṃ pāvisī tadā.",
  "thag5.11:7.1": "Santharitvāna saṅghāṭiṃ,",
  "thag5.11:7.2": "seyyaṃ kappesi gotamo;",
  "thag5.11:7.3": "Sīho selaguhāyaṃva,",
  "thag5.11:7.4": "pahīnabhayabheravo.",
  "thag5.11:8.1": "Tato kalyāṇavākkaraṇo,",
  "thag5.11:8.2": "sammāsambuddhasāvako;",
  "thag5.11:8.3": "Soṇo abhāsi saddhammaṃ,",
  "thag5.11:8.4": "buddhaseṭṭhassa sammukhā.",
  "thag5.11:9.1": "Pañcakkhandhe pariññāya,",
  "thag5.11:9.2": "Bhāvayitvāna añjasaṃ;",
  "thag5.11:9.3": "Pappuyya paramaṃ santiṃ,",
  "thag5.11:9.4": "Parinibbissatyanāsavo”ti.",
  "thag5.11:10.1": "…",
  "thag5.11:10.2": "Soṇo kuṭikaṇṇatthero …."
}