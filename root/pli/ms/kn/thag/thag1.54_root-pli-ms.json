{
  "thag1.54:1.1": "Theragāthā",
  "thag1.54:2.1": "Ekakanipāta",
  "thag1.54:3.1": "Chaṭṭhavagga",
  "thag1.54:4.1": "1.54. Uttiyattheragāthā",
  "thag1.54:5.1": "“Vassati devo yathā sugītaṃ,",
  "thag1.54:5.2": "Channā me kuṭikā sukhā nivātā;",
  "thag1.54:5.3": "Tassaṃ viharāmi adutiyo,",
  "thag1.54:5.4": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.54:6.1": "…",
  "thag1.54:6.2": "Uttiyo thero …."
}