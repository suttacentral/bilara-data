{
  "thag7.5:1.1": "Theragāthā",
  "thag7.5:2.1": "Sattakanipāta",
  "thag7.5:3.1": "Paṭhamavagga",
  "thag7.5:4.1": "7.5. Sarabhaṅgattheragāthā",
  "thag7.5:5.1": "“Sare hatthehi bhañjitvā,",
  "thag7.5:5.2": "katvāna kuṭimacchisaṃ;",
  "thag7.5:5.3": "Tena me sarabhaṅgoti,",
  "thag7.5:5.4": "nāmaṃ sammutiyā ahu.",
  "thag7.5:6.1": "Na mayhaṃ kappate ajja,",
  "thag7.5:6.2": "sare hatthehi bhañjituṃ;",
  "thag7.5:6.3": "Sikkhāpadā no paññattā,",
  "thag7.5:6.4": "gotamena yasassinā.",
  "thag7.5:7.1": "Sakalaṃ samattaṃ rogaṃ,",
  "thag7.5:7.2": "Sarabhaṅgo nāddasaṃ pubbe;",
  "thag7.5:7.3": "Soyaṃ rogo diṭṭho,",
  "thag7.5:7.4": "Vacanakarenātidevassa.",
  "thag7.5:8.1": "Yeneva maggena gato vipassī,",
  "thag7.5:8.2": "Yeneva maggena sikhī ca vessabhū;",
  "thag7.5:8.3": "Kakusandhakoṇāgamano ca kassapo,",
  "thag7.5:8.4": "Tenañjasena agamāsi gotamo.",
  "thag7.5:9.1": "Vītataṇhā anādānā,",
  "thag7.5:9.2": "satta buddhā khayogadhā;",
  "thag7.5:9.3": "Yehāyaṃ desito dhammo,",
  "thag7.5:9.4": "dhammabhūtehi tādibhi.",
  "thag7.5:10.1": "Cattāri ariyasaccāni,",
  "thag7.5:10.2": "anukampāya pāṇinaṃ;",
  "thag7.5:10.3": "Dukkhaṃ samudayo maggo,",
  "thag7.5:10.4": "nirodho dukkhasaṅkhayo.",
  "thag7.5:11.1": "Yasmiṃ nivattate dukkhaṃ,",
  "thag7.5:11.2": "saṃsārasmiṃ anantakaṃ;",
  "thag7.5:11.3": "Bhedā imassa kāyassa,",
  "thag7.5:11.4": "jīvitassa ca saṅkhayā;",
  "thag7.5:11.5": "Añño punabbhavo natthi,",
  "thag7.5:11.6": "suvimuttomhi sabbadhī”ti.",
  "thag7.5:12.1": "…",
  "thag7.5:12.2": "Sarabhaṅgo thero ….",
  "thag7.5:13.1": "Sattakanipāto niṭṭhito.",
  "thag7.5:14.1": "Tatruddānaṃ",
  "thag7.5:15.1": "Sundarasamuddo thero,",
  "thag7.5:15.2": "thero lakuṇḍabhaddiyo;",
  "thag7.5:15.3": "Bhaddo thero ca sopāko,",
  "thag7.5:15.4": "sarabhaṅgo mahāisi;",
  "thag7.5:15.5": "Sattake pañcakā therā,",
  "thag7.5:15.6": "gāthāyo pañcatiṃsatīti."
}