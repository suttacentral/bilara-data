{
  "thag1.114:1.1": "Theragāthā",
  "thag1.114:2.1": "Ekakanipāta",
  "thag1.114:3.1": "Dvādasamavagga",
  "thag1.114:4.1": "1.114. Adhimuttattheragāthā",
  "thag1.114:5.1": "“Kāyaduṭṭhullagaruno,",
  "thag1.114:5.2": "hiyyamānamhi jīvite;",
  "thag1.114:5.3": "Sarīrasukhagiddhassa,",
  "thag1.114:5.4": "kuto samaṇasādhutā”ti.",
  "thag1.114:6.1": "…",
  "thag1.114:6.2": "Adhimutto thero …."
}