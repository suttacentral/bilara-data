{
  "thag1.59:1.1": "Theragāthā",
  "thag1.59:2.1": "Ekakanipāta",
  "thag1.59:3.1": "Chaṭṭhavagga",
  "thag1.59:4.1": "1.59. Kosalavihārittheragāthā",
  "thag1.59:5.1": "“Saddhāyāhaṃ pabbajito,",
  "thag1.59:5.2": "araññe me kuṭikā katā;",
  "thag1.59:5.3": "Appamatto ca ātāpī,",
  "thag1.59:5.4": "sampajāno patissato”ti.",
  "thag1.59:6.1": "…",
  "thag1.59:6.2": "Kosalavihāritthero …."
}