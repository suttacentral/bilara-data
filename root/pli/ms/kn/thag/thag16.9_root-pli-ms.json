{
  "thag16.9:1.1": "Theragāthā",
  "thag16.9:2.1": "Vīsatinipāta",
  "thag16.9:3.1": "Paṭhamavagga",
  "thag16.9:4.1": "16.9. Anuruddhattheragāthā",
  "thag16.9:5.1": "“Pahāya mātāpitaro,",
  "thag16.9:5.2": "bhaginī ñātibhātaro;",
  "thag16.9:5.3": "Pañca kāmaguṇe hitvā,",
  "thag16.9:5.4": "anuruddhova jhāyati.",
  "thag16.9:6.1": "Sameto naccagītehi,",
  "thag16.9:6.2": "sammatāḷappabodhano;",
  "thag16.9:6.3": "Na tena suddhimajjhagaṃ,",
  "thag16.9:6.4": "mārassa visaye rato.",
  "thag16.9:7.1": "Etañca samatikkamma,",
  "thag16.9:7.2": "rato buddhassa sāsane;",
  "thag16.9:7.3": "Sabboghaṃ samatikkamma,",
  "thag16.9:7.4": "anuruddhova jhāyati.",
  "thag16.9:8.1": "Rūpā saddā rasā gandhā,",
  "thag16.9:8.2": "phoṭṭhabbā ca manoramā;",
  "thag16.9:8.3": "Ete ca samatikkamma,",
  "thag16.9:8.4": "anuruddhova jhāyati.",
  "thag16.9:9.1": "Piṇḍapātamatikkanto,",
  "thag16.9:9.2": "eko adutiyo muni;",
  "thag16.9:9.3": "Esati paṃsukūlāni,",
  "thag16.9:9.4": "anuruddho anāsavo.",
  "thag16.9:10.1": "Vicinī aggahī dhovi,",
  "thag16.9:10.2": "rajayī dhārayī muni;",
  "thag16.9:10.3": "Paṃsukūlāni matimā,",
  "thag16.9:10.4": "anuruddho anāsavo.",
  "thag16.9:11.1": "Mahiccho ca asantuṭṭho,",
  "thag16.9:11.2": "saṃsaṭṭho yo ca uddhato;",
  "thag16.9:11.3": "Tassa dhammā ime honti,",
  "thag16.9:11.4": "pāpakā saṃkilesikā.",
  "thag16.9:12.1": "Sato ca hoti appiccho,",
  "thag16.9:12.2": "santuṭṭho avighātavā;",
  "thag16.9:12.3": "Pavivekarato vitto,",
  "thag16.9:12.4": "niccamāraddhavīriyo.",
  "thag16.9:13.1": "Tassa dhammā ime honti,",
  "thag16.9:13.2": "kusalā bodhipakkhikā;",
  "thag16.9:13.3": "Anāsavo ca so hoti,",
  "thag16.9:13.4": "iti vuttaṃ mahesinā.",
  "thag16.9:14.1": "Mama saṅkappamaññāya,",
  "thag16.9:14.2": "satthā loke anuttaro;",
  "thag16.9:14.3": "Manomayena kāyena,",
  "thag16.9:14.4": "iddhiyā upasaṅkami.",
  "thag16.9:15.1": "Yadā me ahu saṅkappo,",
  "thag16.9:15.2": "tato uttari desayi;",
  "thag16.9:15.3": "Nippapañcarato buddho,",
  "thag16.9:15.4": "nippapañcamadesayi.",
  "thag16.9:16.1": "Tassāhaṃ dhammamaññāya,",
  "thag16.9:16.2": "vihāsiṃ sāsane rato;",
  "thag16.9:16.3": "Tisso vijjā anuppattā,",
  "thag16.9:16.4": "kataṃ buddhassa sāsanaṃ.",
  "thag16.9:17.1": "Pañcapaññāsavassāni,",
  "thag16.9:17.2": "yato nesajjiko ahaṃ;",
  "thag16.9:17.3": "Pañcavīsativassāni,",
  "thag16.9:17.4": "yato middhaṃ samūhataṃ.",
  "thag16.9:18.1": "Nāhu assāsapassāsā,",
  "thag16.9:18.2": "ṭhitacittassa tādino;",
  "thag16.9:18.3": "Anejo santimārabbha,",
  "thag16.9:18.4": "cakkhumā parinibbuto.",
  "thag16.9:19.1": "Asallīnena cittena,",
  "thag16.9:19.2": "vedanaṃ ajjhavāsayi;",
  "thag16.9:19.3": "Pajjotasseva nibbānaṃ,",
  "thag16.9:19.4": "vimokkho cetaso ahu.",
  "thag16.9:20.1": "Ete pacchimakā dāni,",
  "thag16.9:20.2": "munino phassapañcamā;",
  "thag16.9:20.3": "Nāññe dhammā bhavissanti,",
  "thag16.9:20.4": "sambuddhe parinibbute.",
  "thag16.9:21.1": "Natthi dāni punāvāso,",
  "thag16.9:21.2": "devakāyasmi jālini;",
  "thag16.9:21.3": "Vikkhīṇo jātisaṃsāro,",
  "thag16.9:21.4": "natthi dāni punabbhavo.",
  "thag16.9:22.1": "Yassa muhuttena sahassadhā,",
  "thag16.9:22.2": "Loko saṃvidito sabrahmakappo;",
  "thag16.9:22.3": "Vasī iddhiguṇe cutūpapāte,",
  "thag16.9:22.4": "Kāle passati devatā sa bhikkhu.",
  "thag16.9:23.1": "Annabhāro pure āsiṃ,",
  "thag16.9:23.2": "daliddo ghāsahārako;",
  "thag16.9:23.3": "Samaṇaṃ paṭipādesiṃ,",
  "thag16.9:23.4": "upariṭṭhaṃ yasassinaṃ.",
  "thag16.9:24.1": "Somhi sakyakule jāto,",
  "thag16.9:24.2": "anuruddhoti maṃ vidū;",
  "thag16.9:24.3": "Upeto naccagītehi,",
  "thag16.9:24.4": "sammatāḷappabodhano.",
  "thag16.9:25.1": "Athaddasāsiṃ sambuddhaṃ,",
  "thag16.9:25.2": "satthāraṃ akutobhayaṃ;",
  "thag16.9:25.3": "Tasmiṃ cittaṃ pasādetvā,",
  "thag16.9:25.4": "pabbajiṃ anagāriyaṃ.",
  "thag16.9:26.1": "Pubbenivāsaṃ jānāmi,",
  "thag16.9:26.2": "yattha me vusitaṃ pure;",
  "thag16.9:26.3": "Tāvatiṃsesu devesu,",
  "thag16.9:26.4": "aṭṭhāsiṃ sakkajātiyā.",
  "thag16.9:27.1": "Sattakkhattuṃ manussindo,",
  "thag16.9:27.2": "ahaṃ rajjamakārayiṃ;",
  "thag16.9:27.3": "Cāturanto vijitāvī,",
  "thag16.9:27.4": "jambusaṇḍassa issaro;",
  "thag16.9:27.5": "Adaṇḍena asatthena,",
  "thag16.9:27.6": "dhammena anusāsayiṃ.",
  "thag16.9:28.1": "Ito satta tato satta,",
  "thag16.9:28.2": "saṃsārāni catuddasa;",
  "thag16.9:28.3": "Nivāsamabhijānissaṃ,",
  "thag16.9:28.4": "devaloke ṭhito tadā.",
  "thag16.9:29.1": "Pañcaṅgike samādhimhi,",
  "thag16.9:29.2": "sante ekodibhāvite;",
  "thag16.9:29.3": "Paṭippassaddhiladdhamhi,",
  "thag16.9:29.4": "dibbacakkhu visujjhi me.",
  "thag16.9:30.1": "Cutūpapātaṃ jānāmi,",
  "thag16.9:30.2": "sattānaṃ āgatiṃ gatiṃ;",
  "thag16.9:30.3": "Itthabhāvaññathābhāvaṃ,",
  "thag16.9:30.4": "jhāne pañcaṅgike ṭhito.",
  "thag16.9:31.1": "Pariciṇṇo mayā satthā,",
  "thag16.9:31.2": "…",
  "thag16.9:31.3": "pe…",
  "thag16.9:31.4": "bhavanetti samūhatā.",
  "thag16.9:32.1": "Vajjīnaṃ veḷuvagāme,",
  "thag16.9:32.2": "ahaṃ jīvitasaṅkhayā;",
  "thag16.9:32.3": "Heṭṭhato veḷugumbasmiṃ,",
  "thag16.9:32.4": "nibbāyissaṃ anāsavo”ti.",
  "thag16.9:33.1": "…",
  "thag16.9:33.2": "Anuruddho thero …."
}