{
  "thag1.104:1.1": "Theragāthā",
  "thag1.104:2.1": "Ekakanipāta",
  "thag1.104:3.1": "Ekādasamavagga",
  "thag1.104:4.1": "1.104. Khitakattheragāthā",
  "thag1.104:5.1": "“Lahuko vata me kāyo,",
  "thag1.104:5.2": "Phuṭṭho ca pītisukhena vipulena;",
  "thag1.104:5.3": "Tūlamiva eritaṃ mālutena,",
  "thag1.104:5.4": "Pilavatīva me kāyo”ti.",
  "thag1.104:6.1": "…",
  "thag1.104:6.2": "Khitako thero …."
}