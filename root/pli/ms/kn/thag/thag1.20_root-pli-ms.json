{
  "thag1.20:1.1": "Theragāthā",
  "thag1.20:2.1": "Ekakanipāta",
  "thag1.20:3.1": "Dutiyavagga",
  "thag1.20:4.1": "1.20. Ajitattheragāthā",
  "thag1.20:5.1": "“Maraṇe me bhayaṃ natthi,",
  "thag1.20:5.2": "nikanti natthi jīvite;",
  "thag1.20:5.3": "Sandehaṃ nikkhipissāmi,",
  "thag1.20:5.4": "sampajāno paṭissato”ti.",
  "thag1.20:6.1": "…",
  "thag1.20:6.2": "Ajito thero …",
  "thag1.20:6.3": "vaggo dutiyo.",
  "thag1.20:7.1": "Tassuddānaṃ",
  "thag1.20:8.1": "Cūḷavaccho mahāvaccho,",
  "thag1.20:8.2": "vanavaccho ca sīvako;",
  "thag1.20:8.3": "Kuṇḍadhāno ca belaṭṭhi,",
  "thag1.20:8.4": "dāsako ca tatopari;",
  "thag1.20:8.5": "Siṅgālapitiko thero,",
  "thag1.20:8.6": "kulo ca ajito dasāti."
}