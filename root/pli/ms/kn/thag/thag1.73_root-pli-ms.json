{
  "thag1.73:1.1": "Theragāthā",
  "thag1.73:2.1": "Ekakanipāta",
  "thag1.73:3.1": "Aṭṭhamavagga",
  "thag1.73:4.1": "1.73. Māṇavattheragāthā",
  "thag1.73:5.1": "“Jiṇṇañca disvā dukhitañca byādhitaṃ,",
  "thag1.73:5.2": "Matañca disvā gatamāyusaṅkhayaṃ;",
  "thag1.73:5.3": "Tato ahaṃ nikkhamitūna pabbajiṃ,",
  "thag1.73:5.4": "Pahāya kāmāni manoramānī”ti.",
  "thag1.73:6.1": "…",
  "thag1.73:6.2": "Māṇavo thero …."
}