{
  "thag1.58:1.1": "Theragāthā",
  "thag1.58:2.1": "Ekakanipāta",
  "thag1.58:3.1": "Chaṭṭhavagga",
  "thag1.58:4.1": "1.58. Ramaṇīyakuṭikattheragāthā",
  "thag1.58:5.1": "“Ramaṇīyā me kuṭikā,",
  "thag1.58:5.2": "Saddhādeyyā manoramā;",
  "thag1.58:5.3": "Na me attho kumārīhi,",
  "thag1.58:5.4": "Yesaṃ attho tahiṃ gacchatha nāriyo”ti.",
  "thag1.58:6.1": "…",
  "thag1.58:6.2": "Ramaṇīyakuṭiko thero …."
}