{
  "thag1.111:1.1": "Theragāthā",
  "thag1.111:2.1": "Ekakanipāta",
  "thag1.111:3.1": "Dvādasamavagga",
  "thag1.111:4.1": "1.111. Jentattheragāthā",
  "thag1.111:5.1": "“Duppabbajjaṃ ve duradhivāsā gehā,",
  "thag1.111:5.2": "Dhammo gambhīro duradhigamā bhogā;",
  "thag1.111:5.3": "Kicchā vutti no itarītareneva,",
  "thag1.111:5.4": "Yuttaṃ cintetuṃ satatamaniccatan”ti.",
  "thag1.111:6.1": "…",
  "thag1.111:6.2": "Jento thero …."
}