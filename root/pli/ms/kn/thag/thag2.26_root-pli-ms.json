{
  "thag2.26:1.1": "Theragāthā",
  "thag2.26:2.1": "Dukanipāta",
  "thag2.26:3.1": "Tatiyavagga",
  "thag2.26:4.1": "2.26. Puṇṇamāsattheragāthā",
  "thag2.26:5.1": "“Pañca nīvaraṇe hitvā,",
  "thag2.26:5.2": "yogakkhemassa pattiyā;",
  "thag2.26:5.3": "Dhammādāsaṃ gahetvāna,",
  "thag2.26:5.4": "ñāṇadassanamattano.",
  "thag2.26:6.1": "Paccavekkhiṃ imaṃ kāyaṃ,",
  "thag2.26:6.2": "sabbaṃ santarabāhiraṃ;",
  "thag2.26:6.3": "Ajjhattañca bahiddhā ca,",
  "thag2.26:6.4": "tuccho kāyo adissathā”ti.",
  "thag2.26:7.1": "…",
  "thag2.26:7.2": "Puṇṇamāso thero …."
}