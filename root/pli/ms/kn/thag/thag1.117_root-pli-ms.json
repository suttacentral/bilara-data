{
  "thag1.117:1.1": "Theragāthā",
  "thag1.117:2.1": "Ekakanipāta",
  "thag1.117:3.1": "Dvādasamavagga",
  "thag1.117:4.1": "1.117. Yasattheragāthā",
  "thag1.117:5.1": "“Suvilitto suvasano,",
  "thag1.117:5.2": "sabbābharaṇabhūsito;",
  "thag1.117:5.3": "Tisso vijjā ajjhagamiṃ,",
  "thag1.117:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.117:6.1": "…",
  "thag1.117:6.2": "Yaso thero …."
}