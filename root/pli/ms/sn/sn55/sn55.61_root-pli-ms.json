{
  "sn55.61:0.1": "Saṃyutta Nikāya 55 ",
  "sn55.61:0.2": "6. Sappaññavagga ",
  "sn55.61:0.3": "61. Paññāvepullasutta ",
  "sn55.61:1.1": "“…. Paññāvepullāya … saṃvattantī”ti. ",
  "sn55.61:1.2": "Ekādasamaṃ. ",
  "sn55.61:1.3": "Sappaññavaggo chaṭṭho. ",
  "sn55.61:2.1": "Sagāthakaṃ vassaṃvutthaṃ, ",
  "sn55.61:2.2": "dhammadinnañca gilānaṃ; ",
  "sn55.61:2.3": "Caturo phalā paṭilābho, ",
  "sn55.61:2.4": "vuddhi vepullatāya cāti. "
}