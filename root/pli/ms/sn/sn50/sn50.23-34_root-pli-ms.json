{
  "sn50.23-34:0.1": "Saṃyutta Nikāya 50 ",
  "sn50.23-34:0.2": "3. Balakaraṇīyavagga ",
  "sn50.23-34:0.3": "23. Balakaraṇīyavagga ",
  "sn50.23-34:1.1": "(Balakaraṇīyavaggo vitthāretabbo.) ",
  "sn50.23-34:2.1": "Balaṃ bījañca nāgo ca, ",
  "sn50.23-34:2.2": "rukkho kumbhena sūkiyā; ",
  "sn50.23-34:2.3": "Ākāsena ca dve meghā, ",
  "sn50.23-34:2.4": "nāvā āgantukā nadīti. "
}