{
  "sn46.43:0.1": "Saṃyutta Nikāya 46 ",
  "sn46.43:0.2": "5. Cakkavattivagga ",
  "sn46.43:0.3": "43. Mārasutta ",
  "sn46.43:1.1": "“Mārasenappamaddanaṃ vo, bhikkhave, maggaṃ desessāmi; ",
  "sn46.43:1.2": "taṃ suṇātha. ",
  "sn46.43:1.3": "Katamo ca, bhikkhave, mārasenappamaddano maggo? ",
  "sn46.43:1.4": "Yadidaṃ—",
  "sn46.43:1.5": "satta bojjhaṅgā. ",
  "sn46.43:1.6": "Katame satta? ",
  "sn46.43:1.7": "Satisambojjhaṅgo …pe… upekkhāsambojjhaṅgo—",
  "sn46.43:1.8": "ayaṃ kho, bhikkhave, mārasenappamaddano maggo”ti. ",
  "sn46.43:1.9": "Tatiyaṃ. "
}