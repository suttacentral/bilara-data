{
  "sn46.143-152:0.1": "Saṃyutta Nikāya 46 ",
  "sn46.143-152:0.2": "15. Punaappamādavagga ",
  "sn46.143-152:0.3": "143–152. Punatathāgatādisutta ",
  "sn46.143-152:1.1": "…pe… ",
  "sn46.143-152:1.2": "Pannarasamo. ",
  "sn46.143-152:2.1": "Tathāgataṃ padaṃ kūṭaṃ, ",
  "sn46.143-152:2.2": "mūlaṃ sārena vassikaṃ; ",
  "sn46.143-152:2.3": "Rājā candimasūriyā ca, ",
  "sn46.143-152:2.4": "vatthena dasamaṃ padanti. ",
  "sn46.143-152:3.1": "(Appamādavaggo rāgavasena vitthāretabbo.) "
}