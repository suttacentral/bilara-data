{
  "sn48.1:0.1": "Saṃyutta Nikāya 48 ",
  "sn48.1:0.2": "1. Suddhikavagga ",
  "sn48.1:0.3": "1. Suddhikasutta ",
  "sn48.1:1.1": "Sāvatthinidānaṃ. ",
  "sn48.1:1.2": "Tatra kho bhagavā etadavoca. ",
  "sn48.1:1.3": "“Pañcimāni, bhikkhave, indriyāni. ",
  "sn48.1:1.4": "Katamāni pañca? ",
  "sn48.1:1.5": "Saddhindriyaṃ, vīriyindriyaṃ, satindriyaṃ, samādhindriyaṃ, paññindriyaṃ—",
  "sn48.1:1.6": "imāni kho, bhikkhave, pañcindriyānī”ti. ",
  "sn48.1:1.7": "Paṭhamaṃ. "
}