{
  "sn22.32:0.1": "Saṃyutta Nikāya 22 ",
  "sn22.32:0.2": "3. Bhāravagga ",
  "sn22.32:0.3": "32. Pabhaṅgusutta ",
  "sn22.32:1.1": "Sāvatthinidānaṃ. ",
  "sn22.32:1.2": "“Pabhaṅguñca, bhikkhave, desessāmi appabhaṅguñca. ",
  "sn22.32:1.3": "Taṃ suṇātha. ",
  "sn22.32:1.4": "Kiñca, bhikkhave, pabhaṅgu, kiṃ appabhaṅgu? ",
  "sn22.32:1.5": "Rūpaṃ, bhikkhave, pabhaṅgu. ",
  "sn22.32:1.6": "Yo tassa nirodho vūpasamo atthaṅgamo, idaṃ appabhaṅgu. ",
  "sn22.32:1.7": "Vedanā pabhaṅgu. ",
  "sn22.32:1.8": "Yo tassā nirodho vūpasamo atthaṅgamo, idaṃ appabhaṅgu. ",
  "sn22.32:1.9": "Saññā pabhaṅgu … ",
  "sn22.32:1.10": "saṅkhārā pabhaṅgu. ",
  "sn22.32:1.11": "Yo tesaṃ nirodho vūpasamo atthaṅgamo, idaṃ appabhaṅgu. ",
  "sn22.32:1.12": "Viññāṇaṃ pabhaṅgu. ",
  "sn22.32:1.13": "Yo tassa nirodho vūpasamo atthaṅgamo, idaṃ appabhaṅgū”ti. ",
  "sn22.32:1.14": "Ekādasamaṃ. ",
  "sn22.32:1.15": "Bhāravaggo tatiyo. ",
  "sn22.32:2.1": "Bhāraṃ pariññaṃ abhijānaṃ, ",
  "sn22.32:2.2": "chandarāgaṃ catutthakaṃ; ",
  "sn22.32:2.3": "Assādā ca tayo vuttā, ",
  "sn22.32:2.4": "abhinandanamaṭṭhamaṃ; ",
  "sn22.32:2.5": "Uppādaṃ aghamūlañca, ",
  "sn22.32:2.6": "ekādasamo pabhaṅgūti. "
}