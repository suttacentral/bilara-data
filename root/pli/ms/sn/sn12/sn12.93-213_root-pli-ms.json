{
  "sn12.93-103:0.1": "Saṃyutta Nikāya 12 ",
  "sn12.93-103:0.2": "9. Antarapeyyāla ",
  "sn12.93-103:0.3": "93. Sikkhāsuttādipeyyālaekādasaka ",
  "sn12.93-103:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ jarāmaraṇe yathābhūtaṃ ñāṇāya sikkhā karaṇīyā. ",
  "sn12.93-103:1.2": "(93–103.: 364–374.) ",
  "sn12.93-103:2.1": "(Peyyālo. Catusaccikaṃ kātabbaṃ.) ",
  "sn12.104-114:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.104-114:1.2": "yogo karaṇīyo …pe…. ",
  "sn12.104-114:1.3": "(104–114.: 375–385.) ",
  "sn12.115-125:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.115-125:1.2": "chando karaṇīyo …pe…. ",
  "sn12.115-125:1.3": "(115–125.: 386–396.) ",
  "sn12.126-136:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.126-136:1.2": "ussoḷhī karaṇīyā …pe…. ",
  "sn12.126-136:1.3": "(126–136.: 397–407.) ",
  "sn12.137-147:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.137-147:1.2": "appaṭivānī karaṇīyā …pe…. ",
  "sn12.137-147:1.3": "(137–147.: 408–418.) ",
  "sn12.148-158:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.148-158:1.2": "ātappaṃ karaṇīyaṃ …pe…. ",
  "sn12.148-158:1.3": "(148–158.: 419–429.) ",
  "sn12.159-169:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.159-169:1.2": "vīriyaṃ karaṇīyaṃ …pe…. ",
  "sn12.159-169:1.3": "(159–169.: 430–440.) ",
  "sn12.170-180:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.170-180:1.2": "sātaccaṃ karaṇīyaṃ …pe…. ",
  "sn12.170-180:1.3": "(170–180.: 441–451.) ",
  "sn12.181-191:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.181-191:1.2": "sati karaṇīyā …pe…. ",
  "sn12.181-191:1.3": "(181–191.: 452–462.) ",
  "sn12.192-202:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.192-202:1.2": "sampajaññaṃ karaṇīyaṃ …pe…. ",
  "sn12.192-202:1.3": "(192–202.: 463–473.) ",
  "sn12.203-213:1.1": "“Jarāmaraṇaṃ, bhikkhave, ajānatā …pe… ",
  "sn12.203-213:1.2": "appamādo karaṇīyo …pe…. ",
  "sn12.203-213:1.3": "(203–213.: 474–484.) ",
  "sn12.203-213:1.4": "Antarapeyyālo navamo. ",
  "sn12.203-213:2.1": "Satthā sikkhā ca yogo ca, ",
  "sn12.203-213:2.2": "chando ussoḷhipañcamī; ",
  "sn12.203-213:2.3": "Appaṭivāni yātappaṃ, ",
  "sn12.203-213:2.4": "vīriyaṃ sātaccamuccati; ",
  "sn12.203-213:2.5": "Sati ca sampajaññañca, ",
  "sn12.203-213:2.6": "appamādena dvādasāti. ",
  "sn12.203-213:2.7": "Suttantā antarapeyyālā niṭṭhitā. ",
  "sn12.203-213:3.1": "Pare te dvādasa honti, ",
  "sn12.203-213:3.2": "suttā dvattiṃsa satāni; ",
  "sn12.203-213:3.3": "Catusaccena te vuttā, ",
  "sn12.203-213:3.4": "peyyālaantaramhi yeti. ",
  "sn12.203-213:3.5": "Antarapeyyālesu uddānaṃ samattaṃ. ",
  "sn12.203-213:3.6": "Nidānasaṃyuttaṃ samattaṃ. "
}