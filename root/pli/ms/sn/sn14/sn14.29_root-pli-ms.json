{
  "sn14.29:0.1": "Saṃyutta Nikāya 14 ",
  "sn14.29:0.2": "3. Kammapathavagga ",
  "sn14.29:0.3": "29. Dasaṅgasutta ",
  "sn14.29:1.1": "Sāvatthiyaṃ viharati. ",
  "sn14.29:1.2": "“Dhātusova, bhikkhave, sattā saṃsandanti samenti. ",
  "sn14.29:1.3": "Micchādiṭṭhikā micchādiṭṭhikehi saddhiṃ saṃsandanti samenti; ",
  "sn14.29:1.4": "micchāsaṅkappā …pe… ",
  "sn14.29:1.5": "micchāvācā … ",
  "sn14.29:1.6": "micchākammantā … ",
  "sn14.29:1.7": "micchāājīvā … ",
  "sn14.29:1.8": "micchāvāyāmā … ",
  "sn14.29:1.9": "micchāsatino … ",
  "sn14.29:1.10": "micchāsamādhino micchāsamādhīhi saddhiṃ saṃsandanti samenti; ",
  "sn14.29:1.11": "micchāñāṇino micchāñāṇīhi saddhiṃ saṃsandanti samenti; ",
  "sn14.29:1.12": "micchāvimuttino micchāvimuttīhi saddhiṃ saṃsandanti samenti. ",
  "sn14.29:2.1": "Sammādiṭṭhikā sammādiṭṭhikehi saddhiṃ saṃsandanti samenti; ",
  "sn14.29:2.2": "sammāsaṅkappā …pe… ",
  "sn14.29:2.3": "sammāvācā … ",
  "sn14.29:2.4": "sammākammantā … ",
  "sn14.29:2.5": "sammāājīvā … ",
  "sn14.29:2.6": "sammāvāyāmā … ",
  "sn14.29:2.7": "sammāsatino … ",
  "sn14.29:2.8": "sammāsamādhino … ",
  "sn14.29:2.9": "sammāñāṇino sammāñāṇīhi saddhiṃ saṃsandanti samenti; ",
  "sn14.29:2.10": "sammāvimuttino sammāvimuttīhi saddhiṃ saṃsandanti samentī”ti. ",
  "sn14.29:2.11": "Sattamaṃ. ",
  "sn14.29:2.12": "(Sabbattha atītānāgatapaccuppannaṃ kātabbaṃ.) ",
  "sn14.29:3.1": "Asamāhitaṃ dussīlaṃ, ",
  "sn14.29:3.2": "pañca sikkhāpadāni ca; ",
  "sn14.29:3.3": "Satta kammapathā vuttā, ",
  "sn14.29:3.4": "dasakammapathena ca; ",
  "sn14.29:3.5": "Chaṭṭhaṃ aṭṭhaṅgiko vutto, ",
  "sn14.29:3.6": "dasaṅgena ca sattamaṃ. ",
  "sn14.29:3.7": "Kammapathavaggo tatiyo. "
}