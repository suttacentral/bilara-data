{
  "sn14.28:0.1": "Saṃyutta Nikāya 14 ",
  "sn14.28:0.2": "3. Kammapathavagga ",
  "sn14.28:0.3": "28. Aṭṭhaṅgikasutta ",
  "sn14.28:1.1": "Sāvatthiyaṃ viharati. ",
  "sn14.28:1.2": "“Dhātusova, bhikkhave, sattā saṃsandanti samenti. ",
  "sn14.28:1.3": "Micchādiṭṭhikā micchādiṭṭhikehi saddhiṃ saṃsandanti samenti; ",
  "sn14.28:1.4": "micchāsaṅkappā …pe… ",
  "sn14.28:1.5": "micchāvācā … ",
  "sn14.28:1.6": "micchākammantā … ",
  "sn14.28:1.7": "micchāājīvā … ",
  "sn14.28:1.8": "micchāvāyāmā … ",
  "sn14.28:1.9": "micchāsatino … ",
  "sn14.28:1.10": "micchāsamādhino micchāsamādhīhi saddhiṃ saṃsandanti samenti. ",
  "sn14.28:2.1": "Sammādiṭṭhikā sammādiṭṭhikehi saddhiṃ saṃsandanti samenti; ",
  "sn14.28:2.2": "sammāsaṅkappā …pe… ",
  "sn14.28:2.3": "sammāvācā … ",
  "sn14.28:2.4": "sammākammantā … ",
  "sn14.28:2.5": "sammāājīvā … ",
  "sn14.28:2.6": "sammāvāyāmā … ",
  "sn14.28:2.7": "sammāsatino … ",
  "sn14.28:2.8": "sammāsamādhino sammāsamādhīhi saddhiṃ saṃsandanti samentī”ti. ",
  "sn14.28:2.9": "Chaṭṭhaṃ. "
}