{
  "sn35.156:0.1": "Saṃyutta Nikāya 35 ",
  "sn35.156:0.2": "16. Nandikkhayavagga ",
  "sn35.156:0.3": "156. Ajjhattanandikkhayasutta ",
  "sn35.156:1.1": "“Aniccaṃyeva, bhikkhave, bhikkhu cakkhuṃ aniccanti passati, sāssa hoti sammādiṭṭhi. ",
  "sn35.156:1.2": "Sammā passaṃ nibbindati. ",
  "sn35.156:1.3": "Nandikkhayā rāgakkhayo; ",
  "sn35.156:1.4": "rāgakkhayā nandikkhayo. ",
  "sn35.156:1.5": "Nandirāgakkhayā cittaṃ suvimuttanti vuccati …pe… ",
  "sn35.156:1.6": "aniccaṃyeva, bhikkhave, bhikkhu jivhaṃ aniccanti passati, sāssa hoti sammādiṭṭhi. ",
  "sn35.156:1.7": "Sammā passaṃ nibbindati. ",
  "sn35.156:1.8": "Nandikkhayā rāgakkhayo; ",
  "sn35.156:1.9": "rāgakkhayā …pe… ",
  "sn35.156:1.10": "cittaṃ suvimuttanti vuccati …pe… ",
  "sn35.156:1.11": "aniccaṃyeva, bhikkhave, bhikkhu manaṃ aniccanti passati, sāssa hoti sammādiṭṭhi. ",
  "sn35.156:1.12": "Sammā passaṃ nibbindati. ",
  "sn35.156:1.13": "Nandikkhayā rāgakkhayo; ",
  "sn35.156:1.14": "rāgakkhayā nandikkhayo. ",
  "sn35.156:1.15": "Nandirāgakkhayā cittaṃ suvimuttanti vuccatī”ti. ",
  "sn35.156:1.16": "Paṭhamaṃ. "
}