{
  "sn1.25:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.25:0.2": "3. Sattivagga ",
  "sn1.25:0.3": "25. Arahantasutta ",
  "sn1.25:1.1": "“Yo hoti bhikkhu arahaṃ katāvī, ",
  "sn1.25:1.2": "Khīṇāsavo antimadehadhārī; ",
  "sn1.25:1.3": "Ahaṃ vadāmītipi so vadeyya, ",
  "sn1.25:1.4": "Mamaṃ vadantītipi so vadeyyā”ti. ",
  "sn1.25:2.1": "“Yo hoti bhikkhu arahaṃ katāvī, ",
  "sn1.25:2.2": "Khīṇāsavo antimadehadhārī; ",
  "sn1.25:2.3": "Ahaṃ vadāmītipi so vadeyya, ",
  "sn1.25:2.4": "Mamaṃ vadantītipi so vadeyya; ",
  "sn1.25:2.5": "Loke samaññaṃ kusalo viditvā, ",
  "sn1.25:2.6": "Vohāramattena so vohareyyā”ti. ",
  "sn1.25:3.1": "“Yo hoti bhikkhu arahaṃ katāvī, ",
  "sn1.25:3.2": "Khīṇāsavo antimadehadhārī; ",
  "sn1.25:3.3": "Mānaṃ nu kho so upagamma bhikkhu, ",
  "sn1.25:3.4": "Ahaṃ vadāmītipi so vadeyya; ",
  "sn1.25:3.5": "Mamaṃ vadantītipi so vadeyyā”ti. ",
  "sn1.25:4.1": "“Pahīnamānassa na santi ganthā, ",
  "sn1.25:4.2": "Vidhūpitā mānaganthassa sabbe; ",
  "sn1.25:4.3": "Sa vītivatto maññataṃ sumedho, ",
  "sn1.25:4.4": "Ahaṃ vadāmītipi so vadeyya. ",
  "sn1.25:5.1": "Mamaṃ vadantītipi so vadeyya, ",
  "sn1.25:5.2": "Loke samaññaṃ kusalo viditvā; ",
  "sn1.25:5.3": "Vohāramattena so vohareyyā”ti. "
}