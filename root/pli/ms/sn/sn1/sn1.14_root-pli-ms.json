{
  "sn1.14:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.14:0.2": "2. Nandanavagga ",
  "sn1.14:0.3": "14. Khattiyasutta ",
  "sn1.14:1.1": "“Khattiyo dvipadaṃ seṭṭho, ",
  "sn1.14:1.2": "balībaddo catuppadaṃ; ",
  "sn1.14:1.3": "Komārī seṭṭhā bhariyānaṃ, ",
  "sn1.14:1.4": "yo ca puttāna pubbajo”ti. ",
  "sn1.14:2.1": "“Sambuddho dvipadaṃ seṭṭho, ",
  "sn1.14:2.2": "ājānīyo catuppadaṃ; ",
  "sn1.14:2.3": "Sussūsā seṭṭhā bhariyānaṃ, ",
  "sn1.14:2.4": "yo ca puttānamassavo”ti. "
}