{
  "sn1.43:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.43:0.2": "5. Ādittavagga ",
  "sn1.43:0.3": "43. Annasutta ",
  "sn1.43:1.1": "“Annamevābhinandanti, ",
  "sn1.43:1.2": "ubhaye devamānusā; ",
  "sn1.43:1.3": "Atha ko nāma so yakkho, ",
  "sn1.43:1.4": "yaṃ annaṃ nābhinandatī”ti. ",
  "sn1.43:2.1": "“Ye naṃ dadanti saddhāya, ",
  "sn1.43:2.2": "vippasannena cetasā; ",
  "sn1.43:2.3": "Tameva annaṃ bhajati, ",
  "sn1.43:2.4": "asmiṃ loke paramhi ca. ",
  "sn1.43:3.1": "Tasmā vineyya maccheraṃ, ",
  "sn1.43:3.2": "dajjā dānaṃ malābhibhū; ",
  "sn1.43:3.3": "Puññāni paralokasmiṃ, ",
  "sn1.43:3.4": "patiṭṭhā honti pāṇinan”ti. "
}