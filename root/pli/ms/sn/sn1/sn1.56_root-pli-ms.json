{
  "sn1.56:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.56:0.2": "6. Jarāvagga ",
  "sn1.56:0.3": "56. Dutiyajanasutta ",
  "sn1.56:1.1": "“Kiṃsu janeti purisaṃ, ",
  "sn1.56:1.2": "kiṃsu tassa vidhāvati; ",
  "sn1.56:1.3": "Kiṃsu saṃsāramāpādi, ",
  "sn1.56:1.4": "kismā na parimuccatī”ti. ",
  "sn1.56:2.1": "“Taṇhā janeti purisaṃ, ",
  "sn1.56:2.2": "cittamassa vidhāvati; ",
  "sn1.56:2.3": "Satto saṃsāramāpādi, ",
  "sn1.56:2.4": "dukkhā na parimuccatī”ti. "
}