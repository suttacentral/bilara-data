{
  "sn1.74:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.74:0.2": "8. Chetvāvagga ",
  "sn1.74:0.3": "74. Vuṭṭhisutta ",
  "sn1.74:1.1": "“Kiṃsu uppatataṃ seṭṭhaṃ, ",
  "sn1.74:1.2": "kiṃsu nipatataṃ varaṃ; ",
  "sn1.74:1.3": "Kiṃsu pavajamānānaṃ, ",
  "sn1.74:1.4": "kiṃsu pavadataṃ varan”ti. ",
  "sn1.74:2.1": "“Bījaṃ uppatataṃ seṭṭhaṃ, ",
  "sn1.74:2.2": "vuṭṭhi nipatataṃ varā; ",
  "sn1.74:2.3": "Gāvo pavajamānānaṃ, ",
  "sn1.74:2.4": "putto pavadataṃ varo”ti. ",
  "sn1.74:3.1": "“Vijjā uppatataṃ seṭṭhā, ",
  "sn1.74:3.2": "avijjā nipatataṃ varā; ",
  "sn1.74:3.3": "Saṅgho pavajamānānaṃ, ",
  "sn1.74:3.4": "buddho pavadataṃ varo”ti. "
}