{
  "sn1.30:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.30:0.2": "3. Sattivagga ",
  "sn1.30:0.3": "30. Eṇijaṅghasutta ",
  "sn1.30:1.1": "“Eṇijaṅghaṃ kisaṃ vīraṃ, ",
  "sn1.30:1.2": "appāhāraṃ alolupaṃ; ",
  "sn1.30:1.3": "Sīhaṃvekacaraṃ nāgaṃ, ",
  "sn1.30:1.4": "kāmesu anapekkhinaṃ; ",
  "sn1.30:1.5": "Upasaṅkamma pucchāma, ",
  "sn1.30:1.6": "kathaṃ dukkhā pamuccatī”ti. ",
  "sn1.30:2.1": "“Pañca kāmaguṇā loke, ",
  "sn1.30:2.2": "manochaṭṭhā paveditā; ",
  "sn1.30:2.3": "Ettha chandaṃ virājetvā, ",
  "sn1.30:2.4": "evaṃ dukkhā pamuccatī”ti. ",
  "sn1.30:2.5": "Sattivaggo tatiyo. ",
  "sn1.30:3.1": "Sattiyā phusati ceva, ",
  "sn1.30:3.2": "jaṭā manonivāraṇā; ",
  "sn1.30:3.3": "Arahantena pajjoto, ",
  "sn1.30:3.4": "sarā mahaddhanena ca; ",
  "sn1.30:3.5": "Catucakkena navamaṃ, ",
  "sn1.30:3.6": "eṇijaṅghena te dasāti. "
}