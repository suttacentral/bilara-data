{
  "sn1.6:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.6:0.2": "1. Naḷavagga ",
  "sn1.6:0.3": "6. Jāgarasutta ",
  "sn1.6:1.1": "Sāvatthinidānaṃ. ",
  "sn1.6:1.2": "Ekamantaṃ ṭhitā kho sā devatā bhagavato santike imaṃ gāthaṃ abhāsi: ",
  "sn1.6:2.1": "“Kati jāgarataṃ suttā, ",
  "sn1.6:2.2": "kati suttesu jāgarā; ",
  "sn1.6:2.3": "Katibhi rajamādeti, ",
  "sn1.6:2.4": "katibhi parisujjhatī”ti. ",
  "sn1.6:3.1": "“Pañca jāgarataṃ suttā, ",
  "sn1.6:3.2": "pañca suttesu jāgarā; ",
  "sn1.6:3.3": "Pañcabhi rajamādeti, ",
  "sn1.6:3.4": "pañcabhi parisujjhatī”ti. "
}