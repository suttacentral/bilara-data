{
  "sn1.78:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.78:0.2": "8. Chetvāvagga ",
  "sn1.78:0.3": "78. Kāmasutta ",
  "sn1.78:1.1": "“Kimatthakāmo na dade, ",
  "sn1.78:1.2": "kiṃ macco na pariccaje; ",
  "sn1.78:1.3": "Kiṃsu muñceyya kalyāṇaṃ, ",
  "sn1.78:1.4": "pāpikaṃ na ca mocaye”ti. ",
  "sn1.78:2.1": "“Attānaṃ na dade poso, ",
  "sn1.78:2.2": "attānaṃ na pariccaje; ",
  "sn1.78:2.3": "Vācaṃ muñceyya kalyāṇaṃ, ",
  "sn1.78:2.4": "pāpikañca na mocaye”ti. "
}