{
  "sn1.46:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.46:0.2": "5. Ādittavagga ",
  "sn1.46:0.3": "46. Accharāsutta ",
  "sn1.46:1.1": "“Accharāgaṇasaṅghuṭṭhaṃ, ",
  "sn1.46:1.2": "pisācagaṇasevitaṃ; ",
  "sn1.46:1.3": "Vanantaṃ mohanaṃ nāma, ",
  "sn1.46:1.4": "kathaṃ yātrā bhavissatī”ti. ",
  "sn1.46:2.1": "“Ujuko nāma so maggo, ",
  "sn1.46:2.2": "abhayā nāma sā disā; ",
  "sn1.46:2.3": "Ratho akūjano nāma, ",
  "sn1.46:2.4": "dhammacakkehi saṃyuto. ",
  "sn1.46:3.1": "Hirī tassa apālambo, ",
  "sn1.46:3.2": "satyassa parivāraṇaṃ; ",
  "sn1.46:3.3": "Dhammāhaṃ sārathiṃ brūmi, ",
  "sn1.46:3.4": "sammādiṭṭhipurejavaṃ. ",
  "sn1.46:4.1": "Yassa etādisaṃ yānaṃ, ",
  "sn1.46:4.2": "itthiyā purisassa vā; ",
  "sn1.46:4.3": "Sa ve etena yānena, ",
  "sn1.46:4.4": "nibbānasseva santike”ti. "
}