{
  "sn1.64:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.64:0.2": "7. Addhavagga ",
  "sn1.64:0.3": "64. Saṃyojanasutta ",
  "sn1.64:1.1": "“Kiṃsu saṃyojano loko, ",
  "sn1.64:1.2": "kiṃsu tassa vicāraṇaṃ; ",
  "sn1.64:1.3": "Kissassu vippahānena, ",
  "sn1.64:1.4": "nibbānaṃ iti vuccatī”ti. ",
  "sn1.64:2.1": "“Nandīsaṃyojano loko, ",
  "sn1.64:2.2": "vitakkassa vicāraṇaṃ; ",
  "sn1.64:2.3": "Taṇhāya vippahānena, ",
  "sn1.64:2.4": "nibbānaṃ iti vuccatī”ti. "
}