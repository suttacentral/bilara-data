{
  "sn1.52:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.52:0.2": "6. Jarāvagga ",
  "sn1.52:0.3": "52. Ajarasāsutta ",
  "sn1.52:1.1": "“Kiṃsu ajarasā sādhu, ",
  "sn1.52:1.2": "kiṃsu sādhu adhiṭṭhitaṃ; ",
  "sn1.52:1.3": "Kiṃsu narānaṃ ratanaṃ, ",
  "sn1.52:1.4": "kiṃsu corehyahāriyan”ti. ",
  "sn1.52:2.1": "“Sīlaṃ ajarasā sādhu, ",
  "sn1.52:2.2": "saddhā sādhu adhiṭṭhitā; ",
  "sn1.52:2.3": "Paññā narānaṃ ratanaṃ, ",
  "sn1.52:2.4": "puññaṃ corehyahāriyan”ti. "
}