{
  "sn1.60:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.60:0.2": "6. Jarāvagga ",
  "sn1.60:0.3": "60. Kavisutta ",
  "sn1.60:1.1": "“Kiṃsu nidānaṃ gāthānaṃ, ",
  "sn1.60:1.2": "kiṃsu tāsaṃ viyañjanaṃ; ",
  "sn1.60:1.3": "Kiṃsu sannissitā gāthā, ",
  "sn1.60:1.4": "kiṃsu gāthānamāsayo”ti. ",
  "sn1.60:2.1": "“Chando nidānaṃ gāthānaṃ, ",
  "sn1.60:2.2": "akkharā tāsaṃ viyañjanaṃ; ",
  "sn1.60:2.3": "Nāmasannissitā gāthā, ",
  "sn1.60:2.4": "kavi gāthānamāsayo”ti. ",
  "sn1.60:2.5": "Jarāvaggo chaṭṭho. ",
  "sn1.60:3.1": "Jarā ajarasā mittaṃ, ",
  "sn1.60:3.2": "vatthu tīṇi janāni ca; ",
  "sn1.60:3.3": "Uppatho ca dutiyo ca, ",
  "sn1.60:3.4": "kavinā pūrito vaggoti. "
}