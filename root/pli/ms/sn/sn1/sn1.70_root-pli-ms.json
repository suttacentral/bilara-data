{
  "sn1.70:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.70:0.2": "7. Addhavagga ",
  "sn1.70:0.3": "70. Lokasutta ",
  "sn1.70:1.1": "“Kismiṃ loko samuppanno, ",
  "sn1.70:1.2": "kismiṃ kubbati santhavaṃ; ",
  "sn1.70:1.3": "Kissa loko upādāya, ",
  "sn1.70:1.4": "kismiṃ loko vihaññatī”ti. ",
  "sn1.70:2.1": "“Chasu loko samuppanno, ",
  "sn1.70:2.2": "chasu kubbati santhavaṃ; ",
  "sn1.70:2.3": "Channameva upādāya, ",
  "sn1.70:2.4": "chasu loko vihaññatī”ti. ",
  "sn1.70:2.5": "Addhavaggo sattamo. ",
  "sn1.70:3.1": "Nāmaṃ cittañca taṇhā ca, ",
  "sn1.70:3.2": "saṃyojanañca bandhanā; ",
  "sn1.70:3.3": "Abbhāhatuḍḍito pihito, ",
  "sn1.70:3.4": "icchā lokena te dasāti. "
}