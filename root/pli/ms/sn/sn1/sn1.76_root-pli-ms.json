{
  "sn1.76:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.76:0.2": "8. Chetvāvagga ",
  "sn1.76:0.3": "76. Najīratisutta ",
  "sn1.76:1.1": "“Kiṃ jīrati kiṃ na jīrati, ",
  "sn1.76:1.2": "kiṃsu uppathoti vuccati; ",
  "sn1.76:1.3": "Kiṃsu dhammānaṃ paripantho, ",
  "sn1.76:1.4": "kiṃsu rattindivakkhayo; ",
  "sn1.76:1.5": "Kiṃ malaṃ brahmacariyassa, ",
  "sn1.76:1.6": "kiṃ sinānamanodakaṃ. ",
  "sn1.76:2.1": "Kati lokasmiṃ chiddāni, ",
  "sn1.76:2.2": "yattha vittaṃ na tiṭṭhati; ",
  "sn1.76:2.3": "Bhagavantaṃ puṭṭhumāgamma, ",
  "sn1.76:2.4": "kathaṃ jānemu taṃ mayan”ti. ",
  "sn1.76:3.1": "“Rūpaṃ jīrati maccānaṃ, ",
  "sn1.76:3.2": "nāmagottaṃ na jīrati; ",
  "sn1.76:3.3": "Rāgo uppathoti vuccati. ",
  "sn1.76:4.1": "Lobho dhammānaṃ paripantho, ",
  "sn1.76:4.2": "Vayo rattindivakkhayo; ",
  "sn1.76:4.3": "Itthī malaṃ brahmacariyassa, ",
  "sn1.76:4.4": "Etthāyaṃ sajjate pajā; ",
  "sn1.76:4.5": "Tapo ca brahmacariyañca, ",
  "sn1.76:4.6": "Taṃ sinānamanodakaṃ. ",
  "sn1.76:5.1": "Cha lokasmiṃ chiddāni, ",
  "sn1.76:5.2": "yattha vittaṃ na tiṭṭhati; ",
  "sn1.76:5.3": "Ālasyañca pamādo ca, ",
  "sn1.76:5.4": "anuṭṭhānaṃ asaṃyamo; ",
  "sn1.76:5.5": "Niddā tandī ca te chidde, ",
  "sn1.76:5.6": "sabbaso taṃ vivajjaye”ti. "
}