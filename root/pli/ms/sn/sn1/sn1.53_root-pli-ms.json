{
  "sn1.53:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.53:0.2": "6. Jarāvagga ",
  "sn1.53:0.3": "53. Mittasutta ",
  "sn1.53:1.1": "“Kiṃsu pavasato mittaṃ, ",
  "sn1.53:1.2": "kiṃsu mittaṃ sake ghare; ",
  "sn1.53:1.3": "Kiṃ mittaṃ atthajātassa, ",
  "sn1.53:1.4": "kiṃ mittaṃ samparāyikan”ti. ",
  "sn1.53:2.1": "“Sattho pavasato mittaṃ, ",
  "sn1.53:2.2": "mātā mittaṃ sake ghare; ",
  "sn1.53:2.3": "Sahāyo atthajātassa, ",
  "sn1.53:2.4": "hoti mittaṃ punappunaṃ; ",
  "sn1.53:2.5": "Sayaṅkatāni puññāni, ",
  "sn1.53:2.6": "taṃ mittaṃ samparāyikan”ti. "
}