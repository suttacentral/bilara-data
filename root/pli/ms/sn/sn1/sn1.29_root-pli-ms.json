{
  "sn1.29:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.29:0.2": "3. Sattivagga ",
  "sn1.29:0.3": "29. Catucakkasutta ",
  "sn1.29:1.1": "“Catucakkaṃ navadvāraṃ, ",
  "sn1.29:1.2": "puṇṇaṃ lobhena saṃyutaṃ; ",
  "sn1.29:1.3": "Paṅkajātaṃ mahāvīra, ",
  "sn1.29:1.4": "kathaṃ yātrā bhavissatī”ti. ",
  "sn1.29:2.1": "“Chetvā naddhiṃ varattañca, ",
  "sn1.29:2.2": "icchā lobhañca pāpakaṃ; ",
  "sn1.29:2.3": "Samūlaṃ taṇhamabbuyha, ",
  "sn1.29:2.4": "evaṃ yātrā bhavissatī”ti. "
}