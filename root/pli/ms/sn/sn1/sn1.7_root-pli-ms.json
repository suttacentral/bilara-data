{
  "sn1.7:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.7:0.2": "1. Naḷavagga ",
  "sn1.7:0.3": "7. Appaṭividitasutta ",
  "sn1.7:1.1": "Sāvatthinidānaṃ. ",
  "sn1.7:1.2": "Ekamantaṃ ṭhitā kho sā devatā bhagavato santike imaṃ gāthaṃ abhāsi: ",
  "sn1.7:2.1": "“Yesaṃ dhammā appaṭividitā, ",
  "sn1.7:2.2": "Paravādesu nīyare; ",
  "sn1.7:2.3": "Suttā te nappabujjhanti, ",
  "sn1.7:2.4": "Kālo tesaṃ pabujjhitun”ti. ",
  "sn1.7:3.1": "“Yesaṃ dhammā suppaṭividitā, ",
  "sn1.7:3.2": "Paravādesu na nīyare; ",
  "sn1.7:3.3": "Te sambuddhā sammadaññā, ",
  "sn1.7:3.4": "Caranti visame saman”ti. "
}