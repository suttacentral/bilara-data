{
  "sn1.79:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.79:0.2": "8. Chetvāvagga ",
  "sn1.79:0.3": "79. Pātheyyasutta ",
  "sn1.79:1.1": "“Kiṃsu bandhati pātheyyaṃ, ",
  "sn1.79:1.2": "kiṃsu bhogānamāsayo; ",
  "sn1.79:1.3": "Kiṃsu naraṃ parikassati, ",
  "sn1.79:1.4": "kiṃsu lokasmi dujjahaṃ; ",
  "sn1.79:1.5": "Kismiṃ baddhā puthū sattā, ",
  "sn1.79:1.6": "pāsena sakuṇī yathā”ti. ",
  "sn1.79:2.1": "“Saddhā bandhati pātheyyaṃ, ",
  "sn1.79:2.2": "sirī bhogānamāsayo; ",
  "sn1.79:2.3": "Icchā naraṃ parikassati, ",
  "sn1.79:2.4": "icchā lokasmi dujjahā; ",
  "sn1.79:2.5": "Icchābaddhā puthū sattā, ",
  "sn1.79:2.6": "pāsena sakuṇī yathā”ti. "
}