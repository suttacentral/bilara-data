{
  "sn1.21:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.21:0.2": "3. Sattivagga ",
  "sn1.21:0.3": "21. Sattisutta ",
  "sn1.21:1.1": "Sāvatthinidānaṃ. ",
  "sn1.21:1.2": "Ekamantaṃ ṭhitā kho sā devatā bhagavato santike imaṃ gāthaṃ abhāsi: ",
  "sn1.21:2.1": "“Sattiyā viya omaṭṭho, ",
  "sn1.21:2.2": "ḍayhamānova matthake; ",
  "sn1.21:2.3": "Kāmarāgappahānāya, ",
  "sn1.21:2.4": "sato bhikkhu paribbaje”ti. ",
  "sn1.21:3.1": "“Sattiyā viya omaṭṭho, ",
  "sn1.21:3.2": "ḍayhamānova matthake; ",
  "sn1.21:3.3": "Sakkāyadiṭṭhippahānāya, ",
  "sn1.21:3.4": "sato bhikkhu paribbaje”ti. "
}