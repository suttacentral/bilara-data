{
  "sn1.23:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.23:0.2": "3. Sattivagga ",
  "sn1.23:0.3": "23. Jaṭāsutta ",
  "sn1.23:1.1": "“Antojaṭā bahijaṭā, ",
  "sn1.23:1.2": "Jaṭāya jaṭitā pajā; ",
  "sn1.23:1.3": "Taṃ taṃ gotama pucchāmi, ",
  "sn1.23:1.4": "Ko imaṃ vijaṭaye jaṭan”ti. ",
  "sn1.23:2.1": "“Sīle patiṭṭhāya naro sapañño, ",
  "sn1.23:2.2": "Cittaṃ paññañca bhāvayaṃ; ",
  "sn1.23:2.3": "Ātāpī nipako bhikkhu, ",
  "sn1.23:2.4": "So imaṃ vijaṭaye jaṭaṃ. ",
  "sn1.23:3.1": "Yesaṃ rāgo ca doso ca, ",
  "sn1.23:3.2": "avijjā ca virājitā; ",
  "sn1.23:3.3": "Khīṇāsavā arahanto, ",
  "sn1.23:3.4": "tesaṃ vijaṭitā jaṭā. ",
  "sn1.23:4.1": "Yattha nāmañca rūpañca, ",
  "sn1.23:4.2": "asesaṃ uparujjhati; ",
  "sn1.23:4.3": "Paṭighaṃ rūpasaññā ca, ",
  "sn1.23:4.4": "etthesā chijjate jaṭā”ti. "
}