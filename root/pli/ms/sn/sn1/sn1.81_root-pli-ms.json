{
  "sn1.81:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.81:0.2": "8. Chetvāvagga ",
  "sn1.81:0.3": "81. Araṇasutta ",
  "sn1.81:1.1": "“Kesūdha araṇā loke, ",
  "sn1.81:1.2": "kesaṃ vusitaṃ na nassati; ",
  "sn1.81:1.3": "Kedha icchaṃ parijānanti, ",
  "sn1.81:1.4": "kesaṃ bhojissiyaṃ sadā. ",
  "sn1.81:2.1": "Kiṃsu mātā pitā bhātā, ",
  "sn1.81:2.2": "vandanti naṃ patiṭṭhitaṃ; ",
  "sn1.81:2.3": "Kiṃsu idha jātihīnaṃ, ",
  "sn1.81:2.4": "abhivādenti khattiyā”ti. ",
  "sn1.81:3.1": "“Samaṇīdha araṇā loke, ",
  "sn1.81:3.2": "Samaṇānaṃ vusitaṃ na nassati; ",
  "sn1.81:3.3": "Samaṇā icchaṃ parijānanti, ",
  "sn1.81:3.4": "Samaṇānaṃ bhojissiyaṃ sadā. ",
  "sn1.81:4.1": "Samaṇaṃ mātā pitā bhātā, ",
  "sn1.81:4.2": "vandanti naṃ patiṭṭhitaṃ; ",
  "sn1.81:4.3": "Samaṇīdha jātihīnaṃ, ",
  "sn1.81:4.4": "abhivādenti khattiyā”ti. ",
  "sn1.81:4.5": "Chetvāvaggo aṭṭhamo. ",
  "sn1.81:5.1": "Chetvā rathañca cittañca, ",
  "sn1.81:5.2": "vuṭṭhi bhītā najīrati; ",
  "sn1.81:5.3": "Issaraṃ kāmaṃ pātheyyaṃ, ",
  "sn1.81:5.4": "pajjoto araṇena cāti. ",
  "sn1.81:5.5": "Devatāsaṃyuttaṃ samattaṃ. "
}