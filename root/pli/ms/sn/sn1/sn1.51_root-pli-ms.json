{
  "sn1.51:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.51:0.2": "6. Jarāvagga ",
  "sn1.51:0.3": "51. Jarāsutta ",
  "sn1.51:1.1": "“Kiṃsu yāva jarā sādhu, ",
  "sn1.51:1.2": "kiṃsu sādhu patiṭṭhitaṃ; ",
  "sn1.51:1.3": "Kiṃsu narānaṃ ratanaṃ, ",
  "sn1.51:1.4": "kiṃsu corehi dūharan”ti. ",
  "sn1.51:2.1": "“Sīlaṃ yāva jarā sādhu, ",
  "sn1.51:2.2": "saddhā sādhu patiṭṭhitā; ",
  "sn1.51:2.3": "Paññā narānaṃ ratanaṃ, ",
  "sn1.51:2.4": "puññaṃ corehi dūharan”ti. "
}