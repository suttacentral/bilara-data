{
  "sn1.22:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.22:0.2": "3. Sattivagga ",
  "sn1.22:0.3": "22. Phusatisutta ",
  "sn1.22:1.1": "“Nāphusantaṃ phusati ca, ",
  "sn1.22:1.2": "phusantañca tato phuse; ",
  "sn1.22:1.3": "Tasmā phusantaṃ phusati, ",
  "sn1.22:1.4": "appaduṭṭhapadosinan”ti. ",
  "sn1.22:2.1": "“Yo appaduṭṭhassa narassa dussati, ",
  "sn1.22:2.2": "Suddhassa posassa anaṅgaṇassa; ",
  "sn1.22:2.3": "Tameva bālaṃ pacceti pāpaṃ, ",
  "sn1.22:2.4": "Sukhumo rajo paṭivātaṃva khitto”ti. "
}