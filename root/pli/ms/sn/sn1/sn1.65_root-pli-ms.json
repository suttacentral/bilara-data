{
  "sn1.65:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.65:0.2": "7. Addhavagga ",
  "sn1.65:0.3": "65. Bandhanasutta ",
  "sn1.65:1.1": "“Kiṃsu sambandhano loko, ",
  "sn1.65:1.2": "kiṃsu tassa vicāraṇaṃ; ",
  "sn1.65:1.3": "Kissassu vippahānena, ",
  "sn1.65:1.4": "sabbaṃ chindati bandhanan”ti. ",
  "sn1.65:2.1": "“Nandīsambandhano loko, ",
  "sn1.65:2.2": "vitakkassa vicāraṇaṃ; ",
  "sn1.65:2.3": "Taṇhāya vippahānena, ",
  "sn1.65:2.4": "sabbaṃ chindati bandhanan”ti. "
}