{
  "sn1.72:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.72:0.2": "8. Chetvāvagga ",
  "sn1.72:0.3": "72. Rathasutta ",
  "sn1.72:1.1": "“Kiṃsu rathassa paññāṇaṃ, ",
  "sn1.72:1.2": "kiṃsu paññāṇamaggino; ",
  "sn1.72:1.3": "Kiṃsu raṭṭhassa paññāṇaṃ, ",
  "sn1.72:1.4": "kiṃsu paññāṇamitthiyā”ti. ",
  "sn1.72:2.1": "“Dhajo rathassa paññāṇaṃ, ",
  "sn1.72:2.2": "dhūmo paññāṇamaggino; ",
  "sn1.72:2.3": "Rājā raṭṭhassa paññāṇaṃ, ",
  "sn1.72:2.4": "bhattā paññāṇamitthiyā”ti. "
}