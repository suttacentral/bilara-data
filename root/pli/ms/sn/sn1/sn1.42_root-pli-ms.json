{
  "sn1.42:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.42:0.2": "5. Ādittavagga ",
  "sn1.42:0.3": "42. Kiṃdadasutta ",
  "sn1.42:1.1": "“Kiṃdado balado hoti, ",
  "sn1.42:1.2": "kiṃdado hoti vaṇṇado; ",
  "sn1.42:1.3": "Kiṃdado sukhado hoti, ",
  "sn1.42:1.4": "kiṃdado hoti cakkhudo; ",
  "sn1.42:1.5": "Ko ca sabbadado hoti, ",
  "sn1.42:1.6": "taṃ me akkhāhi pucchito”ti. ",
  "sn1.42:2.1": "“Annado balado hoti, ",
  "sn1.42:2.2": "vatthado hoti vaṇṇado; ",
  "sn1.42:2.3": "Yānado sukhado hoti, ",
  "sn1.42:2.4": "dīpado hoti cakkhudo. ",
  "sn1.42:3.1": "So ca sabbadado hoti, ",
  "sn1.42:3.2": "yo dadāti upassayaṃ; ",
  "sn1.42:3.3": "Amataṃ dado ca so hoti, ",
  "sn1.42:3.4": "yo dhammamanusāsatī”ti. "
}