{
  "sn1.54:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.54:0.2": "6. Jarāvagga ",
  "sn1.54:0.3": "54. Vatthusutta ",
  "sn1.54:1.1": "“Kiṃsu vatthu manussānaṃ, ",
  "sn1.54:1.2": "kiṃsūdha paramo sakhā; ",
  "sn1.54:1.3": "Kiṃsu bhūtā upajīvanti, ",
  "sn1.54:1.4": "ye pāṇā pathavissitā”ti. ",
  "sn1.54:2.1": "“Puttā vatthu manussānaṃ, ",
  "sn1.54:2.2": "bhariyā ca paramo sakhā; ",
  "sn1.54:2.3": "Vuṭṭhiṃ bhūtā upajīvanti, ",
  "sn1.54:2.4": "ye pāṇā pathavissitā”ti. "
}