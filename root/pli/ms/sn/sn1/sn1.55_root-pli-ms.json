{
  "sn1.55:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.55:0.2": "6. Jarāvagga ",
  "sn1.55:0.3": "55. Paṭhamajanasutta ",
  "sn1.55:1.1": "“Kiṃsu janeti purisaṃ, ",
  "sn1.55:1.2": "kiṃsu tassa vidhāvati; ",
  "sn1.55:1.3": "Kiṃsu saṃsāramāpādi, ",
  "sn1.55:1.4": "kiṃsu tassa mahabbhayan”ti. ",
  "sn1.55:2.1": "“Taṇhā janeti purisaṃ, ",
  "sn1.55:2.2": "cittamassa vidhāvati; ",
  "sn1.55:2.3": "Satto saṃsāramāpādi, ",
  "sn1.55:2.4": "dukkhamassa mahabbhayan”ti. "
}