{
  "sn1.77:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.77:0.2": "8. Chetvāvagga ",
  "sn1.77:0.3": "77. Issariyasutta ",
  "sn1.77:1.1": "“Kiṃsu issariyaṃ loke, ",
  "sn1.77:1.2": "kiṃsu bhaṇḍānamuttamaṃ; ",
  "sn1.77:1.3": "Kiṃsu satthamalaṃ loke, ",
  "sn1.77:1.4": "kiṃsu lokasmimabbudaṃ. ",
  "sn1.77:2.1": "Kiṃsu harantaṃ vārenti, ",
  "sn1.77:2.2": "haranto pana ko piyo; ",
  "sn1.77:2.3": "Kiṃsu punappunāyantaṃ, ",
  "sn1.77:2.4": "abhinandanti paṇḍitā”ti. ",
  "sn1.77:3.1": "“Vaso issariyaṃ loke, ",
  "sn1.77:3.2": "itthī bhaṇḍānamuttamaṃ; ",
  "sn1.77:3.3": "Kodho satthamalaṃ loke, ",
  "sn1.77:3.4": "corā lokasmimabbudā. ",
  "sn1.77:4.1": "Coraṃ harantaṃ vārenti, ",
  "sn1.77:4.2": "haranto samaṇo piyo; ",
  "sn1.77:4.3": "Samaṇaṃ punappunāyantaṃ, ",
  "sn1.77:4.4": "abhinandanti paṇḍitā”ti. "
}