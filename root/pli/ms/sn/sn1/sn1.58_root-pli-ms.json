{
  "sn1.58:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.58:0.2": "6. Jarāvagga ",
  "sn1.58:0.3": "58. Uppathasutta ",
  "sn1.58:1.1": "“Kiṃsu uppatho akkhāto, ",
  "sn1.58:1.2": "kiṃsu rattindivakkhayo; ",
  "sn1.58:1.3": "Kiṃ malaṃ brahmacariyassa, ",
  "sn1.58:1.4": "kiṃ sinānamanodakan”ti. ",
  "sn1.58:2.1": "“Rāgo uppatho akkhāto, ",
  "sn1.58:2.2": "Vayo rattindivakkhayo; ",
  "sn1.58:2.3": "Itthī malaṃ brahmacariyassa, ",
  "sn1.58:2.4": "Etthāyaṃ sajjate pajā; ",
  "sn1.58:2.5": "Tapo ca brahmacariyañca, ",
  "sn1.58:2.6": "Taṃ sinānamanodakan”ti. "
}