{
  "sn1.57:0.1": "Saṃyutta Nikāya 1 ",
  "sn1.57:0.2": "6. Jarāvagga ",
  "sn1.57:0.3": "57. Tatiyajanasutta ",
  "sn1.57:1.1": "“Kiṃsu janeti purisaṃ, ",
  "sn1.57:1.2": "kiṃsu tassa vidhāvati; ",
  "sn1.57:1.3": "Kiṃsu saṃsāramāpādi, ",
  "sn1.57:1.4": "kiṃsu tassa parāyanan”ti. ",
  "sn1.57:2.1": "“Taṇhā janeti purisaṃ, ",
  "sn1.57:2.2": "cittamassa vidhāvati; ",
  "sn1.57:2.3": "Satto saṃsāramāpādi, ",
  "sn1.57:2.4": "kammaṃ tassa parāyanan”ti. "
}