{
  "dk-name:1.dk1": "Avidyāpratyayāḥ saṁskārāḥ",
  "dk-name:2.dk2": "Saṁskārapratyayaṁ vijñānaṁ",
  "dk-name:3.dk3": "Vijñānapratyayam nāmarūpaṁ",
  "dk-name:4.dk4": "Nāmarūpapratyayaṁ vijñānaṁ",
  "dk-name:5.dk5": "Nāmarūpapratyayaṁ ṣaḍāyatanam",
  "dk-name:6.dk6": "Nāmarūpapratyayaḥ sparśaḥ",
  "dk-name:7.dk7": "Ṣaḍāyatanapratyayaḥ sparśaḥ",
  "dk-name:8.dk8": "Sparśapratyayā vedanā",
  "dk-name:9.dk9": "Vedanāpratyayā tṛṣṇā",
  "dk-name:10.dk10": "Tṛṣṇāpratyayam upādānam",
  "dk-name:11.dk11": "Upādānapratyayo bhavaḥ",
  "dk-name:12.dk12": "Bhavapratyayā jātiḥ",
  "dk-name:13.dk13": "Jātipratyayā jarāmaraṇaśokaparidevaduḥkhadaurmanasyopāyāsāḥ",
  "dk-name:14.dk14": "Duḥkhaskandhasyābhinirvṛttiḥ",
  "dk-name:15.dk15": "Pañca bhayāni vadyāni vairāṇi",
  "dk-name:16.dk16": "Upāsakaḥ",
  "dk-name:17.dk17": "Pañcopāsakasya śikṣāpadāni",
  "dk-name:18.dk18": "Maitrī cetaḥsamadhiḥ",
  "dk-name:19.dk19": "Karuṇā cetaḥsamādhiḥ"
}