{
  "dk1": "Avidyāpratyayāḥ saṁskārāḥ",
  "dk2": "Saṁskārapratyayaṁ vijñānaṁ",
  "dk3": "Vijñānapratyayam nāmarūpaṁ",
  "dk4": "Nāmarūpapratyayaṁ vijñānaṁ",
  "dk5": "Nāmarūpapratyayaṁ ṣaḍāyatanam",
  "dk6": "Nāmarūpapratyayaḥ sparśaḥ",
  "dk7": "Ṣaḍāyatanapratyayaḥ sparśaḥ",
  "dk8": "Sparśapratyayā vedanā",
  "dk9": "Vedanāpratyayā tṛṣṇā",
  "dk10": "Tṛṣṇāpratyayam upādānam",
  "dk11": "Upādānapratyayo bhavaḥ",
  "dk12": "Bhavapratyayā jātiḥ",
  "dk13": "Jātipratyayā jarāmaraṇaśokaparidevaduḥkhadaurmanasyopāyāsāḥ",
  "dk14": "Duḥkhaskandhasyābhinirvṛttiḥ",
  "dk15": "Pañca bhayāni vadyāni vairāṇi",
  "dk16": "Upāsakaḥ",
  "dk17": "Pañcopāsakasya śikṣāpadāni",
  "dk18": "Maitrī cetaḥsamadhiḥ",
  "dk19": "Karuṇā cetaḥsamādhiḥ"
}