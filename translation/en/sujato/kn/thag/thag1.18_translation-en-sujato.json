{
  "thag1.18:1.1": "Verses of the Senior Monks",
  "thag1.18:2.1": "The Book of the Ones",
  "thag1.18:3.1": "Chapter Two",
  "thag1.18:4.1": "1.18. Siṅgāla’s Father",
  "thag1.18:5.1": "There was an heir of the Buddha,",
  "thag1.18:5.2": "a monk in Bhesakaḷā forest,",
  "thag1.18:5.3": "who suffused the entire earth ",
  "thag1.18:5.4": "with the perception of bones.",
  "thag1.18:5.5": "I think he will quickly",
  "thag1.18:5.6": "get rid of sensual desire."
}