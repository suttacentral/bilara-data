{
  "thag1.76:1.1": "Verses of the Senior Monks",
  "thag1.76:2.1": "The Book of the Ones",
  "thag1.76:3.1": "Chapter Eight",
  "thag1.76:4.1": "1.76. Piyañjaha",
  "thag1.76:5.1": "Settle down when others spring up;",
  "thag1.76:5.2": "spring up when others settle down.",
  "thag1.76:5.3": "Remain when others have departed;",
  "thag1.76:5.4": "don’t delight when others delight."
}