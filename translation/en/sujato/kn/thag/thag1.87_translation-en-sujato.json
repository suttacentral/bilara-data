{
  "thag1.87:1.1": "Verses of the Senior Monks",
  "thag1.87:2.1": "The Book of the Ones",
  "thag1.87:3.1": "Chapter Nine",
  "thag1.87:4.1": "1.87. Paviṭṭha",
  "thag1.87:5.1": "The aggregates are seen as they truly are;",
  "thag1.87:5.2": "all rebirths are shattered;",
  "thag1.87:5.3": "transmigration is finished;",
  "thag1.87:5.4": "now there are no more future lives."
}