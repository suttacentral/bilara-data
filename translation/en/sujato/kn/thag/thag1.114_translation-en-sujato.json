{
  "thag1.114:1.1": "Verses of the Senior Monks",
  "thag1.114:2.1": "The Book of the Ones",
  "thag1.114:3.1": "Chapter Twelve",
  "thag1.114:4.1": "1.114. Adhimutta (1st)",
  "thag1.114:5.1": "Your body is uncomfortably heavy,",
  "thag1.114:5.2": "and life is running out;",
  "thag1.114:5.3": "greedy for physical pleasure,",
  "thag1.114:5.4": "how can you find happiness as an ascetic?"
}