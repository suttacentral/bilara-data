{
  "thag1.63:1.1": "Verses of the Senior Monks",
  "thag1.63:2.1": "The Book of the Ones",
  "thag1.63:3.1": "Chapter Seven",
  "thag1.63:4.1": "1.63. Pakkha",
  "thag1.63:5.1": "They fall, collapsed and fallen;",
  "thag1.63:5.2": "greedy, they return.",
  "thag1.63:5.3": "The work is done, the joyful is enjoyed,",
  "thag1.63:5.4": "happiness is found through happiness."
}