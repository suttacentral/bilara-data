{
  "thag1.9:1.1": "Verses of the Senior Monks",
  "thag1.9:2.1": "The Book of the Ones",
  "thag1.9:3.1": "Chapter One",
  "thag1.9:4.1": "1.9. Pilindavaccha",
  "thag1.9:5.1": "It was welcome, not unwelcome,",
  "thag1.9:5.2": "the advice I got was good.",
  "thag1.9:5.3": "Of things which are shared,",
  "thag1.9:5.4": "I encountered the best.",
  "thag1.9:6.1": "It was thus that this verse was recited by the senior venerable Pilindavaccha."
}