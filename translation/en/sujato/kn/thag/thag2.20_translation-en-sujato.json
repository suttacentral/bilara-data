{
  "thag2.20:1.1": "Verses of the Senior Monks",
  "thag2.20:2.1": "The Book of the Twos",
  "thag2.20:3.1": "Chapter Two",
  "thag2.20:4.1": "2.20. Sirima",
  "thag2.20:5.1": "If others praise one",
  "thag2.20:5.2": "who has no immersion,",
  "thag2.20:5.3": "they praise in vain,",
  "thag2.20:5.4": "as one has no immersion.",
  "thag2.20:6.1": "If others rebuke one",
  "thag2.20:6.2": "who does have immersion,",
  "thag2.20:6.3": "they rebuke in vain,",
  "thag2.20:6.4": "as one does have immersion."
}