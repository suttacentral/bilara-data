{
  "thag1.93:1.1": "Verses of the Senior Monks",
  "thag1.93:2.1": "The Book of the Ones",
  "thag1.93:3.1": "Chapter Ten",
  "thag1.93:4.1": "1.93. Eraka",
  "thag1.93:5.1": "Sensual pleasures are suffering, Eraka!",
  "thag1.93:5.2": "Sensual pleasures aren’t happiness, Eraka!",
  "thag1.93:5.3": "One who enjoys sensual pleasures",
  "thag1.93:5.4": "enjoys suffering, Eraka!",
  "thag1.93:5.5": "One who doesn’t enjoy sensual pleasures",
  "thag1.93:5.6": "doesn’t enjoy suffering, Eraka!"
}