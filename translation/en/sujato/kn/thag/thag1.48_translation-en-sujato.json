{
  "thag1.48:1.1": "Verses of the Senior Monks",
  "thag1.48:2.1": "The Book of the Ones",
  "thag1.48:3.1": "Chapter Five",
  "thag1.48:4.1": "1.48. Sañjaya",
  "thag1.48:5.1": "Since I went forth",
  "thag1.48:5.2": "from the lay life to homelessness,",
  "thag1.48:5.3": "I’ve not been aware of any thought",
  "thag1.48:5.4": "that is ignoble and hateful."
}