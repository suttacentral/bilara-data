{
  "thag1.47:1.1": "Verses of the Senior Monks",
  "thag1.47:2.1": "The Book of the Ones",
  "thag1.47:3.1": "Chapter Five",
  "thag1.47:4.1": "1.47. Ujjaya",
  "thag1.47:5.1": "Homage to you, O Buddha, O hero,",
  "thag1.47:5.2": "freed in every way!",
  "thag1.47:5.3": "Meditating in the fruits of your practice,",
  "thag1.47:5.4": "I live without defilements."
}