{
  "thag1.13:1.1": "Verses of the Senior Monks",
  "thag1.13:2.1": "The Book of the Ones",
  "thag1.13:3.1": "Chapter Two",
  "thag1.13:4.1": "1.13. Vanavaccha (1st)",
  "thag1.13:5.1": "Glistening, they look like blue storm clouds,",
  "thag1.13:5.2": "with waters cool and streams so clear,",
  "thag1.13:5.3": "and covered all in ladybugs:",
  "thag1.13:5.4": "these rocky crags delight me!"
}