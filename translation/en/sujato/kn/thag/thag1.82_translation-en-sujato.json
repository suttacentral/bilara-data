{
  "thag1.82:1.1": "Verses of the Senior Monks",
  "thag1.82:2.1": "The Book of the Ones",
  "thag1.82:3.1": "Chapter Nine",
  "thag1.82:4.1": "1.82. Kassapa",
  "thag1.82:5.1": "Go, child, to any place",
  "thag1.82:5.2": "where there’s plenty of food,",
  "thag1.82:5.3": "where it’s safe and free of peril—",
  "thag1.82:5.4": "may you not be overcome by sorrow!"
}