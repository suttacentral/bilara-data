{
  "thig1.15:1.1": "Verses of the Senior Nuns",
  "thig1.15:2.1": "The Book of the Ones",
  "thig1.15:3.1": "1.15. Uttarā (1st)",
  "thig1.15:4.1": "I was restrained",
  "thig1.15:4.2": "in body, speech, and mind.",
  "thig1.15:4.3": "Having plucked out craving root and all,",
  "thig1.15:4.4": "I’m cooled and quenched."
}