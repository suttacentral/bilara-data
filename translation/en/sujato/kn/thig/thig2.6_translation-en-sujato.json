{
  "thig2.6:1.1": "Verses of the Senior Nuns",
  "thig2.6:2.1": "The Book of the Twos",
  "thig2.6:3.1": "2.6. Mettikā",
  "thig2.6:4.1": "Though in pain,",
  "thig2.6:4.2": "feeble, my youth long gone,",
  "thig2.6:4.3": "I climb the mountain,",
  "thig2.6:4.4": "leaning on a staff.",
  "thig2.6:5.1": "Having laid down my outer robe",
  "thig2.6:5.2": "and overturned my bowl,",
  "thig2.6:5.3": "sitting on a rock,",
  "thig2.6:5.4": "my mind was freed.",
  "thig2.6:5.5": "I’ve attained the three knowledges,",
  "thig2.6:5.6": "and fulfilled the Buddha’s instructions."
}