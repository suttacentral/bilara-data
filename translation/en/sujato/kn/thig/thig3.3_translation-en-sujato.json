{
  "thig3.3:1.1": "Verses of the Senior Nuns",
  "thig3.3:2.1": "The Book of the Threes",
  "thig3.3:3.1": "3.3. Another Uttamā",
  "thig3.3:4.1": "Of the seven awakening factors,",
  "thig3.3:4.2": "the path for attaining extinguishment,",
  "thig3.3:4.3": "I have developed them all,",
  "thig3.3:4.4": "just as the Buddha taught.",
  "thig3.3:5.1": "I attain the meditations on emptiness",
  "thig3.3:5.2": "and signlessness whenever I want.",
  "thig3.3:5.3": "I am the Buddha’s rightful daughter,",
  "thig3.3:5.4": "always delighting in quenching.",
  "thig3.3:6.1": "All sensual pleasures are cut off,",
  "thig3.3:6.2": "whether human or divine.",
  "thig3.3:6.3": "Transmigration through births is finished,",
  "thig3.3:6.4": "now there are no more future lives."
}