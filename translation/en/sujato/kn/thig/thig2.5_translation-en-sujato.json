{
  "thig2.5:1.1": "Verses of the Senior Nuns",
  "thig2.5:2.1": "The Book of the Twos",
  "thig2.5:3.1": "2.5. Cittā",
  "thig2.5:4.1": "Though I’m skinny,",
  "thig2.5:4.2": "sick, and very feeble,",
  "thig2.5:4.3": "I climb the mountain,",
  "thig2.5:4.4": "leaning on a staff.",
  "thig2.5:5.1": "Having laid down my outer robe,",
  "thig2.5:5.2": "and overturned my bowl,",
  "thig2.5:5.3": "propping myself against a rock,",
  "thig2.5:5.4": "I shattered the mass of darkness."
}