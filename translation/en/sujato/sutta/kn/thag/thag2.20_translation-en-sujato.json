{
  "thag2.20:0.1": "Verses of the Senior Monks",
  "thag2.20:0.2": "The Book of the Twos",
  "thag2.20:0.3": "Chapter Two",
  "thag2.20:0.4": "2.20. Sirima",
  "thag2.20:1.1": "If others praise one",
  "thag2.20:1.2": "who has no immersion,",
  "thag2.20:1.3": "they praise in vain,",
  "thag2.20:1.4": "as one has no immersion.",
  "thag2.20:2.1": "If others rebuke one",
  "thag2.20:2.2": "who does have immersion,",
  "thag2.20:2.3": "they rebuke in vain,",
  "thag2.20:2.4": "as one does have immersion."
