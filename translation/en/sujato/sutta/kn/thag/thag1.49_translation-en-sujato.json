{
  "thag1.49:0.1": "Verses of the Senior Monks",
  "thag1.49:0.2": "The Book of the Ones",
  "thag1.49:0.3": "Chapter Five",
  "thag1.49:0.4": "1.49. Rāmaṇeyyaka",
  "thag1.49:1.1": "Even with all the sounds,",
  "thag1.49:1.2": "the chirping and cheeping of the birds,",
  "thag1.49:1.3": "my mind doesn’t waver,",
  "thag1.49:1.4": "for I’m devoted to oneness."
}