{
  "thag1.18:0.1": "Verses of the Senior Monks",
  "thag1.18:0.2": "The Book of the Ones",
  "thag1.18:0.3": "Chapter Two",
  "thag1.18:0.4": "1.18. Siṅgāla’s Father",
  "thag1.18:1.1": "There was an heir of the Buddha,",
  "thag1.18:1.2": "a monk in Bhesakaḷā forest,",
  "thag1.18:1.3": "who suffused the entire earth ",
  "thag1.18:1.4": "with the perception of bones.",
  "thag1.18:1.5": "I think he will quickly",
  "thag1.18:1.6": "get rid of sensual desire."
}