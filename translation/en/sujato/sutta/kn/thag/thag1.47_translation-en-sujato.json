{
  "thag1.47:0.1": "Verses of the Senior Monks 1.47",
  "thag1.47:0.2": "The Book of the Ones",
  "thag1.47:0.3": "Chapter Five",
  "thag1.47:0.4": "Ujjaya",
  "thag1.47:1.1": "Homage to you, O Buddha, O hero,",
  "thag1.47:1.2": "freed in every way!",
  "thag1.47:1.3": "Meditating in the fruits of your practice,",
  "thag1.47:1.4": "I live without defilements."
}