{
  "thag1.23:0.1": "Verses of the Senior Monks",
  "thag1.23:0.2": "The Book of the Ones",
  "thag1.23:0.3": "Chapter Three",
  "thag1.23:0.4": "1.23. Gosāla",
  "thag1.23:1.1": "I’ll eat honey and milk-rice",
  "thag1.23:1.2": "in Veḷugumba.",
  "thag1.23:1.3": "And then, skillfully scrutinizing",
  "thag1.23:1.4": "the rise and fall of the aggregates,",
  "thag1.23:1.5": "I’ll return to my forest hill",
  "thag1.23:1.6": "and foster seclusion."
}