{
  "thag2.25:0.1": "Verses of the Senior Monks",
  "thag2.25:0.2": "The Book of the Twos",
  "thag2.25:0.3": "Chapter Three",
  "thag2.25:0.4": "2.25. Vītasoka",
  "thag2.25:1.1": "The barber approached",
  "thag2.25:1.2": "to shave my head.",
  "thag2.25:1.3": "I picked up a mirror",
  "thag2.25:1.4": "and examined my body.",
  "thag2.25:2.1": "My body appeared hollow;",
  "thag2.25:2.2": "I once was blind, but the darkness disappeared.",
  "thag2.25:2.3": "My fancy hairdo has been cut off:",
  "thag2.25:2.4": "now there’ll be no more future lives."
}