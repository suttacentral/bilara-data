{
  "thag1.76:0.1": "Verses of the Senior Monks",
  "thag1.76:0.2": "The Book of the Ones",
  "thag1.76:0.3": "Chapter Eight",
  "thag1.76:0.4": "1.76. Piyañjaha",
  "thag1.76:1.1": "Settle down when others spring up;",
  "thag1.76:1.2": "spring up when others settle down.",
  "thag1.76:1.3": "Remain when others have departed;",
  "thag1.76:1.4": "don’t delight when others delight."
}