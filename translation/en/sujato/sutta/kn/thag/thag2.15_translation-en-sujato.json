{
  "thag2.15:0.1": "Verses of the Senior Monks",
  "thag2.15:0.2": "The Book of the Twos",
  "thag2.15:0.3": "Chapter Two",
  "thag2.15:0.4": "2.15. Sabbamitta",
  "thag2.15:1.1": "People are attached to people;",
  "thag2.15:1.2": "people depend on people;",
  "thag2.15:1.3": "people are hurt by people;",
  "thag2.15:1.4": "and people hurt people.",
  "thag2.15:2.1": "So what’s the point of people,",
  "thag2.15:2.2": "or those born of people?",
  "thag2.15:2.3": "Go, abandon these people,",
  "thag2.15:2.4": "who’ve hurt so many people."
}