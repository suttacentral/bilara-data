{
  "thag1.11:0.1": "Verses of the Senior Monks",
  "thag1.11:0.2": "The Book of the Ones",
  "thag1.11:0.3": "Chapter Two",
  "thag1.11:0.4": "1.11. Cūḷavaccha",
  "thag1.11:1.1": "A monk full of joy",
  "thag1.11:1.2": "in the teaching proclaimed by the Buddha",
  "thag1.11:1.3": "would realize the peaceful state,",
  "thag1.11:1.4": "the blissful stilling of conditions."
}