{
  "thag1.55:0.1": "Verses of the Senior Monks",
  "thag1.55:0.2": "The Book of the Ones",
  "thag1.55:0.3": "Chapter Six",
  "thag1.55:0.4": "1.55. Añjanavaniya",
  "thag1.55:1.1": "I plunged into the Añjana forest",
  "thag1.55:1.2": "and made a little hut to live in.",
  "thag1.55:1.3": "I’ve attained the three knowledges",
  "thag1.55:1.4": "and fulfilled the Buddha’s instructions."
}