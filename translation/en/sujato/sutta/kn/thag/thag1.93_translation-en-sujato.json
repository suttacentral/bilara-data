{
  "thag1.93:0.1": "Verses of the Senior Monks",
  "thag1.93:0.2": "The Book of the Ones",
  "thag1.93:0.3": "Chapter Ten",
  "thag1.93:0.4": "1.93. Eraka",
  "thag1.93:1.1": "Sensual pleasures are suffering, Eraka!",
  "thag1.93:1.2": "Sensual pleasures aren’t happiness, Eraka!",
  "thag1.93:1.3": "One who enjoys sensual pleasures",
  "thag1.93:1.4": "enjoys suffering, Eraka!",
  "thag1.93:1.5": "One who doesn’t enjoy sensual pleasures",
  "thag1.93:1.6": "doesn’t enjoy suffering, Eraka!"
}