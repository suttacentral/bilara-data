{
  "thag1.63:0.1": "Verses of the Senior Monks",
  "thag1.63:0.2": "The Book of the Ones",
  "thag1.63:0.3": "Chapter Seven",
  "thag1.63:0.4": "1.63. Pakkha",
  "thag1.63:1.1": "They fall, collapsed and fallen;",
  "thag1.63:1.2": "greedy, they return.",
  "thag1.63:1.3": "The work is done, the joyful is enjoyed,",
  "thag1.63:1.4": "happiness is found through happiness."
}