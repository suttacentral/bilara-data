{
  "thag1.82:0.1": "Verses of the Senior Monks",
  "thag1.82:0.2": "The Book of the Ones",
  "thag1.82:0.3": "Chapter Nine",
  "thag1.82:0.4": "1.82. Kassapa",
  "thag1.82:1.1": "Go, child, to any place",
  "thag1.82:1.2": "where there’s plenty of food,",
  "thag1.82:1.3": "where it’s safe and free of peril—",
  "thag1.82:1.4": "may you not be overcome by sorrow!"
}