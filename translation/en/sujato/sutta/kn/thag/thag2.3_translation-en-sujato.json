{
  "thag2.3:0.1": "Verses of the Senior Monks",
  "thag2.3:0.2": "The Book of the Twos",
  "thag2.3:0.3": "Chapter One",
  "thag2.3:0.4": "2.3. Valliya (2nd)",
  "thag2.3:1.1": "A monkey went up to the little hut",
  "thag2.3:1.2": "with five doors.",
  "thag2.3:1.3": "He circles around, knocking",
  "thag2.3:1.4": "on each door, again and again.",
  "thag2.3:2.1": "Stand still monkey, don’t run!",
  "thag2.3:2.2": "Things are different now;",
  "thag2.3:2.3": "you’ve been caught by wisdom—",
  "thag2.3:2.4": "you won’t go far."
}