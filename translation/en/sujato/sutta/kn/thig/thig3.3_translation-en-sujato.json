{
  "thig3.3:0.1": "Verses of the Senior Nuns",
  "thig3.3:0.2": "The Book of the Threes",
  "thig3.3:0.3": "3.3. Another Uttamā",
  "thig3.3:1.1": "Of the seven awakening factors,",
  "thig3.3:1.2": "the path for attaining extinguishment,",
  "thig3.3:1.3": "I have developed them all,",
  "thig3.3:1.4": "just as the Buddha taught.",
  "thig3.3:2.1": "I attain the meditations on emptiness",
  "thig3.3:2.2": "and signlessness whenever I want.",
  "thig3.3:2.3": "I am the Buddha’s rightful daughter,",
  "thig3.3:2.4": "always delighting in quenching.",
  "thig3.3:3.1": "All sensual pleasures are cut off,",
  "thig3.3:3.2": "whether human or divine.",
  "thig3.3:3.3": "Transmigration through births is finished,",
  "thig3.3:3.4": "now there’ll be no more future lives."
}