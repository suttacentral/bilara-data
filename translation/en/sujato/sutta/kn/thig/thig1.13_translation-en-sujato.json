{
  "thig1.13:0.1": "Verses of the Senior Nuns 1.13",
  "thig1.13:0.2": "The Book of the Ones",
  "thig1.13:0.3": "Visākhā",
  "thig1.13:1.1": "Do the Buddha’s bidding,",
  "thig1.13:1.2": "you won’t regret it.",
  "thig1.13:1.3": "Having quickly washed your feet,",
  "thig1.13:1.4": "sit in a discreet place to meditate."
}