{
  "thig1.13:0.1": "Verses of the Elder Bhikkhunīs",
  "thig1.13:0.2": "The Chapter of the Ones",
  "thig1.13:0.3": "The Elder Visākhā",
  "thig1.13:1.1": "\"Do the teaching of the Buddha,",
  "thig1.13:1.2": "and there will be no regrets.",
  "thig1.13:1.3": "After quickly washing the feet,",
  "thig1.13:1.4": "sit down on one side.\""
}