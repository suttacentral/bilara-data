{
  "an10.240-266:0.1": "Nummerierte Lehrreden 10.240–266 ",
  "an10.240-266:0.2": "23. Abgekürzte Texte, die mit Gier beginnen ",
  "an10.240-266:0.3": "Lehrreden ohne Titel über Gier ",
  "an10.240-266:1.1": "„Um Gier vollständig zu verstehen … ",
  "an10.240-266:1.2": "sie vollständig aufzulösen … ",
  "an10.240-266:1.3": "sie aufzugeben … ",
  "an10.240-266:1.4": "sie aufzulösen … ",
  "an10.240-266:1.5": "sie zum Verschwinden zu bringen … ",
  "an10.240-266:1.6": "zum Schwinden … ",
  "an10.240-266:1.7": "zum Aufhören … ",
  "an10.240-266:1.8": "um sie herzugeben … ",
  "an10.240-266:1.9": "um sie loszulassen … ",
  "an10.240-266:1.10": "müssen diese zehn Dinge entwickelt werden.“ "
}