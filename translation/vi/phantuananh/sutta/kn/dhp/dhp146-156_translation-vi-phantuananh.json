{
  "dhp146:0.1": "Tiểu Bộ Kinh",
  "dhp146:0.2": "Kinh Pháp Cú",
  "dhp146:0.3": "Phẩm Già",
  "dhp146:0.4": "Chuyện những người bạn của bà Visākhā (Thiện Chi)",
  "dhp146:1": "Cười gì, hân hoan gì,",
  "dhp146:2": "Khi đời mãi bị thiêu?",
  "dhp146:3": "Bị tối tăm bao trùm,",
  "dhp146:4": "Sao không tìm ngọn đèn?",
  "dhp147:0": "Chuyện kỹ nữ Sirimā",
  "dhp147:1": "Hãy xem bong bóng đẹp,",
  "dhp147:2": "Chỗ chất chứa vết thương,",
  "dhp147:3": "Bệnh hoạn nhiều suy tư,",
  "dhp147:4": "Thật không gì trường cửu.",
  "dhp148:0": "Chuyện Trưởng lão ni Uttarā",
  "dhp148:1": "Sắc này bị suy già,",
  "dhp148:2": "Ổ tật bệnh, mỏng manh,",
  "dhp148:3": "Nhóm bất tịnh đổ vỡ,",
  "dhp148:4": "Chết chấm dứt mạng sống.",
  "dhp149:0": "Chuyện nhóm Tỳ-kheo tăng thượng mạn",
  "dhp149:1": "Như trái bầu mùa thu,",
  "dhp149:2": "Bị vất bỏ quăng đi,",
  "dhp149:3": "Nhóm xương trắng bồ câu,",
  "dhp149:4": "Thấy chúng còn vui gì?",
  "dhp150:0": "Chuyện Trưởng lão ni Rūpanandā (Sắc Khánh Hỷ)",
  "dhp150:1": "Thành này làm bằng xương,",
  "dhp150:2": "Quét tô bằng thịt máu,",
  "dhp150:3": "Ở đây già và chết,",
  "dhp150:4": "Mạn, lừa đảo chất chứa.",
  "dhp151:0": "Chuyện hoàng hậu Mallikā (Mạt-lỵ) băng hà",
  "dhp151:1": "Xe vua đẹp cũng già,",
  "dhp151:2": "Thân này rồi sẽ già.",
  "dhp151:3": "Pháp bậc thiện, không già.",
  "dhp151:4": "Như vậy bậc chí thiện Nói lên cho bậc thiện.",
  "dhp152:0": "Chuyện Trưởng lão Lāḷudāyī",
  "dhp152:1": "Người ít nghe kém học,",
  "dhp152:2": "Lớn già như trâu đực.",
  "dhp152:3": "Thịt nó tuy lớn lên,",
  "dhp152:4": "Nhưng tuệ không tăng trưởng.",
  "dhp153:0": "Chuyện Phật thành đạo",
  "dhp153:1": "Lang thang bao kiếp sống",
  "dhp153:2": "Ta tìm nhưng chẳng gặp,",
  "dhp153:3": "Người xây dựng nhà này,",
  "dhp153:4": "Khổ thay, phải tái sanh.",
  "dhp154:1": "Ôi! Người làm nhà kia Nay ta đã thấy ngươi!",
  "dhp154:2": "Người không làm nhà nữa.",
  "dhp154:3": "Ðòn tay ngươi bị gẫy,",
  "dhp154:4": "Kèo cột ngươi bị tan",
  "dhp154:5": "Tâm ta đạt tịch diệt,",
  "dhp154:6": "Tham ái thảy tiêu vong.",
  "dhp155:0": "Chuyện bá hộ Mahādhana (Cự Phú)",
  "dhp155:1": "Lúc trẻ, không phạm hạnh,",
  "dhp155:2": "Không tìm kiếm bạc tiền.",
  "dhp155:3": "Như cò già bên ao,",
  "dhp155:4": "Ủ rũ, không tôm cá.",
  "dhp156:1": "Lúc trẻ, không phạm hạnh,",
  "dhp156:2": "Không tìm kiếm bạc tiền.",
  "dhp156:3": "Như cây cung bị gãy,",
  "dhp156:4": "Thở than những ngày qua.",
  "dhp156:5": "Hết phẩm Già"
}
