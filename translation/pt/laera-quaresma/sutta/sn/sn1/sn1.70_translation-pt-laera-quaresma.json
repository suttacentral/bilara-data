{
  "sn1.70:0.1": "Saṃyutta Nikāya 1",
  "sn1.70:0.2": "7. Oprimido",
  "sn1.70:0.3": "70. O Mundo",
  "sn1.70:1.1": "\"Em que surgiu o mundo?",
  "sn1.70:1.2": "De que se aproxima?",
  "sn1.70:1.3": "Ao se apegar ao que",
  "sn1.70:1.4": "fica o mundo perturbado em que?\"",
  "sn1.70:2.1": "\"O mundo surgiu em seis.",
  "sn1.70:2.2": "Se aproxima de seis.",
  "sn1.70:2.3": "Ao se apegar a estes seis,",
  "sn1.70:2.4": "em seis fica o mundo fica perturbado.\""
}