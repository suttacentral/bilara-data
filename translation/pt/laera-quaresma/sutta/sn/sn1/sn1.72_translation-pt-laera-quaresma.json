{
  "sn1.72:0.1": "Saṃyutta Nikāya 1",
  "sn1.72:0.2": "8. Aniquilado",
  "sn1.72:0.3": "72. Uma Carruagem",
  "sn1.72:1.1": "\"O que caracteriza uma carruagem?",
  "sn1.72:1.2": "O que caracteriza o fogo?",
  "sn1.72:1.3": "O que caracteriza uma nação?",
  "sn1.72:1.4": "O que caracteriza uma mulher?\"",
  "sn1.72:2.1": "\"Um estandarte caracteriza uma carruagem.",
  "sn1.72:2.2": "A fumaça caracteriza o fogo.",
  "sn1.72:2.3": "Um governante caracteriza uma nação.",
  "sn1.72:2.4": "Um marido caracteriza uma mulher.\""
}