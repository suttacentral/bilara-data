{
  "sn1.15:0.1": "Saṃyutta Nikāya 1",
  "sn1.15:0.2": "2. O Jardim do Deleite",
  "sn1.15:0.3": "15. Sussurrando",
  "sn1.15:1.1": "\"Na quietude do meio-dia,",
  "sn1.15:1.2": "quando sossegam os pássaros,",
  "sn1.15:1.3": "a formidável floresta sussura para si própria:",
  "sn1.15:1.4": "quão assustador isso me parece!\"",
  "sn1.15:2.1": "\"Na quietude do meio-dia,",
  "sn1.15:2.2": "quando sossegam os pássaros,",
  "sn1.15:2.3": "a formidável floresta sussurra para si própria:",
  "sn1.15:2.4": "quão encantador isso me parece!\""
}