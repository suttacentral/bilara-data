{
  "sn1.65:0.1": "Saṃyutta Nikāya 1",
  "sn1.65:0.2": "7. Oprimido",
  "sn1.65:0.3": "65. Prisão",
  "sn1.65:1.1": "\"O que amarra o mundo?",
  "sn1.65:1.2": "O que o explora?",
  "sn1.65:1.3": "Com o abandono do que",
  "sn1.65:1.4": "são todas as amarras cortadas?\"",
  "sn1.65:2.1": "\"O deleite amarra o mundo.",
  "sn1.65:2.2": "O pensamento o explora.",
  "sn1.65:2.3": "Com o abandono do anseio",
  "sn1.65:2.4": "todas as amarras são cortadas.\""
}