{
  "sn1.66:0.1": "Saṃyutta Nikāya 1",
  "sn1.66:0.2": "7. Oprimido",
  "sn1.66:0.3": "66. Abatido",
  "sn1.66:1.1": "\"Pelo que é o mundo afligido?",
  "sn1.66:1.2": "Pelo que está cercado?",
  "sn1.66:1.3": "Que dardo o feriu?",
  "sn1.66:1.4": "Com o que está sempre fumegando?\"",
  "sn1.66:2.1": "\"O mundo é afligido pela morte.",
  "sn1.66:2.2": "Está cercado pela velhice.",
  "sn1.66:2.3": "O dardo do anseio o feriu.",
  "sn1.66:2.4": "Está sempre fumegando com desejo.\""
}