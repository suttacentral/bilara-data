{
  "thig1.4:0.1": "Canti delle Anziane Bhikkhunī",
  "thig1.4:0.2": "Capitolo degli Uno",
  "thig1.4:0.3": "Canto dell'Anziana Tissā",
  "thig1.4:1.1": "“Allenati, Tissā,",
  "thig1.4:1.2": "non farti sopraffare da alcun vincolo.",
  "thig1.4:1.3": "Libera da ogni vincolo, ",
  "thig1.4:1.4": "cammina nel mondo senza effusioni.”",
  "thig1.4:2.1": "L'Anziana Tissā"
}