{
  "thig1.7:0.1": "Canti delle Anziane Bhikkhunī",
  "thig1.7:0.2": "Capitolo degli Uno",
  "thig1.7:0.3": "Canto dell'Anziana Vīrā",
  "thig1.7:1.1": "\"Vīrā, dalle eroiche doti,",
  "thig1.7:1.2": "Bhikkhunī dalle facoltà mature,",
  "thig1.7:1.3": "porta il suo ultimo corpo,",
  "thig1.7:1.4": "dopo aver sconfitto Māra e il suo esercito.\"",
  "thig1.7:2.1": "L'Anziana Vīrā"
}