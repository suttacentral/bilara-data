{
  "thig1.8:0.1": "Canti delle Anziane Bhikkhunī",
  "thig1.8:0.2": "Capitolo degli Uno",
  "thig1.8:0.3": "Canto dell'Anziana Mittā",
  "thig1.8:1.1": "\"Andando avanti con convinzione,",
  "thig1.8:1.2": "Mittā, dedicati all'amicizia spirituale. ",
  "thig1.8:1.3": "Coltiva qualità virtuose,",
  "thig1.8:1.4": "per ottenere l'emancipazione da ogni vincolo.\"",
  "thig1.8:2.1": "L'Anziana Mittā"
}