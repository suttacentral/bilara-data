{
  "thig1.6:0.1": "Canti delle Anziane Bhikkhunī",
  "thig1.6:0.2": "Capitolo degli Uno",
  "thig1.6:0.3": "Canto dell'Anziana Dhīrā",
  "thig1.6:1.1": "“Dhīrā, realizza la cessazione,",
  "thig1.6:1.2": "la percezione di pace e felicità.",
  "thig1.6:1.3": "Realizza il Nirvana,",
  "thig1.6:1.4": "l'impareggiabile emancipazione da ogni vincolo.”",
  "thig1.6:2.1": "L'Anziana Dhīrā"
}