{
  "sn1.16:0.1": "Budini govori tematski grupisani 1.16 ",
  "sn1.16:0.2": "Nandana ",
  "sn1.16:0.3": "Pospanost i omamljenost ",
  "sn1.16:1.1": "„Pospanost, omamljenost i izležavanje, ",
  "sn1.16:1.2": "nezadovoljstvo i tromost posle jela; ",
  "sn1.16:1.3": "zbog svega toga ovde među bićima ",
  "sn1.16:1.4": "plemeniti put u senci ostaje.” ",
  "sn1.16:2.1": "„Pospanost, omamljenost i izležavanje, ",
  "sn1.16:2.2": "nezadovoljstvo i tromost posle jela; ",
  "sn1.16:2.3": "kad čovek sve to energično razveje, ",
  "sn1.16:2.4": "plemeniti put u punom sjaju blista.” "
}