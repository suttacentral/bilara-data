{
  "sn1.42:0.1": "Pagal temą susijusių suttų rinkinys 1 ",
  "sn1.42:0.2": "5. Liepsnoja ",
  "sn1.42:0.3": "42. Duodantis ką? ",
  "sn1.42:1.1": "„Duodantis ką dovanoja stiprumą? ",
  "sn1.42:1.2": "Duodantis ką dovanoja gražumą? ",
  "sn1.42:1.3": "Duodantis ką dovanoja patogumą? ",
  "sn1.42:1.4": "Duodantis ką dovanoja regėjimą? ",
  "sn1.42:1.5": "Kas yra dovanojantis viską? ",
  "sn1.42:1.6": "To paklaustas, pasakyki man.” ",
  "sn1.42:2.1": "„Duodantis maistą dovanoja stiprumą. ",
  "sn1.42:2.2": "Duodantis rūbą dovanoja gražumą. ",
  "sn1.42:2.3": "Padedantis keliauti dovanoja patogumą. ",
  "sn1.42:2.4": "Duodantis žibintą dovanoja regėjimą. ",
  "sn1.42:3.1": "Duoda viską ",
  "sn1.42:3.2": "dovanojantysis būstą. ",
  "sn1.42:3.3": "Nemirtingumą duoda ",
  "sn1.42:3.4": "pamokantysis _Dhammos_.” "
}